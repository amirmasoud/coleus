{
    "name": "cretueusebiu/laravel-vue-spa",
    "description": "A Laravel-Vue SPA starter project template.",
    "keywords": ["spa", "laravel", "vue"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.2",
        "cviebrock/laravel-elasticsearch": "^3.4",
        "fideloper/proxy": "^4.0",
        "folklore/graphql": "~1.0.0",
        "guzzlehttp/guzzle": "^6.3",
        "kalnoy/nestedset": "^4.3",
        "laravel/framework": "5.8.*",
        "laravel/tinker": "~1.0",
        "predis/predis": "^1.1",
        "spatie/laravel-backup": "^6.1",
        "spatie/laravel-cors": "^1.3",
        "spatie/laravel-medialibrary": "^7.5",
        "spatie/laravel-permission": "^2.36",
        "spatie/laravel-schemaless-attributes": "^1.4"
    },
    "require-dev": {
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "laravel/dusk": "^3.0",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/helper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
              "laravel/dusk"
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
