{
    "name": "cretueusebiu/laravel-vue-spa",
    "description": "A Laravel-Vue SPA starter project template.",
    "keywords": ["spa", "laravel", "vue"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.3.0",
        "algolia/algoliasearch-client-php": "^1.28",
        "backpack/MenuCRUD": "^1.0",
        "backpack/backupmanager": "^1.4",
        "backpack/crud": "^3.5",
        "backpack/logmanager": "^2.3",
        "backpack/pagemanager": "^1.1",
        "backpack/permissionmanager": "^4.0",
        "backpack/settings": "^2.1",
        "barryvdh/laravel-elfinder": "^0.4.1",
        "binarytorch/larecipe": "^1.2",
        "cviebrock/laravel-elasticsearch": "^3.4",
        "fideloper/proxy": "^4.0",
        "folklore/graphql": "~1.0.0",
        "guzzlehttp/guzzle": "^6.3",
        "itsgoingd/clockwork": "^3.1",
        "kalnoy/nestedset": "^4.3",
        "laravel/framework": "5.7.*",
        "laravel/scout": "^6.1",
        "laravel/socialite": "^3.0",
        "laravel/tinker": "~1.0",
        "overtrue/laravel-follow": "^1.1",
        "predis/predis": "^1.1",
        "sentry/sentry-laravel": "^0.10.0",
        "spatie/laravel-backup": "^5.10",
        "spatie/laravel-cors": "^1.3",
        "spatie/laravel-medialibrary": "^7.5",
        "torann/geoip": "^1.0",
        "tymon/jwt-auth": "^1.0.0-rc.2"
    },
    "require-dev": {
        "backpack/generators": "^1.2",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "laracasts/generators": "dev-master",
        "laravel/dusk": "^3.0",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^7.0",
        "spatie/phpunit-watcher": "^1.6"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/helper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
              "laravel/dusk"
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate",
            "@php artisan jwt:secret --force"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
