{"text": {"0": {"m1": "\u0627\u06cc \u0634\u0628 \u0642\u062f\u0631 \u0648 \u0627\u06cc \u062e\u0632\u0627\u0646\u0647\u0654 \u0627\u062c\u0631", "m2": "\u0648 \u0633\u0644\u0627\u0645 \u0639\u0644\u06cc\u06a9 \u062d\u062a\u06cc\u200c\u0627\u0644\u0641\u062c\u0631"}, "1": {"m1": "\u0631\u062e \u062e\u0648\u0628\u062a \u0628\u0646\u0627\u06a9\u0646\u0646\u062f\u0647\u0654 \u0648\u0635\u0644", "m2": "\u0633\u0631 \u0632\u0644\u0641\u062a \u0641\u0646\u0627\u06a9\u0646\u0646\u062f\u0647\u0654 \u0647\u062c\u0631"}, "2": {"m1": "\u062f\u0644 \u067e\u0627\u06a9\u062a \u0632\u0644\u0627\u0644 \u0686\u0634\u0645\u0647\u0654 \u0641\u06cc\u0636", "m2": "\u062f\u0645 \u0635\u0628\u062d\u062a \u06a9\u0644\u06cc\u062f \u0645\u062e\u0632\u0646 \u0627\u062c\u0631"}, "3": {"t2": "\u0628\u0647\u062a\u0631 \u0627\u0631 \u0635\u062f\u0647\u0632\u0627\u0631 \u0645\u0627\u0647\u06cc \u062a\u0648", "t1": "\u0646\u0648\u0631 \u0628\u062e\u0634\u06cc \u0648 \u0631\u0646\u062c \u06a9\u0627\u0647\u06cc \u062a\u0648"}, "4": {"m1": "\u0634\u062f\u0645 \u0627\u0632 \u0641\u0631\u0642\u062a\u062a \u0628\u062a\u0627\u0628\u200c\u060c \u0627\u06cc \u0634\u0628", "m2": "\u0627\u0632\u0645\u0646 \u062e\u0633\u062a\u0647 \u0631\u062e \u0645\u062a\u0627\u0628 \u0627\u06cc \u0634\u0628"}, "5": {"m1": "\u0645\u0637\u0644\u0639 \u062f\u06cc\u062f\u0647\u200c\u0627\u0645 \u0632 \u062f\u06cc\u062f\u0646 \u062a\u0648", "m2": "\u0634\u062f\u0647 \u067e\u0631\u0646\u0648\u0631 \u0622\u0641\u062a\u0627\u0628 \u0627\u06cc \u0634\u0628"}, "6": {"m1": "\u0648\u0635\u0644\u062a\u200c\u0627\u06cc\u200c\u0634\u0628\u200c\u0628\u0647\u200c\u0645\u0646\u200c\u0634\u062f\u0647\u200c\u0627\u0633\u062a\u200c\u062d\u0644\u0627\u0644", "m2": "\u06af\u0634\u062a\u0647 \u0628\u0631\u0645\u0646 \u062d\u0631\u0627\u0645 \u062e\u0648\u0627\u0628 \u0627\u06cc \u0634\u0628"}, "7": {"t2": "\u06a9\u0644 \u0646\u0648\u0645 \u0639\u0644\u06cc \u0627\u0644\u0645\u062d\u0628 \u062d\u0631\u0627\u0645", "t1": "\u0639\u062c\u0628\u0627 \u0644\u0644\u0645\u062d\u0628 \u06a9\u06cc\u0641 \u06cc\u0646\u0627\u0645"}, "8": {"m1": "\u062f\u0627\u0634\u062a\u0645 \u062f\u0631 \u062f\u0644 \u0627\u06cc \u0634\u0628 \u0637\u0646\u0627\u0632", "m2": "\u0622\u0631\u0632\u0648\u06cc \u062a\u0648 \u062f\u0631 \u0634\u0628\u0627\u0646 \u062f\u0631\u0627\u0632"}, "9": {"m1": "\u0647\u062c\u0631\u062a \u0627\u0645\u0634\u0628 \u06a9\u0634\u06cc\u062f\u0647 \u067e\u0631\u062f\u0647 \u0628\u0647 \u0631\u0648\u06cc", "m2": "\u0648\u0635\u0644\u062a\u200c\u0627\u0645\u0634\u0628 \u06af\u0634\u0627\u062f\u0647 \u0686\u0647\u0631\u0647 \u0628\u0647 \u0646\u0627\u0632"}, "10": {"m1": "\u0641\u062e\u0631\u062a\u200c\u0627\u06cc\u0646 \u0628\u0633 \u0628\u0647\u200c\u0631\u0648\u0632\u0647\u0627\u06a9\u0647 \u0634\u062f\u06cc", "m2": "\u0634\u0628 \u0645\u06cc\u0644\u0627\u062f \u0633\u0628\u0637 \u0634\u0627\u0647 \u062d\u062c\u0627\u0632"}, "11": {"t2": "\u0628\u0648\u0627\u0644\u062d\u0633\u0646 \u0645\u0627\u0644\u06a9\u200c\u0627\u0644\u0631\u0642\u0627\u0628 \u0645\u0644\u0648\u06a9", "t1": "\u067e\u0627\u062f\u0634\u0627\u0647 \u0635\u0641\u0627 \u0648 \u0633\u06cc\u0631 \u0648 \u0633\u0644\u0648\u06a9"}, "12": {"m1": "\u0622\u0646 \u06a9\u0647 \u0628\u062f\u0631\u0627\u0644\u0628\u062f\u0648\u0631 \u0627\u06cc\u0642\u0627\u0646 \u0627\u0633\u062a", "m2": "\u0648\u0627\u0646 \u06a9\u0647 \u0634\u0645\u0633\u200c\u0627\u0644\u0634\u0645\u0648\u0633\u200c \u0639\u0631\u0641\u0627\u0646\u0633\u062a"}, "13": {"m1": "\u062f\u0631 \u0634\u0631\u06cc\u0639\u062a \u0642\u0628\u0648\u0644 \u062d\u0636\u0631\u062a \u0627\u0648", "m2": "\u0647\u0633\u062a \u0641\u0631\u0639\u06cc \u06a9\u0647 \u0627\u0635\u0644 \u0627\u06cc\u0645\u0627\u0646\u0633\u062a"}, "14": {"m1": "\u0641\u0631\u0634 \u0627\u06cc\u0648\u0627\u0646\u0634 \u0639\u0631\u0634\u200c\u062a\u0642\u062f\u06cc\u0633 \u0627\u0633\u062a", "m2": "\u062e\u0627\u06a9 \u062f\u0631\u06af\u0627\u0647\u0634 \u0622\u0628 \u062d\u06cc\u0648\u0627\u0646\u0633\u062a"}, "15": {"t2": "\u0648 \u0645\u0646 \u0627\u0644\u0645\u0627\u0621 \u06a9\u0644 \u0634\u06cc\u0626\u06cc \u062d\u06cc", "t1": "\u0632\u0622\u0628 \u062d\u06cc\u0648\u0627\u0646\u0634 \u062d\u06cc \u0634\u0648\u062f \u0644\u0627\u0634\u06cc"}, "16": {"m1": "\u0647\u0633\u062a\u06cc \u0627\u0648 \u0627\u0633\u0627\u0633 \u062a\u062c\u0631\u06cc\u062f \u0627\u0633\u062a", "m2": "\u0631\u0648\u0634 \u0627\u0648 \u0627\u0635\u0648\u0644 \u062a\u0648\u062d\u06cc\u062f \u0627\u0633\u062a"}, "17": {"m1": "\u062d\u06a9\u0645\u062a\u200c\u0634\u0631\u0639 \u0648 \u062f\u06cc\u0646 \u0628\u0647\u200c\u0641\u06a9\u0631\u062a \u0627\u0648\u0633\u062a", "m2": "\u0641\u06a9\u0631\u062a \u062f\u06cc\u06af\u0631\u0627\u0646 \u0628\u0647 \u062a\u0642\u0644\u06cc\u062f \u0627\u0633\u062a"}, "18": {"m1": "\u06af\u0646\u062c\u0647\u200c\u0627\u06cc \u0628\u0637\u0648\u0646 \u0642\u0631\u0622\u0646 \u0631\u0627", "m2": "\u067e\u0646\u062c \u0627\u0646\u06af\u0634\u062a \u0627\u0648 \u0645\u0642\u0627\u0644\u06cc\u062f \u0627\u0633\u062a"}, "19": {"t2": "\u0642\u0635\u062f\u0634 \u0627\u06cc\u0646 \u06af\u0646\u062c \u067e\u0627\u06a9 \u0639\u0631\u0641\u0627\u0646 \u0628\u0648\u062f", "t1": "\u06a9\u0646\u062a \u06a9\u0646\u0632\u0627 \u06a9\u0647 \u06a9\u0631\u062f\u06af\u0627\u0631 \u0633\u0631\u0648\u062f"}, "20": {"m1": "\u0645\u0646 \u06a9\u0647 \u062f\u0631 \u0628\u0646\u062f\u06af\u06cc\u0634 \u062f\u0644\u0634\u0627\u062f\u0645", "m2": "\u0627\u0632 \u063a\u0645 \u0647\u0633\u062a \u0648 \u0646\u06cc\u0633\u062a \u0622\u0632\u0627\u062f\u0645"}, "21": {"m1": "\u0646\u06cc\u0633\u062a\u0645 \u0645\u0628\u0631\u0645 \u0648 \u062d\u0633\u0648\u062f \u0648 \u0644\u0626\u06cc\u0645", "m2": "\u0648\u0627\u0633\u0639\u200c\u0627\u0644\u0635\u062f\u0631 \u0648 \u0645\u0634\u0641\u0642 \u0648 \u0631\u0627\u062f\u0645"}, "22": {"m1": "\u06af\u0631 \u0632 \u06a9\u06cc\u062f \u0632\u0645\u0627\u0646\u0647 \u062f\u0631 \u0631\u0646\u062c\u0645", "m2": "\u0646\u06cc\u0633\u062a \u063a\u0645 \u06a9\u0632 \u0648\u0644\u0627\u06cc \u0627\u0648 \u0634\u0627\u062f\u0645"}, "23": {"t2": "\u0628\u0627 \u0648\u0644\u0627\u06cc \u0631\u0636\u0627 \u0648\u0644\u06cc\u200c\u0627\u0644\u0644\u0647", "t1": "\u06a9\u0648\u0647 \u067e\u06cc\u0634 \u0645\u0646 \u0627\u0633\u062a \u0647\u0645\u0633\u0631 \u06a9\u0627\u0647"}, "24": {"m1": "\u0647\u0631\u0686\u0647 \u06af\u0648\u06cc\u0646\u062f \u062e\u0644\u0642 \u062f\u0627\u0645\u0646 \u0686\u0627\u06a9", "m2": "\u0645\u0646 \u06a9\u0647 \u062f\u0627\u0631\u0645 \u062a\u0648 \u0631\u0627\u061b \u0646\u062f\u0627\u0631\u0645 \u0628\u0627\u06a9"}, "25": {"m1": "\u0645\u0646 \u0648 \u062f\u0648\u0631\u06cc \u0632 \u062f\u0631\u06af\u0647\u062a \u0647\u0631\u06af\u0632!!", "m2": "\u0645\u0646\u200c \u0648 \u0647\u062c\u0631\u0627\u0646 \u0632 \u062d\u0636\u0631\u062a\u062a \u062d\u0627\u0634\u0627\u06a9\u200c!!"}, "26": {"m1": "\u062f\u0631 \u0631\u0647\u062a \u062e\u0627\u06a9\u200c \u0627\u06af\u0631 \u0634\u0648\u0645 \u063a\u0645 \u0646\u06cc\u0633\u062a", "m2": "\u0647\u0631\u06a9\u0647 \u0627\u0632 \u062e\u0627\u06a9 \u0632\u0627\u062f \u06af\u0631\u062f\u062f \u062e\u0627\u06a9"}, "27": {"t2": "\u062e\u0627\u0635\u0647 \u062f\u0631\u0627\u06cc\u0646\u0686\u0646\u06cc\u0646 \u0634\u0628\u06cc \u0641\u06cc\u0631\u0648\u0632", "t1": "\u0645\u0646 \u062a\u0648 \u0631\u0627 \u0645\u0646\u0642\u0628\u062a \u06a9\u0646\u0645 \u0634\u0628 \u0648 \u0631\u0648\u0632"}, "28": {"m1": "\u062a\u0627\u06a9\u0647 \u0627\u0641\u0644\u0627\u06a9 \u0631\u0627 \u0628\u0642\u0627 \u0628\u06cc\u0646\u0645", "m2": "\u0627\u06cc\u0646 \u0645\u0647\u06cc\u0646 \u062c\u0634\u0646 \u0631\u0627 \u0628\u067e\u0627 \u0628\u06cc\u0646\u06cc\u0645"}, "29": {"m1": "\u0686\u0648\u0646 \u062f\u0631 \u0627\u0633\u062a\u0645 \u067e\u06cc \u062b\u0646\u0627\u06cc \u0627\u0645\u0627\u0645", "m2": "\u067e\u0627\u06cc \u062e\u0648\u062f \u0628\u0631 \u0633\u0631 \u0633\u0647\u0627 \u0628\u06cc\u0646\u0645"}, "30": {"m1": "\u0645\u06cc\u0647\u0645\u0627\u0646\u0627\u0646 \u0648 \u0645\u06cc\u0632\u0628\u0627\u0646\u0627\u0646 \u0631\u0627", "m2": "\u0628\u0631\u06a9\u0646\u0627\u0631 \u0627\u0632 \u063a\u0645 \u0648 \u0628\u0644\u0627 \u0628\u06cc\u0646\u0645"}, "31": {"t2": "\u0628\u0647 \u0639\u0644\u06cc \u0648 \u0622\u0644\u0647 \u0627\u0644\u0627\u0646\u062c\u0627\u0628", "t1": "\u0634\u0627\u062f\u0645\u0627\u0646 \u0628\u0646\u06af\u0631\u0645 \u062f\u0644 \u0627\u062d\u0628\u0627\u0628"}}}