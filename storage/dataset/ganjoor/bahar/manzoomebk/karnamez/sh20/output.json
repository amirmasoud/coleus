{"text": {"0": {"m1": "\u0628\u0648\u062f \u0645\u0631\u062f\u06cc \u0632 \u0647\u0631 \u0647\u0646\u0631 \u0639\u0627\u0631\u06cc", "m2": "\u0631\u0648\u0632\u06a9\u06cc \u0686\u0646\u062f \u06a9\u0631\u062f\u0647 \u0646\u062c\u0651\u0627\u0631\u06cc"}, "1": {"m1": "\u0646\u0627\u0645 \u0631\u0646\u062f\u0647 \u0634\u0646\u06cc\u062f\u0647 \u0648 \u06af\u0648\u0646\u06cc\u0627", "m2": "\u06af\u0650\u0631\u062f \u0627\u0632 \u0646\u06cc\u0645\u06af\u0650\u0631\u062f \u06a9\u0631\u062f\u0647 \u062c\u062f\u0627"}, "2": {"m1": "\u067e\u0633 \u0634\u062f \u0627\u0646\u062f\u0631 \u062f\u06a9\u0627\u0646 \u0622\u0647\u0646\u06af\u0631", "m2": "\u062f\u0645 \u0648 \u062e\u0627\u06cc\u0633\u06a9\u200c \u062f\u06cc\u062f \u0648 \u067e\u062a\u06a9 \u0648 \u062a\u0628\u0631"}, "3": {"m1": "\u0646\u0648\u0632 \u0646\u0627\u0645\u062e\u062a\u0647 \u0686\u06cc\u0632\u06cc \u0627\u0632 \u0627\u0633\u062a\u0627\u062f", "m2": "\u0631\u06cc\u0634 \u062e\u0648\u062f \u0631\u0627 \u0628\u0647\u200c\u062f\u0633\u062a \u0628\u0646\u0627 \u062f\u0627\u062f"}, "4": {"m1": "\u0645\u0627\u0644\u0647 \u0648 \u0686\u0648\u0628 \u06a9\u0627\u0631 \u0647\u0634\u062a\u0647 \u0628\u0647 \u06a9\u0648\u0644", "m2": "\u062f\u06cc\u062f\u0647 \u06af\u0686\u06a9\u0648\u0628 \u0648 \u062a\u06cc\u0634\u0647 \u0648 \u0634\u0627\u063a\u0648\u0644"}, "5": {"m1": "\u0632\u0627\u0646 \u0633\u067e\u0633 \u0634\u062f \u0628\u0647 \u062f\u06a9\u0647\u0654 \u062e\u06cc\u0627\u0637", "m2": "\u0628\u0627 \u062f\u0644\u06cc \u062a\u0646\u06af\u200c\u062a\u0631 \u0632 \u0633\u0645 \u062e\u06cc\u0627\u0637"}, "6": {"m1": "\u0646\u062e \u0648\u0633\u0648\u0632\u0646 \u0628\u062f\u06cc\u062f \u0648\u06a9\u0648\u06a9 \u0648 \u0631\u0641\u0648", "m2": "\u062f\u0631\u0632 \u0648 \u062f\u0648\u0632 \u0648 \u0642\u0648\u0627\u0631\u0647 \u0648 \u0627\u0644\u06af\u0648"}, "7": {"m1": "\u0686\u0648\u0646\u200c \u062f\u0631 \u0622\u0646\u200c \u067e\u06cc\u0634\u0647\u200c \u062f\u06cc\u062f \u0633\u0633\u062a\u06cc \u062e\u0648\u06cc\u0634", "m2": "\u067e\u06cc\u0634\u0647 \u062f\u06cc\u06af\u0631\u06cc \u06af\u0631\u0641\u062a \u0628\u0647 \u067e\u06cc\u0634"}, "8": {"m1": "\u0647\u06cc\u0686\u200c \u06cc\u06a9 \u0631\u0627 \u0628\u0647\u200c \u0633\u0631 \u0646\u0628\u0631\u062f \u062a\u0645\u0627\u0645", "m2": "\u062f\u06cc\u06af\u200c \u062f\u0631 \u062f\u06cc\u06af\u200c \u0634\u062f \u0686\u0648 \u06a9\u0644\u0647\u0654 \u062e\u0627\u0645"}, "9": {"m1": "\u06af\u0634\u062a\u200c \u0631\u06cc\u0634\u0634\u200c \u062f\u0648 \u0645\u0648\u06cc\u200c \u0648 \u067e\u06cc\u0632\u06cc\u200c \u0633\u0633\u062a", "m2": "\u0645\u0632\u062f \u0627\u0648 \u0644\u06cc\u06a9 \u0647\u0645\u0686\u0648 \u0631\u0648\u0632 \u0646\u062e\u0633\u062a"}, "10": {"m1": "\u062e\u0648\u06cc\u0634 \u0631\u0627 \u0646\u0648\u0628\u062a\u06cc \u062f\u0631 \u0622\u06cc\u0646\u0647 \u062f\u06cc\u062f", "m2": "\u0627\u0628\u0644\u0647\u0627\u0646\u0647 \u0628\u0647 \u0631\u06cc\u0634 \u062e\u0648\u062f \u062e\u0646\u062f\u06cc\u062f"}, "11": {"m1": "\u06af\u0641\u062a \u0627\u0632\u06cc\u0646 \u0634\u0647\u0631 \u0631\u062e\u062a \u0628\u0627\u06cc\u062f \u0628\u0633\u062a", "m2": "\u063a\u0631\u0628\u062a\u06cc \u062c\u0633\u062a \u0648 \u0644\u0627\u0641 \u062f\u0631 \u067e\u06cc\u0648\u0633\u062a"}, "12": {"m1": "\u0628\u0648\u062f \u0622\u0628\u0627\u062f\u06cc\u0626\u06cc \u0628\u0647 \u0634\u0647\u0631 \u0642\u0631\u06cc\u0628", "m2": "\u0631\u062e\u062a \u0622\u0646\u062c\u0627 \u06a9\u0634\u06cc\u062f \u0645\u0631\u062f \u063a\u0631\u06cc\u0628"}, "13": {"m1": "\u0628\u0648\u062f \u062f\u0631 \u0642\u0631\u06cc\u0647 \u0686\u0646\u062f \u0627\u0633\u062a\u0627\u06a9\u0627\u0631", "m2": "\u06af\u0634\u062a\u0647 \u0647\u0631\u06cc\u06a9 \u0628\u0647 \u06a9\u0627\u0631\u062e\u0648\u06cc\u0634 \u0633\u0648\u0627\u0631"}, "14": {"m1": "\u0631\u0641\u062a \u0622\u0646\u062c\u0627 \u0628\u0647 \u06af\u0648\u0634\u0647\u200c\u0627\u06cc \u0628\u062e\u0632\u06cc\u062f", "m2": "\u0627\u0646\u0632\u0648\u0627\u06cc\u06cc \u0628\u062e\u0648\u0631\u062f \u062e\u0648\u0628\u0634 \u06af\u0632\u06cc\u062f"}, "15": {"m1": "\u067e\u06cc\u0634 \u06af\u0650\u0644 \u06a9\u0627\u0631\u06af\u0641\u062a \u0646\u062c\u0651\u0627\u0631\u0645", "m2": "\u0632 \u0627\u0631\u0647 \u0648 \u0631\u0646\u062f\u0647 \u0645\u0639\u0631\u0641\u062a \u062f\u0627\u0631\u0645"}, "16": {"m1": "\u067e\u06cc\u0634 \u0646\u062c\u0651\u0627\u0631 \u06af\u0641\u062a \u0628\u0646\u0651\u0627\u06cc\u0645", "m2": "\u0637\u0627\u0642\u200c\u0628\u0646\u062f \u0648\u06af\u0644\u0648\u06cc\u06cc \u0622\u0631\u0627\u06cc\u0645"}, "17": {"m1": "\u06af\u0641\u062a \u0645\u0646 \u062f\u0631\u0632\u06cc\u0645 \u0628\u0647 \u0622\u0647\u0646\u06af\u0631", "m2": "\u06af\u0641\u062a\u200c \u0622\u0647\u0646\u06af\u0631\u0645\u200c \u0628\u0647 \u0645\u0631\u062f \u062f\u06af\u0631"}, "18": {"m1": "\u062a\u0627 \u06a9\u0647 \u0627\u0628\u0632\u0627\u0631\u06a9\u0627\u0631 \u0633\u0627\u0632\u062f \u0631\u0627\u0633\u062a", "m2": "\u0632\u0627\u0646\u200c \u0641\u0642\u06cc\u0631\u0627\u0646 \u0628\u0647 \u0648\u0627\u0645 \u0686\u06cc\u0632\u06cc \u062e\u0648\u0627\u0633\u062a"}, "19": {"m1": "\u0627\u0635\u0637\u0644\u0627\u062d\u0627\u062a \u062e\u0648\u06cc\u0634 \u0631\u0627 \u0628\u0641\u0631\u0648\u062e\u062a", "m2": "\u0686\u0646\u062f \u063a\u0627\u0632\u06cc \u0628\u0647\u200c\u0686\u0646\u062f \u0631\u0648\u0632 \u0627\u0646\u062f\u0648\u062e\u062a"}, "20": {"m1": "\u0686\u0646\u062f \u0645\u0627\u0647\u06cc \u0632 \u0641\u0636\u0644 \u06a9\u0644\u0651\u0627\u0634\u06cc", "m2": "\u0686\u0631\u0686\u0631\u06cc \u06a9\u0631\u062f \u0645\u0631\u062f\u06a9 \u0646\u0627\u0634\u06cc"}, "21": {"m1": "\u062a\u0627 \u06a9\u0647 \u0631\u0648\u0632\u06cc \u0642\u0636\u0627\u06cc \u0628\u06cc\u200c\u0628\u0631\u06a9\u062a", "m2": "\u062f\u0627\u062f\u0634 \u0627\u0632\u06a9\u0646\u062c \u0627\u0646\u0632\u0648\u0627 \u062d\u0631\u06a9\u062a"}, "22": {"m1": "\u0628\u062e\u062a \u0634\u0648\u0631\u06cc\u062f\u0647 \u0631\u0647\u0646\u0645\u0627\u06cc\u0634 \u06af\u0634\u062a", "m2": "\u0631\u0648\u0632 \u062c\u0645\u0639\u0647 \u0628\u0647 \u0642\u0647\u0648\u0647\u200c\u062e\u0627\u0646\u0647 \u06af\u0630\u0634\u062a"}, "23": {"m1": "\u0633\u0627\u06cc\u0647\u0654 \u0628\u06cc\u062f \u0648 \u0686\u0634\u0645\u0647\u0654 \u062c\u0627\u0631\u06cc", "m2": "\u0631\u0648\u0632 \u0622\u062f\u06cc\u0646\u0647 \u0648\u0642\u062a \u0628\u06cc\u06a9\u0627\u0631\u06cc"}, "24": {"m1": "\u0627\u0648\u0633\u062a\u0627\u062f\u0627\u0646 \u062f\u06cc\u0647 \u0648 \u0628\u0631\u0632\u06af\u0631\u0627\u0646", "m2": "\u067e\u06cc\u0634 \u0686\u0627\u06cc\u200c \u0648 \u0686\u067e\u0642\u200c\u060c \u062e\u0648\u0631\u0627\u0646 \u0648 \u0686\u0631\u0627\u0646"}, "25": {"m1": "\u0686\u0634\u0645\u0634\u0627\u0646 \u0686\u0648\u0646 \u0628\u0647 \u0627\u0648\u0633\u062a\u0627\u062f \u0627\u0641\u062a\u0627\u062f", "m2": "\u0645\u0647\u0631 \u062f\u06cc\u0631\u06cc\u0646\u0647 \u06cc\u200c\u0634\u0627\u0646 \u0628\u0647 \u06cc\u0627\u062f \u0627\u0641\u062a\u0627\u062f"}, "26": {"m1": "\u0622\u0646\u200c\u06cc\u06a9\u06cc \u0646\u0632\u062f \u062e\u0648\u06cc\u0634 \u062c\u0627\u06cc\u0634 \u06a9\u0631\u062f", "m2": "\u0648\u06cc\u0646\u200c\u062f\u06af\u0631 \u0645\u06cc\u0647\u0645\u0627\u0646 \u0628\u0647\u200c \u0686\u0627\u06cc\u0634 \u06a9\u0631\u062f"}, "27": {"m1": "\u06af\u0641\u062a \u0628\u0646\u0627 \u0647\u0646\u0648\u0632 \u0628\u06cc\u06a9\u0627\u0631\u06cc", "m2": "\u06a9\u06cc \u06a9\u0633\u06cc \u0631\u0627\u0633\u062a \u0634\u063a\u0644 \u0646\u062c\u0627\u0631\u06cc\u200c\u061f"}, "28": {"m1": "\u06af\u0641\u062a \u0646\u062c\u0651\u0627\u0631: \u06a9\u0627\u0648 \u0646\u0647 \u0646\u062c\u0651\u0627\u0631 \u0627\u0633\u062a", "m2": "\u0627\u0648\u0633\u062a \u0628\u0646\u0627 \u0648 \u0628\u0627 \u062a\u0648 \u0647\u0645\u06a9\u0627\u0631 \u0627\u0633\u062a"}, "29": {"m1": "\u06af\u0641\u062a \u062e\u06cc\u0627\u0637 \u06a9\u0627\u0648\u0633\u062a \u0622\u0647\u0646\u06af\u0631", "m2": "\u06af\u0641\u062a \u0622\u0647\u0646\u06af\u0631 \u0627\u0648\u0633\u062a \u0633\u0648\u0632\u0646\u06af\u0631"}, "30": {"m1": "\u0686\u0648\u0646 \u0647\u0645\u06cc \u0634\u062f \u0633\u0648\u0627\u0644\u200c\u0647\u0627 \u062a\u06a9\u0631\u06cc\u0631", "m2": "\u0645\u0631\u062f\u06a9 \u0627\u0632 \u0634\u0631\u0645 \u0633\u0631 \u0641\u06a9\u0646\u062f \u0628\u0647 \u0632\u06cc\u0631"}, "31": {"m1": "\u06af\u0634\u062a\u200c\u0641\u0636\u0644 \u062d\u06a9\u06cc\u0645 \u0635\u0627\u062d\u0628\u200c\u060c \u0641\u0627\u0634", "m2": "\u0634\u062f \u0647\u0648\u06cc\u062f\u0627 \u06a9\u0647 \u0646\u06cc\u0633\u062a \u062c\u0632 \u0642\u0644\u0651\u0627\u0634"}, "32": {"m1": "\u0644\u0627\u062c\u0631\u0645 \u0647\u0645\u0686\u0648 \u0633\u06af \u062f\u0648\u0627\u0646\u062f\u0646\u062f\u0634", "m2": "\u06a9\u064e\u0648 \u0628\u0647 \u06a9\u0648\u0646\u0634 \u0632\u062f\u0646\u062f \u0648 \u0631\u0627\u0646\u062f\u0646\u062f\u0634"}, "33": {"m1": "\u0647\u0645\u0647 \u062f\u0627\u0646\u0633\u062a \u06a9\u0627\u0648\u0633\u062a \u0647\u06cc\u0686 \u0645\u062f\u0627\u0646\u200c", "m2": "\u0639\u0627\u0642\u0628\u062a \u0631\u0641\u062a \u0648 \u0645\u0631\u062f \u062f\u0631 \u0647\u0645\u062f\u0627\u0646"}, "34": {"m1": "\u0622\u0646 \u06a9\u0647 \u0627\u0632 \u0647\u0631 \u062f\u0631\u06cc \u0633\u062e\u0646 \u0631\u0627\u0646\u062f", "m2": "\u062e\u0648\u0628\u0634 \u0631\u0627 \u0645\u0631\u062f \u0630\u0648\u0641\u0646\u0648\u0646 \u062e\u0648\u0627\u0646\u062f"}, "35": {"m1": "\u06cc\u0627 \u0628\u0648\u062f \u0627\u0632 \u0646\u0648\u0627\u062f\u0631 \u062f\u0648\u0631\u0627\u0646", "m2": "\u06a9\u06cc\u0645\u06cc\u0627\u0633\u0627\u0646 \u0632 \u0686\u0634\u0645 \u062e\u0644\u0642 \u0646\u0647\u0627\u0646"}, "36": {"m1": "\u0646\u0627\u062f\u0631 \u0648 \u0634\u0627\u0630 \u0628\u0627\u0634\u062f \u0627\u06cc\u0646 \u0627\u0633\u062a\u0627\u0630", "m2": "\u062d\u06a9\u0645 \u0646\u0628\u0648\u062f \u0631\u0648\u0627 \u0628\u0647 \u0646\u0627\u062f\u0631 \u0648 \u0634\u0627\u0630"}, "37": {"m1": "\u06cc\u0627\u06a9\u0647 \u0627\u0648 \u0645\u0631\u062f \u0631\u0646\u062f \u0648 \u0639\u06cc\u0627\u0631 \u0627\u0633\u062a", "m2": "\u0627\u0635\u0637\u0644\u0627\u062d\u0627\u062a \u06af\u0641\u062a\u0646\u0634 \u06a9\u0627\u0631 \u0627\u0633\u062a"}, "38": {"m1": "\u062e\u0648\u06cc\u0634 \u0631\u0627 \u062f\u0631 \u0645\u062d\u0627\u0641\u0644 \u0639\u0627\u0645\u0647", "m2": "\u062e\u0648\u0627\u0646\u062f\u0647 \u0627\u0633\u062a\u0627\u062f \u0648 \u0641\u062d\u0644 \u0648 \u0639\u0644\u0627\u0645\u0647"}, "39": {"m1": "\u0686\u0648\u0646 \u0628\u0631\u0627\u0628\u0631 \u0634\u0648\u062f \u0628\u0647 \u0627\u0633\u062a\u0627\u062f\u0627\u0646", "m2": "\u0647\u0645\u0647 \u062f\u0627\u0646\u0646\u062f \u06a9\u0627\u0648 \u0628\u0648\u062f \u0646\u0627\u062f\u0627\u0646"}}}