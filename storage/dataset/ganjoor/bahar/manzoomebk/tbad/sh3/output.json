{"text": {"0": {"m1": "\u0628\u0648\u062f \u0628\u0627 \u0627\u0647\u0631\u06cc\u0645\u0646\u0627\u0646 \u062f\u0627\u0646\u0634\u200c\u0641\u0632\u0648\u0646", "m2": "\u067e\u062e\u062a\u0646 \u0648 \u0645\u0639\u0645\u0627\u0631\u06cc \u0648 \u0631\u0645\u06cc \u0648 \u0641\u0633\u0648\u0646"}, "1": {"m1": "\u062e\u0637 \u0648 \u0631\u0633\u0645 \u0648\u067e\u0648\u0634\u0634 \u0648 \u0628\u0627\u0641\u0646\u062f\u06af\u06cc", "m2": "\u067e\u0627\u06cc \u06a9\u0648\u0628\u06cc\u200c\u060c \u0645\u06cc \u06a9\u0634\u06cc\u200c\u060c \u062e\u0648\u0627\u0646\u0646\u062f\u06af\u06cc"}, "2": {"t2": "\u0628\u0627 \u062f\u06af\u0631 \u0639\u0644\u0645 \u0648 \u0641\u0646\u0648\u0646", "t1": "\u0628\u0627 \u062f\u06af\u0631 \u0639\u0644\u0645 \u0648 \u0641\u0646\u0648\u0646"}, "3": {"m1": "\u0686\u0647\u0631 \u0622\u0646\u0627\u0646 \u0633\u0631 \u0628\u0633\u0631 \u0628\u06cc\u200c\u0645\u0648\u06cc \u0628\u0648\u062f", "m2": "\u0646\u0633\u0644\u200c \u0632\u06cc\u0628\u0627 \u0631\u0648\u06cc \u0646\u0627\u062e\u0648\u0634 \u062e\u0648\u06cc \u0628\u0648\u062f"}, "4": {"m1": "\u0645\u0631\u062f \u0648 \u0632\u0646 \u0632\u06cc\u0628\u0627 \u0631\u062e \u0648 \u0633\u06cc\u0645\u06cc\u0646\u0647\u200c\u062a\u0646", "m2": "\u0632\u0646 \u0686\u0648 \u0645\u0631\u062f\u0627\u0646\u200c\u0633\u0627\u062f\u0647\u200c\u0648\u0645\u0631\u062f\u0627\u0646\u200c\u0686\u0648 \u0632\u0646"}, "5": {"t2": "\u062c\u0645\u0644\u0647 \u0628\u0627 \u0645\u06a9\u0631 \u0648 \u0641\u0633\u0648\u0646", "t1": "\u062c\u0645\u0644\u0647 \u0628\u0627 \u0645\u06a9\u0631 \u0648 \u0641\u0633\u0648\u0646"}, "6": {"m1": "\u0627\u0635\u0644\u0634\u0627\u0646\u200c\u0627\u0641\u0631\u0627\u0634\u062a\u0647\u200c\u060c \u0644\u06cc\u06a9\u0646 \u062f\u06cc\u0648\u062e\u0648\u06cc", "m2": "\u0628\u06cc\u0648\u0641\u0627 \u0637\u0628\u0639 \u0648 \u0647\u0648\u0633\u0631\u0627\u0646 \u0648 \u062f\u0648\u0631\u0648\u06cc"}, "7": {"m1": "\u062a\u0646\u062f\u062d\u0633 \u0648 \u0632\u0648\u062f \u0631\u0646\u062c \u0648 \u06af\u0631\u0645\u200c\u062c\u0648\u0634", "m2": "\u0628\u06cc\u200c\u062a\u0641\u06a9\u0631\u060c \u06a9\u0645\u200c\u062e\u0631\u062f\u060c \u0628\u0633\u06cc\u0627\u0631\u0647\u0648\u0634"}, "8": {"t2": "\u0635\u0628\u0631\u06a9\u0645\u060c \u0634\u0647\u0648\u062a \u0641\u0632\u0648\u0646", "t1": "\u0635\u0628\u0631\u06a9\u0645\u060c \u0634\u0647\u0648\u062a \u0641\u0632\u0648\u0646"}, "9": {"m1": "\u062d\u06cc\u0644\u0647 \u0648 \u062d\u0631\u0635 \u0648 \u062f\u0631\u0648\u063a \u0648 \u0622\u0632 \u0648 \u06a9\u06cc\u0646", "m2": "\u0645\u0633\u062a\u06cc\u200c\u0648 \u0634\u0628 \u06af\u0631\u062f\u06cc \u0648 \u0642\u062a\u0644 \u0648\u06a9\u0645\u06cc\u0646"}, "10": {"m1": "\u0627\u062d\u062a\u06a9\u0627\u0631 \u0648 \u0627\u0631\u062a\u0634\u0627\u0621 \u0648 \u0627\u062e\u062a\u0644\u0627\u0633", "m2": "\u062c\u0646\u06af \u0648 \u062f\u0639\u0648\u06cc\u200c \u062f\u0627\u0631\u06cc\u200c \u0648 \u062c\u0628\u0646\u200c \u0648 \u0647\u0631\u0627\u0646"}, "11": {"t2": "\u0631\u0646\u062f\u06cc \u0648 \u0631\u0634\u06a9 \u0648 \u062c\u0646\u0648\u0646", "t1": "\u0631\u0646\u062f\u06cc \u0648 \u0631\u0634\u06a9 \u0648 \u062c\u0646\u0648\u0646"}, "12": {"m1": "\u0622\u062f\u0645\u06cc\u0632\u0627\u062f\u0627\u0646 \u0641\u0642\u06cc\u0631 \u0648 \u0628\u0631\u062f\u0628\u0627\u0631", "m2": "\u0645\u0647\u0631\u0628\u0627\u0646 \u0648 \u0633\u0627\u062f\u0647\u200c\u0644\u0648\u062d \u0648 \u0631\u0627\u0633\u062a\u06a9\u0627\u0631"}, "13": {"m1": "\u0645\u0631\u062f \u0648 \u0632\u0646 \u0633\u0631\u06af\u0631\u0645 \u06a9\u0627\u0631 \u0648\u06a9\u0633\u0628 \u0646\u0627\u0646", "m2": "\u0631\u0648\u0632 \u062f\u0631 \u0635\u062d\u0631\u0627 \u0648 \u0634\u0628 \u062f\u0631 \u0622\u0634\u06cc\u0627\u0646"}, "14": {"t2": "\u062e\u0648\u0634 \u062f\u0644 \u0648 \u0635\u0627\u0641\u06cc\u200c \u062f\u0631\u0648\u0646", "t1": "\u062e\u0648\u0634 \u062f\u0644 \u0648 \u0635\u0627\u0641\u06cc\u200c \u062f\u0631\u0648\u0646"}, "15": {"m1": "\u0634\u063a\u0644 \u0622\u0646\u0627\u0646 \u0648\u0631\u0632\u0634 \u0648 \u0628\u0631\u0632\u06cc\u06af\u0631\u06cc", "m2": "\u06af\u0627\u0648\u062f\u0627\u0631\u06cc \u0648 \u0645\u0648\u0627\u0634\u06cc\u200c \u067e\u0631\u0648\u0631\u06cc"}, "16": {"m1": "\u062e\u0627\u0646\u0647\u200c \u0647\u0627\u0634\u0627\u0646\u200c \u062e\u06cc\u0645\u0647 \u0648 \u063a\u0627\u0631 \u0648 \u062f\u0631\u062e\u062a", "m2": "\u06a9\u0631\u062f\u0647 \u0627\u0632 \u0686\u0631\u0645 \u062f\u062f\u0627\u0646 \u0627\u0646\u0628\u0627\u0646 \u0648 \u0631\u062e\u062a"}, "17": {"t2": "\u062d\u0631\u0628\u0647\u200c\u0634\u0627\u0646 \u0633\u0646\u06af \u0648 \u0633\u062a\u0648\u0646", "t1": "\u062d\u0631\u0628\u0647\u200c\u0634\u0627\u0646 \u0633\u0646\u06af \u0648 \u0633\u062a\u0648\u0646"}, "18": {"m1": "\u062c\u0645\u0644\u0647 \u0628\u0627 \u0647\u0645\u200c\u060c \u0647\u0645\u200c\u062a\u0628\u0627\u0631 \u0648 \u0647\u0645\u200c\u0628\u0646\u0647", "m2": "\u06cc\u06a9\u062f\u0644 \u0648 \u06cc\u06a9\u0631\u0648\u06cc \u0647\u0645\u0686\u0648\u0646 \u0622\u06cc\u0646\u0647"}, "19": {"m1": "\u062f\u0631 \u062e\u0648\u0631\u0634 \u0627\u0646\u0628\u0627\u0632 \u0648 \u062f\u0631\u06a9\u0648\u0634\u0634\u200c \u0631\u0641\u06cc\u0642", "m2": "\u067e\u06cc\u0631 \u0648 \u0628\u0631\u0646\u0627 \u0647\u0645\u062f\u0645 \u0648\u06cc\u0627\u0631 \u0648 \u0634\u0641\u06cc\u0642"}, "20": {"t2": "\u0627\u0632 \u062f\u0631\u0648\u0646 \u0648 \u0627\u0632 \u0628\u0631\u0648\u0646", "t1": "\u0627\u0632 \u062f\u0631\u0648\u0646 \u0648 \u0627\u0632 \u0628\u0631\u0648\u0646"}, "21": {"m1": "\u06a9\u0631\u062f\u0647 \u0628\u0631 \u0647\u0631\u062f\u0648\u0631\u0647 \u067e\u06cc\u0631\u06cc \u0645\u0647\u062a\u0631\u06cc", "m2": "\u062c\u0633\u062a\u0647 \u062e\u0648\u0627\u0647\u0631 \u0628\u0627 \u0628\u0631\u0627\u062f\u0631 \u0647\u0645\u0633\u0631\u06cc"}, "22": {"m1": "\u0647\u0631 \u067e\u0633\u0631 \u06a9\u0627\u0648 \u0645\u0647\u062a\u0631 \u0627\u0628\u0646\u0627 \u0628\u064f\u062f\u06cc", "m2": "\u062c\u0627\u0646\u0634\u06cc\u0646 \u0648 \u0648\u0627\u0631\u062b \u0628\u0627\u0628\u0627 \u0634\u062f\u06cc"}, "23": {"t2": "\u0686\u0648\u0646 \u067e\u062f\u0631 \u06af\u0634\u062a\u06cc \u0646\u06af\u0648\u0646", "t1": "\u0686\u0648\u0646 \u067e\u062f\u0631 \u06af\u0634\u062a\u06cc \u0646\u06af\u0648\u0646"}, "24": {"m1": "\u0645\u0647\u062a\u06cc\u200c\u0628\u0646 \u0641\u0631\u0632\u0646\u062f \u067e\u06cc\u0631 \u0627\u0648\u0644\u06cc\u0646", "m2": "\u067e\u0627\u062f\u0634\u0627 \u0628\u0648\u062f\u06cc \u0628\u0631 \u0627\u0642\u0648\u0627\u0645 \u06a9\u0647\u06cc\u0646"}, "25": {"m1": "\u0645\u0627\u0646 \u0648 \u0648\u06cc\u0633 \u0648 \u0632\u0646\u062f \u0632\u06cc\u0631\u062f\u0633\u062a \u0627\u0648", "m2": "\u062c\u0645\u0644\u0647 \u062f\u0647\u06cc\u0648 \u0628\u0633\u062a\u0647 \u0648 \u067e\u0627\u0628\u0633\u062a \u0627\u0648"}, "26": {"t2": "\u067e\u06cc\u0634 \u062f\u0647\u06cc\u0648\u067e\u062f \u0632\u0628\u0648\u0646", "t1": "\u067e\u06cc\u0634 \u062f\u0647\u06cc\u0648\u067e\u062f \u0632\u0628\u0648\u0646"}, "27": {"m1": "\u06a9\u0648\u0686\u06a9\u0627\u0646 \u0645\u062d\u06a9\u0648\u0645 \u067e\u06cc\u0631 \u062e\u0627\u0646\u0645\u0627\u0646", "m2": "\u062e\u0627\u0646\u0645\u0627\u0646\u200c\u0647\u0627 \u0632\u06cc\u0631 \u062d\u06a9\u0645 \u062e\u0627\u0646\u062f\u0627\u0646"}, "28": {"m1": "\u062e\u0627\u0646\u062f\u0627\u0646\u200c\u0647\u0627 \u062a\u0627\u0628\u0639 \u0632\u0646\u062f\u0648 \u0628\u062f\u0646\u062f", "m2": "\u0632\u0646\u062f\u0648\u0627\u0646 \u0641\u0631\u0645\u0627\u0646\u0628\u0631 \u062f\u0647\u06cc\u0648 \u0628\u062f\u0646\u062f"}, "29": {"t2": "\u0634\u062f \u0628\u0647 \u062f\u0647\u06cc\u0648 \u0631\u0647\u0646\u0645\u0648\u0646", "t1": "\u0634\u062f \u0628\u0647 \u062f\u0647\u06cc\u0648 \u0631\u0647\u0646\u0645\u0648\u0646"}, "30": {"m1": "\u0632 \u0627\u0646\u0642\u0644\u0627\u0628\u0627\u062a \u0637\u0628\u06cc\u0639\u062a\u200c\u060c \u062e\u06cc\u0644 \u062e\u06cc\u0644", "m2": "\u0631\u0639\u062f \u0648 \u0628\u0631\u0642\u200c \u0648 \u0644\u0631\u0632\u0647 \u0648 \u0637\u0648\u0641\u0627\u0646\u200c \u0648 \u0633\u06cc\u0644"}, "31": {"m1": "\u0642\u0648\u0645 \u0631\u0627 \u06af\u0647 \u0628\u06cc\u0645 \u0648 \u06af\u0647 \u0627\u0645\u06cc\u062f \u0628\u0648\u062f", "m2": "\u062a\u06a9\u06cc\u0647\u200c\u06af\u0647\u0634\u0627\u0646 \u0622\u062a\u0634 \u0648 \u062e\u0648\u0631\u0634\u06cc\u062f \u0628\u0648\u062f"}, "32": {"t2": "\u0648\u06cc\u0646 \u0633\u067e\u0647\u0631 \u0646\u06cc\u0644\u06af\u0648\u0646", "t1": "\u0648\u06cc\u0646 \u0633\u067e\u0647\u0631 \u0646\u06cc\u0644\u06af\u0648\u0646"}, "33": {"m1": "\u0644\u0634\u06a9\u0631\u06cc \u0645\u0631\u062f \u0648 \u0632\u0646 \u0648 \u0628\u0631\u0646\u0627 \u0648 \u067e\u06cc\u0631", "m2": "\u0628\u0631 \u0632\u0645\u06cc\u0646 \u0647\u0646\u062f\u0648\u0627\u0646 \u0634\u062f \u062c\u0627\u06cc\u06af\u06cc\u0631"}, "34": {"m1": "\u062c\u0646\u06af\u200c\u062e\u0648\u0646\u06cc\u0646 \u0647\u0631 \u0637\u0631\u0641 \u0628\u0627\u0644\u0627\u06af\u0631\u0641\u062a", "m2": "\u0633\u0646\u06af\u200c\u0647\u0627 \u062f\u0631\u06a9\u0627\u0633\u0647\u0654 \u0633\u0631 \u062c\u0627 \u06af\u0631\u0641\u062a"}, "35": {"t2": "\u0631\u06cc\u062e\u062a \u0627\u0632 \u0647\u0631\u06a9\u0627\u0633\u0647 \u062e\u0648\u0646", "t1": "\u0631\u06cc\u062e\u062a \u0627\u0632 \u0647\u0631\u06a9\u0627\u0633\u0647 \u062e\u0648\u0646"}}}