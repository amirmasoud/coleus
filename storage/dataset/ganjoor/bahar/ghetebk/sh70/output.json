{"text": {"0": {"m1": "\u0646\u0645\u0627\u0646\u062f \u062f\u0631\u062f \u0648 \u062f\u0631\u0645\u0627\u0646 \u0647\u0645 \u0646\u0645\u0627\u0646\u062f", "m2": "\u0646\u0645\u0627\u0646\u062f \u0648\u0635\u0644 \u0648 \u0647\u062c\u0631\u0627\u0646 \u0647\u0645 \u0646\u0645\u0627\u0646\u062f"}, "1": {"m1": "\u0628\u0647\u0627\u0631\u0627 \u063a\u0645 \u0645\u062e\u0648\u0631\u06a9\u0627\u0646\u062f\u0631 \u0632\u0645\u0627\u0646\u0647", "m2": "\u0646\u0645\u0627\u0646\u062f \u0639\u06cc\u0634 \u0648 \u062e\u0630\u0644\u0627\u0646 \u0647\u0645 \u0646\u0645\u0627\u0646\u062f"}, "2": {"m1": "\u0628\u0647 \u062a\u0647\u0631\u0627\u0646 \u062f\u0631 \u0645\u0646\u0627\u0644 \u0627\u0632\u06cc\u0627\u062f \u0627\u0633\u062a\u062e\u0631", "m2": "\u06a9\u0647 \u0631\u0641\u062a \u0627\u0633\u062a\u062e\u0631 \u0648\u062a\u0647\u0631\u0627\u0646 \u0647\u0645 \u0646\u0645\u0627\u0646\u062f"}, "3": {"m1": "\u0634\u0648\u062f \u0627\u06cc\u0631\u0627\u0646 \u0628\u0633\u06cc \u0622\u0628\u0627\u062f \u0648 \u0648\u06cc\u0631\u0627\u0646", "m2": "\u0647\u0645\u0627\u0646 \u0622\u0628\u0627\u062f \u0648 \u0648\u0628\u0631\u0627\u0646 \u0647\u0645 \u0646\u0645\u0627\u0646\u062f"}, "4": {"m1": "\u0646\u067e\u0627\u06cc\u062f \u0686\u06cc\u0646 \u0648 \u0698\u0627\u067e\u0648\u0646 \u0647\u0645 \u0646\u067e\u0627\u06cc\u062f", "m2": "\u0646\u0645\u0627\u0646\u062f \u0631\u0648\u0633 \u0648 \u0622\u0644\u0645\u0627\u0646 \u0647\u0645 \u0646\u0645\u0627\u0646\u062f"}, "5": {"m1": "\u0646\u0645\u0627\u0646\u062f \u0627\u0646\u06af\u0644\u06cc\u0633\u06cc \u062e\u0631\u062f\u0645\u0646\u062f", "m2": "\u0647\u0645\u0627\u0646 \u0647\u0646\u062f\u0648\u06cc \u0646\u0627\u062f\u0627\u0646 \u0647\u0645 \u0646\u0645\u0627\u0646\u062f"}, "6": {"m1": "\u0628\u0645\u06cc\u0631\u062f \u0645\u0631\u063a \u0648 \u0645\u0627\u0647\u06cc \u0647\u0645 \u0628\u0645\u06cc\u0631\u062f", "m2": "\u0646\u0645\u0627\u0646\u062f \u0648\u062d\u0634 \u0648 \u0627\u0646\u0633\u0627\u0646 \u0647\u0645 \u0646\u0645\u0627\u0646\u062f"}, "7": {"m1": "\u0627\u06af\u0631\u0686\u0647 \u062f\u06cc\u0631 \u0645\u0627\u0646\u062f \u0646\u0627\u0645 \u0646\u06cc\u06a9\u0648", "m2": "\u0633\u0631\u0627\u0646\u062c\u0627\u0645 \u0627\u06cc \u067e\u0633\u0631 \u0622\u0646 \u0647\u0645 \u0646\u0645\u0627\u0646\u062f"}, "8": {"m1": "\u0628\u062a\u0648\u0641\u062f \u067e\u0631\u062f\u0647\u0654 \u0627\u06cc\u0646 \u0646\u062c\u0645 \u0633\u0627\u06a9\u0646", "m2": "\u0632\u0645\u06cc\u0646 \u06af\u0631\u062f \u06af\u0631\u062f\u0627\u0646 \u0647\u0645 \u0646\u0645\u0627\u0646\u062f"}, "9": {"m1": "\u0628\u0631 \u0627\u06cc\u0646 \u0627\u0641\u0631\u0627\u0634\u062a\u0647 \u0633\u0642\u0641 \u0645\u0631\u0635\u0639", "m2": "\u0642\u0646\u0627\u062f\u06cc\u0644 \u0641\u0631\u0648\u0632\u0627\u0646 \u0647\u0645 \u0646\u0645\u0627\u0646\u062f"}, "10": {"m1": "\u0628\u062c\u0632 \u06cc\u06a9\u200c\u0630\u0627\u062a \u06a9\u0627\u0635\u0644 \u06a9\u0627\u06cc\u0646\u0627\u062a \u0627\u0633\u062a", "m2": "\u0635\u0648\u0631 \u0648 \u0627\u0633\u0645\u0627\u0621 \u0648 \u0627\u0639\u06cc\u0627\u0646 \u0647\u0645 \u0646\u0645\u0627\u0646\u062f"}, "11": {"t2": "\u067e\u0633\u200c \u0627\u06cc\u0646\u200c \u062c\u0646\u06af \u0648 \u062c\u062f\u0627\u0644 \u0645\u0627 \u062e\u06cc\u0627\u0644 \u200c\u0627\u0633\u062a", "t1": "\u0628\u062f \u0648 \u062e\u0648\u0628\u200c \u062c\u0647\u0627\u0646 \u0627\u0646\u062f\u0631 \u0632\u0648\u0627\u0644 \u0627\u0633\u062a"}}}