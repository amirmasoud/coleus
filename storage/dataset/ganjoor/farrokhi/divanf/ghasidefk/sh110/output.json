{"text": {"0": {"m1": "\u0647\u0645\u06cc\u0634\u0647 \u06af\u0641\u062a\u0645\u06cc \u0627\u0646\u062f\u0631 \u062c\u0647\u0627\u0646 \u0628\u0647 \u062d\u0633\u0646 \u0648 \u062c\u0645\u0627\u0644", "m2": "\u0686\u0648 \u06cc\u0627\u0631 \u0645\u0646 \u0646\u0628\u0648\u062f \u0648\u06cc\u0646 \u062d\u062f\u06cc\u062b \u0628\u0648\u062f \u0645\u062d\u0627\u0644"}, "1": {"m1": "\u0645\u0646 \u0622\u0646\u0686\u0647 \u062f\u0639\u0648\u06cc \u06a9\u0631\u062f\u0645 \u0645\u062d\u0627\u0644 \u0628\u0648\u062f \u0648 \u0646\u0628\u0648\u062f", "m2": "\u0627\u0632 \u0622\u0646\u06a9\u0647 \u0686\u0634\u0645 \u0645\u0646 \u0627\u0648 \u0631\u0627 \u0646\u062f\u06cc\u062f\u0647 \u0628\u0648\u062f \u0647\u0645\u0627\u0644"}, "2": {"m1": "\u0632 \u0646\u06cc\u06a9\u0648\u06cc\u06cc \u06a9\u0647 \u0628\u0647 \u0686\u0634\u0645 \u0645\u0646 \u0622\u0645\u062f\u06cc \u0647\u0645\u0647 \u0648\u0642\u062a", "m2": "\u0634\u06a9\u0646\u062c \u0648\u06a9\u0648\u0698\u06cc \u062f\u0631 \u0632\u0644\u0641 \u0648 \u062c\u0639\u062f \u0648 \u0622\u0646 \u0645\u062d\u062a\u0627\u0644"}, "3": {"m1": "\u0632 \u0628\u0647\u0631 \u0622\u0646\u06a9\u0647 \u0628\u0647 \u062c\u0639\u062f \u0648 \u0628\u0647 \u0632\u0644\u0641 \u0648\u0627\u0648\u0645\u0627\u0646\u0645", "m2": "\u0628\u062d\u06cc\u0644\u0647 \u062a\u0646 \u0631\u0627 \u06af\u0647 \u062c\u06cc\u0645 \u06a9\u0631\u062f\u0645\u06cc \u06af\u0647 \u062f\u0627\u0644"}, "4": {"m1": "\u0648\u06af\u0631 \u0628\u0647 \u0628\u0627\u063a \u0641\u0631\u0627 \u0631\u0641\u062a\u0645\u06cc \u0632\u0628\u0627\u0646\u0645 \u0647\u06cc\u0686", "m2": "\u0646\u06cc\u0627\u0641\u062a\u06cc \u0632 \u062e\u0631\u0648\u0634\u06cc\u062f\u0646 \u0648 \u0646\u06a9\u0648\u0647\u0634 \u0647\u0627\u0644"}, "5": {"m1": "\u0632\u0628\u0633 \u0645\u0646\u0627\u0638\u0631\u0647 \u06a9\u0627\u0646\u062c\u0627 \u0632\u0628\u0627\u0646 \u0645\u0646 \u06a9\u0631\u062f\u06cc", "m2": "\u0628\u0631 \u0622\u0646 \u0646\u06a9\u0648\u06cc \u0633\u067e\u0631 \u063a\u0645 \u0628\u0631 \u0622\u0646 \u062e\u062c\u0633\u062a\u0647 \u0646\u0647\u0627\u0644"}, "6": {"m1": "\u0628\u0647 \u0644\u0627\u0644\u0647 \u06af\u0641\u062a\u0645\u06cc: \u0627\u06cc \u0644\u0627\u0644\u0647! \u0634\u0631\u0645 \u062f\u0627\u0631\u0648 \u0645\u0631\u0648\u06cc", "m2": "\u0628\u0647 \u0633\u0631\u0648 \u06af\u0641\u062a\u0645\u06cc \u0627\u06cc \u0633\u0631\u0648! \u0634\u0631\u0645 \u062f\u0627\u0631\u0648 \u0645\u0628\u0627\u0644"}, "7": {"m1": "\u06a9\u0647 \u067e\u06cc\u0634 \u0642\u0627\u0645\u062a \u0648 \u0631\u062e\u0633\u0627\u0631 \u0627\u0648 \u0634\u0645\u0627 \u0647\u0631 \u062f\u0648", "m2": "\u0686\u0648 \u067e\u06cc\u0634 \u062a\u06cc\u0631 \u06a9\u0645\u0627\u0646\u06cc\u062f \u0648 \u067e\u06cc\u0634 \u0628\u062f\u0631 \u0647\u0644\u0627\u0644"}, "8": {"m1": "\u0628\u0686\u0634\u0645 \u0645\u0646 \u0628\u062a \u0645\u0646 \u067e\u06cc\u0634 \u0627\u0632\u06cc\u0646 \u0628\u062f\u06cc\u0646\u0633\u0627\u0646 \u0628\u0648\u062f", "m2": "\u0628\u062a\u0645 \u0686\u0646\u06cc\u0646 \u0648 \u062f\u0644\u0645 \u062f\u0631 \u0647\u0648\u0627\u0634 \u0628\u0631 \u06cc\u06a9 \u062d\u0627\u0644"}, "9": {"m1": "\u0628\u0646\u06cc\u0645 \u0628\u0648\u0633\u0647 \u0632 \u0645\u0646 \u062e\u0648\u0627\u0633\u062a\u06cc \u0647\u0632\u0627\u0631 \u0633\u062c\u0648\u062f", "m2": "\u0628\u06cc\u06a9 \u062c\u0648\u0627\u0628 \u0632\u0645\u0646 \u062e\u0648\u0627\u0633\u062a\u06cc \u0647\u0632\u0627\u0631 \u0633\u0624\u0627\u0644"}, "10": {"m1": "\u0645\u0631\u0627 \u062f\u0648 \u0686\u0634\u0645 \u0628\u062f\u0627\u0646 \u062a\u0627 \u0686\u0647 \u062e\u0648\u0627\u0647\u062f \u0648 \u0686\u0647 \u06a9\u0646\u062f", "m2": "\u0628\u0631 \u0627\u06cc\u0646 \u062f\u0648 \u062d\u0627\u0644 \u0632\u0645\u0627\u0646 \u062a\u0627 \u0632\u0645\u0627\u0646 \u0633\u06a9\u0627\u0644 \u0633\u06a9\u0627\u0644"}, "11": {"m1": "\u0647\u0648\u0627 \u0648 \u062e\u0648\u0628\u06cc \u0627\u0648 \u062f\u0631\u062f\u0644 \u0648 \u062f\u0648 \u062f\u06cc\u062f\u0647 \u0645\u0646", "m2": "\u0632\u0648\u0627\u0644 \u06a9\u0631\u062f \u0641\u0631\u0633\u062a\u0627\u062f\u0647 \u0627\u0645\u06cc\u0631 \u0632\u0648\u0627\u0644"}, "12": {"m1": "\u0645\u0639\u06cc\u0646 \u062f\u0648\u0644\u062a \u0648 \u062f\u06cc\u0646 \u06cc\u0648\u0633\u0641 \u0628\u0646 \u0646\u0627\u0635\u0631 \u062f\u06cc\u0646", "m2": "\u0628\u0631\u0627\u062f\u0631 \u0645\u0644\u06a9 \u0634\u0627\u0647 \u0628\u0646\u062f \u0627\u0639\u062f\u0627 \u0645\u0627\u0644"}, "13": {"m1": "\u0632 \u062f\u0634\u062a \u0648 \u0628\u0633\u062a\u0627\u0646 \u0686\u0648\u0646 \u0628\u0627\u0632\u06af\u0634\u062a \u0631\u0648\u0632 \u0634\u06a9\u0627\u0631", "m2": "\u0628\u0646\u06cc\u06a9 \u0631\u0648\u0632 \u0648\u0628\u0641\u0631\u062e \u0632\u0645\u0627\u0646 \u0648 \u0645\u06cc\u0645\u0648\u0646 \u0641\u0627\u0644"}, "14": {"m1": "\u06cc\u06a9 \u062a\u0630\u0631\u0648 \u0641\u0631\u0633\u062a\u0627\u062f \u0645\u0631\u0645\u0631\u0627 \u06a9\u0647 \u0645\u06af\u0631", "m2": "\u0628\u062d\u06cc\u0644\u0647 \u0622\u06cc\u0645 \u062f\u0631 \u0628\u0646\u062f \u062d\u0633\u0646 \u0622\u0646 \u0645\u062d\u062a\u0627\u0644"}, "15": {"m1": "\u0686\u0648 \u062f\u0633\u062a \u0648 \u067e\u0627\u06cc \u0639\u0631\u0648\u0633\u0627\u0646 \u0646\u06af\u0627\u0634\u062a\u0647 \u0633\u0631 \u0648 \u062f\u0645", "m2": "\u0686\u0648 \u0631\u0648\u06cc \u062e\u0648\u0628\u0627\u0646 \u0622\u0631\u0627\u0633\u062a\u0647 \u0647\u0645\u0647 \u067e\u0631\u0648\u0628\u0627\u0644"}, "16": {"m1": "\u0632 \u0647\u0641\u062a \u06af\u0648\u0646\u0647 \u0628\u0631 \u0648 \u0647\u0641\u062a \u0631\u0646\u06af \u0648 \u0628\u0631 \u0647\u0631 \u0631\u0646\u06af", "m2": "\u0647\u0632\u0627\u0631 \u06af\u0648\u0646\u0647 \u0645\u062d\u0627\u0633\u0646\u060c \u0647\u0632\u0627\u0631 \u06af\u0648\u0646\u0647 \u062c\u0645\u0627\u0644"}, "17": {"m1": "\u0686\u0648 \u0632\u0631 \u062e\u0641\u0686\u0647 \u0647\u0645\u0647 \u067e\u0634\u062a \u0648 \u0628\u0631\u0634 \u0622\u062a\u0634 \u0631\u0646\u06af", "m2": "\u0686\u0648 \u0646\u062e\u0644 \u0628\u0633\u062a\u0647 \u0647\u0645\u0647 \u0633\u06cc\u0646\u0647 \u062f\u0627\u06cc\u0631\u0647 \u0627\u0634\u06a9\u0627\u0644"}, "18": {"m1": "\u06af\u0647 \u062e\u0631\u0627\u0645\u0634 \u0686\u0648\u0646 \u0644\u0639\u0628\u062a\u06cc \u06a9\u0631\u0634\u0645\u0647 \u06a9\u0646\u0627\u0646", "m2": "\u0628\u0647\u0631 \u062e\u0631\u0627\u0645\u0634 \u0627\u0632\u0648 \u0635\u062f \u0647\u0632\u0627\u0631 \u063a\u0646\u062c \u0648 \u062f\u0644\u0627\u0644"}, "19": {"m1": "\u062f\u0648\u0644\u0628: \u0686\u0648 \u0646\u0627\u0631 \u06a9\u0641\u06cc\u062f\u0647\u060c \u0686\u0648 \u0628\u0631\u06af \u0633\u0648\u0633\u0646 \u0632\u0631\u062f", "m2": "\u062f\u0648 \u0631\u062e: \u0686\u0648 \u0646\u0627\u0631 \u0634\u06a9\u0641\u062a\u0647\u060c \u0686\u0648 \u0628\u0631\u06af \u0644\u0627\u0644\u0647 \u0644\u0627\u0644"}, "20": {"m1": "\u0686\u0648 \u0642\u0637\u0646 \u0645\u06cc\u0631\u06cc \u062f\u0631 \u0632\u06cc\u0631 \u067e\u0648\u0634\u0634 \u0645\u0646\u0633\u0648\u062c", "m2": "\u0628\u0631\u0627\u06cc \u067e\u0648\u0632\u0634 \u0628\u0627\u0632 \u0627\u0645\u06cc\u0631 \u062e\u0648\u0628 \u062e\u0635\u0627\u0644"}, "21": {"m1": "\u0686\u06af\u0648\u0646\u0647 \u0628\u0627\u0632\u06cc \u0686\u0648\u0646 \u067e\u0627\u0631\u0647 \u0627\u06cc \u0632 \u0627\u0628\u0631 \u0633\u0641\u06cc\u062f", "m2": "\u0628\u0647 \u0633\u0646\u06af \u0648\u0632\u0646 \u062f\u0631\u0645 \u0633\u0646\u06af \u0627\u0648 \u0628\u0647 \u062f\u0647 \u0645\u062b\u0642\u0627\u0644"}, "22": {"m1": "\u0645\u0628\u0627\u0631\u0632\u06cc\u0633\u062a\u060c \u0644\u0628\u0627\u0633\u0634 \u0632\u0633\u06cc\u0645\u06af\u0648\u0646 \u062c\u0648\u0634\u0646", "m2": "\u0645\u0628\u0627\u0631\u0632\u06cc\u0633\u062a \u0633\u0644\u0627\u062d\u0634 \u0645\u062e\u0627\u0644\u0628 \u0648 \u0686\u0646\u06af\u0627\u0644"}, "23": {"m1": "\u0646\u0634\u0627\u0646 \u062c\u0644\u0627\u062c\u0644 \u0648 \u062e\u0644\u062e\u0627\u0644 \u062f\u0627\u0631\u062f \u0648 \u0639\u062c\u0628\u0633\u062a", "m2": "\u06a9\u0647 \u0648\u062d\u0634\u06cc\u0627\u0646\u0631\u0627 \u0628\u0627\u0634\u062f \u062c\u0644\u0627\u062c\u0644 \u0648 \u062e\u0644\u062e\u0627\u0644"}, "24": {"m1": "\u0628\u0647 \u062a\u0646 \u0628\u06af\u0648\u0646\u0647 \u0633\u06cc\u0645 \u0648\u0628\u0647 \u067e\u0634\u062a \u0648 \u0628\u0627\u0644 \u0633\u067e\u06cc\u062f", "m2": "\u062f\u0631\u0648 \u0646\u0634\u0627\u0646\u062f\u0647 \u062a\u0646\u06a9 \u067e\u0627\u0631\u0647 \u0647\u0627\u06cc \u0633\u06cc\u0645 \u062d\u0644\u0627\u0644"}, "25": {"m1": "\u0628\u0631\u0648\u0632 \u062c\u0646\u06af \u0645\u0631 \u0627\u0648 \u0631\u0627 \u0628\u0686\u0646\u06af \u0628\u0633\u062a\u0647 \u0628\u0631\u0646\u062f", "m2": "\u0646\u0647 \u0632\u0627\u0646 \u0642\u0628\u0644 \u06a9\u0647 \u0632 \u062c\u0646\u06af \u0622\u06cc\u062f\u0634 \u0646\u0647\u06cc\u0628 \u0648 \u0645\u0644\u0627\u0644"}, "26": {"m1": "\u0648\u0644\u06cc\u06a9\u0646 \u0627\u0632 \u067e\u06cc \u0622\u0646 \u06a9\u0648 \u0686\u0648 \u062e\u0635\u0645 \u062f\u06cc\u062f \u0627\u0632 \u062f\u0648\u0631", "m2": "\u0628\u06cc \u0622\u0646\u06a9\u0647 \u0648\u0642\u062a \u0628\u0648\u062f \u0686\u06cc\u0631\u06af\u06cc \u06a9\u0646\u062f\u0628\u0647 \u062c\u062f\u0627\u0644"}, "27": {"m1": "\u0639\u0642\u0627\u0628 \u06af\u06cc\u0631\u062f \u0628\u0627\u0632 \u06a9\u0633\u06cc \u06a9\u0647 \u0627\u0648 \u0628\u06a9\u0645\u0646\u062f", "m2": "\u06af\u0631\u0641\u062a\u0647 \u0628\u0627\u0634\u062f \u06a9\u0631\u06af \u0648 \u0628\u06af\u0631\u0632 \u06a9\u0648\u0641\u062a\u0647 \u06cc\u0627\u0644"}, "28": {"m1": "\u0627\u06af\u0631 \u0639\u0642\u0627\u0628 \u0633\u0648\u06cc \u062c\u0646\u06af \u0627\u0648 \u0634\u062a\u0627\u0628 \u06a9\u0646\u062f", "m2": "\u0639\u0642\u0627\u0628\u0631\u0627 \u0628\u0647 \u0628\u0644\u06a9 \u0628\u0634\u06a9\u0646\u062f \u0633\u0631\u0648 \u062a\u0646 \u0648 \u0628\u0627\u0644"}, "29": {"m1": "\u0627\u0645\u06cc\u0631 \u06cc\u0648\u0633\u0641 \u06a9\u0631\u06af \u0627\u0641\u06a9\u0646\u0633\u062a \u0648\u0634\u06cc\u0631 \u06a9\u0634\u0633\u062a", "m2": "\u0632 \u06a9\u0631\u06af \u0648\u0634\u06cc\u0631\u0628\u062c\u0627\u0646 \u0631\u0633\u062a\u0647 \u0628\u0648\u062f \u0631\u0633\u062a\u0645 \u0632\u0627\u0644"}, "30": {"m1": "\u0632 \u0622\u062a\u0634 \u0622\u0628 \u06a9\u0646\u062f \u062d\u0644\u0645\u0634 \u0648\u0632\u0628\u0627\u062f \u0627\u0648 \u0631\u0633\u062a", "m2": "\u0632 \u067e\u06cc\u0644 \u067e\u0634\u0647 \u06a9\u0646\u062f \u0633\u0647\u0645\u0634 \u0648 \u0632 \u0634\u06cc\u0631 \u0634\u06a9\u0627\u0644"}, "31": {"m1": "\u0628\u0647 \u062e\u0648\u060c \u0628\u0647\u0627\u0631 \u0628\u0631\u0648\u0646 \u0622\u0648\u0631\u062f\u060c \u0645\u06cc\u0627\u0646\u0647 \u062f\u06cc", "m2": "\u0628\u0647 \u062c\u0648\u062f\u060c \u0686\u0634\u0645\u0647 \u062f\u0648\u0627\u0646\u062f \u0632 \u062a\u0644 \u0647\u0627\u06cc \u0631\u0645\u0627\u0644"}, "32": {"m1": "\u0686\u0648 \u0632\u0627\u06cc\u0631\u06cc \u0633\u0648\u06cc \u0627\u0648 \u0642\u0635\u062f \u06a9\u0631\u062f \u0632\u0627\u06cc\u0631 \u0631\u0627", "m2": "\u0632 \u062d\u0631\u0635 \u0628\u0627\u0632 \u0634\u062f \u062c\u0648\u062f \u0627\u0648 \u0628\u0627\u0633\u062a\u0642\u0628\u0627\u0644"}, "33": {"m1": "\u0628\u0633\u06cc \u0646\u0645\u0627\u0646\u062f\u0647 \u06a9\u0647 \u0627\u0632 \u062c\u0648\u062f \u062d\u062c\u0631\u0647 \u0647\u0627 \u0633\u0627\u0632\u062f", "m2": "\u0632 \u0628\u0647\u0631 \u0633\u0627\u06cc\u0644 \u062f\u0631 \u06af\u0646\u062c\u0647\u0627\u06cc \u0628\u06cc\u062a \u0627\u0644\u0645\u0627\u0644"}, "34": {"m1": "\u0686\u0646\u0627\u0646\u06a9\u0647 \u062c\u0648\u062f \u0628\u062f\u0627\u0646 \u062f\u0633\u062a\u0647\u0627\u06cc \u0645\u06a9\u0646\u062a \u0628\u062e\u0634", "m2": "\u0632 \u0628\u0647\u0631 \u0634\u06cc\u0631 \u0632 \u067e\u0633\u062a\u0627\u0646 \u0645\u0627\u062f\u0631\u0627\u0646 \u0627\u0637\u0641\u0627\u0644"}, "35": {"m1": "\u0632 \u0647\u0648\u0644 \u062e\u0648\u0646 \u0634\u0648\u062f \u0627\u0646\u062f\u0631 \u062f\u0648 \u0686\u0634\u0645 \u0622\u0632 \u0633\u0631\u0634\u06a9", "m2": "\u0686\u0648 \u062a\u06cc\u0631 \u0628\u0631 \u06a9\u0634\u062f \u0627\u0632 \u0646\u0632\u0644 \u062f\u0627\u0646 \u0628\u0631\u0648\u0632 \u0646\u0648\u0627\u0644"}, "36": {"m1": "\u062d\u0633\u0627\u0645 \u0627\u0648 \u0628\u062c\u0647\u0627\u0646 \u0627\u0646\u062f\u0631 \u0627\u0641\u06a9\u0646\u062f \u0641\u0631\u06cc\u0627\u062f", "m2": "\u0646\u0647\u06cc\u0628 \u0627\u0648 \u0628\u0632\u0645\u06cc\u0646 \u0627\u0646\u062f\u0631 \u0627\u0641\u06a9\u0646\u062f \u0632\u0644\u0632\u0627\u0644"}, "37": {"m1": "\u062a\u0646 \u0645\u062e\u0627\u0644\u0641 \u0627\u0648 \u06af\u0631 \u0642\u0648\u06cc \u062f\u0631\u062e\u062a \u0628\u0648\u062f", "m2": "\u0686\u0648 \u062f\u06cc\u062f \u0647\u0648\u0644\u0634 \u0644\u0631\u0632\u0627\u0646 \u0634\u0648\u062f \u0628\u06af\u0648\u0646\u0647 \u0646\u0627\u0644"}, "38": {"m1": "\u0633\u0647 \u0686\u06cc\u0631 \u0627\u0641\u06a9\u0646\u062f \u0627\u0632 \u062f\u0634\u0645\u0646\u0627\u0646 \u0628\u0631\u0648\u0632 \u0646\u0628\u0631\u062f", "m2": "\u0686\u0648 \u062a\u06cc\u063a \u0627\u0648\u0628\u06af\u0634\u0627\u06cc\u062f \u0632 \u062d\u0644\u0642\u0634\u0627\u0646 \u0642\u06cc\u0641\u0627\u0644"}, "39": {"m1": "\u0632 \u062f\u0633\u062a\u0647\u0627\u0634\u0627\u0646 \u067e\u0647\u0646\u0647 \u0632 \u067e\u0627\u06cc\u0647\u0627 \u0686\u0648 \u06af\u0627\u0646", "m2": "\u0632 \u06af\u0631\u062f \u0633\u0631\u0647\u0627 \u06af\u0648\u06cc\u060c \u0627\u06cc\u0646\u062a \u0634\u0627\u0647 \u0648 \u0627\u06cc\u0646\u062a \u062c\u0644\u0627\u0644"}, "40": {"m1": "\u062c\u0647\u0627\u0646\u06cc\u0627\u0646 \u0647\u0645\u0647 \u0632\u0648 \u0634\u0627\u06a9\u0631\u0646\u062f \u067e\u06cc\u0631 \u0648 \u062c\u0648\u0627\u0646", "m2": "\u0628\u062e\u0627\u0635\u0647 \u0645\u0646 \u06a9\u0647 \u0634\u062f\u0645 \u0632\u0648 \u0628\u0631\u0627\u062f\u0631 \u0627\u0642\u0628\u0627\u0644"}, "41": {"m1": "\u0632 \u062c\u0627\u0647 \u0627\u0648 \u063a\u0646\u06cc\u0645 \u0686\u0648 \u0646 \u0632\u0645\u0627\u0644 \u0627\u0648 \u063a\u0646\u06cc\u0645", "m2": "\u0628\u062f\u06cc\u0646 \u062f\u0648\u062c\u0627\u0647 \u0648 \u062c\u06cc\u0647\u0645 \u0645\u06cc\u0627\u0646\u0647 \u0627\u0634\u06a9\u0627\u0644"}, "42": {"m1": "\u062e\u062f\u0627\u06cc \u0646\u0627\u0635\u0631 \u0622\u0646 \u0634\u0627\u0647 \u0628\u0627\u062f \u0648 \u06af\u0631\u062f\u0648\u0646 \u06cc\u0627\u0631", "m2": "\u0628\u0631\u0627\u06cc \u0627\u0648 \u0634\u0628 \u0648\u0631\u0648\u0632 \u0648 \u0628\u06a9\u0627\u0645 \u0627\u0648\u0645\u0647 \u0648\u0633\u0627\u0644"}, "43": {"m1": "\u0686\u0646\u0627\u0646\u06a9\u0647 \u0627\u0648\u062f\u0644 \u0645\u0646 \u0634\u0627\u062f \u06a9\u0631\u062f \u0634\u0627\u062f\u0627\u0646 \u0628\u0627\u062f", "m2": "\u0632 \u062e\u0644\u0642 \u0648 \u0645\u0630\u0647\u0628 \u067e\u0627\u06a9\u0634 \u062f\u0644 \u0645\u062d\u0645\u062f \u0648 \u0622\u0644"}}}