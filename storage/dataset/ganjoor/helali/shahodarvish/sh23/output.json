{"text": {"0": {"m1": "\u0686\u0646\u062f \u0631\u0648\u0632\u06cc \u06a9\u0647 \u0634\u0627\u0647\u0632\u0627\u062f\u0647\u0654 \u0639\u0635\u0631", "m2": "\u0622\u0645\u062f \u0648 \u062c\u0627 \u06af\u0631\u0641\u062a \u0628\u0631 \u0644\u0628 \u0642\u0635\u0631"}, "1": {"m1": "\u0622\u0646 \u06af\u062f\u0627 \u0631\u0648 \u0628\u0647 \u0642\u0635\u0631 \u0634\u0647 \u0645\u06cc\u200c\u06a9\u0631\u062f", "m2": "\u0628\u0631 \u062f\u0631 \u0648 \u0628\u0627\u0645 \u0627\u0648 \u0646\u06af\u0647 \u0645\u06cc\u200c\u06a9\u0631\u062f"}, "2": {"m1": "\u0628\u0647 \u0647\u0648\u0627\u06cc \u0634\u0647 \u0648 \u0646\u0638\u0627\u0631\u0647\u0654 \u0628\u0627\u0645", "m2": "\u0645\u0627\u0646\u062f \u0633\u0631 \u062f\u0631 \u0647\u0648\u0627 \u0633\u062d\u0631 \u062a\u0627 \u0634\u0627\u0645"}, "3": {"m1": "\u062c\u0632 \u0628\u0647 \u0633\u0648\u06cc \u0647\u0648\u0627 \u0646\u0645\u06cc\u200c\u0646\u06af\u0631\u06cc\u0633\u062a", "m2": "\u0647\u06cc\u0686 \u0628\u0631 \u067e\u0634\u062a \u067e\u0627 \u0646\u0645\u06cc\u200c\u0646\u06af\u0631\u06cc\u0633\u062a"}, "4": {"m1": "\u062f\u0631 \u0647\u0648\u0627 \u0628\u0633 \u06a9\u0647 \u0628\u0648\u062f \u0648\u0627\u0644\u0647 \u0648 \u0645\u0633\u062a", "m2": "\u062e\u0644\u0642 \u06af\u0641\u062a\u0646\u062f\u0634 \u0622\u0641\u062a\u0627\u0628\u200c\u067e\u0631\u0633\u062a"}, "5": {"m1": "\u062a\u0627 \u0628\u0647 \u062c\u0627\u06cc\u06cc \u0631\u0633\u06cc\u062f \u06af\u0641\u062a \u0648 \u0634\u0646\u0641\u062a", "m2": "\u06a9\u0647 \u0631\u0642\u06cc\u0628 \u0622\u0646 \u0634\u0646\u06cc\u062f \u0648 \u0628\u0647 \u0627\u0648\u06cc \u06af\u0641\u062a"}, "6": {"m1": "\u0627\u06cc\u0646 \u06af\u062f\u0627 \u0627\u0632 \u062e\u062f\u0627\u06cc \u0646\u0648\u0645\u06cc\u062f\u0633\u062a", "m2": "\u0642\u0628\u0644\u0647\u0654 \u0627\u0648 \u062c\u0645\u0627\u0644 \u062e\u0648\u0631\u0634\u06cc\u062f\u0633\u062a"}, "7": {"m1": "\u06a9\u0627\u0641\u0631\u0633\u062a \u0648 \u0632 \u0627\u0647\u0644 \u0627\u06cc\u0645\u0627\u0646 \u0646\u06cc\u0633\u062a", "m2": "\u06a9\u0641\u0631 \u0645\u06cc\u200c\u0648\u0631\u0632\u062f \u0648 \u0645\u0633\u0644\u0645\u0627\u0646 \u0646\u06cc\u0633\u062a"}, "8": {"m1": "\u062e\u0648\u0631\u062f \u062f\u0631\u0648\u06cc\u0634 \u0628\u06cc\u200c\u06af\u0646\u0647 \u0633\u0648\u06af\u0646\u062f", "m2": "\u0628\u0647 \u062e\u062f\u0627\u06cc\u06cc \u06a9\u0647 \u0647\u0633\u062a \u0628\u06cc\u200c\u0645\u0627\u0646\u0646\u062f"}, "9": {"m1": "\u0627\u0648\u0633\u062a \u062e\u0648\u0631\u0634\u06cc\u062f \u0648 \u0639\u0634\u0642 \u0644\u0627\u06cc\u0642 \u0627\u0648\u0633\u062a", "m2": "\u0647\u0645\u0647 \u0630\u0631\u0627\u062a \u06a9\u0648\u0646 \u0639\u0627\u0634\u0642 \u0627\u0648\u0633\u062a"}, "10": {"m1": "\u067e\u06cc\u0634 \u062e\u0648\u0631\u0634\u06cc\u062f \u0627\u0648 \u062d\u062c\u0627\u0628\u06cc \u0646\u06cc\u0633\u062a", "m2": "\u063a\u06cc\u0631 \u0627\u0648 \u0647\u06cc\u0686 \u0622\u0641\u062a\u0627\u0628\u06cc \u0646\u06cc\u0633\u062a"}, "11": {"m1": "\u0634\u062f \u0645\u0639\u06cc\u0646 \u0645\u06cc\u0627\u0646 \u062f\u0634\u0645\u0646 \u0648 \u062f\u0648\u0633\u062a", "m2": "\u06a9\u0647 \u0628\u0647 \u0639\u0627\u0644\u0645 \u062e\u062f\u067e\u0631\u0633\u062a \u062e\u0648\u062f \u0627\u0648\u0633\u062a"}, "12": {"m1": "\u0628\u0627\u0632 \u062e\u0648\u062f \u0631\u0627 \u0628\u0647 \u06a9\u0648\u06cc \u0634\u0627\u0647 \u0627\u0641\u06af\u0646\u062f", "m2": "\u0648\u0632 \u06a9\u0641 \u062e\u0635\u0645 \u062f\u0631 \u067e\u0646\u0627\u0647 \u0627\u0641\u06af\u0646\u062f"}, "13": {"m1": "\u0644\u06cc\u06a9 \u0637\u0641\u0644\u0627\u0646 \u06a9\u0648\u0686\u0647 \u0648 \u0628\u0627\u0632\u0627\u0631", "m2": "\u0628\u0627\u0632 \u062c\u0633\u062a\u0646\u062f\u0634 \u062f\u0631 \u067e\u06cc \u0622\u0632\u0627\u0631"}, "14": {"m1": "\u0647\u0631 \u0637\u0631\u0641 \u0645\u06cc\u200c\u0634\u062f\u0646\u062f \u0633\u0646\u06af \u0628\u0647 \u062f\u0633\u062a", "m2": "\u06a9\u0647: \u06a9\u062c\u0627 \u0631\u0641\u062a \u0622\u0641\u062a\u0627\u0628\u200c\u067e\u0631\u0633\u062a\u061f"}, "15": {"m1": "\u0647\u0631 \u06a9\u0647 \u06a9\u0631\u062f\u06cc \u0628\u0647 \u0622\u0646 \u0637\u0631\u0641 \u0622\u0647\u0646\u06af", "m2": "\u062a\u0627 \u0632\u0646\u062f \u0628\u0631 \u06af\u062f\u0627\u06cc \u0645\u0633\u06a9\u06cc\u0646 \u0633\u0646\u06af"}, "16": {"m1": "\u0633\u0646\u06af \u0627\u0632\u0627\u0646 \u0622\u0633\u062a\u0627\u0646 \u0634\u0647 \u06a9\u0646\u062f\u06cc", "m2": "\u0628\u0631\u062f\u06cc \u0648 \u062e\u0648\u062f \u0628\u0647 \u0633\u0648\u06cc\u0634 \u0627\u0641\u06af\u0646\u062f\u06cc"}, "17": {"m1": "\u06af\u0641\u062a \u0627\u0632 \u0633\u0646\u06af \u0628\u06cc\u0646\u0645 \u0622\u0632\u0627\u0631\u06cc", "m2": "\u0633\u0646\u06af \u0622\u0646 \u0622\u0633\u062a\u0627\u0646 \u0628\u0648\u062f \u06cc\u0627\u0631\u06cc"}, "18": {"m1": "\u0628\u0633 \u06a9\u0647 \u0637\u0641\u0644\u0627\u0646 \u0632\u062f\u0646\u062f \u0633\u0646\u06af \u0628\u0631\u0648", "m2": "\u0639\u0631\u0635\u0647\u0654 \u0634\u0647\u0631 \u06af\u0634\u062a \u062a\u0646\u06af \u0628\u0631\u0648"}, "19": {"m1": "\u0628\u0647 \u0636\u0631\u0648\u0631\u062a \u0632 \u0634\u0647\u0631 \u0628\u06cc\u0631\u0648\u0646 \u062c\u0633\u062a", "m2": "\u06a9\u0646\u062c \u0648\u06cc\u0631\u0627\u0646\u0647\u200c\u0627\u06cc \u06af\u0631\u0641\u062a \u0648 \u0646\u0634\u0633\u062a"}, "20": {"m1": "\u0686\u0648\u0646 \u0628\u0647 \u0648\u06cc\u0631\u0627\u0646\u0647 \u0633\u0627\u062e\u062a \u0645\u0633\u06a9\u0646 \u062e\u0648\u06cc\u0634", "m2": "\u067e\u06cc\u0631\u0647\u0646 \u0686\u0627\u06a9 \u06a9\u0631\u062f \u0628\u0631 \u062a\u0646 \u062e\u0648\u06cc\u0634"}, "21": {"m1": "\u06a9\u0647 \u0645\u0646 \u0645\u0631\u062f\u0647 \u067e\u06cc\u0631\u0647\u0646 \u0686\u0647 \u06a9\u0646\u0645\u061f", "m2": "\u0645\u0631\u062f\u0647 \u06af\u0631 \u0646\u06cc\u0633\u062a\u0645\u060c \u06a9\u0641\u0646 \u0686\u0647 \u06a9\u0646\u0645\u061f"}, "22": {"m1": "\u0647\u0631 \u0632\u0645\u0627\u0646 \u062e\u0627\u06a9 \u0631\u06cc\u062e\u062a \u0628\u0631 \u0633\u0631 \u0648 \u062a\u0646", "m2": "\u06a9\u06cc\u0646 \u0686\u0647 \u0639\u0645\u0631\u0633\u062a\u061f \u062e\u0627\u06a9 \u0628\u0631 \u0633\u0631 \u0645\u0646"}, "23": {"m1": "\u06cc\u06a9 \u0633\u0631 \u0645\u0648 \u0646\u06a9\u0627\u0633\u062a \u0646\u0627\u062e\u0646 \u062e\u0648\u06cc\u0634", "m2": "\u062e\u0648\u0627\u0633\u062a \u0646\u0627\u062e\u0646 \u0632\u0646\u062f \u0628\u0647 \u0633\u06cc\u0646\u0647\u0654 \u0631\u06cc\u0634"}, "24": {"m1": "\u0645\u0648\u06cc \u0698\u0648\u0644\u06cc\u062f\u0647 \u0631\u0627 \u06af\u0630\u0627\u0634\u062a \u0628\u0647 \u0633\u0631", "m2": "\u0628\u0644\u06a9\u0647 \u0645\u0648\u06cc\u06cc \u0632 \u0633\u0631 \u0646\u062f\u0627\u0634\u062a \u062e\u0628\u0631"}, "25": {"m1": "\u0628\u0627 \u062e\u0648\u062f \u0627\u0632 \u0628\u06cc\u062e\u0648\u062f\u06cc \u0633\u062e\u0646 \u0645\u06cc\u200c\u06a9\u0631\u062f", "m2": "\u06af\u0644\u0647 \u0627\u0632 \u0628\u062e\u062a \u062e\u0648\u06cc\u0634\u062a\u0646 \u0645\u06cc\u200c\u06a9\u0631\u062f"}, "26": {"m1": "\u06a9\u0647 \u0631\u0633\u0627\u0646\u062f\u06cc \u0633\u0631\u0645 \u0686\u0631\u062e \u0628\u0631\u06cc\u0646", "m2": "\u0628\u0627\u0632\u0645 \u0627\u0632 \u0622\u0633\u0645\u0627\u0646 \u0632\u062f\u06cc \u0628\u0647 \u0632\u0645\u06cc\u0646"}, "27": {"m1": "\u06af\u0631 \u0628\u0647 \u0645\u0646 \u0644\u062d\u0638\u0647\u200c\u0627\u06cc \u0648\u0641\u0627 \u06af\u0631\u062f\u06cc", "m2": "\u0647\u0645 \u062f\u0631 \u0622\u0646 \u0644\u062d\u0638\u0647 \u0635\u062f \u062c\u0641\u0627 \u06a9\u0631\u062f\u06cc"}, "28": {"m1": "\u062d\u062f \u062c\u0648\u0631 \u0648 \u062c\u0641\u0627 \u0647\u0645\u06cc\u0646 \u0628\u0627\u0634\u062f", "m2": "\u0628\u0627\u0631\u06a9 \u0627\u0644\u0644\u0647! \u0648\u0641\u0627 \u0647\u0645\u06cc\u0646 \u0628\u0627\u0634\u062f"}}}