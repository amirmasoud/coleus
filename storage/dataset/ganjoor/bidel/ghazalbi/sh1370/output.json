{"text": {"0": {"m1": "\u06af\u0631 \u0622\u06af\u0647\u06cc \u0628\u0647 \u0633\u06cc\u0631 \u0641\u0646\u0627 \u0648 \u0628\u0642\u0627 \u0628\u062e\u0646\u062f", "m2": "\u0639\u0628\u0631\u062a \u0628\u0647\u0627\u0646\u0647\u200c\u062c\u0648\u0633\u062a \u0628\u0631 \u0627\u06cc\u0646 \u062e\u0646\u062f\u0647\u200c\u0647\u0627 \u0628\u062e\u0646\u062f"}, "1": {"m1": "\u06af\u0644 \u0631\u0633\u062a\u0646 \u0648 \u0628\u0647\u0627\u0631 \u062f\u0645\u06cc\u062f\u0646 \u0686\u0647 \u0644\u0627\u0632\u0645 \u0627\u0633\u062a", "m2": "\u062f\u0631 \u0632\u06cc\u0631 \u0644\u0628 \u0686\u0648 \u0622\u0628\u0644\u0647\u0654 \u0632\u06cc\u0631 \u067e\u0627 \u0628\u062e\u0646\u062f"}, "2": {"m1": "\u0627\u0641\u0633\u0631\u062f\u06cc \u0627\u06cc \u0634\u0631\u0631 \u0628\u0647 \u0641\u0634\u0627\u0631 \u0634\u06a9\u0641\u062a\u06af\u06cc", "m2": "\u0622\u062e\u0631\u062a\u0648 \u0631\u0627\u06a9\u0647\u200c\u06af\u0641\u062a \u062f\u0631 \u0627\u06cc\u0646 \u062a\u0646\u06af\u0646\u0627 \u0628\u062e\u0646\u062f"}, "3": {"m1": "\u0645\u0633\u062a\u063a\u0646\u06cc \u0627\u0632 \u06af\u0644 \u0627\u0633\u062a \u0645\u0632\u0627\u0631 \u0634\u0647\u06cc\u062f \u0639\u0634\u0642", "m2": "\u0627\u06cc \u063a\u0646\u0686\u0647 \u0644\u0628\u200c\u060c \u062a\u0648\u0628\u0631 \u0633\u0631\u062e\u0627\u06a9\u0645 \u0628\u06cc\u0627 \u0628\u062e\u0646\u062f"}, "4": {"m1": "\u0641\u0631\u0635\u062a \u06a9\u0645\u06cc\u0646 \u0648\u0639\u062f\u0647\u0654 \u0641\u0631\u062f\u0627 \u062f\u0645\u0627\u063a \u06a9\u06cc\u0633\u062a", "m2": "\u0627\u06cc\u200c\u06af\u0644 \u0628\u0647\u0627\u0631 \u0631\u0641\u062a \u0628\u0631\u0627\u06cc \u062e\u062f\u0627 \u0628\u062e\u0646\u062f"}, "5": {"m1": "\u0645\u0646\u0639\u0645\u200c! \u063a\u0628\u0627\u0631 \u0686\u0647\u0631\u0647\u0654 \u0645\u062d\u062a\u0627\u062c\u200c\u060c \u0634\u0633\u062a\u0646\u06cc\u200c\u0627\u0633\u062a", "m2": "\u0628\u0631 \u0641\u0642\u0631 \u06af\u0631\u06cc\u0647 \u06af\u0631 \u0646\u06a9\u0646\u06cc \u0628\u0631 \u063a\u0646\u0627 \u0628\u062e\u0646\u062f"}, "6": {"m1": "\u0686\u0646\u062f\u06cc\u0646 \u0633\u062d\u0631 \u0628\u0647 \u0648\u0647\u0645 \u067e\u0631\u0627\u0641\u0634\u0627\u0646 \u0646\u0627\u0632 \u0631\u0641\u062a", "m2": "\u06cc\u06a9\u200c\u06af\u0644 \u062a\u0648\u0646\u06cc\u0632 \u0627\u0632 \u0644\u0628 \u0628\u0627\u0645 \u0647\u0648\u0627 \u0628\u062e\u0646\u062f"}, "7": {"m1": "\u062f\u0631\u067e\u0631\u062f\u0647 \u062e\u0648\u0646 \u062d\u0633\u0631\u062a \u0628\u06cc\u200c\u062f\u0633\u062a \u0648\u067e\u0627 \u0645\u0631\u06cc\u0632", "m2": "\u06af\u0627\u0647\u06cc \u0686\u0648 \u0627\u0634\u06a9 \u06af\u0631\u06cc\u0647\u0654 \u062f\u0646\u062f\u0627\u0646\u200c\u0646\u0645\u0627 \u0628\u062e\u0646\u062f"}, "8": {"m1": "\u0635\u062f\u06af\u0644 \u0628\u0647\u0627\u0631\u0645\u0646\u062a\u0638\u0631 \u06cc\u06a9 \u062c\u0646\u0648\u0646 \u062a\u0648\u0633\u062a", "m2": "\u0622\u062a\u0634 \u0628\u0647 \u0635\u0641\u062d\u0647\u200c\u0627\u062a \u0632\u0646 \u0648 \u0633\u0631\u062a\u0627 \u0628\u0647 \u067e\u0627 \u0628\u062e\u0646\u062f"}, "9": {"m1": "\u0628\u0627 \u0635\u0628\u062d \u200c\u06af\u0641\u062a\u0645 \u0627\u0632 \u0686\u0647 \u0628\u0647\u0627\u0631 \u0627\u0633\u062a \u062e\u0646\u062f\u0647\u200c\u0627\u062a", "m2": "\u06af\u0641\u062a \u0627\u0646\u062f\u06a9\u06cc \u062a\u0648 \u0647\u0645 \u0632\u062a\u06a9\u0644\u0641 \u0628\u0631\u0622 \u0628\u062e\u0646\u062f"}, "10": {"m1": "\u0628\u0631 \u0634\u0627\u0645 \u0645\u0627 \u0686\u0648 \u0634\u0645\u0639 \u062c\u0648\u0627\u0646\u06cc \u0628\u0633\u06cc \u200c\u06af\u0631\u06cc\u0633\u062a", "m2": "\u067e\u06cc\u0631\u06cc\u200c \u06a9\u0646\u0648\u0646 \u062a\u0648 \u06af\u0644\u200c \u06a9\u0646 \u0648 \u0628\u0631 \u0635\u0628\u062d \u0645\u0627 \u0628\u062e\u0646\u062f"}, "11": {"m1": "\u0628\u06cc\u062f\u0644 \u0628\u0647\u0627\u0631 \u0639\u0645\u0631 \u0634\u06a9\u0641\u062a\u0646 \u0686\u0647 \u062e\u0646\u062f\u0647 \u0627\u0633\u062a", "m2": "\u0627\u06cc \u063a\u0627\u0641\u0644 \u0627\u0632 \u0646\u0641\u0633 \u0639\u0631\u0642\u06cc \u0627\u0632 \u062d\u06cc\u0627 \u0628\u062e\u0646\u062f"}}}