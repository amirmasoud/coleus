{"text": {"0": {"m1": "\u062f\u0631\u06cc \u0627\u0632 \u0627\u0633\u0628\u0627\u0628 \u0645\u0627 \u0648 \u0645\u0646 \u0628\u0647 \u062d\u0642 \u067e\u06cc\u0648\u0633\u062a\u0646 \u0627\u0633\u062a", "m2": "\u0642\u0637\u0631\u0647 \u0631\u0627 \u0627\u0632 \u062e\u0648\u062f\u06af\u0633\u062a\u0646 \u062f\u0644 \u0628\u0647 \u062f\u0631\u06cc\u0627 \u0628\u0633\u062a\u0646 \u0627\u0633\u062a"}, "1": {"m1": "\u0633\u0628\u062d\u0647\u0654 \u0645\u0646 \u0646\u0627\u0644\u0647 \u0631\u0627 \u0628\u0627 \u0639\u0642\u062f \u062f\u0644 \u067e\u06cc\u0648\u0633\u062a\u0646 \u0627\u0633\u062a", "m2": "\u0647\u0645\u062c\u0648 \u0645\u0698\u06af\u0627\u0646 \u200c\u0633\u062c\u062f\u0647\u200c\u0627\u0645 \u0686\u0634\u0645\u200c\u200c \u0627\u0632 \u062f\u0648 \u0639\u0627\u0644\u0645\u200c \u0628\u0633\u062a\u0646 \u0627\u0633\u062a"}, "2": {"m1": "\u062a\u0627 \u062a\u0648\u0627\u0646\u06cc \u06af\u0627\u0647\u06af\u0627\u0647\u06cc \u0628\u06cc\u200c\u062a\u06a9\u0644\u0641 \u0632\u06cc\u0633\u062a\u0646", "m2": "\u0632\u06cc\u0646 \u062a\u0639\u0644\u0642\u0647\u0627 \u06a9\u0647 \u062f\u0627\u0631\u06cc \u0627\u0646\u062f\u06a9\u06cc \u0648\u200c\u0627\u0631\u0633\u062a\u0646 \u0627\u0633\u062a"}, "3": {"m1": "\u0628\u0627 \u062f\u0631\u0634\u062a\u0627\u0646 \u062c\u0632 \u0628\u0647 \u062a\u0631\u06a9 \u0631\u0627\u0633\u062a\u06cc \u0635\u062d\u0628\u062a \u0645\u062e\u0648\u0627\u0647", "m2": "\u0646\u0642\u0634 \u0631\u0627 \u0628\u06cc\u200c\u06a9\u062c\u200c\u0646\u0647\u0627\u062f\u06cc \u0628\u0627 \u0646\u06af\u06cc\u0646 \u0646\u0646\u0634\u0633\u062a\u0646 \u0627\u0633\u062a"}, "4": {"m1": "\u0639\u0627\u0641\u06cc\u062a \u0627\u062d\u0631\u0627\u0645\u06cc \u0639\u0634\u0627\u0642\u200c\u060c \u0633\u0639\u06cc \u0646\u0627\u0631\u0633\u0627\u0633\u062a", "m2": "\u0634\u0639\u0644\u0647\u200c\u0647\u0627 \u0631\u0627 \u062f\u0627\u063a \u06af\u0634\u062a\u0646 \u0646\u0642\u0634 \u0631\u0627\u062d\u062a \u0628\u0633\u062a\u0646 \u0627\u0633\u062a"}, "5": {"m1": "\u062f\u0631 \u06af\u0644\u0633\u062a\u0627\u0646 \u062e\u0631\u0627\u0645 \u0627\u0648\u060c \u0632 \u0647\u0631 \u0646\u0642\u0634 \u0639\u062f\u0645", "m2": "\u0631\u0646\u06af \u0648 \u0628\u0648\u06cc \u06af\u0644 \u200c\u06a9\u0645\u06cc\u0646\u200c\u0633\u0627\u0632 \u0627\u062f\u0627\u06cc \u062c\u0633\u062a\u0646 \u0627\u0633\u062a"}, "6": {"m1": "\u0627\u0644\u0641\u062a \u0628\u0639\u062f \u0627\u0632 \u062c\u062f\u0627\u06cc\u06cc \u0633\u062e\u062a \u0645\u062d\u06a9\u0645 \u0645\u06cc\u200c\u0634\u0648\u062f", "m2": "\u0631\u0634\u062a\u0647 \u0631\u0627 \u067e\u06cc\u0648\u0646\u062f \u062f\u0634\u0648\u0627\u0631 \u0627\u0633\u062a \u062a\u0627 \u0646\u06af\u0633\u0633\u062a\u0646 \u0627\u0633\u062a"}, "7": {"m1": "\u06af\u0631 \u062a\u0627\u0645\u0644 \u0645\u062d\u0631\u0645 \u0633\u0627\u0645\u0627\u0646 \u0627\u06cc\u0646 \u062f\u0631\u06cc\u0627 \u0634\u0648\u062f", "m2": "\u0627\u0632 \u062a\u0647\u06cc\u200c\u200c\u062f\u0633\u062a\u06cc\u200c\u06af\u0647\u0631 \u0647\u0645\u0686\u0648\u0646 \u062d\u0628\u0627\u0628 \u0622\u0628\u0633\u062a\u0646 \u0627\u0633\u062a"}, "8": {"m1": "\u062a\u0627\u06a9\u06cc \u0627\u06cc \u0628\u06cc\u062f\u0631\u062f \u062f\u0644 \u0631\u0627 \u062e\u0648\u0627\u0631 \u062e\u0648\u0627\u0647\u06cc \u062f\u0634\u062a\u0646", "m2": "\u0634\u06cc\u0634\u0647\u0654 \u062f\u0631\u06cc\u200c\u0647\u200c\u0628\u0631 \u0633\u0646\u06af\u0634\u200c\u0632\u062f\u0646\u200c\u0646\u0634\u06a9\u0633\u062a\u0646 \u0627\u0633\u062a"}, "9": {"m1": "\u0633\u0639\u06cc \u0628\u06cc\u062f\u0631\u062f\u0627\u0646 \u0628\u0647 \u0628\u0627\u062f \u0647\u0631\u0632\u0647\u200c\u06af\u0631\u062f\u06cc \u0645\u06cc\u200c\u0631\u0648\u062f", "m2": "\u0645\u0648\u062c \u062e\u0648\u200c\u0646 \u0634\u0648\u200c\u060c \u0627\u06cc\u200c\u0646\u0641\u0633 \u06af\u0631 \u0628\u0627 \u062f\u0644\u062a \u067e\u06cc\u0648\u0633\u062a\u0646 \u0627\u0633\u062a"}, "10": {"m1": "\u0647\u0645\u0686\u0648 \u062f\u0631\u06cc\u0627 \u0628\u06cc\u062f\u0644 \u0622\u0633\u0627\u0646 \u0646\u06cc\u0633\u062a\u200c\u06a9\u0633\u0628 \u0627\u0639\u062a\u0628\u0627\u0631", "m2": "\u062f\u0631\u062e\u0648\u0631 \u0627\u0645\u0648\u0627\u062c \u0627\u06cc\u0646\u062c\u0627 \u0631\u0648 \u0628\u0647 \u0646\u0627\u062e\u0646 \u062e\u0633\u062a\u0646 \u0627\u0633\u062a"}}}