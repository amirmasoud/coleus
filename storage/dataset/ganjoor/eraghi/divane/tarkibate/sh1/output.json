{"text": {"0": {"m1": "\u0639\u0634\u0642 \u0627\u0631 \u0628\u0647 \u062a\u0648 \u0631\u062e \u0639\u06cc\u0627\u0646 \u0646\u0645\u0627\u06cc\u062f", "m2": "\u062f\u0631 \u0622\u06cc\u0646\u0647\u0654 \u062c\u0647\u0627\u0646 \u0646\u0645\u0627\u06cc\u062f"}, "1": {"m1": "\u0627\u06cc\u0646 \u0622\u06cc\u0646\u0647 \u0686\u0647\u0631\u0647\u0654 \u062d\u0642\u06cc\u0642\u062a", "m2": "\u0647\u0631 \u062f\u0645 \u0628\u0647 \u062a\u0648 \u0631\u0627\u06cc\u06af\u0627\u0646 \u0646\u0645\u0627\u06cc\u062f"}, "2": {"m1": "\u06cc\u06a9 \u062f\u0627\u06cc\u0631\u0647 \u0641\u0631\u0636 \u06a9\u0646 \u062c\u0647\u0627\u0646 \u0631\u0627", "m2": "\u0647\u0631 \u0646\u0642\u0637\u0647 \u0627\u0632\u0648 \u0645\u06cc\u0627\u0646 \u0646\u0645\u0627\u06cc\u062f"}, "3": {"m1": "\u0627\u06cc\u0646 \u062f\u0627\u06cc\u0631\u0647 \u0628\u06cc\u0634 \u0646\u0642\u0637\u0647\u200c\u0627\u06cc \u0646\u06cc\u0633\u062a", "m2": "\u0644\u06cc\u06a9\u0646 \u0628\u0647 \u0646\u0638\u0631 \u0686\u0646\u0627\u0646 \u0646\u0645\u0627\u06cc\u062f"}, "4": {"m1": "\u0631\u0648 \u0646\u0642\u0637\u0647\u0654 \u0622\u062a\u0634\u06cc \u0628\u06af\u0631\u062f\u0627\u0646", "m2": "\u062a\u0627 \u062f\u0627\u06cc\u0631\u0647\u200c\u0627\u06cc \u0631\u0648\u0627\u0646 \u0646\u0645\u0627\u06cc\u062f"}, "5": {"m1": "\u0627\u06cc\u0646 \u0646\u0642\u0637\u0647 \u0632 \u0633\u0631\u0639\u062a \u062a\u062d\u0631\u06a9", "m2": "\u0635\u062f \u062f\u0627\u06cc\u0631\u0647 \u0647\u0631 \u0632\u0645\u0627\u0646 \u0646\u0645\u0627\u06cc\u062f"}, "6": {"m1": "\u0627\u06cc\u0646 \u0646\u0642\u0637\u0647 \u0628\u0647 \u062a\u0648 \u0634\u0647\u0627\u062f\u062a \u0648 \u063a\u06cc\u0628", "m2": "\u0647\u0645 \u0638\u0627\u0647\u0631 \u0648 \u0647\u0645 \u0646\u0647\u0627\u0646 \u0646\u0645\u0627\u06cc\u062f"}, "7": {"m1": "\u0622\u0646 \u0646\u0642\u0637\u0647 \u0628\u0647 \u062a\u0648 \u06a9\u0645\u0627\u0644 \u0645\u0637\u0644\u0642", "m2": "\u062f\u0631 \u0635\u0648\u0631\u062a \u0627\u06cc\u0646 \u0648 \u0622\u0646 \u0646\u0645\u0627\u06cc\u062f"}, "8": {"m1": "\u0622\u0646 \u0633\u0631\u0639\u062a \u062f\u0648\u0631 \u0646\u0642\u0637\u0647 \u062f\u0627\u06cc\u0645", "m2": "\u0633\u0627\u06a9\u0646 \u0628\u0647 \u06cc\u06a9\u06cc \u0645\u06a9\u0627\u0646 \u0646\u0645\u0627\u06cc\u062f"}, "9": {"m1": "\u0647\u0631 \u0644\u0645\u062d\u0647 \u0628\u0647 \u062a\u0648 \u06a9\u0645\u0627\u0644 \u0647\u0633\u062a\u06cc", "m2": "\u062f\u0631 \u06a9\u0633\u0648\u062a \u0646\u0627\u0642\u0635\u0627\u0646 \u0646\u0645\u0627\u06cc\u062f"}, "10": {"m1": "\u0622\u0646 \u0646\u0642\u0637\u0647 \u0628\u06cc\u0627\u0646 \u06a9\u0646\u0645 \u0686\u0647 \u0686\u06cc\u0632 \u0627\u0633\u062a", "m2": "\u0647\u0631 \u0686\u0646\u062f \u062a\u0648 \u0631\u0627 \u06af\u0645\u0627\u0646 \u0646\u0645\u0627\u06cc\u062f"}, "11": {"m1": "\u0622\u0646 \u0646\u0642\u0637\u0647 \u0628\u062f\u0627\u0646 \u06a9\u0647 \u0638\u0644 \u0646\u0648\u0631 \u0627\u0633\u062a", "m2": "\u06a9\u0627\u0646 \u0646\u0648\u0631 \u0648\u0631\u0627\u06cc \u062c\u0627\u0646 \u0646\u0645\u0627\u06cc\u062f"}, "12": {"m1": "\u0622\u0646 \u0646\u0648\u0631 \u062f\u0644 \u067e\u06cc\u0645\u0628\u0631 \u0645\u0627\u0633\u062a", "m2": "\u0627\u06a9\u0646\u0648\u0646 \u0628\u0647 \u062a\u0648 \u062d\u0642 \u0639\u06cc\u0627\u0646 \u0646\u0645\u0627\u06cc\u062f"}, "13": {"t2": "\u0648 \u0622\u0646 \u0646\u0648\u0631 \u0628\u0633\u06cc\u0637 \u062c\u0627\u0648\u062f\u0627\u0646\u0647", "t1": "\u0622\u0646 \u0628\u062d\u0631 \u0645\u062d\u06cc\u0637 \u0628\u06cc\u200c\u06a9\u0631\u0627\u0646\u0647"}, "14": {"m1": "\u0622\u0646 \u0628\u062d\u0631\u060c \u06a9\u0647 \u0645\u0648\u062c \u0627\u0648\u0633\u062a \u062f\u0631\u06cc\u0627", "m2": "\u0648 \u0622\u0646 \u0646\u0648\u0631\u060c \u06a9\u0647 \u0638\u0644 \u0627\u0648\u0633\u062a \u0627\u0634\u06cc\u0627"}, "15": {"m1": "\u0646\u0648\u0631\u06cc \u06a9\u0647 \u062c\u0645\u0627\u0644 \u062c\u0645\u0644\u0647 \u0647\u0633\u062a\u06cc", "m2": "\u0627\u0632 \u062a\u0627\u0628 \u062c\u0645\u0627\u0644 \u0627\u0648\u0633\u062a \u067e\u06cc\u062f\u0627"}, "16": {"m1": "\u0627\u0648\u0644 \u0632 \u067e\u06cc \u0646\u0638\u0627\u0631\u0647\u0654 \u0627\u0648", "m2": "\u0634\u062f \u0639\u06cc\u0646 \u0647\u0645\u0647 \u062c\u0647\u0627\u0646 \u0645\u0647\u06cc\u0627"}, "17": {"m1": "\u0648 \u0622\u062e\u0631 \u0647\u0645 \u0622\u0641\u062a\u0627\u0628 \u0631\u0648\u06cc\u0634", "m2": "\u0634\u062f \u0635\u0648\u0631\u062a \u062c\u0633\u0645 \u0648 \u062c\u0627\u0646 \u0647\u0648\u06cc\u062f\u0627"}, "18": {"m1": "\u0627\u0648 \u0631\u0648\u06cc \u062d\u0642 \u0627\u0633\u062a \u0648 \u0639\u06cc\u0646 \u062d\u0642 \u0646\u06cc\u0632", "m2": "\u0628\u0644 \u0639\u06cc\u0646 \u062d\u0642\u06cc\u0642\u062a \u0627\u0633\u062a \u0648 \u0627\u0639\u0644\u0627"}, "19": {"m1": "\u062f\u0631\u06cc\u0627\u0628\u060c \u06a9\u0647 \u0627\u0648\u0633\u062a \u0627\u0633\u0645 \u0627\u0639\u0638\u0645", "m2": "\u0632\u0648 \u06af\u0634\u062a \u0639\u06cc\u0627\u0646 \u0635\u0641\u0627\u062a \u0648 \u0627\u0633\u0645\u0627"}, "20": {"m1": "\u0622\u0646 \u0630\u0627\u062a \u06a9\u0647 \u062d\u0642 \u0628\u0648\u062f \u0635\u0641\u0627\u062a\u0634", "m2": "\u0627\u0648 \u0631\u0627 \u0628\u0646\u06af\u0631\u060c \u0686\u0647 \u0628\u0627\u0634\u062f \u0627\u0633\u0645\u0627\u061f"}, "21": {"m1": "\u0627\u0633\u0645\u06cc \u06a9\u0647 \u0628\u0648\u062f \u0635\u0641\u0627\u062a \u0627\u0648 \u062d\u0642", "m2": "\u0628\u0646\u06af\u0631 \u06a9\u0647 \u0686\u0647 \u0628\u0627\u0634\u062f\u0634 \u0645\u0633\u0645\u0627"}, "22": {"m1": "\u0648 \u0622\u0646 \u0646\u0648\u0631 \u06a9\u0647 \u062d\u0642 \u0628\u062f\u0648 \u062a\u0648\u0627\u0646 \u062f\u06cc\u062f", "m2": "\u0628\u0627\u0634\u062f \u0647\u0645\u0647 \u0648\u0627\u0644\u0636\u062d\u06cc \u0648 \u0637\u0627\u0647\u0627"}, "23": {"m1": "\u0641\u06cc\u200c\u0627\u0644\u062c\u0645\u0644\u0647 \u06a9\u0645\u0627\u0644 \u0635\u0648\u0631\u062a \u0627\u0648\u0633\u062a", "m2": "\u0622\u06cc\u06cc\u0646\u0647\u0654 \u0630\u0627\u062a \u062d\u0642 \u062a\u0639\u0627\u0644\u06cc"}, "24": {"m1": "\u062f\u0631 \u0622\u06cc\u0646\u0647 \u0645\u0635\u0637\u0641\u06cc \u0686\u0647 \u0628\u06cc\u0646\u062f\u061f", "m2": "\u062c\u0632 \u062d\u0633\u0646 \u0648 \u062c\u0645\u0627\u0644 \u0630\u0627\u062a \u0648\u0627\u0644\u0627"}, "25": {"m1": "\u06a9\u0648 \u0639\u0627\u0634\u0642 \u0631\u0648\u06cc \u062d\u0642\u061f \u0628\u06cc\u0627 \u06af\u0648", "m2": "\u0628\u0646\u06af\u0631 \u0631\u062e \u062e\u0648\u0628 \u0645\u0635\u0637\u0641\u06cc \u0631\u0627"}, "26": {"m1": "\u062f\u0631 \u0635\u0648\u0631\u062a \u0627\u0648 \u062d\u0642 \u0627\u0631 \u0646\u062f\u06cc\u062f\u06cc", "m2": "\u0627\u06cc\u0646\u062c\u0627 \u0628\u0647 \u06cc\u0642\u06cc\u0646 \u0628\u0628\u06cc\u0646\u06cc \u0622\u0646\u062c\u0627"}, "27": {"m1": "\u062f\u0631 \u0635\u0648\u0631\u062a \u0634\u0631\u062d \u0627\u0648 \u0639\u0631\u0627\u0642\u06cc", "m2": "\u0686\u0648\u0646 \u062f\u06cc\u062f \u062d\u0642\u06cc\u0642\u062a \u0622\u0634\u06a9\u0627\u0631\u0627"}, "28": {"m1": "\u0627\u0645\u06cc\u062f \u06a9\u0647 \u0627\u0632 \u0634\u0641\u0627\u0639\u062a \u0627\u0648", "m2": "\u062d\u0627\u0635\u0644 \u0634\u0648\u062f\u0634 \u06a9\u0644\u0627\u0645 \u0627\u0639\u0644\u06cc"}, "29": {"t2": "\u0628\u06cc\u0646\u0646\u062f \u0647\u0645\u0647 \u062c\u0645\u0627\u0644 \u0645\u0637\u0644\u0642", "t1": "\u062a\u0627 \u0647\u0631 \u0646\u0641\u0633\u06cc \u0628\u0647 \u062f\u06cc\u062f\u0647\u0654 \u062d\u0642"}}}