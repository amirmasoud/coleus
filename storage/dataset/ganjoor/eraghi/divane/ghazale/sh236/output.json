{"text": {"0": {"m1": "\u0627\u06cc \u062f\u0631 \u0645\u06cc\u0627\u0646 \u062c\u0627\u0646\u0645 \u06af\u0646\u062c\u06cc \u0646\u0647\u0627\u0646 \u0646\u0647\u0627\u062f\u0647", "m2": "\u0628\u0633 \u0646\u06a9\u062a\u0647\u200c\u0647\u0627\u06cc \u0645\u0639\u0646\u06cc \u0627\u0646\u062f\u0631 \u0632\u0628\u0627\u0646 \u0646\u0647\u0627\u062f\u0647"}, "1": {"m1": "\u0633\u0631 \u062d\u06a9\u06cc\u0645 \u0645\u0627 \u0631\u0627 \u062f\u0631 \u0634\u0648\u0642 \u0644\u0627\u06cc\u0632\u0627\u0644\u06cc", "m2": "\u062f\u0631 \u0645\u0646 \u06cc\u0632\u06cc\u062f \u0639\u0634\u0642\u0634 \u067e\u06cc\u0634 \u062f\u06a9\u0627\u0646 \u0646\u0647\u0627\u062f\u0647"}, "2": {"m1": "\u062f\u0631 \u062c\u0644\u0648\u0647\u200c\u06af\u0627\u0647 \u0645\u0639\u0646\u06cc \u0645\u0639\u0634\u0648\u0642 \u0631\u062e \u0646\u0645\u0648\u062f\u0647", "m2": "\u062f\u0631 \u0628\u0627\u0631\u06af\u0627\u0647 \u0635\u0648\u0631\u062a \u062a\u062e\u062a\u0634 \u0639\u06cc\u0627\u0646 \u0646\u0647\u0627\u062f\u0647"}, "3": {"m1": "\u0627\u0632 \u0646\u06cc\u0633\u062a \u0647\u0633\u062a \u06a9\u0631\u062f\u0647\u060c \u0627\u0632 \u0628\u0647\u0631 \u062c\u0644\u0648\u0647\u0654 \u062e\u0648\u062f", "m2": "\u0648\u0627\u0646\u06af\u0647 \u0646\u0634\u0627\u0646 \u0647\u0633\u062a\u06cc \u0628\u0631 \u0628\u06cc\u200c\u0646\u0634\u0627\u0646 \u0646\u0647\u0627\u062f\u0647"}, "4": {"m1": "\u0631\u0648\u062d\u06cc \u0628\u062f\u06cc\u0646 \u0644\u0637\u06cc\u0641\u06cc \u062f\u0631 \u0686\u0627\u0647 \u062a\u0646 \u0641\u06af\u0646\u062f\u0647", "m2": "\u0633\u0631\u06cc \u0628\u062f\u06cc\u0646 \u0639\u0632\u06cc\u0632\u06cc \u062f\u0631 \u0642\u0639\u0631 \u062c\u0627\u0646 \u0646\u0647\u0627\u062f\u0647"}, "5": {"m1": "\u062e\u0648\u062f \u06a9\u0631\u062f\u0647 \u0631\u0647\u0646\u0645\u0627\u06cc\u06cc \u0622\u062f\u0645 \u0628\u0647 \u0633\u0648\u06cc \u06af\u0646\u062f\u0645", "m2": "\u0627\u0628\u0644\u06cc\u0633 \u0628\u0647\u0631 \u062a\u0627\u062f\u06cc\u0628 \u0627\u0646\u062f\u0631 \u0645\u06cc\u0627\u0646 \u0646\u0647\u0627\u062f\u0647"}, "6": {"m1": "\u062e\u0648\u062f \u06a9\u0631\u062f\u0647 \u0622\u0646\u0686\u0647 \u06a9\u0631\u062f\u0647\u060c \u0648\u0627\u0646\u06af\u0647 \u0628\u062f\u06cc\u0646 \u0628\u0647\u0627\u0646\u0647", "m2": "\u0647\u0631 \u0644\u062d\u0638\u0647 \u062c\u0631\u0645 \u0648 \u0639\u0635\u06cc\u0627\u0646 \u0628\u0631 \u0627\u06cc\u0646 \u0648 \u0622\u0646 \u0646\u0647\u0627\u062f\u0647"}, "7": {"m1": "\u0628\u0639\u0636\u06cc \u0628\u0631\u0627\u06cc \u062f\u0648\u0632\u062e\u060c \u0628\u0639\u0636\u06cc \u0628\u0631\u0627\u06cc \u0627\u0646\u0633\u0627\u0646", "m2": "\u0627\u0646\u062f\u0631 \u0628\u0647\u0634\u062a \u0628\u0627\u0642\u06cc \u0627\u0645\u0646 \u0648 \u0627\u0645\u0627\u0646 \u0646\u0647\u0627\u062f\u0647"}, "8": {"m1": "\u06a9\u0633 \u0631\u0627 \u062f\u0631\u06cc\u0646 \u0645\u06cc\u0627\u0646\u0647 \u0686\u0648\u0646 \u0648 \u0686\u0631\u0627 \u0646\u0632\u06cc\u0628\u062f", "m2": "\u0647\u0631 \u06a9\u0633 \u0646\u0635\u06cc\u0628 \u0627\u0648 \u0631\u0627 \u0647\u0645 \u063a\u06cc\u0628\u200c\u062f\u0627\u0646 \u0646\u0647\u0627\u062f\u0647"}, "9": {"m1": "\u0639\u0645\u0631\u06cc \u062f\u0631\u06cc\u0646 \u062a\u0641\u06a9\u0631\u060c \u0627\u0632 \u063a\u0627\u06cc\u062a \u062a\u062d\u06cc\u0631", "m2": "\u06af\u0648\u0634 \u062f\u0644 \u0639\u0631\u0627\u0642\u06cc \u0628\u0631 \u0622\u0633\u062a\u0627\u0646 \u0646\u0647\u0627\u062f\u0647"}}}