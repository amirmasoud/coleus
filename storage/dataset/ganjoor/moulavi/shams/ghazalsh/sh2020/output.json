{"text": {"0": {"m1": "\u0627\u06cc \u062e\u062f\u0627 \u0627\u06cc\u0646 \u0648\u0635\u0644 \u0631\u0627 \u0647\u062c\u0631\u0627\u0646 \u0645\u06a9\u0646", "m2": "\u0633\u0631\u062e\u0648\u0634\u0627\u0646 \u0639\u0634\u0642 \u0631\u0627 \u0646\u0627\u0644\u0627\u0646 \u0645\u06a9\u0646"}, "1": {"m1": "\u0628\u0627\u063a \u062c\u0627\u0646 \u0631\u0627 \u062a\u0627\u0632\u0647 \u0648 \u0633\u0631\u0633\u0628\u0632 \u062f\u0627\u0631", "m2": "\u0642\u0635\u062f \u0627\u06cc\u0646 \u0645\u0633\u062a\u0627\u0646 \u0648 \u0627\u06cc\u0646 \u0628\u0633\u062a\u0627\u0646 \u0645\u06a9\u0646"}, "2": {"m1": "\u0686\u0648\u0646 \u062e\u0632\u0627\u0646 \u0628\u0631 \u0634\u0627\u062e \u0648 \u0628\u0631\u06af \u062f\u0644 \u0645\u0632\u0646", "m2": "\u062e\u0644\u0642 \u0631\u0627 \u0645\u0633\u06a9\u06cc\u0646 \u0648 \u0633\u0631\u06af\u0631\u062f\u0627\u0646 \u0645\u06a9\u0646"}, "3": {"m1": "\u0628\u0631 \u062f\u0631\u062e\u062a\u06cc \u06a9\u0634\u06cc\u0627\u0646 \u0645\u0631\u063a \u062a\u0648\u0633\u062a", "m2": "\u0634\u0627\u062e \u0645\u0634\u06a9\u0646 \u0645\u0631\u063a \u0631\u0627 \u067e\u0631\u0627\u0646 \u0645\u06a9\u0646"}, "4": {"m1": "\u062c\u0645\u0639 \u0648 \u0634\u0645\u0639 \u062e\u0648\u06cc\u0634 \u0631\u0627 \u0628\u0631\u0647\u0645 \u0645\u0632\u0646", "m2": "\u062f\u0634\u0645\u0646\u0627\u0646 \u0631\u0627 \u06a9\u0648\u0631 \u06a9\u0646 \u0634\u0627\u062f\u0627\u0646 \u0645\u06a9\u0646"}, "5": {"m1": "\u06af\u0631 \u0686\u0647 \u062f\u0632\u062f\u0627\u0646 \u062e\u0635\u0645 \u0631\u0648\u0632 \u0631\u0648\u0634\u0646\u0646\u062f", "m2": "\u0622\u0646\u0686 \u0645\u06cc\u200c\u062e\u0648\u0627\u0647\u062f \u062f\u0644 \u0627\u06cc\u0634\u0627\u0646 \u0645\u06a9\u0646"}, "6": {"m1": "\u06a9\u0639\u0628\u0647 \u0627\u0642\u0628\u0627\u0644 \u0627\u06cc\u0646 \u062d\u0644\u0642\u0647 \u0627\u0633\u062a \u0648 \u0628\u0633", "m2": "\u06a9\u0639\u0628\u0647 \u0627\u0648\u0645\u06cc\u062f \u0631\u0627 \u0648\u06cc\u0631\u0627\u0646 \u0645\u06a9\u0646"}, "7": {"m1": "\u0627\u06cc\u0646 \u0637\u0646\u0627\u0628 \u062e\u06cc\u0645\u0647 \u0631\u0627 \u0628\u0631\u0647\u0645 \u0645\u0632\u0646", "m2": "\u062e\u06cc\u0645\u0647 \u062a\u0648\u0633\u062a \u0622\u062e\u0631 \u0627\u06cc \u0633\u0644\u0637\u0627\u0646 \u0645\u06a9\u0646"}, "8": {"m1": "\u0646\u06cc\u0633\u062a \u062f\u0631 \u0639\u0627\u0644\u0645 \u0632 \u0647\u062c\u0631\u0627\u0646 \u062a\u0644\u062e\u062a\u0631", "m2": "\u0647\u0631\u0686 \u062e\u0648\u0627\u0647\u06cc \u06a9\u0646 \u0648\u0644\u06cc\u06a9\u0646 \u0622\u0646 \u0645\u06a9\u0646"}}}