{"text": {"0": {"m1": "\u067e\u0646\u062c \u062f\u0631 \u0686\u0647 \u0641\u0627\u06cc\u062f\u0647 \u0686\u0648\u0646 \u0647\u062c\u0631 \u0631\u0627 \u0634\u0634 \u062a\u0648 \u06a9\u0646\u062f", "m2": "\u062e\u0648\u0646 \u0628\u062f\u0627\u0646 \u0634\u062f \u062f\u0644 \u06a9\u0647 \u0637\u0627\u0644\u0628 \u062e\u0648\u0646 \u062f\u0644 \u0631\u0627 \u0628\u0648 \u06a9\u0646\u062f"}, "1": {"m1": "\u0686\u0646\u06af \u0631\u0627 \u062f\u0631 \u0639\u0634\u0642 \u0627\u0648 \u0627\u0632 \u0628\u0647\u0631 \u0622\u0646 \u0622\u0645\u0648\u062e\u062a\u0645", "m2": "\u06a9\u0633 \u0646\u062f\u0627\u0646\u062f \u062d\u0627\u0644\u062a \u0645\u0646 \u0646\u0627\u0644\u0647 \u0645\u0646 \u0627\u0648 \u06a9\u0646\u062f"}, "2": {"m1": "\u0627\u06cc \u0628\u0647 \u0647\u0631 \u0633\u0648\u06cc\u06cc \u062f\u0648\u06cc\u062f\u0647 \u06a9\u0627\u0631 \u062a\u0648 \u06cc\u06a9 \u0633\u0648 \u0646\u0634\u062f", "m2": "\u0622\u0646\u06a9 \u062f\u0631 \u0634\u0634 \u0633\u0648 \u0646\u06af\u0646\u062c\u062f \u06a9\u0627\u0631 \u0627\u0648 \u06cc\u06a9 \u0633\u0648 \u06a9\u0646\u062f"}, "3": {"m1": "\u0634\u06cc\u0631 \u0622\u0647\u0648 \u0645\u06cc\u200c\u062f\u0631\u0627\u0646\u062f \u0634\u06cc\u0631 \u0645\u0627 \u0628\u0633 \u0646\u0627\u062f\u0631\u0633\u062a", "m2": "\u0646\u0642\u0634 \u0622\u0647\u0648 \u0631\u0627 \u0628\u06af\u06cc\u0631\u062f \u062f\u0631\u062f\u0645\u062f \u0622\u0647\u0648 \u06a9\u0646\u062f"}, "4": {"m1": "\u0628\u0627\u0637\u0646\u062a \u0631\u0627 \u0644\u0627\u0644\u0647 \u0633\u0627\u0632\u062f \u0638\u0627\u0647\u0631\u062a \u0631\u0627 \u0627\u0631\u063a\u0648\u0627\u0646", "m2": "\u06cc\u06a9 \u062f\u0645\u062a \u0633\u0627\u0632\u062f \u0642\u0632\u0644\u0628\u06a9 \u06cc\u06a9 \u062f\u0645\u062a \u0635\u0627\u0631\u0648 \u06a9\u0646\u062f"}, "5": {"m1": "\u0645\u0648\u062c \u0622\u0646 \u062f\u0631\u06cc\u0627 \u0645\u062c\u0648 \u06a9\u0648 \u0631\u0627 \u0645\u062f\u062f \u0627\u0632 \u062c\u0648 \u0628\u0648\u062f", "m2": "\u0622\u0646 \u0628\u062c\u0648 \u06a9\u0632 \u0646\u0648\u0631 \u062c\u0627\u0646 \u062f\u0648 \u067e\u06cc\u0647 \u0631\u0627 \u062f\u0648 \u062c\u0648 \u06a9\u0646\u062f"}, "6": {"m1": "\u062e\u0648\u0634 \u0642\u0645\u0631\u0631\u0648\u06cc\u06cc \u06a9\u0632 \u0627\u06cc\u0646 \u063a\u0645 \u0645\u06cc\u200c\u06af\u0630\u0627\u0631\u062f \u0686\u0648\u0646 \u0647\u0644\u0627\u0644", "m2": "\u062e\u0648\u0634 \u0634\u06a9\u0631\u062e\u0648\u06cc\u06cc \u06a9\u0647 \u0628\u0627 \u0622\u0646 \u0634\u06a9\u0631\u0633\u062a\u0627\u0646 \u062e\u0648 \u06a9\u0646\u062f"}, "7": {"m1": "\u0622\u0647\u0646\u06cc \u06a9\u0648 \u0645\u0648\u0645 \u0634\u062f \u0628\u0647\u0631 \u0642\u0628\u0648\u0644 \u0645\u0647\u0631 \u0639\u0634\u0642", "m2": "\u062e\u0627\u06a9 \u0631\u0627 \u0639\u0646\u0628\u0631 \u06a9\u0646\u062f \u0627\u0648 \u0633\u0646\u06af \u0631\u0627 \u0644\u0624\u0644\u0624 \u06a9\u0646\u062f"}, "8": {"m1": "\u062f\u0644 \u06a9\u0628\u0627\u0628 \u0648 \u062e\u0648\u0646 \u062f\u06cc\u062f\u0647 \u067e\u06cc\u0634\u06a9\u0634 \u067e\u06cc\u0634\u0634 \u0628\u0631\u0645", "m2": "\u06af\u0631 \u062a\u0642\u0627\u0636\u0627\u06cc \u0634\u0631\u0627\u0628 \u0648 \u06cc\u062e\u0646\u06cc \u0648 \u0637\u0631\u063a\u0648 \u06a9\u0646\u062f"}, "9": {"m1": "\u0644\u06a9\u0644\u06a9 \u0622\u0646 \u062d\u0642 \u0634\u0646\u0627\u0633\u062f \u0645\u0644\u06a9 \u0631\u0627 \u0644\u06a9\u0644\u06a9 \u06a9\u0646\u062f", "m2": "\u0641\u0627\u062e\u062a\u0647 \u0645\u062d\u062c\u0648\u0628 \u0628\u0627\u0634\u062f \u0644\u0627\u062c\u0631\u0645 \u06a9\u0648\u06a9\u0648 \u06a9\u0646\u062f"}, "10": {"m1": "\u0622\u0628 \u0648 \u0631\u0648\u063a\u0646 \u06a9\u0645 \u06a9\u0646 \u0648 \u062e\u0627\u0645\u0634 \u0686\u0648 \u0631\u0648\u063a\u0646 \u0645\u06cc\u200c\u06af\u062f\u0627\u0632", "m2": "\u062e\u0631\u0645 \u0622\u0646 \u06a9\u0627\u0646\u062f\u0631 \u063a\u0645 \u0622\u0646 \u0631\u0648\u06cc \u062a\u0646 \u0686\u0648\u0646 \u0645\u0648 \u06a9\u0646\u062f"}}}