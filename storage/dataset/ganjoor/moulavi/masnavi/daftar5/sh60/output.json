{"text": {"0": {"m1": "\u0637\u0648\u0637\u06cc\u06cc \u062f\u0631 \u0622\u06cc\u0646\u0647 \u0645\u06cc\u200c\u0628\u06cc\u0646\u062f \u0627\u0648", "m2": "\u0639\u06a9\u0633 \u062e\u0648\u062f \u0631\u0627 \u067e\u06cc\u0634 \u0627\u0648 \u0622\u0648\u0631\u062f\u0647 \u0631\u0648"}, "1": {"m1": "\u062f\u0631 \u067e\u0633 \u0622\u06cc\u06cc\u0646\u0647 \u0622\u0646 \u0627\u0633\u062a\u0627 \u0646\u0647\u0627\u0646", "m2": "\u062d\u0631\u0641 \u0645\u06cc\u200c\u06af\u0648\u06cc\u062f \u0627\u062f\u06cc\u0628 \u062e\u0648\u0634\u200c\u0632\u0628\u0627\u0646"}, "2": {"m1": "\u0637\u0648\u0637\u06cc\u06a9 \u067e\u0646\u062f\u0627\u0634\u062a\u0647 \u06a9\u06cc\u0646 \u06af\u0641\u062a \u067e\u0633\u062a", "m2": "\u06af\u0641\u062a\u0646 \u0637\u0648\u0637\u06cc\u0633\u062a \u06a9\u0647 \u0627\u0646\u062f\u0631 \u0622\u06cc\u0646\u0647\u200c\u0633\u062a"}, "3": {"m1": "\u067e\u0633 \u0632 \u062c\u0646\u0633 \u062e\u0648\u06cc\u0634 \u0622\u0645\u0648\u0632 \u0633\u062e\u0646", "m2": "\u0628\u06cc\u200c\u062e\u0628\u0631 \u0627\u0632 \u0645\u06a9\u0631 \u0622\u0646 \u06af\u0631\u06af \u06a9\u0647\u0646"}, "4": {"m1": "\u0627\u0632 \u067e\u0633 \u0622\u06cc\u06cc\u0646\u0647 \u0645\u06cc\u200c\u0622\u0645\u0648\u0632\u062f\u0634", "m2": "\u0648\u0631\u0646\u0647 \u0646\u0627\u0645\u0648\u0632\u062f \u062c\u0632 \u0627\u0632 \u062c\u0646\u0633 \u062e\u0648\u062f\u0634"}, "5": {"m1": "\u06af\u0641\u062a \u0631\u0627 \u0622\u0645\u0648\u062e\u062a \u0632\u0627\u0646 \u0645\u0631\u062f \u0647\u0646\u0631", "m2": "\u0644\u06cc\u06a9 \u0627\u0632 \u0645\u0639\u0646\u06cc \u0648 \u0633\u0631\u0634 \u0628\u06cc\u200c\u062e\u0628\u0631"}, "6": {"m1": "\u0627\u0632 \u0628\u0634\u0631 \u0628\u06af\u0631\u0641\u062a \u0645\u0646\u0637\u0642 \u06cc\u06a9 \u0628\u0647 \u06cc\u06a9", "m2": "\u0627\u0632 \u0628\u0634\u0631 \u062c\u0632 \u0627\u06cc\u0646 \u0686\u0647 \u062f\u0627\u0646\u062f \u0637\u0648\u0637\u06cc\u06a9"}, "7": {"m1": "\u0647\u0645\u200c\u0686\u0646\u0627\u0646 \u062f\u0631 \u0622\u06cc\u0646\u0647\u0654 \u062c\u0633\u0645 \u0648\u0644\u06cc", "m2": "\u062e\u0648\u06cc\u0634 \u0631\u0627 \u0628\u06cc\u0646\u062f \u0645\u0631\u062f\u06cc \u0645\u0645\u062a\u0644\u06cc"}, "8": {"m1": "\u0627\u0632 \u067e\u0633 \u0622\u06cc\u06cc\u0646\u0647 \u0639\u0642\u0644 \u06a9\u0644 \u0631\u0627", "m2": "\u06a9\u06cc \u0628\u0628\u06cc\u0646\u062f \u0648\u0642\u062a \u06af\u0641\u062a \u0648 \u0645\u0627\u062c\u0631\u0627"}, "9": {"m1": "\u0627\u0648 \u06af\u0645\u0627\u0646 \u062f\u0627\u0631\u062f \u06a9\u0647 \u0645\u06cc\u200c\u06af\u0648\u06cc\u062f \u0628\u0634\u0631", "m2": "\u0648\u0627\u0646 \u06af\u0631 \u0633\u0631\u0633\u062a \u0648 \u0627\u0648 \u0632\u0627\u0646 \u0628\u06cc\u200c\u062e\u0628\u0631"}, "10": {"m1": "\u062d\u0631\u0641 \u0622\u0645\u0648\u0632\u062f \u0648\u0644\u06cc \u0633\u0631 \u0642\u062f\u06cc\u0645", "m2": "\u0627\u0648 \u0646\u062f\u0627\u0646\u062f \u0637\u0648\u0637\u06cc \u0627\u0633\u062a \u0627\u0648 \u0646\u06cc \u0646\u062f\u06cc\u0645"}, "11": {"m1": "\u0647\u0645 \u0635\u0641\u06cc\u0631 \u0645\u0631\u063a \u0622\u0645\u0648\u0632\u0646\u062f \u062e\u0644\u0642", "m2": "\u06a9\u06cc\u0646 \u0633\u062e\u0646 \u06a9\u0627\u0631 \u062f\u0647\u0627\u0646 \u0627\u0641\u062a\u0627\u062f \u0648 \u062d\u0644\u0642"}, "12": {"m1": "\u0644\u06cc\u06a9 \u0627\u0632 \u0645\u0639\u0646\u06cc \u0645\u0631\u063a\u0627\u0646 \u0628\u06cc\u200c\u062e\u0628\u0631", "m2": "\u062c\u0632 \u0633\u0644\u06cc\u0645\u0627\u0646 \u0642\u0631\u0627\u0646\u06cc \u062e\u0648\u0634\u200c\u0646\u0638\u0631"}, "13": {"m1": "\u062d\u0631\u0641 \u062f\u0631\u0648\u06cc\u0634\u0627\u0646 \u0628\u0633\u06cc \u0622\u0645\u0648\u062e\u062a\u0646\u062f", "m2": "\u0645\u0646\u0628\u0631 \u0648 \u0645\u062d\u0641\u0644 \u0628\u062f\u0627\u0646 \u0627\u0641\u0631\u0648\u062e\u062a\u0646\u062f"}, "14": {"m1": "\u06cc\u0627 \u0628\u0647 \u062c\u0632 \u0622\u0646 \u062d\u0631\u0641\u0634\u0627\u0646 \u0631\u0648\u0632\u06cc \u0646\u0628\u0648\u062f", "m2": "\u06cc\u0627 \u062f\u0631 \u0622\u062e\u0631 \u0631\u062d\u0645\u062a \u0622\u0645\u062f \u0631\u0647 \u0646\u0645\u0648\u062f"}}}