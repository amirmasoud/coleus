{"text": {"0": {"m1": "\u0645\u0631 \u062e\u0644\u06cc\u0641\u0647\u0654 \u0645\u0635\u0631 \u0631\u0627 \u063a\u0645\u0627\u0632 \u06af\u0641\u062a", "m2": "\u06a9\u0647 \u0634\u0647 \u0645\u0648\u0635\u0644 \u0628\u0647 \u062d\u0648\u0631\u06cc \u06af\u0634\u062a \u062c\u0641\u062a"}, "1": {"m1": "\u06cc\u06a9 \u06a9\u0646\u06cc\u0632\u06a9 \u062f\u0627\u0631\u062f \u0627\u0648 \u0627\u0646\u062f\u0631 \u06a9\u0646\u0627\u0631", "m2": "\u06a9\u0647 \u0628\u0647 \u0639\u0627\u0644\u0645 \u0646\u06cc\u0633\u062a \u0645\u0627\u0646\u0646\u062f\u0634 \u0646\u06af\u0627\u0631"}, "2": {"m1": "\u062f\u0631 \u0628\u06cc\u0627\u0646 \u0646\u0627\u06cc\u062f \u06a9\u0647 \u062d\u0633\u0646\u0634 \u0628\u06cc\u200c\u062d\u062f\u0633\u062a", "m2": "\u0646\u0642\u0634 \u0627\u0648 \u0627\u06cc\u0646\u0633\u062a \u06a9\u0647 \u0627\u0646\u062f\u0631 \u06a9\u0627\u063a\u0630\u0633\u062a"}, "3": {"m1": "\u0646\u0642\u0634 \u062f\u0631 \u06a9\u0627\u063a\u0630 \u0686\u0648 \u062f\u06cc\u062f \u0622\u0646 \u06a9\u06cc\u0642\u0628\u0627\u062f", "m2": "\u062e\u06cc\u0631\u0647 \u06af\u0634\u062a \u0648 \u062c\u0627\u0645 \u0627\u0632 \u062f\u0633\u062a\u0634 \u0641\u062a\u0627\u062f"}, "4": {"m1": "\u067e\u0647\u0644\u0648\u0627\u0646\u06cc \u0631\u0627 \u0641\u0631\u0633\u062a\u0627\u062f \u0622\u0646 \u0632\u0645\u0627\u0646", "m2": "\u0633\u0648\u06cc \u0645\u0648\u0635\u0644 \u0628\u0627 \u0633\u067e\u0627\u0647 \u0628\u0633 \u06af\u0631\u0627\u0646"}, "5": {"m1": "\u06a9\u0647 \u0627\u06af\u0631 \u0646\u062f\u0647\u062f \u0628\u0647 \u062a\u0648 \u0622\u0646 \u0645\u0627\u0647 \u0631\u0627", "m2": "\u0628\u0631\u06a9\u0646 \u0627\u0632 \u0628\u0646 \u0622\u0646 \u062f\u0631 \u0648 \u062f\u0631\u06af\u0627\u0647 \u0631\u0627"}, "6": {"m1": "\u0648\u0631 \u062f\u0647\u062f \u062a\u0631\u06a9\u0634 \u06a9\u0646 \u0648 \u0645\u0647 \u0631\u0627 \u0628\u06cc\u0627\u0631", "m2": "\u062a\u0627 \u06a9\u0634\u0645 \u0645\u0646 \u0628\u0631 \u0632\u0645\u06cc\u0646 \u0645\u0647 \u062f\u0631 \u06a9\u0646\u0627\u0631"}, "7": {"m1": "\u067e\u0647\u0644\u0648\u0627\u0646 \u0634\u062f \u0633\u0648\u06cc \u0645\u0648\u0635\u0644 \u0628\u0627 \u062d\u0634\u0645", "m2": "\u0628\u0627 \u0647\u0632\u0627\u0631\u0627\u0646 \u0631\u0633\u062a\u0645 \u0648 \u0637\u0628\u0644 \u0648 \u0639\u0644\u0645"}, "8": {"m1": "\u0686\u0648\u0646 \u0645\u0644\u062e\u0647\u0627 \u0628\u06cc\u200c\u0639\u062f\u062f \u0628\u0631 \u06af\u0631\u062f \u06a9\u0634\u062a", "m2": "\u0642\u0627\u0635\u062f \u0627\u0647\u0644\u0627\u06a9 \u0627\u0647\u0644 \u0634\u0647\u0631 \u06af\u0634\u062a"}, "9": {"m1": "\u0647\u0631 \u0646\u0648\u0627\u062d\u06cc \u0645\u0646\u062c\u0646\u06cc\u0642\u06cc \u0627\u0632 \u0646\u0628\u0631\u062f", "m2": "\u0647\u0645\u200c\u0686\u0648 \u06a9\u0648\u0647 \u0642\u0627\u0641 \u0627\u0648 \u0628\u0631 \u06a9\u0627\u0631 \u06a9\u0631\u062f"}, "10": {"m1": "\u0632\u062e\u0645 \u062a\u06cc\u0631 \u0648 \u0633\u0646\u06af\u0647\u0627\u06cc \u0645\u0646\u062c\u0646\u06cc\u0642", "m2": "\u062a\u06cc\u063a\u0647\u0627 \u062f\u0631 \u06af\u0631\u062f \u0686\u0648\u0646 \u0628\u0631\u0642 \u0627\u0632 \u0628\u0631\u06cc\u0642"}, "11": {"m1": "\u0647\u0641\u062a\u0647\u200c\u0627\u06cc \u06a9\u0631\u062f \u0627\u06cc\u0646 \u0686\u0646\u06cc\u0646 \u062e\u0648\u0646\u200c\u0631\u06cc\u0632 \u06af\u0631\u0645", "m2": "\u0628\u0631\u062c \u0633\u0646\u06af\u06cc\u0646 \u0633\u0633\u062a \u0634\u062f \u0686\u0648\u0646 \u0645\u0648\u0645 \u0646\u0631\u0645"}, "12": {"m1": "\u0634\u0627\u0647 \u0645\u0648\u0635\u0644 \u062f\u06cc\u062f \u067e\u06cc\u06af\u0627\u0631 \u0645\u0647\u0648\u0644", "m2": "\u067e\u0633 \u0641\u0631\u0633\u062a\u0627\u062f \u0627\u0632 \u062f\u0631\u0648\u0646 \u067e\u06cc\u0634\u0634 \u0631\u0633\u0648\u0644"}, "13": {"m1": "\u06a9\u0647 \u0686\u0647 \u0645\u06cc\u200c\u062e\u0648\u0627\u0647\u06cc \u0632 \u062e\u0648\u0646 \u0645\u0624\u0645\u0646\u0627\u0646", "m2": "\u06a9\u0634\u062a\u0647 \u0645\u06cc\u200c\u06af\u0631\u062f\u0646\u062f \u0632\u06cc\u0646 \u062d\u0631\u0628 \u06af\u0631\u0627\u0646"}, "14": {"m1": "\u06af\u0631 \u0645\u0631\u0627\u062f\u062a \u0645\u0644\u06a9 \u0634\u0647\u0631 \u0645\u0648\u0635\u0644\u0633\u062a", "m2": "\u0628\u06cc\u200c\u0686\u0646\u06cc\u0646 \u062e\u0648\u0646\u200c\u0631\u06cc\u0632 \u0627\u06cc\u0646\u062a \u062d\u0627\u0635\u0644\u0633\u062a"}, "15": {"m1": "\u0645\u0646 \u0631\u0648\u0645 \u0628\u06cc\u0631\u0648\u0646 \u0634\u0647\u0631 \u0627\u06cc\u0646\u06a9 \u062f\u0631 \u0622", "m2": "\u062a\u0627 \u0646\u06af\u06cc\u0631\u062f \u062e\u0648\u0646 \u0645\u0638\u0644\u0648\u0645\u0627\u0646 \u062a\u0631\u0627"}, "16": {"m1": "\u0648\u0631 \u0645\u0631\u0627\u062f\u062a \u0645\u0627\u0644 \u0648 \u0632\u0631 \u0648 \u06af\u0648\u0647\u0631\u0633\u062a", "m2": "\u0627\u06cc\u0646 \u0632 \u0645\u0644\u06a9 \u0634\u0647\u0631 \u062e\u0648\u062f \u0622\u0633\u0627\u0646\u200c\u062a\u0631\u0633\u062a"}}}