{"text": {"0": {"m1": "\u06a9\u0634\u0648\u0631 \u0647\u0646\u062f \u0627\u0633\u062a \u0628\u0647\u0634\u062a\u06cc \u0628\u0647 \u0632\u0645\u06cc\u0646", "m2": "\u062d\u062c\u062a\u0634 \u0627\u06cc\u0646\u06a9 \u0628\u0647 \u0631\u062e \u0635\u0641\u062d\u0647 \u0628\u0628\u06cc\u0646"}, "1": {"m1": "\u062d\u062c\u062a \u062b\u0627\u0628\u062a \u0686\u0648 \u062f\u0631 \u0622\u0646 \u0646\u06cc\u0633\u062a \u0634\u06a9\u06cc", "m2": "\u0647\u0641\u062a \u0628\u06af\u0648\u06cc\u0645 \u0628\u0647 \u062f\u0631\u0633\u062a\u06cc \u0646\u0647 \u06cc\u06a9\u06cc:"}, "2": {"m1": "\u0627\u0648\u0644\u0634 \u0627\u06cc\u0646 \u0627\u0633\u062a \u06a9\u0647 \u0622\u062f\u0645 \u0628\u0647 \u062c\u0646\u0627\u0646", "m2": "\u0686\u0648\u0646 \u0632 \u0639\u0635\u06cc \u062e\u0633\u062a\u06af\u06cc\u200c\u0627\u06cc \u06cc\u0627\u0641\u062a \u0686\u0646\u0627\u0646"}, "3": {"m1": "\u0632\u062e\u0645 \u0639\u0635\u06cc \u062e\u0648\u0631\u062f \u0628\u062f \u0627\u0646\u0633\u0627\u0646 \u0632 \u06a9\u0645\u06cc\u0646", "m2": "\u06a9\u0632 \u0641\u0644\u06a9 \u0627\u0641\u062a\u0627\u062f \u0628\u0647 \u0633\u062e\u062a\u06cc \u0628\u0647 \u0632\u0645\u06cc\u0646"}, "4": {"m1": "\u0639\u0635\u0645\u062a \u062d\u0642 \u062f\u0627\u0634\u062a \u0647\u0645\u06cc \u0686\u0648\u0646 \u0646\u06af\u0647\u0634", "m2": "\u062e\u0627\u0631\u0647\u0654 \u06a9\u0647\u0633\u0627\u0631 \u0634\u062f \u0627\u0637\u0644\u0633 \u0628\u0647 \u062a\u0647\u0634"}, "5": {"m1": "\u0622\u0645\u062f\u0646 \u0627\u0632 \u062e\u0644\u062f \u0628\u0647 \u0647\u0646\u062f\u0634 \u0628\u062f \u0627\u0632 \u0622\u0646", "m2": "\u06a9\u0627\u0646 \u06af\u0644 \u062c\u0646\u062a \u06a9\u0647 \u0632\u062f\u0634 \u0628\u0627\u062f \u062e\u0632\u0627\u0646"}, "6": {"m1": "\u06af\u0631 \u0628\u0647 \u062e\u0631\u0627\u0633\u0627\u0646 \u0648 \u0639\u0631\u0628 \u062a\u0627\u0632\u06cc \u0648 \u0686\u06cc\u0646", "m2": "\u06cc\u06a9 \u0646\u0641\u0633\u06cc \u0628\u0647\u0631\u0647 \u06af\u0631\u0641\u062a\u06cc \u0628\u0647 \u0632\u0645\u06cc\u0646"}, "7": {"m1": "\u06af\u0631\u0645\u06cc \u0648 \u0633\u0631\u062f\u06cc \u062e\u0631\u0627\u0633\u0627\u0646 \u0648 \u0639\u0631\u0628", "m2": "\u0648\u0627\u0646 \u0628\u0647 \u0631\u06cc \u0648 \u0686\u06cc\u0646 \u0639\u0630\u0627\u0628\u06cc\u0633\u062a \u0639\u062c\u0628"}, "8": {"m1": "\u0632\u0648 \u0634\u062f\u0647 \u067e\u0631\u0648\u0631\u062f\u0647 \u0628\u0647 \u0641\u0631\u062f\u0648\u0633 \u062f\u0631\u0648\u0646", "m2": "\u0686\u0648\u0646\u0634 \u0628\u0648\u062f\u06cc \u0637\u0627\u0642\u062a \u0627\u06cc\u0646 \u062f\u06cc\u062f\u0647 \u0648 \u062e\u0648\u0646"}, "9": {"m1": "\u06af\u0634\u062a \u0645\u062d\u0642\u0642 \u0686\u0648 \u0686\u0646\u06cc\u0646 \u0648\u0635\u0641 \u0645\u062a\u06cc\u0646", "m2": "\u06a9\u0627\u06cc\u0646 \u062d\u062f \u0647\u0646\u062f \u0627\u0633\u062a \u0628\u0647 \u0641\u0631\u062f\u0648\u0633 \u0628\u0631\u06cc\u0646"}, "10": {"m1": "\u0647\u0646\u062f \u0686\u0648 \u0627\u0632 \u062e\u0644\u062f \u0646\u0634\u0627\u0646 \u0628\u0648\u062f \u062f\u0631\u0648", "m2": "\u0632 \u0627\u0645\u0631 \u062e\u062f\u0627\u06cc\u0634 \u0642\u062f\u0645 \u0622\u0633\u0648\u062f \u062f\u0631\u0648"}, "11": {"m1": "\u0648\u0631 \u0646\u0647 \u0628\u062f\u0627\u0646 \u0646\u0627\u0632\u06a9\u06cc \u0627\u0632 \u062c\u0627\u06cc \u062f\u06af\u0631", "m2": "\u0622\u0645\u062f\u06cc \u0627\u0632 \u0631\u0646\u062c \u0641\u062a\u0627\u062f\u06cc \u0628\u0647 \u0636\u0631\u0631"}, "12": {"m1": "\u062d\u062c\u062a \u062f\u06cc\u06af\u0631 \u06a9\u0647 \u0632 \u0637\u0627\u0648\u0648\u0633 \u06a9\u0634\u0645", "m2": "\u0645\u0631\u063a \u062e\u0631\u062f \u0631\u0627 \u0628\u0647 \u0632\u0645\u06cc\u0646 \u0628\u0648\u0633 \u06a9\u0634\u0645"}, "13": {"m1": "\u06af\u0631 \u0646\u0647 \u0628\u0647\u0634\u062a \u0627\u0633\u062a \u0647\u0645\u06cc\u0646 \u0647\u0646\u062f \u0686\u0631\u0627", "m2": "\u0627\u0632 \u067e\u06cc \u0637\u0627\u0648\u0648\u0633 \u062c\u0646\u0627\u0646 \u06af\u0634\u062a \u0633\u0631\u0627"}, "14": {"m1": "\u0646\u06cc\u0633\u062a \u0686\u0646\u06cc\u0646 \u0637\u0627\u06cc\u0631 \u0641\u0631\u062f\u0648\u0633\u06cc \u0627\u06af\u0631", "m2": "\u0628\u0648\u06cc\u06cc \u0627\u0632 \u0628\u0627\u063a \u0628\u062f\u06cc \u062c\u0627\u06cc \u062f\u06af\u0631"}, "15": {"m1": "\u0644\u0627\u0628\u062f \u0627\u0632\u06cc\u0646 \u062c\u0627\u06cc \u0628\u062f\u0627\u0646 \u062c\u0627\u06cc \u0634\u062f\u06cc", "m2": "\u0648\u0632 \u067e\u06cc \u0631\u0641\u062a\u0646 \u0647\u0645\u0647 \u062a\u0646 \u067e\u0627\u06cc \u0634\u062f\u06cc"}, "16": {"m1": "\u0628\u0648\u062f \u0647\u0645\u06cc\u0646 \u062c\u0627 \u0686\u0648\u0632 \u0641\u0631\u062f\u0648\u0633 \u0627\u062b\u0631\u06cc", "m2": "\u062c\u0627\u0646\u0628 \u062f\u06cc\u06af\u0631 \u0646\u0641\u062a\u0627\u062f\u0634 \u06af\u0630\u0631\u06cc"}, "17": {"m1": "\u062d\u062c\u062a\u0645 \u0627\u06cc\u0646\u0633\u062a \u0633\u0648\u0645 \u06af\u0631 \u0628\u0647 \u0634\u06a9\u06cc", "m2": "\u06a9\u0627\u0645\u062f\u0646 \u0645\u0627\u0631 \u0632 \u0628\u0627\u063a \u0641\u0644\u06a9\u06cc"}, "18": {"m1": "\u0628\u0648\u062f \u0628\u0647 \u0647\u0645\u0631\u0627\u0647\u06cc \u0637\u0627\u0648\u0633 \u0648\u0635\u0641\u06cc", "m2": "\u0642\u0635\u0647 \u0686\u0646\u06cc\u0646 \u06af\u0641\u062a \u0641\u0642\u06cc\u0647 \u062d\u0646\u0641\u06cc"}, "19": {"m1": "\u0644\u06cc\u06a9 \u062c\u0632 \u0627\u0632 \u0647\u0646\u062f \u062f\u06af\u0631 \u06cc\u0627\u0641\u062a \u0645\u062d\u0644", "m2": "\u0632\u0627\u0646\u06a9\u0647 \u0647\u0645\u0647 \u0646\u06cc\u0634 \u0632\u062f\u0646 \u062f\u0627\u0634\u062a \u0639\u0645\u0644"}}}