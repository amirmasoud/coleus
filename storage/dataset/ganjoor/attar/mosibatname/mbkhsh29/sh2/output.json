{"text": {"0": {"m1": "\u0628\u0646\u062f\u0647\u0654 \u0631\u0627 \u0627\u0645\u062a\u062d\u0627\u0646 \u0645\u06cc\u06a9\u0631\u062f \u0634\u0627\u0647", "m2": "\u062e\u0648\u0627\u0646\u062f \u06cc\u06a9 \u0631\u0648\u0632\u06cc\u0634 \u067e\u06cc\u0634 \u062e\u0648\u062f \u067e\u06af\u0627\u0647"}, "1": {"m1": "\u06af\u0641\u062a \u0627\u06cc\u0646 \u062f\u0645 \u062f\u0627\u0645\u0646 \u0645\u0646 \u0628\u0631 \u0633\u0631\u0622\u0631", "m2": "\u0628\u0627 \u0645\u0646 \u0627\u0632 \u06cc\u06a9 \u062c\u06cc\u0628 \u0622\u0646\u06af\u0647 \u0633\u0631\u0628\u0631\u0622\u0631"}, "2": {"m1": "\u062a\u0627 \u0686\u0648 \u0628\u0627 \u0645\u0646 \u06cc\u06a9 \u06af\u0631\u06cc\u0628\u0627\u0646\u062a \u0628\u0648\u062f", "m2": "\u0647\u0631\u0686\u0647 \u0622\u0646 \u0645\u0646 \u0628\u0648\u062f \u0622\u0646\u062a \u0628\u0648\u062f"}, "3": {"m1": "\u0686\u0648\u0646 \u0645\u06cc\u0627\u0646 \u0645\u0627 \u06cc\u06a9\u06cc \u062d\u0627\u0635\u0644 \u0634\u0648\u062f", "m2": "\u06af\u0631 \u062e\u06cc\u0627\u0644\u0633\u062a \u0627\u0632\u062f\u0648\u0626\u06cc \u0628\u0627\u0637\u0644 \u0634\u0648\u062f"}, "4": {"m1": "\u062c\u0633\u0645 \u0648\u062c\u0627\u0646\u0645 \u062c\u0633\u0645 \u0648 \u062c\u0627\u0646 \u062a\u0648 \u0628\u0648\u062f", "m2": "\u0647\u0631\u0686\u0647 \u0647\u0633\u062a \u0622\u0646 \u0645\u0646 \u0622\u0646 \u062a\u0648 \u0628\u0648\u062f"}, "5": {"m1": "\u0628\u0646\u062f\u0647\u0654 \u0646\u0627\u062f\u0627\u0646 \u0628\u062c\u0633\u062a \u0627\u0632 \u062c\u0627\u06cc\u06af\u0627\u0647", "m2": "\u06a9\u0631\u062f \u0628\u06cc\u0631\u0648\u0646 \u0633\u0631 \u0632 \u062c\u06cc\u0628 \u067e\u0627\u062f\u0634\u0627\u0647"}, "6": {"m1": "\u06af\u0634\u062a \u0628\u0627 \u0634\u0627\u0647 \u062c\u0647\u0627\u0646 \u0647\u0645 \u067e\u06cc\u0631\u0647\u0646", "m2": "\u0630\u0631\u0647\u0654 \u0646\u0634\u0646\u0627\u062e\u062a \u062d\u062f \u062e\u0648\u06cc\u0634\u062a\u0646"}, "7": {"m1": "\u0686\u0648\u0646 \u0628\u0631\u0648\u0646 \u0622\u0648\u0631\u062f \u0633\u0631 \u0627\u0632 \u062c\u06cc\u0628 \u0634\u0627\u0647", "m2": "\u062e\u0648\u06cc\u0634\u062a\u0646 \u0631\u0627 \u0633\u0631 \u0646\u062f\u06cc\u062f \u0622\u0646\u062c\u0627\u06cc\u06af\u0627\u0647"}, "8": {"m1": "\u0634\u0647 \u0686\u0648 \u062f\u0631 \u0628\u06cc\u062d\u0631\u0645\u062a\u0633 \u0628\u0634\u0646\u0627\u062e\u062a\u0634", "m2": "\u062a\u0627\u06a9\u0647 \u062f\u0645 \u0632\u062f \u0633\u0631 \u0632 \u062a\u0646 \u0627\u0646\u062f\u0627\u062e\u062a\u0634"}, "9": {"m1": "\u0647\u0631\u06a9\u0647 \u067e\u0627\u06cc \u0627\u0632 \u062d\u062f \u062e\u0648\u062f \u0628\u0631\u062a\u0631 \u0646\u0647\u062f", "m2": "\u0633\u0631 \u062f\u0647\u062f \u0628\u0631 \u0628\u0627\u062f\u0648 \u062f\u06cc\u0646 \u0628\u0631 \u0633\u0631 \u0646\u0647\u062f"}, "10": {"m1": "\u0647\u0631\u06a9\u0647 \u062f\u0631 \u0628\u06cc \u062d\u0631\u0645\u062a\u06cc \u06af\u0627\u0645\u06cc \u0646\u0647\u0627\u062f", "m2": "\u062f\u0631 \u0634\u0642\u0627\u0648\u062a \u062e\u0648\u06cc\u0634 \u0631\u0627 \u062f\u0627\u0645\u06cc \u0646\u0647\u0627\u062f"}, "11": {"m1": "\u0628\u0646\u062f\u0647\u0654 \u0631\u0627 \u062a\u0627 \u0627\u062f\u0628 \u0646\u0628\u0648\u062f \u0646\u062e\u0633\u062a", "m2": "\u0628\u0646\u062f\u06af\u06cc \u0627\u0632 \u0648\u06cc \u06a9\u062c\u0627 \u0622\u06cc\u062f \u062f\u0631\u0633\u062a"}, "12": {"m1": "\u0686\u0648\u0646 \u0628\u0644\u0627\u06cc \u0642\u0631\u0628 \u062f\u06cc\u062f \u0622\u062f\u0645 \u0632 \u062f\u0648\u0631", "m2": "\u0633\u0648\u06cc \u0638\u0644\u0645\u062a \u0622\u0634\u06cc\u0627\u0646 \u0622\u0645\u062f \u0632 \u0646\u0648\u0631"}, "13": {"m1": "\u062f\u06cc\u062f \u062f\u0646\u06cc\u0627 \u06a9\u0634\u062a \u0632\u0627\u0631 \u062e\u0648\u06cc\u0634\u062a\u0646", "m2": "\u0644\u0627\u062c\u0631\u0645 \u06a9\u0631\u062f \u0627\u062e\u062a\u06cc\u0627\u0631 \u062e\u0648\u06cc\u0634\u062a\u0646"}, "14": {"m1": "\u0646\u06cc\u0633\u062a \u062f\u0646\u06cc\u0627 \u0628\u062f \u0627\u06af\u0631 \u06a9\u0627\u0631\u06cc \u06a9\u0646\u06cc", "m2": "\u0628\u062f \u0634\u0648\u062f \u06af\u0631 \u0639\u0632\u0645 \u062f\u06cc\u0646\u0627\u0631\u06cc \u06a9\u0646\u06cc"}}}