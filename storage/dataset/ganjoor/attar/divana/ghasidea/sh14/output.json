{"text": {"0": {"m1": "\u0647\u0631\u06a9\u0647 \u0628\u0631 \u067e\u0633\u062a\u0647\u0654 \u062e\u0646\u062f\u0627\u0646 \u062a\u0648 \u062f\u0646\u062f\u0627\u0646 \u062f\u0627\u0631\u062f", "m2": "\u062c\u0627\u0646 \u06a9\u0634\u062f \u067e\u06cc\u0634 \u0644\u0628 \u0644\u0639\u0644 \u062a\u0648 \u06af\u0631 \u062c\u0627\u0646 \u062f\u0627\u0631\u062f"}, "1": {"m1": "\u0634\u06a9\u0631 \u0648 \u067e\u0633\u062a\u0647\u0654 \u062e\u0646\u062f\u0627\u0646 \u062a\u0648 \u0645\u06cc\u200c\u062f\u0627\u0646\u06cc \u0686\u06cc\u0633\u062a", "m2": "\u0686\u0634\u0645 \u0633\u0648\u0632\u0646 \u06a9\u0647 \u062f\u0631\u0648 \u0686\u0634\u0645\u0647\u0654 \u062d\u06cc\u0648\u0627\u0646 \u062f\u0627\u0631\u062f"}, "2": {"m1": "\u0647\u0631\u06a9\u0647 \u0631\u0627 \u067e\u0633\u062a\u0647\u0654 \u062e\u0646\u062f\u0627\u0646 \u062a\u0648 \u0627\u0632 \u062f\u06cc\u062f\u0647 \u0628\u0634\u062f", "m2": "\u062f\u06cc\u062f\u0647 \u0627\u0632 \u067e\u0633\u062a\u0647\u0654 \u062e\u0646\u062f\u0627\u0646 \u062a\u0648 \u06af\u0631\u06cc\u0627\u0646 \u062f\u0627\u0631\u062f"}, "3": {"m1": "\u0644\u0628 \u062e\u0646\u062f\u0627\u0646 \u062a\u0648 \u0627\u0632 \u062a\u0646\u06af \u062f\u0644\u06cc \u067e\u0631 \u0646\u0645\u06a9 \u0627\u0633\u062a", "m2": "\u06a9\u0647 \u0628\u0633\u06cc \u0632\u06cc\u0631 \u0646\u0645\u06a9 \u067e\u0633\u062a\u0647\u0654 \u062e\u0646\u062f\u0627\u0646 \u062f\u0627\u0631\u062f"}, "4": {"m1": "\u067e\u0633\u062a\u0647 \u0631\u0627 \u0632\u06cc\u0631 \u0646\u0645\u06a9 \u0627\u0632 \u0644\u0628 \u062a\u0648 \u0633\u0648\u062e\u062a \u062c\u06af\u0631", "m2": "\u067e\u0633 \u0644\u0628\u062a \u0633\u0648\u062e\u062a\u0647\u200c\u0627\u06cc \u0631\u0627 \u0628\u0686\u0647 \u0633\u0648\u0632\u0627\u0646 \u062f\u0627\u0631\u062f"}, "5": {"m1": "\u0634\u06a9\u0631 \u0627\u0632 \u067e\u0633\u062a\u0647\u0654 \u0634\u06cc\u0631\u06cc\u0646 \u062a\u0648 \u0634\u0648\u0631 \u0622\u0648\u0631\u062f\u0647 \u0627\u0633\u062a", "m2": "\u06a9\u0647 \u0644\u0628 \u0686\u0648\u0646 \u0634\u06a9\u0631\u062a \u0634\u0648\u0631 \u0646\u0645\u06a9\u062f\u0627\u0646 \u062f\u0627\u0631\u062f"}, "6": {"m1": "\u062c\u0627\u0646\u0645 \u0627\u0632 \u067e\u0633\u062a\u0647\u0654 \u067e\u0631\u0634\u0648\u0631 \u062a\u0648 \u0686\u0648\u0646 \u067e\u0633\u062a\u0647 \u0634\u0648\u062f", "m2": "\u0646\u0645\u06a9 \u0633\u0648\u062e\u062a\u06af\u06cc \u0628\u0631 \u062f\u0644 \u0628\u0631\u06cc\u0627\u0646 \u062f\u0627\u0631\u062f"}, "7": {"m1": "\u0648\u0622\u0646\u06af\u0647 \u0627\u0632 \u067e\u0633\u062a\u0647\u0654 \u062a\u0648 \u0627\u06cc\u0646 \u062f\u0644 \u0634\u0648\u0631 \u0622\u0648\u0631\u062f\u0647", "m2": "\u0628\u0627 \u062c\u06af\u0631 \u067e\u0631 \u0646\u0645\u06a9 \u0627\u0646\u06af\u0634\u062a \u0628\u0647 \u062f\u0646\u062f\u0627\u0646 \u062f\u0627\u0631\u062f"}, "8": {"m1": "\u0639\u0642\u0644 \u0686\u0648\u0646 \u067e\u0633\u062a\u0647 \u062f\u0647\u0646 \u0645\u0627\u0646\u062f\u0647 \u0645\u06af\u0631 \u0627\u0632 \u0647\u0645 \u0628\u0627\u0632", "m2": "\u06a9\u0627\u0646 \u0686\u0647 \u0634\u0648\u0631 \u0627\u0633\u062a \u06a9\u0647 \u0627\u0648 \u0631\u0627 \u0634\u06a9\u0631\u0633\u062a\u0627\u0646 \u062f\u0627\u0631\u062f"}, "9": {"m1": "\u0627\u06cc \u0628\u062a \u067e\u0633\u062a\u0647\u200c\u062f\u0647\u0646 \u0628\u0631 \u062f\u0644 \u0648 \u062c\u0627\u0646\u0645 \u06cc\u06a9 \u0634\u0628", "m2": "\u0646\u0638\u0631\u06cc \u06a9\u0646 \u06a9\u0647 \u062f\u0644\u0645 \u062d\u0627\u0644 \u067e\u0631\u06cc\u0634\u0627\u0646 \u062f\u0627\u0631\u062f"}, "10": {"m1": "\u062a\u0648 \u0645\u0631\u0627 \u0647\u0631 \u0646\u0641\u0633\u06cc \u067e\u0633\u062a\u0647\u200c\u0635\u0641\u062a \u0645\u06cc\u200c\u0634\u06a9\u0646\u06cc", "m2": "\u062f\u0631\u062f\u0645 \u0627\u0632 \u062d\u062f \u0628\u0634\u062f \u0627\u06cc\u0646 \u06a9\u0627\u0631 \u0686\u0647 \u062f\u0631\u0645\u0627\u0646 \u062f\u0627\u0631\u062f"}, "11": {"m1": "\u062c\u0627\u0646 \u0622\u0645\u062f \u0628\u0647 \u0644\u0628 \u0627\u0632 \u067e\u0633\u062a\u0647\u0654 \u0631\u0639\u0646\u0627\u062a \u0645\u0631\u0627", "m2": "\u0641\u0631\u062e \u0622\u0646 \u06a9\u0648 \u0644\u0628 \u062e\u0648\u062f \u0628\u0631 \u0644\u0628 \u062c\u0627\u0646\u0627\u0646 \u062f\u0627\u0631\u062f"}, "12": {"m1": "\u0647\u06cc\u0686 \u0634\u06a9 \u0646\u06cc\u0633\u062a \u06a9\u0647 \u0686\u0648\u0646 \u067e\u0633\u062a\u0647 \u0646\u06af\u0646\u062c\u062f \u062f\u0631 \u067e\u0648\u0633\u062a", "m2": "\u0647\u0631 \u06a9\u0647 \u0644\u0628 \u0628\u0631 \u0644\u0628 \u0622\u0646 \u0644\u0639\u0644 \u0628\u062f\u062e\u0634\u0627\u0646 \u062f\u0627\u0631\u062f"}, "13": {"m1": "\u067e\u0633\u062a\u0647 \u062f\u0631 \u0628\u0627\u0632 \u06a9\u0646 \u0622\u062e\u0631 \u0686\u0647 \u062f\u0631 \u0628\u0633\u062a\u0647 \u062f\u0647\u06cc", "m2": "\u06a9\u0647 \u062f\u0644\u0645 \u06a9\u0627\u0631 \u0641\u0631\u0648 \u0628\u0633\u062a\u0647 \u0641\u0631\u0627\u0648\u0627\u0646 \u062f\u0627\u0631\u062f"}, "14": {"m1": "\u0632\u0644\u0641 \u0628\u0631\u06af\u06cc\u0631 \u06a9\u0647 \u062e\u0648\u0631\u0634\u06cc\u062f \u062a\u0648 \u062f\u0631 \u0633\u0627\u06cc\u0647 \u0628\u0645\u0627\u0646\u062f", "m2": "\u067e\u0633\u062a\u0647 \u0628\u06af\u0634\u0627\u06cc \u06a9\u0647 \u06cc\u0627\u0642\u0648\u062a \u062a\u0648 \u0645\u0631\u062c\u0627\u0646 \u062f\u0627\u0631\u062f"}, "15": {"m1": "\u0628\u0627 \u0645\u0646 \u0633\u0648\u062e\u062a\u0647 \u0686\u0648\u0646 \u067e\u0633\u062a\u0647 \u0628\u0631\u0648\u0646 \u0622\u06cc \u0627\u0632 \u067e\u0648\u0633\u062a", "m2": "\u0686\u0646\u062f\u0645 \u0627\u0632 \u067e\u0633\u062a\u0647\u0654 \u062e\u0646\u062f\u0627\u0646 \u062a\u0648 \u06af\u0631\u06cc\u0627\u0646 \u062f\u0627\u0631\u062f"}, "16": {"m1": "\u0645\u062d\u0646\u062a \u0627\u0632 \u0631\u0648\u06cc \u0641\u0631\u0648\u0628\u0633\u062a\u0647\u0654 \u062e\u0648\u06cc\u0634\u0645 \u0645\u0646\u0645\u0627\u06cc", "m2": "\u06a9\u0647 \u062f\u0644 \u0633\u0648\u062e\u062a\u0647 \u062e\u0648\u062f \u0645\u062d\u0646\u062a \u0647\u062c\u0631\u0627\u0646 \u062f\u0627\u0631\u062f"}, "17": {"m1": "\u0622\u0646 \u062e\u0637 \u0633\u0628\u0632 \u06a9\u0647 \u0627\u0632 \u067e\u0633\u062a\u0647\u0654 \u0644\u0639\u0644 \u062a\u0648 \u062f\u0645\u06cc\u062f", "m2": "\u062a\u0627\u0632\u06af\u06cc \u06af\u0644 \u0648 \u0633\u0631\u0633\u0628\u0632\u06cc \u0631\u06cc\u062d\u0627\u0646 \u062f\u0627\u0631\u062f"}, "18": {"m1": "\u0634\u062f\u0647 \u0627\u06cc\u0646 \u067e\u0633\u062a\u0647\u0654 \u062a\u0648 \u062a\u0627\u0632\u0647 \u0648 \u0633\u0631\u0633\u0628\u0632 \u0686\u0631\u0627\u0633\u062a", "m2": "\u0645\u06af\u0631 \u0627\u0632 \u0627\u0634\u06a9 \u0645\u0646 \u0633\u0648\u062e\u062a\u0647 \u0628\u0627\u0631\u0627\u0646 \u062f\u0627\u0631\u062f"}, "19": {"m1": "\u0646\u0647 \u06a9\u0647 \u062f\u0631 \u067e\u0633\u062a\u0647\u0654 \u062a\u0648 \u062d\u0642\u0647\u0654 \u062e\u0636\u0631 \u0627\u0633\u062a \u0646\u0647\u0627\u0646", "m2": "\u0622\u0628 \u0627\u0632 \u0686\u0634\u0645\u0647 \u062e\u0648\u0631\u062f \u062a\u0627\u0632\u0647 \u0631\u062e \u0627\u0632 \u0622\u0646 \u062f\u0627\u0631\u062f"}, "20": {"m1": "\u062f\u0644\u0645 \u0627\u0632 \u0638\u0644\u0645 \u062e\u0637 \u0641\u0633\u062a\u0642\u06cc\u062a \u0645\u06cc\u200c\u062e\u0648\u0627\u0647\u062f", "m2": "\u062a\u0627 \u062a\u0638\u0644\u0645 \u0632 \u062a\u0648 \u062f\u0631 \u062f\u0631\u06af\u0647 \u0633\u0644\u0637\u0627\u0646 \u062f\u0627\u0631\u062f"}, "21": {"m1": "\u062a\u0627 \u0628\u0647 \u062e\u0634\u0645\u062a \u0628\u0631\u0633\u062f \u0633\u0648\u062e\u062a\u0647 \u06af\u0631\u062f\u062f \u062e\u0648\u0631\u0634\u06cc\u062f", "m2": "\u0632\u0627\u0646 \u06a9\u0647 \u0628\u063a\u0636 \u062a\u0648 \u0634\u0647\u0627 \u0646\u06cc\u0645 \u0633\u067e\u0646\u062f\u0627\u0646 \u062f\u0627\u0631\u062f"}, "22": {"m1": "\u062a\u0627 \u0628\u0642\u0627\u06cc \u0645\u0646 \u062f\u0644\u0633\u0648\u062e\u062a\u0647 \u0635\u0648\u0631\u062a \u0628\u0646\u062f\u062f", "m2": "\u062e\u0627\u0637\u0631\u0645 \u0630\u0627\u062a \u062a\u0648 \u0631\u0627 \u0628\u0633\u062a\u0647\u0654 \u067e\u06cc\u0645\u0627\u0646 \u062f\u0627\u0631\u062f"}, "23": {"m1": "\u062a\u0627 \u062f\u0631\u06cc\u0646 \u062f\u0627\u06cc\u0631\u0647 \u0627\u06cc\u0646 \u0646\u0642\u0637\u0647\u0654 \u062e\u0627\u06a9\u06cc \u0628\u0631\u062c\u0627\u0633\u062a", "m2": "\u062a\u0627 \u06a9\u0647 \u067e\u0631\u06af\u0627\u0631 \u0641\u0644\u06a9 \u06af\u0631\u062f\u0634 \u062f\u0648\u0631\u0627\u0646 \u062f\u0627\u0631\u062f"}, "24": {"m1": "\u0633\u0627\u0644 \u0639\u0645\u0631 \u062a\u0648 \u06a9\u0647 \u0627\u0632 \u06af\u0631\u062f\u0634 \u062f\u0648\u0631\u0627\u0646 \u062e\u06cc\u0632\u062f", "m2": "\u0628\u0627\u062f \u0686\u0646\u062f\u0627\u0646 \u06a9\u0647 \u0627\u06af\u0631 \u0628\u0634\u0645\u0631\u062f \u0627\u0645\u06a9\u0627\u0646 \u062f\u0627\u0631\u062f"}, "25": {"m1": "\u062e\u0633\u0631\u0648\u0627 \u062e\u0627\u0637\u0631 \u0639\u0637\u0627\u0631 \u0628\u0647 \u0645\u062f\u0627\u062d\u06cc \u062a\u0648", "m2": "\u06a9\u0641 \u0645\u0648\u0633\u06cc \u0632 \u062f\u0645 \u0639\u06cc\u0633\u06cc \u0639\u0645\u0631\u0627\u0646 \u062f\u0627\u0631\u062f"}}}