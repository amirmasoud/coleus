{"text": {"0": {"m1": "\u0627\u0641\u062a\u062e\u0627\u0631 \u0647\u0645\u0647 \u0622\u0641\u0627\u0642\u06cc \u0648 \u0645\u0646\u0638\u0648\u0631 \u0645\u0646\u06cc", "m2": "\u0634\u0645\u0639 \u062c\u0645\u0639 \u0647\u0645\u0647 \u0639\u0634\u0627\u0642 \u0628\u0647 \u0647\u0631 \u0627\u0646\u062c\u0645\u0646\u06cc"}, "1": {"m1": "\u0628\u0647 \u0633\u0631 \u0632\u0644\u0641 \u067e\u0631\u06cc\u0634\u0627\u0646 \u062a\u0648 \u062f\u0644\u0647\u0627\u06cc \u067e\u0631\u06cc\u0634", "m2": "\u0647\u0645\u0647 \u062e\u0648 \u06a9\u0631\u062f\u0647 \u0686\u0648 \u0639\u0627\u0631\u0641 \u0628\u0647 \u067e\u0631\u06cc\u0634\u0627\u0646 \u0648\u0637\u0646\u06cc"}, "2": {"m1": "\u0632 \u0686\u0647 \u0631\u0648 \u0634\u06cc\u0634\u06c0 \u062f\u0644 \u0645\u06cc \u0634\u06a9\u0646\u06cc\u061f", "m2": "\u062a\u06cc\u0634\u0647 \u0628\u0631 \u0631\u06cc\u0634\u06c0 \u062c\u0627\u0646 \u0627\u0632 \u0686\u0647 \u0632\u0646\u06cc\u061f"}, "3": {"m1": "\u0633\u06cc\u0645 \u0627\u0646\u062f\u0627\u0645 \u0648\u0644\u06cc \u0633\u0646\u06af \u062f\u0644\u06cc", "m2": "\u0633\u0633\u062a \u067e\u06cc\u0645\u0627\u0646\u06cc \u0648 \u067e\u06cc\u0645\u0627\u0646 \u0634\u06a9\u0646\u06cc"}, "4": {"m1": "\u0627\u06af\u0631 \u062f\u0631\u062f \u0645\u0646 \u0628\u0647 \u062f\u0631\u0645\u0627\u0646 \u0631\u0633\u062f \u0686\u0647 \u0645\u06cc\u0634\u0647\u061f", "m2": "\u0634\u0628 \u0647\u062c\u0631 \u0627\u06af\u0631 \u0628\u0647 \u067e\u0627\u06cc\u0627\u0646 \u0631\u0633\u062f \u0686\u0647 \u0645\u06cc\u0634\u0647\u061f"}, "5": {"m1": "\u0627\u06af\u0631 \u0628\u0627\u0631 \u062f\u0644 \u0628\u0647 \u0645\u0646\u0632\u0644 \u0631\u0633\u062f \u0686\u0647 \u06af\u0631\u062f\u062f\u061f", "m2": "\u0633\u0631 \u0645\u0646 \u0627\u06af\u0631 \u0628\u0647 \u0633\u0627\u0645\u0627\u0646 \u0631\u0633\u062f \u0686\u0647 \u0645\u06cc\u0634\u0647\u061f\u064e"}, "6": {"m1": "\u0633\u0631 \u0645\u0646 \u0627\u06af\u0631 \u0628\u0647 \u0633\u0627\u0645\u0627\u0646 \u0631\u0633\u062f \u0686\u0647 \u0645\u06cc\u0634\u0647\u061f\u064e", "m2": "\u06af\u0631 \u0639\u0627\u0631\u0641 \u00ab\u0646\u0638\u0627\u0645 \u0627\u0644\u0633\u0644\u0637\u0627\u0646\u00bb \u0634\u0648\u062f \u0686\u0647 \u0645\u06cc\u0634\u0647\u061f"}, "7": {"m1": "\u0632 \u063a\u0645\u062a \u062e\u0648\u0646  \u0645\u06cc \u06af\u0631\u06cc\u0645 \u0628\u0646\u06af\u0631 \u0686\u0648\u0646 \u0645\u06cc \u06af\u0631\u06cc\u0645", "m2": "\u0632 \u0645\u0698\u0647 \u062f\u0644 \u0645\u06cc \u0631\u06cc\u0632\u062f \u0632 \u062c\u06af\u0631 \u062e\u0648\u0646 \u0645\u06cc \u0622\u06cc\u062f"}, "8": {"m1": "\u0627\u0641\u062a\u062e\u0627\u0631 \u062f\u0644 \u0648 \u062c\u0627\u0646 \u0645\u06cc \u0622\u06cc\u062f", "m2": "\u06cc\u0627\u0631 \u0628\u06cc \u067e\u0631\u062f\u0647 \u0639\u06cc\u0627\u0646 \u0645\u06cc \u0622\u06cc\u062f"}, "9": {"t2": "***", "t1": "***"}, "10": {"m1": "\u062a\u0648 \u0627\u06af\u0631 \u0639\u0634\u0648\u0647 \u0628\u0631 \u062e\u0633\u0631\u0648\u067e\u0631\u0648\u06cc\u0632 \u06a9\u0646\u06cc", "m2": "\u0647\u0645\u0686\u0648 \u0641\u0631\u0647\u0627\u062f \u0631\u0648\u062f \u062f\u0631 \u0639\u0642\u0628 \u06a9\u0648\u0647 \u06a9\u0646\u06cc"}, "11": {"m1": "\u0645\u062a\u0641\u0631\u0642 \u0646\u0634\u0648\u062f \u0645\u062c\u0645\u0639 \u062f\u0644\u0647\u0627\u06cc \u067e\u0631\u06cc\u0634", "m2": "\u062a\u0648 \u0627\u06af\u0631 \u0634\u0627\u0646\u0647 \u0628\u0631 \u0622\u0646 \u0632\u0644\u0641 \u067e\u0631\u06cc\u0634\u0627\u0646 \u0646\u0632\u0646\u06cc"}, "12": {"m1": "\u0632 \u0686\u0647 \u0631\u0648 \u0634\u06cc\u0634\u06c0 \u062f\u0644 \u0645\u06cc \u0634\u06a9\u0646\u06cc\u061f", "m2": "\u062a\u06cc\u0634\u0647 \u0628\u0631 \u0631\u06cc\u0634\u06c0 \u062c\u0627\u0646 \u0627\u0632 \u0686\u0647 \u0632\u0646\u06cc\u061f"}, "13": {"m1": "\u0633\u06cc\u0645 \u0627\u0646\u062f\u0627\u0645 \u0648\u0644\u06cc \u0633\u0646\u06af \u062f\u0644\u06cc", "m2": "\u0633\u0633\u062a \u067e\u06cc\u0645\u0627\u0646\u06cc \u0648 \u067e\u06cc\u0645\u0627\u0646 \u0634\u06a9\u0646\u06cc (\u0633\u0633\u062a \u067e\u06cc\u0645\u0627\u0646\u06cc \u0648 \u067e\u06cc\u0645\u0627\u0646 \u0634\u06a9\u0646\u06cc)"}, "14": {"m1": "\u0628\u0647 \u0686\u0634\u0645\u062a \u06a9\u0647 \u062f\u06cc\u062f\u0647 \u0627\u0632 \u0635\u0648\u0631\u062a\u062a \u0646\u06af\u06cc\u0631\u0645", "m2": "\u0627\u06af\u0631 \u0645\u06cc \u06a9\u0634\u06cc \u0648 \u06af\u0631 \u0645\u06cc \u0632\u0646\u06cc \u0628\u0647 \u062a\u06cc\u0631\u0645"}, "15": {"t2": "\u062a\u0648 \u0633\u0644\u0637\u0627\u0646 \u062d\u0633\u0646 \u0648 \u0645\u0646 \u06a9\u0645\u062a\u0631\u06cc\u0646 \u0641\u0642\u06cc\u0631\u0645", "t1": "\u062a\u0648 \u0633\u0644\u0637\u0627\u0646 \u062d\u0633\u0646 \u0648 \u0645\u0646 \u06a9\u0645\u062a\u0631\u06cc\u0646 \u0641\u0642\u06cc\u0631\u0645"}, "16": {"m1": "\u06af\u0632\u0646\u062f\u0645 \u0627\u06af\u0631 \u0632 \u0633\u0644\u0637\u0627\u0646 \u0631\u0633\u062f \u0686\u0647 \u0645\u06cc\u0634\u0647\u061f", "m2": "\u06af\u0632\u0646\u062f\u0645 \u0627\u06af\u0631 \u0632 \u0633\u0644\u0637\u0627\u0646 \u0631\u0633\u062f \u0686\u0647 \u0645\u06cc\u0634\u0647\u061f"}, "17": {"m1": "\u0632 \u063a\u0645\u062a \u062e\u0648\u0646 \u0645\u06cc \u06af\u0631\u06cc\u0645", "m2": "\u0628\u0646\u06af\u0631 \u0686\u0648\u0646 \u0645\u06cc \u06af\u0631\u06cc\u0645"}, "18": {"m1": "\u0632 \u0645\u0698\u0647 \u062f\u0644 \u0645\u06cc \u0631\u06cc\u0632\u062f", "m2": "\u0632 \u062c\u06af\u0631 \u062e\u0648\u0646 \u0645\u06cc \u0622\u06cc\u062f"}, "19": {"m1": "\u0628\u0647 \u0633\u0631 \u06a9\u0634\u062a\u06c0 \u062c\u0627\u0646 \u0645\u06cc \u0622\u06cc\u062f", "m2": "\u062e\u0648\u0646 \u0635\u062f \u0633\u0644\u0633\u0644\u0647 \u062c\u0627\u0646 \u0645\u06cc \u0631\u06cc\u0632\u062f"}}}