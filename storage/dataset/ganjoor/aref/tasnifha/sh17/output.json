{"text": {"0": {"m1": "\u0628\u0645\u0627\u0646\u062f\u06cc\u0645 \u0645\u0627\u060c \u0645\u0633\u062a\u0642\u0644 \u0634\u062f \u0627\u0631\u0645\u0646\u0633\u062a\u0627\u0646", "m2": "(\u0627\u0631\u0645\u0646\u0633\u062a\u0627\u0646 \u0627\u0631\u0645\u0646\u0633\u062a\u0627\u0646 \u0634\u062f \u0627\u0631\u0645\u0646\u0633\u062a\u0627\u0646)"}, "1": {"m1": "\u0632\u0628\u0631\u062f\u0633\u062a \u0634\u062f\u060c \u0632\u06cc\u0631\u062f\u0633\u062a \u0632\u06cc\u0631\u062f\u0633\u062a\u0627\u0646", "m2": "(\u062f\u0633\u062a\u0627\u0646 \u0632\u06cc\u0631\u062f\u0633\u062a\u0627\u0646 \u0632\u06cc\u0631\u062f\u0633\u062a\u0627\u0646)"}, "2": {"m1": "\u0627\u06af\u0631 \u0645\u0644\u06a9 \u062c\u0645 \u0634\u062f \u062e\u0631\u0627\u0628\u060c \u06af\u0648 \u0628\u0647 \u0633\u0627\u0642\u06cc", "m2": "(\u06af\u0648 \u0628\u0647 \u0633\u0627\u0642\u06cc \u062a\u0648 \u0628\u0627\u0634 \u0628\u0627\u0642\u06cc \u062a\u0648 \u0628\u0627\u0634 \u0628\u0627\u0642\u06cc)"}, "3": {"m1": "\u0635\u0628\u0648\u062d\u06cc \u0628\u062f\u0647 \u0632\u0627\u0646 \u0634\u0631\u0627\u0628 \u0634\u0628 \u0628\u0647 \u0645\u0633\u062a\u0627\u0646", "m2": "(\u0628\u062f\u0647 \u0628\u0647 \u0645\u0633\u062a\u0627\u0646\u060c \u0628\u062f\u0647 \u0628\u0647 \u0645\u0633\u062a\u0627\u0646)"}, "4": {"m1": "\u0628\u0633 \u0627\u0633\u062a \u0645\u0627 \u0631\u0627 \u0647\u0648\u0627\u06cc \u0628\u0633\u062a\u0627\u0646", "m2": "\u06a9\u0647 \u06af\u0644 \u062f\u0648 \u0631\u0648\u0632 \u0627\u0633\u062a \u062f\u0631 \u06af\u0644\u0633\u062a\u0627\u0646"}, "5": {"m1": "\u0628\u062f\u0647 \u0645\u06cc \u06a9\u0647 \u062f\u0646\u06cc\u0627 \u062f\u0648 \u0631\u0648\u0632 \u0628\u06cc\u0634\u062a\u0631 \u0646\u06cc\u0633\u062a", "m2": "\u0645\u062e\u0648\u0631 \u063a\u0645 \u06a9\u0647 \u0627\u06cc\u0631\u0627\u0646 \u0632 \u0645\u0627 \u062e\u0631\u0627\u0628\u062a\u0631 \u0646\u06cc\u0633\u062a"}, "6": {"t2": "(\u062c\u0627\u0646\u0645 \u062e\u0631\u0627\u0628 \u0646\u06cc\u0633\u062a)", "t1": "\u0628\u062f \u0622\u0646 \u0645\u0644\u062a\u06cc \u06a9\u0632 \u062e\u0631\u0627\u0628\u06cc\u0634 \u062e\u0628\u0631 \u0646\u06cc\u0633\u062a"}, "7": {"m1": "\u0622\u0647 \u06a9\u0647 \u06af\u0631 \u0622\u0647 \u067e\u0631 \u0628\u06af\u06cc\u0631\u062f", "m2": "\u062f\u0627\u0645\u0646 \u0647\u0631 \u062e\u0634\u06a9 \u0648 \u062a\u0631 \u0628\u06af\u06cc\u0631\u062f"}, "8": {"m1": "\u0628\u06cc \u062e\u0628\u0631\u0627\u0646 \u0631\u0627 \u062e\u0628\u0631 \u0631\u0633\u0627\u0646\u06cc\u062f", "m2": "\u0632 \u0634\u0627\u0646 \u0628\u0631 \u0645\u0627 \u062e\u0628\u0631 \u0628\u06af\u06cc\u0631\u062f"}, "9": {"t2": "**********", "t1": "**********"}, "10": {"m1": "\u0632 \u062f\u0627\u0631\u0627\u0644\u0641\u0646\u0648\u0646 \u0628\u0647 \u062c\u0632 \u062c\u0646\u0648\u0646 \u0646\u062f\u0627\u0631\u06cc\u0645", "m2": "\u0645\u0639\u0627\u0631\u0641 \u0646\u0647\u060c \u0645\u0627\u0644\u06cc\u0647 \u0646\u06cc\u060c \u0642\u0634\u0648\u0646 \u0646\u062f\u0627\u0631\u06cc\u0645"}, "11": {"m1": "\u0628\u0631\u0641\u062a \u062d\u0633 \u0645\u0644\u062a \u0622\u0646 \u0686\u0646\u0627\u0646 \u06a9\u0647 \u06af\u0648\u0626\u06cc", "m2": "\u0628\u0647 \u062a\u0646 \u062c\u0627\u0646 \u0628\u0647 \u062c\u0627\u0646 \u0631\u06af \u0628\u0647 \u0631\u06af \u062e\u0648\u0646 \u0646\u062f\u0627\u0631\u06cc\u0645"}, "12": {"t2": "\u0628\u0647 \u063a\u06cc\u0631 \u0639\u0634\u0642 \u062c\u0646\u0648\u0646 \u0646\u062f\u0627\u0631\u06cc\u0645", "t1": "\u0628\u0647 \u063a\u06cc\u0631 \u0639\u0634\u0642 \u062c\u0646\u0648\u0646 \u0646\u062f\u0627\u0631\u06cc\u0645"}, "13": {"m1": "\u0686\u0647 \u062e\u0648\u0646 \u062a\u0648\u0627\u0646 \u062e\u0648\u0631\u062f \u06a9\u0647 \u062e\u0648\u0646 \u0646\u062f\u0627\u0631\u06cc\u0645", "m2": "\u0646\u062f\u0627\u0631\u06cc\u0645 \u0627\u06af\u0631 \u0647\u06cc\u0686\u060c \u0647\u06cc\u0686 \u063a\u0645 \u0646\u062f\u0627\u0631\u06cc\u0645"}, "14": {"m1": "\u0632  \u0627\u0633\u0628\u0627\u0628 \u0628\u062f\u0628\u062e\u062a\u06cc \u0647\u06cc\u0686 \u06a9\u0645 \u0646\u062f\u0627\u0631\u06cc\u0645", "m2": "\u0648\u062c\u0648\u062f\u06cc \u06a9\u0647 \u0628\u0627\u0634\u062f \u0628\u0647 \u0627\u0632 \u0639\u062f\u0645 \u0646\u062f\u0627\u0631\u06cc\u0645"}, "15": {"m1": "\u067e\u0646\u062f \u067e\u062f\u0631 \u06af\u0631 \u067e\u0633\u0631 \u0628\u06af\u06cc\u0631\u062f", "m2": "\u062f\u0627\u0645\u0646 \u0641\u0636\u0644 \u0648 \u0647\u0646\u0631 \u0628\u06af\u06cc\u0631\u062f"}, "16": {"m1": "\u0645\u0627 \u0632 \u0646\u06cc\u0627\u06a9\u0627\u0646 \u0646\u0634\u0627\u0646 \u0686\u0647 \u062f\u0627\u0631\u06cc\u0645\u061f", "m2": "\u062a\u0627 \u06a9\u0647 \u0632 \u0645\u0627 \u0622\u0646 \u062f\u06af\u0631 \u0628\u06af\u06cc\u0631\u062f"}}}