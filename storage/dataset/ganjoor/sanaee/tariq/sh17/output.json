{"text": {"0": {"m1": "\u062e\u0631\u062f\u0645 \u062f\u0648\u0634 \u0627\u0646\u062f\u067e\u0646 \u0645\u0639\u0646\u06cc", "m2": "\u0646\u06a9\u062a\u0647\u200c\u0627\u06cc \u0686\u0646\u062f \u0646\u063a\u0632 \u06a9\u0631\u062f \u0627\u0645\u0644\u06cc"}, "1": {"m1": "\u06af\u0641\u062a \u0634\u0647\u0631\u06cc\u200c\u06a9\u0647 \u062c\u0627 \u0648 \u0645\u0628\u06cc\u0646 \u0645\u0627\u0633\u062a", "m2": "\u0635\u062d\u0646 \u0627\u0648 \u0633\u0642\u0641\u200c\u06af\u0646\u0628\u062f \u0627\u0639\u0644\u0627\u200c\u0633\u062a"}, "2": {"m1": "\u062e\u0627\u06a9 \u0627\u0648 \u0631\u0627\u0633\u062a \u0646\u06a9\u0647\u062a \u0639\u0646\u0628\u0631", "m2": "\u0622\u0628 \u0627\u0648 \u0631\u0627\u0633\u062a\u200c\u060c \u0644\u0630\u062a \u0634\u06a9\u0631"}, "3": {"m1": "\u0646\u0632 \u0628\u0631\u0648\u062f\u062a \u062f\u0631 \u0627\u0648 \u0627\u062b\u0631 \u0628\u06cc\u0646\u06cc\u200c", "m2": "\u0646\u0632 \u062d\u0631\u0627\u0631\u062a \u062f\u0631 \u0627\u0648 \u0634\u0631\u0631 \u0628\u06cc\u0646\u06cc"}, "4": {"m1": "\u0627\u0646\u062f\u0631 \u0622\u0646 \u0634\u0647\u0631 \u0645\u0627 \u06af\u0644\u0633\u062a\u0627\u0646\u0647\u0627\u0633\u062a", "m2": "\u06a9\u0647 \u0686\u0645\u0646\u0647\u0627\u0634 \u0646\u0632\u0647\u062a \u062c\u0627\u0646\u0647\u0627\u0633\u062a"}, "5": {"m1": "\u0637\u0648\u0637\u06cc\u0627\u0646 \u0628\u06cc\u0646\u06cc \u0646\u062f\u0631 \u0622\u0646 \u0628\u0633\u062a\u0627\u0646", "m2": "\u0647\u0645\u0647 \u0631\u0627 \u0630\u06a9\u0631 \u062d\u0642 \u0628\u0648\u062f \u0627\u0644\u062d\u0627\u0646"}, "6": {"m1": "\u0686\u0648\u0646 \u06a9\u0646\u062f \u0644\u0637\u0641 \u0627\u0648 \u062a\u0639\u0644\u0645\u0634\u0627\u0646", "m2": "\u00ab\u200c\u0631\u0628\u06cc \u0627\u0644\u0644\u0647\u00bb \u0628\u0648\u062f \u062a\u0631\u0646\u0645\u0651\u0634\u0627\u0646"}, "7": {"m1": "\u062f\u0631 \u0686\u0645\u0646\u0647\u0627\u0634 \u0628\u0644\u0628\u0644\u0627\u0646 \u060c \u06af\u0648\u06cc\u0627", "m2": "\u0646\u063a\u0645\u0647\u200c\u0634\u0627\u0646 \u062c\u0645\u0644\u0647 \u00ab\u200c\u0631\u0628\u0646\u0627 \u0627\u0644\u0627\u0639\u0644\u06cc\u00bb"}, "8": {"m1": "\u00ab\u200c\u0645\u0642\u0639\u062f \u0635\u062f\u0642\u200c\u00bb \u0627\u0632\u0648 \u0648\u0644\u0627\u06cc\u062a \u0645\u0627\u0633\u062a", "m2": "\u0647\u0631\u06a9\u0647 \u0622\u0646\u062c\u0627\u0633\u062a \u062f\u0631 \u062d\u0645\u0627\u06cc\u062a \u0645\u0627\u0633\u062a"}, "9": {"m1": "\u0647\u0645\u06af\u0627\u0646 \u062e\u0627\u0635 \u062d\u0636\u0631\u062a \u0633\u0644\u0637\u0627\u0646", "m2": "\u062c\u0633\u062a\u0647 \u0627\u0632 \u0628\u0646\u062f \u0627\u0646\u062c\u0645 \u0648 \u0627\u0631\u06a9\u0627\u0646"}, "10": {"m1": "\u0631\u0647\u0631\u0648\u0627\u0646 \u0628\u06cc\u0646\u06cc \u0627\u0632 \u0633\u0631 \u063a\u06cc\u0631\u062a", "m2": "\u0647\u0645\u0647 \u0627\u0648\u0641\u062a\u0627\u062f\u0647 \u062f\u0631 \u0631\u0647 \u062d\u06cc\u0631\u062a"}, "11": {"m1": "\u0633\u0627\u06a9\u0646\u0627\u0646 \u0628\u06cc\u0646\u06cc \u0627\u0632 \u0633\u0631 \u0627\u062e\u0644\u0627\u0635", "m2": "\u0686\u0634\u0645 \u0628\u06af\u0634\u0627\u062f\u0647 \u0628\u0631 \u0633\u0631\u0627\u062f\u0642 \u062e\u0627\u0635"}, "12": {"m1": "\u0686\u0648\u0646 \u0628\u062f\u0627\u0646 \u0634\u0647\u0631 \u062c\u0627\u0646 \u0641\u0631\u0648\u062f \u0622\u06cc\u06cc", "m2": "\u067e\u0646 \u0647\u0645\u0647 \u062f\u0631\u062f\u0633\u0631 \u0628\u06cc\u0627\u0633\u0627\u06cc\u06cc\u200c"}, "13": {"m1": "\u0645\u0633\u06a9\u0646 \u0648 \u062c\u0627\u06cc\u06af\u0627\u0647 \u0645\u0627 \u0628\u06cc\u0646\u06cc", "m2": "\u0645\u062c\u0644\u0633 \u062e\u0627\u0635 \u0634\u0627\u0647 \u0645\u0627 \u0628\u06cc\u0646\u06cc"}, "14": {"m1": "\u062e\u0644\u0639\u062a \u0634\u0627\u0647\u200c\u060c \u0628\u06cc \u0628\u062f\u0646 \u067e\u0648\u0634\u06cc", "m2": "\u0628\u0627\u062f\u0647\u0654 \u0634\u0648\u0642\u200c\u060c \u0628\u06cc \u062f\u0647\u0646 \u0646\u0648\u0634\u06cc"}, "15": {"m1": "\u0646\u063a\u0645\u0647\u0654 \u0628\u0644\u0628\u0644\u0627\u0646 \u0631\u0647 \u0634\u0646\u0648\u06cc", "m2": "\u00ab\u200c\u0648\u062d\u062f\u0647 \u0644\u0627\u0634\u0631\u06cc\u06a9 \u0644\u0647\u00bb \u0634\u0646\u0648\u06cc"}}}