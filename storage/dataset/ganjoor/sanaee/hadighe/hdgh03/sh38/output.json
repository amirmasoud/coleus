{"text": {"0": {"p": "\u0630\u06a9\u0631 \u0627\u0644\u0627\u0645\u0627\u0645 \u0627\u0644\u0639\u0627\u0644\u0645 \u0627\u0644\u0639\u0627\u0631\u0641 \u062c\u0645\u0627\u0644\u200c\u0627\u0644\u062f\u0651\u06cc\u0646 \u06a9\u0645\u0627\u0644 \u0627\u0644\u0627\u0633\u0644\u0627\u0645 \u0645\u0641\u062a\u06cc \u0627\u0644\u0634\u0631\u0642 \u0648\u0627\u0644\u063a\u0631\u0628 \u0633\u06cc\u0651\u062f\u0627\u0644\u0639\u0644\u0645\u0627\u0621 \u0648\u0627\u0644\u0641\u0642\u0647\u0627\u0621 \u0645\u0641\u062a\u0627\u062d \u0627\u0644\u0634\u0631\u06cc\u0639\u0629 \u0633\u0631\u0627\u062c\u200c\u0627\u0644\u0633\u0646\u0651\u0647 \u06a9\u0646\u0648\u0632 \u0627\u0644\u0627\u062d\u0627\u062f\u06cc\u062b \u0627\u0628\u06cc\u200c\u0639\u0628\u062f\u0627\u0644\u0644\u0651\u0670\u0647 \u0645\u062d\u0645\u0651\u062f\u0628\u0646 \u0627\u062f\u0631\u06cc\u0633 \u0634\u0627\u0641\u0639\u06cc \u0631\u062d\u0645\u0629\u200c\u0627\u0644\u0644\u0651\u0670\u0647 \u0639\u0644\u06cc\u0647."}, "1": {"m1": "\u0686\u0648\u0646 \u0641\u0631\u0648\u0634\u062f \u0686\u0631\u0627\u063a \u062f\u06cc\u0646 \u0646\u0628\u06cc", "m2": "\u0631\u0648\u06cc \u0628\u0646\u0645\u0648\u062f \u0645\u0627\u0647 \u0645\u0637\u0651\u0644\u0628\u06cc"}, "2": {"m1": "\u0627\u0632 \u067e\u0633 \u0628\u062f\u0631\u0650 \u062f\u06cc\u0646 \u0646\u0647 \u062f\u06cc\u0631 \u0686\u0647 \u0632\u0648\u062f", "m2": "\u0622\u0641\u062a\u0627\u0628\u0650 \u0632\u0645\u0627\u0646\u0647 \u0686\u0647\u0631\u0647 \u0646\u0645\u0648\u062f"}, "3": {"m1": "\u0631\u064e\u0648 \u0628\u062c\u0648 \u0627\u0631 \u0632 \u062f\u06cc\u062f\u0647 \u062f\u0631 \u0637\u0644\u0628\u06cc", "m2": "\u0631\u0627\u0647\u0650 \u0634\u0631\u0639 \u0627\u0632 \u0627\u0645\u0627\u0645 \u0645\u0637\u0644\u0628\u06cc"}, "4": {"m1": "\u0627\u0635\u0644 \u0627\u0648 \u062f\u0631 \u0642\u0648\u0627\u0639\u062f \u0648 \u0628\u0646\u06cc\u0627\u0646", "m2": "\u0641\u0631\u0639 \u0646\u0633\u0644 \u0645\u064f\u0639\u062f \u0628\u0646 \u0639\u062f\u0646\u0627\u0646"}, "5": {"m1": "\u0646\u0633\u0628\u0634 \u0628\u0627 \u0631\u0633\u0648\u0644 \u067e\u06cc\u0648\u0633\u062a\u0647", "m2": "\u0627\u062f\u0628\u0634 \u0627\u0632 \u0641\u0636\u0648\u0644 \u0628\u06af\u0633\u0633\u062a\u0647"}, "6": {"m1": "\u062f\u0631\u0633\u0650 \u062f\u06cc\u0646 \u0633\u0627\u062e\u062a \u0627\u0632 \u067e\u06cc \u062a\u0642\u062f\u06cc\u0633", "m2": "\u0635\u062f\u0631 \u0633\u0646\u0651\u062a \u0645\u062d\u0645\u0651\u062f \u0627\u062f\u0631\u06cc\u0633"}, "7": {"m1": "\u0627\u0632 \u067e\u06cc \u0637\u0627\u0644\u0628\u0627\u0646 \u0646\u0648\u0631\u0650 \u06cc\u0642\u06cc\u0646", "m2": "\u062e\u0648\u06cc\u0634\u062a\u0646 \u0648\u0642\u0641 \u06a9\u0631\u062f\u0647 \u0628\u0631 \u062f\u0631\u0650 \u062f\u06cc\u0646"}, "8": {"m1": "\u0628\u0631 \u062e\u0648\u062f \u0627\u0632 \u0639\u0642\u0644 \u062e\u0648\u06cc\u0634 \u0647\u06cc\u0686 \u0646\u0633\u0627\u062e\u062a", "m2": "\u062f\u0631 \u0631\u0647 \u0634\u0631\u0639 \u062e\u0648\u06cc\u0634\u062a\u0646 \u062f\u0631\u0628\u0627\u062e\u062a"}, "9": {"m1": "\u0645\u0635\u0637\u0641\u06cc \u06af\u0641\u062a\u0647 \u0627\u0648 \u0634\u0646\u06cc\u062f\u0647 \u0628\u0647 \u062c\u0627\u0646", "m2": "\u0632\u0627\u0646 \u0646\u0645\u0648\u062f\u0647 \u0628\u0647 \u0634\u0631\u0639 \u0627\u0648 \u0628\u0631\u0647\u0627\u0646"}, "10": {"m1": "\u062a\u0627 \u062d\u062f\u06cc\u062b\u0650 \u067e\u06cc\u0645\u0628\u0631 \u0627\u0648 \u062e\u0648\u0627\u0646\u062f\u0647", "m2": "\u0628\u0631 \u062e\u0648\u062f\u0634 \u0627\u0639\u062a\u0645\u0627\u062f \u0646\u0627\u0645\u0627\u0646\u062f\u0647"}, "11": {"m1": "\u0622\u0646\u06a9\u0647 \u0646\u0627\u0631\u062f \u0686\u0646\u0648 \u0635\u0646\u0627\u06cc\u0639 \u062f\u0647\u0631", "m2": "\u06a9\u0631\u062f\u0647 \u062e\u0635\u0645\u0627\u0646 \u062f\u06cc\u0646\u0650 \u062d\u0642 \u0631\u0627 \u0642\u0647\u0631"}, "12": {"m1": "\u0628\u0648\u062f\u0647 \u062f\u0631 \u0631\u0627\u0647\u0650 \u062f\u06cc\u0646 \u0627\u0645\u0627\u0645 \u0628\u0647 \u062d\u0642", "m2": "\u06a9\u0647 \u0627\u0645\u0627\u0645\u062a \u0648\u0631\u0627 \u0633\u0632\u062f \u0645\u0637\u0644\u0642"}, "13": {"m1": "\u0647\u0645\u0651\u062a\u0634 \u062f\u06cc\u0646 \u0641\u0631\u0648\u0632 \u0648 \u0639\u0631\u0634 \u06af\u0630\u0627\u0631", "m2": "\u0641\u0637\u0646\u062a\u0634 \u0641\u062a\u0646\u0647 \u0633\u0648\u0632 \u0648 \u0634\u063a\u0644 \u06af\u0632\u0627\u0631"}, "14": {"m1": "\u06a9\u0631\u062f\u0647 \u0634\u0627\u06af\u0631\u062f\u06cc \u062d\u062f\u06cc\u062b \u0646\u0628\u06cc", "m2": "\u063a\u0627\u0634\u06cc\u0647 \u0628\u0631 \u06a9\u062a\u0641 \u0632 \u067e\u06cc\u0634 \u0648\u0635\u06cc"}, "15": {"m1": "\u0631\u0627\u06a9\u0628\u0627\u0646\u0650 \u062f\u0631\u0634 \u0627\u062b\u06cc\u0631 \u0641\u0631\u0633", "m2": "\u0647\u0645\u0631\u0647\u0627\u0646\u0650 \u062f\u0645\u0634 \u0639\u0628\u06cc\u0631 \u0646\u0641\u0633"}, "16": {"m1": "\u062c\u0648\u062f \u0627\u0648 \u0647\u0645\u0686\u0648 \u06a9\u0639\u0628\u0647 \u0627\u0646\u0628\u0647 \u062c\u0648\u06cc", "m2": "\u062e\u064f\u0644\u0642 \u0627\u0648 \u0686\u0648\u0646 \u0628\u0647\u0627\u0631 \u062e\u0646\u062f\u0627\u0646 \u0631\u0648\u06cc"}, "17": {"m1": "\u0634\u0631\u0639 \u062a\u0627 \u06a9\u062f\u062e\u062f\u0627\u06cc \u0627\u06cc\u0646 \u062e\u0627\u0646\u0647 \u0627\u0633\u062a", "m2": "\u0639\u0642\u0644\u0647\u0627 \u0631\u0627 \u0642\u0628\u0627 \u063a\u0644\u0627\u0645\u0627\u0646\u0647 \u0627\u0633\u062a"}, "18": {"m1": "\u062f\u0631 \u062a\u0631\u0627\u062c\u0639 \u0632 \u062e\u0644\u0642 \u0648 \u062e\u0644\u0642\u0634 \u062c\u06cc\u0646", "m2": "\u062f\u0631 \u062a\u0631\u0651\u0641\u0639 \u0632 \u0639\u0644\u0645 \u0648 \u062d\u0644\u0645\u0634 \u062f\u06cc\u0646"}, "19": {"m1": "\u062f\u06cc\u0646 \u0645\u0631\u0641\u0651\u0647 \u0628\u0647 \u062e\u0648\u0628 \u06af\u0641\u062a\u0627\u0631\u0634", "m2": "\u0647\u0645\u0647 \u0639\u0627\u0644\u0645 \u0631\u0633\u06cc\u062f\u0647 \u0622\u062b\u0627\u0631\u0634"}, "20": {"m1": "\u0628\u062e\u0634\u0634 \u0627\u0632 \u062d\u0642 \u0628\u0647\u0627\u0646\u0647 \u0628\u0631 \u0633\u0639\u062f\u0633\u062a", "m2": "\u062c\u0648\u062f \u0627\u0632 \u0627\u0628\u0631\u0648\u0644\u0627\u0641 \u0628\u0631 \u0631\u0639\u062f\u0633\u062a"}, "21": {"m1": "\u06af\u0631 \u067e\u0631\u0627\u06af\u0646\u062f\u0647 \u0632\u0648 \u0634\u062f\u0646\u062f \u0627\u0648\u0628\u0627\u0634", "m2": "\u0633\u0646\u0651\u062a \u0645\u0635\u0637\u0641\u06cc \u0627\u0632 \u0627\u0648 \u0634\u062f \u0641\u0627\u0634"}, "22": {"m1": "\u0647\u0631 \u062d\u062f\u06cc\u062b\u06cc \u06a9\u0647 \u0645\u0635\u0637\u0641\u06cc \u0628\u0631\u06af\u0641\u062a", "m2": "\u0634\u0631\u062d\u0634 \u0627\u0648 \u062f\u0627\u062f \u0648 \u0639\u0644\u0645 \u0622\u0646 \u0646\u0646\u0647\u0641\u062a"}, "23": {"m1": "\u06a9\u0644\u06a9 \u0627\u0648 \u0634\u062f \u062e\u0632\u0627\u0646\u0647\u0654 \u0627\u0633\u0631\u0627\u0631", "m2": "\u062f\u0631\u0633 \u0627\u0648 \u0631\u0627 \u0641\u0631\u0634\u062a\u0647 \u0634\u062f \u0646\u0638\u0651\u0627\u0631"}, "24": {"m1": "\u06af\u0627\u0647 \u062a\u062f\u0631\u06cc\u0633 \u0648 \u06af\u0627\u0647 \u0634\u0631\u062d\u0650 \u0639\u0644\u0648\u0645", "m2": "\u062d\u0627\u06a9\u0645 \u0627\u0648 \u0628\u0648\u062f \u0648 \u0639\u0627\u0644\u0645\u06cc \u0645\u062d\u06a9\u0648\u0645"}, "25": {"m1": "\u06af\u0627\u0645 \u0648 \u06a9\u0627\u0645\u0634 \u0686\u0648 \u0645\u0631\u06a9\u0628\u0627\u0646\u0650 \u0634\u06a9\u0627\u0631", "m2": "\u0646\u064e\u0648\u0631 \u0648 \u0646\u064f\u0648\u0631\u0634 \u0686\u0648 \u0631\u0648\u0632\u06af\u0627\u0631 \u0628\u0647\u0627\u0631"}, "26": {"m1": "\u0638\u0627\u0647\u0631 \u0637\u0627\u0647\u0631\u0634 \u0645\u062f\u0628\u0651\u0631 \u0628\u0650\u0631", "m2": "\u062e\u0627\u0637\u0631 \u0639\u0627\u0637\u0631\u0634 \u0645\u0641\u0633\u0651\u0631 \u0633\u0631\u0651"}, "27": {"m1": "\u0648\u0627\u0639\u0638\u0650 \u0639\u0642\u0644 \u0648 \u062d\u0627\u0641\u0638\u0650 \u062a\u0646\u0632\u06cc\u0644", "m2": "\u0645\u064f\u062d\u0631\u0645\u0650 \u0639\u0634\u0642 \u0648 \u0645\u064e\u062d\u0631\u0645 \u062a\u0623\u0648\u06cc\u0644"}, "28": {"m1": "\u062e\u06cc\u0644\u0650 \u0637\u0627\u0644\u0648\u062a \u0631\u0627 \u0633\u06a9\u06cc\u0646\u0647\u0654 \u062d\u0644\u0645", "m2": "\u0627\u064f\u0645\u0651\u062a\u0650\u0650 \u0646\u0648\u062d \u0631\u0627 \u0633\u0641\u06cc\u0646\u0647\u0654 \u0639\u0644\u0645"}, "29": {"m1": "\u0635\u0648\u0631\u062a\u0634 \u0639\u06cc\u0646 \u0639\u0644\u0645 \u0648 \u062f\u0627\u0646\u0634 \u0628\u0648\u062f", "m2": "\u0632\u0627\u0646\u06a9\u0647 \u0628\u0633 \u067e\u0627\u06a9 \u062e\u0627\u0646\u062f\u0627\u0646\u0634 \u0628\u0648\u062f"}, "30": {"m1": "\u062e\u0627\u0646\u062f\u0627\u0646\u06cc \u06a9\u0647 \u0627\u0632 \u0642\u0631\u06cc\u0634 \u0628\u064f\u0648\u064e\u062f", "m2": "\u0628\u06cc\u0634\u06a9\u06cc \u0633\u0631\u0641\u0631\u0627\u0632\u0650 \u062c\u06cc\u0634 \u0628\u064f\u0648\u064e\u062f"}, "31": {"m1": "\u0647\u0633\u062a \u06a9\u0648\u062a\u0647 \u0632 \u0628\u0647\u0631 \u0634\u0631\u0639 \u0648 \u0634\u0639\u0627\u0631", "m2": "\u062f\u0633\u062a \u0627\u0648 \u0647\u0645\u0686\u0648 \u0632\u06cc\u0631 \u067e\u0648\u0634 \u0628\u0647\u0627\u0631"}, "32": {"m1": "\u0633\u062e\u0646\u0634 \u0628\u06a9\u0631 \u0648 \u0644\u0641\u0638 \u062f\u0648\u0634\u06cc\u0632\u0647", "m2": "\u0645\u0630\u0647\u0628 \u0627\u0648 \u062f\u0631\u0633\u062a \u0648 \u067e\u0627\u06a9\u06cc\u0632\u0647"}, "33": {"m1": "\u06cc\u0627\u0641\u062a\u0647 \u062d\u0644\u0651\u0647\u0654 \u0635\u0641\u0627 \u0648 \u0635\u0641\u0627\u062a", "m2": "\u062f\u0633\u062a \u0648 \u06a9\u0644\u06a9\u0634 \u0628\u0647 \u06a9\u0627\u0631 \u0634\u0631\u0639 \u062b\u0628\u0627\u062a"}, "34": {"m1": "\u0627\u0632 \u063a\u0631\u0648\u0631\u0650 \u0633\u067e\u0647\u0631 \u0645\u0624\u0645\u0646 \u0638\u0646", "m2": "\u0648\u0632 \u0645\u0631\u0648\u0631 \u0632\u0645\u0627\u0646\u0647 \u0627\u06cc\u0645\u0646 \u062a\u0646"}, "35": {"m1": "\u06af\u0631\u0686\u0647 \u06a9\u0698 \u0633\u06cc\u0631\u062a\u0645 \u0628\u06af\u0648\u06cc\u0645 \u0631\u0627\u0633\u062a", "m2": "\u06a9\u0647 \u0633\u062e\u0627 \u0648 \u0645\u0631\u0648\u062a \u0627\u06cc\u0634\u0627\u0646 \u0631\u0627\u0633\u062a"}, "36": {"m1": "\u062f\u06cc\u0646 \u0627\u0632 \u0627\u0648 \u06cc\u0627\u0641\u062a \u0632\u06cc\u0646\u062a \u0648 \u0631\u0648\u0646\u0642", "m2": "\u062f\u0631 \u062a\u0628\u0639 \u0645\u062a\u0641\u0642 \u0634\u062f\u0646\u062f \u0641\u0650\u0631\u0642"}, "37": {"m1": "\u0628\u0646\u062f\u0647\u0654 \u0627\u0648 \u0634\u062f\u0647 \u0648\u0636\u06cc\u0639 \u0648 \u0634\u0631\u06cc\u0641", "m2": "\u0639\u0627\u0644\u0645 \u0648 \u0639\u0627\u0631\u0641 \u0648 \u0648\u062c\u06cc\u0647 \u0648 \u0639\u0641\u06cc\u0641"}, "38": {"m1": "\u0639\u0644\u0645 \u062f\u06cc\u0646 \u062a\u0627 \u0628\u062f\u0648 \u0633\u067e\u0631\u062f \u0642\u0636\u0627", "m2": "\u062c\u0647\u0644 \u0632 \u0627\u0633\u0644\u0627\u0645 \u0628\u0631\u06af\u0631\u0641\u062a \u0641\u0646\u0627"}, "39": {"m1": "\u0632\u0646\u062f\u0642\u0647 \u0627\u0632 \u0639\u0644\u0645 \u0627\u0648 \u0647\u0632\u06cc\u0645\u062a \u06af\u0634\u062a", "m2": "\u0637\u0627\u0644\u0628\u0650 \u0639\u0644\u0645 \u0628\u0627 \u063a\u0646\u06cc\u0645\u062a \u06af\u0634\u062a"}}}