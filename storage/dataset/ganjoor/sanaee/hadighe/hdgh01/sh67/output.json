{"text": {"0": {"m1": "\u0627\u0632 \u067e\u06cc \u0631\u0627\u0647 \u062d\u0642 \u06a9\u0645 \u0627\u0632 \u06a9\u0648\u062f\u06a9", "m2": "\u0646\u062a\u0648\u0627\u0646 \u0628\u0648\u062f\u0646 \u0627\u06cc \u06a9\u0645 \u0627\u0632 \u06cc\u06a9 \u06cc\u06a9"}, "1": {"m1": "\u06af\u0631 \u062f\u0631 \u0622\u0645\u0648\u062e\u062a\u0646 \u06a9\u0646\u062f \u062a\u0642\u0635\u06cc\u0631", "m2": "\u0647\u0631\u0686\u0647 \u062e\u0648\u0627\u0647\u062f \u0633\u0628\u06a9 \u0632\u0648\u06cc \u0628\u067e\u0630\u06cc\u0631"}, "2": {"m1": "\u0628\u0647 \u062a\u0644\u0637\u0651\u0641 \u0628\u062f\u0627\u0631 \u0648 \u0628\u0646\u0648\u0627\u0632\u0634", "m2": "\u062e\u06cc\u0631\u0647 \u062f\u0631 \u0627\u0646\u062a\u0638\u0627\u0631 \u0645\u06af\u062f\u0627\u0632\u0634"}, "3": {"m1": "\u062f\u0631 \u06a9\u0646\u0627\u0631\u0634 \u0646\u0647 \u0622\u0646 \u0632\u0645\u0627\u0646 \u06a9\u0627\u06a9\u0627", "m2": "\u062a\u0627 \u0634\u0648\u062f \u0631\u0627\u0636\u06cc \u0648 \u0645\u06a9\u0646\u0634 \u062c\u0641\u0627"}, "4": {"m1": "\u062f\u0631 \u0646\u0645\u0627\u0632\u0634 \u0646\u0647 \u0622\u0646 \u0632\u0645\u0627\u0646 \u06a9\u0627\u0646\u062c\u0627", "m2": "\u062a\u0627 \u0634\u0648\u062f \u0633\u0631\u062e \u0686\u0647\u0631\u0647\u200c\u0627\u0634 \u0686\u0648 \u0644\u06a9\u0627"}, "5": {"m1": "\u0648\u0631 \u0646\u062e\u0648\u0627\u0646\u062f \u0628\u062e\u0648\u0627\u0647 \u0632\u0648\u062f \u062f\u0648\u0627\u0644", "m2": "\u06af\u0648\u0634\u0647\u0627\u06cc\u0634 \u0628\u06af\u06cc\u0631 \u0648 \u0633\u062e\u062a \u0628\u0645\u0627\u0644"}, "6": {"m1": "\u0628\u0647 \u0645\u0639\u0644\u0645 \u0646\u0645\u0627\u06cc \u062a\u0647\u062f\u06cc\u062f\u0634", "m2": "\u062a\u0627 \u0628\u0648\u062f \u06af\u0648\u0634\u0645\u0627\u0644 \u062a\u0645\u0647\u06cc\u062f\u0634"}, "7": {"m1": "\u0628\u0646\u062f \u0648 \u062d\u0628\u0633\u0634 \u06a9\u0646\u062f \u0628\u0647 \u062e\u0627\u0646\u0647\u0654 \u0645\u0648\u0634", "m2": "\u0645\u06cc\u0631 \u0645\u0648\u0634\u0627\u0646 \u06a9\u0646\u062f \u0641\u0634\u0631\u062f\u0647 \u06af\u0644\u0648\u0634"}, "8": {"m1": "\u062f\u0631 \u0631\u0647 \u0622\u062e\u0631\u062a \u0632 \u0628\u0647\u0631 \u0634\u0646\u0648\u062f", "m2": "\u06a9\u0645\u062a\u0631 \u0627\u0632 \u06a9\u0648\u062f\u06a9\u06cc \u0646\u0634\u0627\u06cc\u062f \u0628\u0648\u062f"}, "9": {"m1": "\u062e\u0644\u062f \u06a9\u0627\u06a9\u0627\u06cc \u062a\u0633\u062a \u0647\u0627\u0646 \u0628\u0634\u062a\u0627\u0628", "m2": "\u0628\u0647 \u062f\u0648 \u0631\u06a9\u0639\u062a \u0628\u0647\u0634\u062a \u0631\u0627 \u062f\u0631\u06cc\u0627\u0628"}, "10": {"m1": "\u0648\u0631\u0646\u0647 \u0634\u062f \u0645\u0648\u0634\u062e\u0627\u0646\u0647 \u062f\u0648\u0632\u062e\u0650 \u062a\u0648", "m2": "\u062f\u0631 \u0631\u0647 \u0622\u0646 \u0633\u0631\u0627\u06cc \u0628\u0631\u0632\u062e\u0650 \u062a\u0648"}, "11": {"m1": "\u0631\u0648 \u0628\u0647 \u06a9\u062a\u0651\u0627\u0628 \u0627\u0646\u0628\u06cc\u0627 \u06cc\u06a9 \u0686\u0646\u062f", "m2": "\u0628\u0631 \u062e\u0648\u062f \u0627\u06cc\u0646 \u062c\u0647\u0644 \u0648 \u0627\u06cc\u0646 \u0633\u062a\u0645 \u0645\u067e\u0633\u0646\u062f"}, "12": {"m1": "\u0644\u0648\u062d\u06cc \u0627\u0632 \u0634\u0631\u062d \u0627\u0646\u0628\u06cc\u0627 \u0628\u0631\u062e\u0648\u0627\u0646", "m2": "\u0686\u0648\u0646 \u0646\u062f\u0627\u0646\u06cc \u0628\u0631\u0648 \u0628\u062e\u0648\u0627\u0646 \u0648 \u0628\u062f\u0627\u0646"}, "13": {"m1": "\u062a\u0627 \u0645\u06af\u0631 \u06cc\u0627\u0631 \u0627\u0646\u0628\u06cc\u0627 \u06af\u0631\u062f\u06cc", "m2": "\u0632\u06cc\u0646 \u062c\u0647\u0627\u0644\u062a \u0645\u06af\u0631 \u062c\u062f\u0627 \u06af\u0631\u062f\u06cc"}, "14": {"m1": "\u062f\u0631 \u062c\u0647\u0627\u0646 \u062e\u0631\u0627\u0628 \u067e\u0631 \u0632 \u0636\u0631\u0631", "m2": "\u0627\u0632 \u062c\u0647\u0627\u0644\u062a \u0645\u062f\u0627\u0646 \u062a\u0648 \u0647\u06cc\u0686 \u0628\u062a\u0631"}}}