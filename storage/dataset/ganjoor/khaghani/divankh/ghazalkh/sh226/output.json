{"text": {"0": {"m1": "\u0645\u0646\u0645 \u0622\u0646 \u06a9\u0632 \u0637\u0631\u0628 \u063a\u0645\u06cc\u0646 \u0628\u0627\u0634\u0645", "m2": "\u0644\u06cc\u06a9\u0646 \u0627\u0632 \u063a\u0645 \u0637\u0631\u0628 \u06af\u0632\u06cc\u0646 \u0628\u0627\u0634\u0645"}, "1": {"m1": "\u062f\u0631\u062f \u063a\u0645 \u0628\u0627\u06cc\u062f\u0645 \u0646\u0647 \u0635\u0627\u0641 \u0637\u0631\u0628", "m2": "\u0632\u0627\u0646\u06a9\u0647 \u0628\u0627 \u062f\u0631\u062f\u06a9\u0634 \u0642\u0631\u06cc\u0646 \u0628\u0627\u0634\u0645"}, "2": {"m1": "\u06cc\u06a9\u200c\u062f\u0645 \u0648 \u0646\u06cc\u0645 \u062c\u0627\u0646 \u06af\u0631\u0648 \u062f\u0627\u0631\u0645", "m2": "\u0645\u0646 \u0645\u0642\u0627\u0645\u0631 \u062f\u0644\u0645 \u0686\u0646\u06cc\u0646 \u0628\u0627\u0634\u0645"}, "3": {"m1": "\u0633\u0647 \u06cc\u06a9 \u062f\u0648\u0633\u062a\u0627\u0646 \u0633\u0647 \u0634\u0634 \u062e\u0648\u0627\u0647\u0645", "m2": "\u06a9\u0647 \u0647\u0645\u0647 \u0628\u0627 \u06af\u0631\u0648 \u0628\u0647 \u06a9\u06cc\u0646 \u0628\u0627\u0634\u0645"}, "4": {"m1": "\u0648\u0631 \u0633\u0647 \u0634\u0634 \u0646\u0642\u0634 \u062e\u0648\u06cc\u0634 \u06cc\u06a9 \u0628\u06cc\u0646\u0645", "m2": "\u0647\u0645 \u0646\u062e\u0648\u0627\u0647\u0645 \u06a9\u0647 \u0646\u0642\u0634\u200c\u0628\u06cc\u0646 \u0628\u0627\u0634\u0645"}, "5": {"m1": "\u0631\u0627\u0633\u062a \u0628\u06cc\u0631\u0648\u0646 \u062f\u0647\u0645 \u0647\u0645\u0647 \u06a9\u0698 \u062e\u0648\u06cc\u0634", "m2": "\u06af\u0631\u0686\u0647 \u06a9\u0698 \u0646\u0642\u0634 \u0686\u0648\u0646 \u0646\u06af\u06cc\u0646 \u0628\u0627\u0634\u0645"}, "6": {"m1": "\u0622\u0641\u062a\u0627\u0628\u0645 \u06a9\u0647 \u062e\u0627\u06a9 \u0631\u0647 \u0628\u0648\u0633\u0645", "m2": "\u0646\u0647 \u0647\u0644\u0627\u0644\u0645 \u06a9\u0647 \u0646\u0627\u0632\u0646\u06cc\u0646 \u0628\u0627\u0634\u0645"}, "7": {"m1": "\u0646\u0647 \u0686\u0646\u0648\u0647\u0645 \u06a9\u0645\u0627\u0646 \u06a9\u0634\u0645 \u0628\u0631 \u062e\u0644\u0642", "m2": "\u0628\u0647\u0631 \u06cc\u06a9 \u0634\u0628 \u06a9\u0647 \u062f\u0631 \u06a9\u0645\u06cc\u0646 \u0628\u0627\u0634\u0645"}, "8": {"m1": "\u062c\u0631\u0639\u0647 \u0628\u0631\u0686\u06cc\u0646\u062f \u0622\u0641\u062a\u0627\u0628 \u0627\u0632 \u062e\u0627\u06a9", "m2": "\u0645\u0646 \u0647\u0645 \u0627\u0632 \u062e\u0627\u06a9 \u062c\u0631\u0639\u0647 \u0686\u06cc\u0646 \u0628\u0627\u0634\u0645"}, "9": {"m1": "\u06a9\u0648 \u062e\u0631\u0627\u0628\u0627\u062a \u06a9\u0647\u0641 \u0634\u06cc\u0631 \u062f\u0644\u0627\u0646", "m2": "\u062a\u0627 \u0633\u06af \u0622\u0633\u062a\u0627\u0646 \u0646\u0634\u06cc\u0646 \u0628\u0627\u0634\u0645"}, "10": {"m1": "\u0646\u0647 \u0646\u0647 \u0622\u0646 \u062c\u0645\u0639 \u0647\u0641\u062a \u0645\u0631\u062f\u0627\u0646\u0646\u062f", "m2": "\u0645\u0646 \u06a9\u0647 \u0628\u0627\u0634\u0645 \u06a9\u0647 \u0647\u0634\u062a\u0645\u06cc\u0646 \u0628\u0627\u0634\u0645"}, "11": {"m1": "\u0645\u0646 \u06a9\u0647 \u0628\u0627\u0634\u0645 \u06a9\u0647 \u062f\u0631 \u0648\u062c\u0648\u062f \u0646\u06cc\u0645", "m2": "\u062a\u0627 \u062f\u0631 \u0627\u06cc\u0646 \u062f\u0648\u0631 \u06a9\u0645 \u062d\u0632\u06cc\u0646 \u0628\u0627\u0634\u0645"}, "12": {"m1": "\u06cc\u0627 \u0628\u0647 \u0635\u062f \u0633\u0627\u0644 \u067e\u06cc\u0634 \u0627\u0632\u06cc\u0646 \u0628\u0648\u062f\u0645", "m2": "\u06cc\u0627 \u0628\u0647 \u0635\u062f \u0633\u0627\u0644 \u0628\u0639\u062f \u0627\u0632\u06cc\u0646 \u0628\u0627\u0634\u0645"}, "13": {"m1": "\u0686\u0648\u0646 \u0645\u0646 \u0627\u0632 \u0639\u0647\u062f \u0647\u06cc\u0686 \u0646\u0646\u062f\u06cc\u0634\u0645", "m2": "\u0627\u0632 \u0628\u062f\u06cc \u0639\u0647\u062f \u0686\u0648\u0646 \u063a\u0645\u06cc\u0646 \u0628\u0627\u0634\u0645"}, "14": {"m1": "\u0686\u0648\u0646 \u0645\u0646 \u0627\u0645\u0631\u0648\u0632 \u062f\u0631 \u0645\u06cc\u0627\u0646\u0647 \u0646\u06cc\u0645", "m2": "\u0686\u0647 \u0645\u06cc\u0627\u0646\u062c\u06cc \u06a9\u0641\u0631 \u0648 \u062f\u06cc\u0646 \u0628\u0627\u0634\u0645"}, "15": {"m1": "\u0645\u0646 \u0646\u0647 \u062e\u0627\u0642\u0627\u0646\u06cc\u0645 \u06a9\u0647 \u062e\u0627\u0642\u0627\u0646\u0645", "m2": "\u062a\u0627 \u06a9\u0644\u0647\u200c\u062f\u0627\u0631 \u0631\u0627\u0633\u062a\u06cc\u0646 \u0628\u0627\u0634\u0645"}, "16": {"m1": "\u0634\u0631\u0642 \u0648 \u063a\u0631\u0628 \u0627\u062a\u0641\u0627\u0642 \u06a9\u0631\u062f \u0628\u0631 \u0622\u0646\u06a9", "m2": "\u0645\u0628\u062f\u0639 \u0645\u0639\u0646\u06cc \u0622\u0641\u0631\u06cc\u0646 \u0628\u0627\u0634\u0645"}}}