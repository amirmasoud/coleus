{"text": {"0": {"m1": "\u0627\u06cc \u062f\u0644 \u0628\u0647 \u062c\u0641\u0627\u062a \u062c\u0627\u0646 \u0646\u0647\u0627\u062f\u0647", "m2": "\u062c\u0627\u0646 \u067e\u06cc\u0634\u200c\u06a9\u0634\u062a \u062c\u0647\u0627\u0646 \u0646\u0647\u0627\u062f\u0647"}, "1": {"m1": "\u0634\u0647\u0631\u06cc \u0647\u0645\u0647 \u0632 \u0622\u0647\u0646\u06cc\u0646 \u062f\u0644 \u062a\u0648", "m2": "\u0642\u0641\u0644\u06cc \u0632\u062f\u0647 \u0628\u0631 \u062f\u0647\u0627\u0646 \u0646\u0647\u0627\u062f\u0647"}, "2": {"m1": "\u0628\u0631 \u0637\u0631\u0641 \u0644\u0628 \u062a\u0648 \u062c\u0627\u0646 \u0639\u06cc\u0633\u06cc", "m2": "\u0627\u0632 \u0646\u06cc\u0644 \u0648 \u0628\u0642\u0645 \u062f\u06a9\u0627\u0646 \u0646\u0647\u0627\u062f\u0647"}, "3": {"m1": "\u0627\u0632 \u06a9\u0648\u06cc \u0633\u0648\u0627\u0631 \u0686\u0648\u0646 \u0628\u0631\u0622\u0626\u06cc", "m2": "\u0634\u0628\u200c\u067e\u0648\u0634 \u0628\u0631 \u0627\u0628\u0631\u0648\u0627\u0646 \u0646\u0647\u0627\u062f\u0647"}, "4": {"m1": "\u062a\u0631\u06a9\u0627\u0646 \u06a9\u0645\u06cc\u0646 \u063a\u0645\u0632\u0647\u0654 \u062a\u0648", "m2": "\u06cc\u0627\u0633\u062c \u0647\u0645\u0647 \u0628\u0631 \u06a9\u0645\u0627\u0646 \u0646\u0647\u0627\u062f\u0647"}, "5": {"m1": "\u062a\u0648 \u0639\u0627\u0634\u0642 \u0635\u06cc\u062f \u0648 \u062a\u06cc\u063a \u0628\u0631 \u06a9\u0641", "m2": "\u0639\u0634\u0627\u0642 \u062a\u0648 \u062f\u0644 \u0628\u0631 \u0622\u0646 \u0646\u0647\u0627\u062f\u0647"}, "6": {"m1": "\u0645\u0646 \u067e\u06cc\u0634 \u062a\u0648 \u0628\u0631 \u0632\u0645\u06cc\u0646 \u0646\u0647\u0645 \u0633\u0631", "m2": "\u06a9\u0627\u06cc \u067e\u0627\u06cc \u0628\u0631 \u0622\u0633\u0645\u0627\u0646 \u0646\u0647\u0627\u062f\u0647"}, "7": {"m1": "\u0627\u0633\u0628 \u0627\u0632 \u062f\u0631 \u0645\u0646 \u0645\u0631\u0627\u0646 \u0648 \u0645\u06af\u0630\u0631", "m2": "\u0647\u0627\u0646 \u0646\u0639\u0644 \u0628\u0647\u0627\u062a \u062c\u0627\u0646 \u0646\u0647\u0627\u062f\u0647"}, "8": {"m1": "\u062e\u0627\u0642\u0627\u0646\u06cc \u0631\u0627 \u062f\u0631 \u0622\u062a\u0634 \u0639\u0634\u0642", "m2": "\u0646\u0639\u0644 \u0647\u0648\u0633 \u0627\u0632 \u0646\u0647\u0627\u0646 \u0646\u0647\u0627\u062f\u0647"}}}