{"text": {"0": {"m1": "\u0628\u0627\u0631\u062f \u0686\u0647\u061f \u062e\u0648\u0646! \u06a9\u0647\u061f \u062f\u06cc\u062f\u0647! \u0686\u0633\u0627\u0646\u061f \u0631\u0648\u0632 \u0648 \u0634\u0628! \u0686\u0631\u0627\u061f", "m2": "\u0627\u0632 \u063a\u0645! \u06a9\u062f\u0627\u0645 \u063a\u0645\u061f \u063a\u0645 \u0633\u0644\u0637\u0627\u0646 \u0627\u0648\u0644\u06cc\u0627"}, "1": {"m1": "\u0646\u0627\u0645\u0634 \u06a9\u0647 \u0628\u062f\u061f \u062d\u0633\u06cc\u0646! \u0632 \u0646\u0698\u0627\u062f \u06a9\u0647\u200c\u061f \u0627\u0632 \u0639\u0644\u06cc", "m2": "\u0645\u0627\u0645\u0634 \u06a9\u0647 \u0628\u0648\u062f\u061f \u200c\u0641\u0627\u0637\u0645\u0647\u200c \u062c\u062f\u0634 \u06a9\u0647\u200c\u061f \u200c\u0645\u0635\u0637\u0641\u06cc!"}, "2": {"m1": "\u0686\u0648\u0646 \u0634\u062f\u061f \u0634\u0647\u06cc\u062f \u0634\u062f! \u0628\u0647 \u06a9\u062c\u0627\u061f \u200c\u062f\u0634\u062a \u0645\u0627\u0631\u06cc\u0647", "m2": "\u06a9\u06cc\u200c\u061f \u0639\u0627\u0634\u0631 \u0645\u062d\u0631\u0645\u060c \u067e\u0646\u0647\u0627\u0646\u200c\u061f \u0646\u0647 \u0628\u0631\u0645\u0644\u0627"}, "3": {"m1": "\u0634\u0628 \u06a9\u0634\u062a\u0647 \u0634\u062f\u061f \u200c\u0646\u0647 \u200c\u0631\u0648\u0632 \u0686\u0647 \u0647\u0646\u06af\u0627\u0645\u200c\u061f \u200c\u0648\u0642\u062a \u0638\u0647\u0631", "m2": "\u0634\u062f \u0627\u0632 \u06af\u0644\u0648 \u0628\u0631\u06cc\u062f\u0647 \u0633\u0631\u0634\u061f \u0646\u06cc \u0646\u06cc \u0627\u0632 \u0642\u0641\u0627"}, "4": {"m1": "\u0633\u06cc\u0631\u0627\u0628 \u06a9\u0634\u062a\u0647 \u0634\u062f\u061f \u200c\u0646\u0647! \u06a9\u0633 \u0622\u0628\u0634 \u200c\u0646\u062f\u0627\u062f\u061f \u062f\u0627\u062f!", "m2": "\u06a9\u0647 \u0634\u0645\u0631 \u0627\u0632 \u0686\u0647 \u0686\u0634\u0645\u0647\u200c\u061f \u0632 \u0633\u0631\u0686\u0634\u0645\u0647\u0654 \u0641\u0646\u0627"}, "5": {"m1": "\u0645\u0638\u0644\u0648\u0645 \u0634\u062f \u0634\u0647\u06cc\u062f\u061f \u0628\u0644\u06cc \u062c\u0631\u0645 \u062f\u0627\u0634\u062a\u061f\u200c \u0646\u0647", "m2": "\u06a9\u0627\u0631\u0634 \u0686\u0647 \u0628\u062f\u061f \u0647\u062f\u0627\u06cc\u062a\u200c\u060c \u06cc\u0627\u0631\u0634\u200c \u06a9\u0647 \u0628\u062f\u061f \u062e\u062f\u0627"}, "6": {"m1": "\u0627\u06cc\u0646 \u0638\u0644\u0645 \u0631\u0627 \u06a9\u0647 \u06a9\u0631\u062f\u061f \u06cc\u0632\u06cc\u062f! \u0627\u06cc\u0646 \u06cc\u0632\u06cc\u062f \u06a9\u06cc\u0633\u062a\u200c\u061f", "m2": "\u0632\u0627\u0648\u0644\u0627\u062f \u0647\u0646\u062f \u060c \u0627\u0632 \u0686\u0647 \u06a9\u0633\u200c\u061f \u0627\u0632 \u0646\u0637\u0641\u0647\u0654 \u0632\u0646\u0627!"}, "7": {"m1": "\u062e\u0648\u062f \u06a9\u0631\u062f \u0627\u06cc\u0646 \u0639\u0645\u0644\u200c\u061f \u0646\u0647! \u0641\u0631\u0633\u062a\u0627\u062f \u0646\u0627\u0645\u0647\u200c\u0627\u06cc", "m2": "\u0646\u0632\u062f \u06a9\u0647\u200c\u061f \u0646\u0632\u062f \u0632\u0627\u062f\u0647\u0654 \u0645\u0631\u062c\u0627\u0646\u0647\u0654 \u062f\u063a\u0627"}, "8": {"m1": "\u0627\u0628\u0646 \u0632\u06cc\u0627\u062f \u0632\u0627\u062f\u0647\u0654 \u0645\u0631\u062c\u0627\u0646\u0647 \u0628\u062f\u061f \u0646\u0639\u0645!", "m2": "\u0627\u0632 \u06af\u0641\u062a\u0647\u0654 \u06cc\u0632\u06cc\u062f \u062a\u062e\u0644\u0641 \u0646\u06a9\u0631\u062f\u061f \u0644\u0627"}, "9": {"m1": "\u0627\u06cc\u0646 \u0646\u0627\u0628\u06a9\u0627\u0631 \u06a9\u0634\u062a \u062d\u0633\u06cc\u0646 \u0631\u0627 \u0628\u0647 \u062f\u0633\u062a \u062e\u0648\u06cc\u0634\u200c\u061f", "m2": "\u0646\u0647 \u0627\u0648 \u0631\u0648\u0627\u0646\u0647 \u06a9\u0631\u062f \u0633\u067e\u0647 \u0633\u0648\u06cc \u06a9\u0631\u0628\u0644\u0627"}, "10": {"m1": "\u0645\u06cc\u0631 \u0633\u067e\u0647 \u06a9\u0647 \u0628\u062f\u061f\u0639\u0645\u0631\u0633\u0639\u062f! \u0627\u0648 \u0628\u0631\u06cc\u062f\u061f", "m2": "\u062d\u0644\u0642 \u0639\u0632\u06cc\u0632 \u0641\u0627\u0637\u0645\u0647 \u0646\u0647 \u0634\u0645\u0631 \u0628\u06cc\u200c\u062d\u06cc\u0627"}, "11": {"m1": "\u062e\u0646\u062c\u0631 \u0628\u0631\u06cc\u062f \u062d\u0646\u062c\u0631 \u0627\u0648 \u0631\u0627 \u0646\u06a9\u0631\u062f \u0634\u0631\u0645", "m2": "\u06a9\u0631\u062f \u0627\u0632 \u0686\u0647 \u067e\u0633 \u0628\u0631\u06cc\u062f\u061f \u0646\u067e\u0630\u0631\u0641\u062a \u0627\u0632\u0648 \u0642\u0636\u0627"}, "12": {"m1": "\u0628\u0647\u0631 \u0686\u0647\u200c\u061f \u0628\u0647\u0631 \u0622\u0646 \u06a9\u0647 \u0634\u0648\u062f \u062e\u0644\u0642 \u0631\u0627 \u0634\u0641\u06cc\u0639", "m2": "\u0634\u0631\u0637 \u0634\u0641\u0627\u0639\u062a\u0634 \u0686\u0647 \u0628\u0648\u062f\u061f \u0646\u0648\u062d\u0647 \u0648 \u0628\u06a9\u0627!"}, "13": {"m1": "\u06a9\u0633 \u06a9\u0634\u062a\u0647 \u0634\u062f \u0647\u0645 \u0627\u0632 \u067e\u0633\u0631\u0627\u0646\u0634\u200c\u061f \u0628\u0644\u06cc \u062f\u0648 \u062a\u0646", "m2": "\u062f\u06cc\u06af\u0631 \u06a9\u0647 \u0646\u0647 \u0628\u0631\u0627\u062f\u0631 \u062f\u06cc\u06af\u0631 \u06a9\u0647 \u0627\u0642\u0631\u0628\u0627"}, "14": {"m1": "\u062f\u06cc\u06af\u0631 \u067e\u0633\u0631 \u0646\u062f\u0627\u0634\u062a\u200c\u061f \u0686\u0631\u0627 \u062f\u0627\u0634\u062a \u0622\u0646 \u06a9\u0647 \u0628\u0648\u062f", "m2": "\u0633\u062c\u0627\u062f \u0686\u0648\u0646 \u0628\u062f \u0627\u0648 \u0628\u0647 \u063a\u0645 \u0648 \u0631\u0646\u062c \u0645\u0628\u062a\u0644\u0627"}, "15": {"m1": "\u0645\u0627\u0646\u062f \u0627\u0648 \u0628\u0647 \u06a9\u0631\u0628\u0644\u0627\u06cc \u067e\u062f\u0631\u061f \u0646\u06cc !\u0628\u0647 \u0634\u0627\u0645 \u0631\u0641\u062a", "m2": "\u0628\u0627 \u0639\u0632 \u0648 \u0627\u062d\u062a\u0634\u0627\u0645\u200c\u061f \u0646\u0647 ! \u0628\u0627 \u0630\u0644\u062a \u0648 \u0639\u0646\u0627"}, "16": {"m1": "\u062a\u0646\u0647\u0627\u061f \u0646\u0647 ! \u0628\u0627 \u0632\u0646\u0627\u0646 \u062d\u0631\u0645\u061b \u0646\u0627\u0645\u0634\u0627\u0646 \u0686\u0647 \u0628\u0648\u062f\u061f", "m2": "\u0632\u06cc\u0646\u0628 \u0633\u06a9\u06cc\u0646\u0647 \u0641\u0627\u0637\u0645\u0647 \u06a9\u0644\u062b\u0648\u0645 \u0628\u06cc\u0646\u0648\u0627"}, "17": {"m1": "\u0628\u0631 \u062a\u0646 \u0644\u0628\u0627\u0633 \u062f\u0627\u0634\u062a\u200c\u061f \u0628\u0644\u06cc! \u06af\u0631\u062f \u0631\u0647\u06af\u0630\u0627\u0631", "m2": "\u0628\u0631 \u0633\u0631 \u0639\u0645\u0627\u0645\u0647 \u062f\u0627\u0634\u062a\u200c\u061f \u0628\u0644\u06cc !\u0686\u0648\u0628 \u0627\u0634\u0642\u06cc\u0627"}, "18": {"m1": "\u0628\u06cc\u0645\u0627\u0631 \u0628\u062f\u061f \u0628\u0644\u06cc \u0686\u0647 \u062f\u0648\u0627 \u062f\u0627\u0634\u062a\u200c\u061f \u0627\u0634\u06a9 \u0686\u0634\u0645", "m2": "\u0628\u0639\u062f \u0627\u0632 \u062f\u0648\u0627 \u063a\u0630\u0627\u0634 \u0686\u0647 \u0628\u062f\u061f \u062e\u0648\u0646 \u062f\u0644 \u063a\u0630\u0627"}, "19": {"m1": "\u06a9\u0633 \u0628\u0648\u062f \u0647\u0645\u0631\u0647\u0634\u200c\u061f \u0628\u0644\u06cc \u0627\u0637\u0641\u0627\u0644 \u0628\u06cc\u200c\u067e\u062f\u0631", "m2": "\u062f\u06cc\u06af\u0631 \u06a9\u0647 \u0628\u0648\u062f\u061f \u062a\u0628 !\u06a9\u0647 \u0646\u0645\u06cc \u06af\u0634\u062a \u0627\u0632\u0648 \u062c\u062f\u0627"}, "20": {"m1": "\u0627\u0632 \u0632\u06cc\u0646\u0628 \u0648 \u0632\u0646\u0627\u0646 \u0686\u0647 \u0628\u0647 \u062c\u0627 \u0645\u0627\u0646\u062f\u0647 \u0628\u062f\u061f \u062f\u0648 \u0686\u06cc\u0632", "m2": "\u0637\u0648\u0642 \u0633\u062a\u0645 \u0628\u0647 \u06af\u0631\u062f\u0646 \u0648 \u062e\u0644\u062e\u0627\u0644 \u063a\u0645 \u0628\u0647 \u067e\u0627"}, "21": {"m1": "\u06af\u0628\u0631 \u0627\u06cc\u0646 \u0633\u062a\u0645 \u06a9\u0646\u062f\u061f \u0646\u0647! \u06cc\u0647\u0648\u062f \u0648 \u0645\u062c\u0648\u0633\u061f \u0646\u0647!", "m2": "\u0647\u0646\u062f\u0648\u061f \u0646\u0647! \u0628\u062a\u200c\u067e\u0631\u0633\u062a\u061f \u0646\u0647! \u0641\u0631\u06cc\u0627\u062f \u0627\u0632\u06cc\u0646 \u062c\u0641\u0627"}, "22": {"m1": "\u0642\u0627\u0622\u0646\u06cc \u0627\u0633\u062a \u0642\u0627\u06cc\u0644 \u0627\u06cc\u0646 \u0634\u0639\u0631\u0647\u0627 \u0628\u0644\u06cc", "m2": "\u062e\u0648\u0627\u0647\u062f \u0686\u0647\u061f \u0631\u062d\u0645\u062a! \u200c\u0627\u0632\u06a9\u0647\u200c\u061f \u0632 \u062d\u0642! \u06a9\u06cc\u061f \u0635\u0641 \u062c\u0632\u0627!"}}}