{"text": {"0": {"m1": "\u062f\u0648\u0634 \u062f\u06cc\u062f\u0645 \u06cc\u06a9\u06cc \u062e\u062c\u0633\u062a\u0647 \u0648\u062b\u0627\u0642", "m2": "\u0637\u0627\u0642 \u0627\u0648 \u062c\u0641\u062a \u0637\u0627\u0642 \u0647\u0641\u062a \u0637\u0628\u0627\u0642"}, "1": {"m1": "\u0635\u062d\u0646 \u0627\u0648 \u062e\u0648\u0631\u062f\u0647 \u0628\u0627 \u0627\u0631\u0645 \u0633\u0648\u06af\u0646\u062f", "m2": "\u0633\u0642\u0641 \u0627\u0648 \u0628\u0633\u062a\u0647 \u0628\u0627 \u0641\u0644\u06a9 \u0645\u06cc\u062b\u0627\u0642"}, "2": {"m1": "\u0627\u0632 \u06cc\u06a9\u06cc \u0633\u0648 \u0646\u0647\u0627\u062f\u0647 \u062a\u0627 \u0633\u0631 \u0633\u0642\u0641", "m2": "\u0627\u0632 \u06cc\u06a9\u06cc\u200c\u06af\u0648\u0634\u0647 \u0686\u06cc\u062f\u0647 \u062a\u0627 \u062f\u0645 \u0637\u0627\u0642"}, "3": {"m1": "\u0646\u0633\u062e\u0647\u0654 \u0647\u06cc\u0623\u062a \u0648 \u06a9\u062a\u0627\u0628 \u0646\u062c\u0648\u0645", "m2": "\u062c\u0644\u062f \u062a\u0647\u0630\u06cc\u0628 \u0648 \u062f\u0641\u062a\u0631 \u0627\u062e\u0644\u0627\u0642"}, "4": {"m1": "\u0635\u062d\u0641 \u0641\u0636\u0644 \u0648 \u0645\u0646\u0637\u0642\u06cc \u0627\u062c\u0632\u0627", "m2": "\u06a9\u062a\u0628 \u0646\u0638\u0645 \u0648 \u0647\u0646\u062f\u0633\u06cc \u0627\u0648\u0631\u0627\u0642"}, "5": {"m1": "\u0633\u0641\u0631\u0647\u0627 \u0627\u0632 \u0645\u0628\u0627\u062d\u062b \u0645\u0634\u0627\u0621", "m2": "\u062c\u0644\u062f\u0647\u0627 \u0627\u0632 \u062f\u0642\u0627\u06cc\u0642 \u0627\u0634\u0631\u0627\u0642"}, "6": {"m1": "\u0627\u0632 \u062a\u0622\u0644\u06cc\u0641 \u06af\u0648\u0634\u06cc\u0627\u0631 \u062f\u0642\u06cc\u0642", "m2": "\u0627\u0632 \u062a\u0635\u0627\u0646\u06cc\u0641 \u0628\u0648\u0639\u0644\u06cc \u062f\u0642\u0627\u0642\u200c"}, "7": {"m1": "\u0646\u0633\u062e\u0647\u200c\u06cc\u06cc \u0686\u0646\u062f \u0647\u0645 \u0632 \u0645\u0648\u0633\u06cc\u0642\u06cc", "m2": "\u062f\u0631 \u0645\u0642\u0627\u0645\u0627\u062a\u200c\u06a9\u0648\u0686\u06a9 \u0648 \u0639\u0634\u0627\u0642"}, "8": {"m1": "\u0627\u0632 \u0646\u0634\u0627\u0628\u0648\u0631 \u0648 \u0632\u0627\u0628\u0644 \u0648 \u062a\u0628\u0631\u06cc\u0632", "m2": "\u0627\u0632 \u0646\u0647\u0627\u0648\u0646\u062f \u0648 \u0627\u0635\u0641\u0647\u0627\u0646 \u0648 \u0639\u0631\u0627\u0642"}, "9": {"m1": "\u0646\u064f\u0633\u064e\u062e\u064f \u0646\u0633\u062e\u064f \u0648 \u0631\u0642\u0639\u0647\u0627\u06cc \u0631\u0642\u0627\u0639", "m2": "\u0635\u062d\u0641 \u062b\u0644\u062b \u0648 \u0641\u0631\u062f\u0647\u0627\u06cc \u0633\u06cc\u0627\u0642"}, "10": {"m1": "\u062a\u0647\u0646\u06cc\u062a\u200c\u062e\u0648\u0627\u0646 \u0628\u0647 \u0646\u0632\u062f \u0639\u0642\u0644 \u0634\u062f\u0645", "m2": "\u06a9\u0627\u06cc \u062d\u06a9\u06cc\u0645 \u062c\u0647\u0627\u0646 \u0639\u0644\u06cc\u200c\u0627\u0644\u0627\u0637\u0644\u0627\u0642"}, "11": {"m1": "\u0628\u0647\u0631 \u062a\u0639\u0644\u06cc\u0645 \u0639\u0644\u0645 \u0631\u0633\u0637\u0627\u0644\u06cc\u0633", "m2": "\u062c\u0627\u06a9\u0646\u062f \u0627\u0646\u062f\u0631\u06cc\u0646 \u062e\u062c\u0633\u062a\u0647 \u0631\u0648\u0627\u0642"}, "12": {"m1": "\u06cc\u0627 \u0646\u0647 \u0627\u062f\u0631\u06cc\u0633 \u0627\u0632 \u067e\u06cc \u062a\u062f\u0631\u06cc\u0633", "m2": "\u062c\u0627 \u062f\u0631 \u0627\u06cc\u0646\u062c\u0627 \u06a9\u0646\u062f \u0628\u0647 \u0627\u0633\u062a\u062d\u0642\u0627\u0642"}, "13": {"m1": "\u06cc\u0627 \u0646\u0647 \u0635\u062f\u0631\u0627 \u0628\u0647 \u0635\u062f\u0631 \u0627\u06cc\u0646 \u0645\u062d\u0641\u0644", "m2": "\u0631\u0645\u0632 \u0627\u0634\u0631\u0627\u0642\u200c\u06af\u0648\u06cc\u062f \u0627\u0632 \u0627\u0634\u0641\u0627\u0642"}, "14": {"m1": "\u06cc\u0627 \u0627\u0628\u0648\u0646\u0635\u0631 \u0627\u0646\u062f\u0631\u06cc\u0646 \u0645\u0646\u0632\u0644", "m2": "\u0628\u062d\u062b \u0645\u0634\u0627\u0621 \u0631\u0627 \u06a9\u0646\u062f \u0627\u0637\u0644\u0627\u0642"}, "15": {"m1": "\u06cc\u0627 \u0634\u0647\u06cc\u062f\u06cc\u0646 \u0627\u0646\u062f\u0631\u06cc\u0646 \u0645\u062c\u0644\u0633", "m2": "\u0644\u0628\u200c\u06af\u0634\u0627\u06cc\u0646\u062f \u0628\u0647\u0631 \u0627\u0633\u062a\u0646\u0637\u0627\u0642"}, "16": {"m1": "\u06cc\u0627 \u067e\u0633 \u0627\u0632 \u062d\u0644 \u0648\u0639\u0642\u062f \u0645\u0644\u06a9 \u0645\u0644\u06a9", "m2": "\u062c\u0627\u06af\u0632\u06cc\u0646\u062f \u062f\u0631\u06cc\u0646 \u062e\u062c\u0633\u062a\u0647 \u0648\u062b\u0627\u0642"}, "17": {"m1": "\u0634\u0627\u0647 \u063a\u0627\u0632\u06cc \u0627\u0628\u0648\u0627\u0644\u0634\u062c\u0627\u0639\u200c \u06a9\u0647 \u0647\u0633\u062a", "m2": "\u06a9\u0641 \u06a9\u0627\u0641\u06cc\u0634 \u0648\u0627\u0647\u0628 \u0627\u0644\u0627\u0631\u0632\u0627\u0642"}, "18": {"m1": "\u0622\u0646\u06a9\u0647 \u0627\u0632 \u062b\u0642\u0644 \u0628\u0627\u0631 \u062e\u062f\u0645\u062a \u0627\u0648", "m2": "\u0634\u062f\u0647 \u0646\u0647 \u0686\u0631\u062e \u062e\u0627\u0636\u0639 \u0627\u0644\u0627\u0639\u0646\u0627\u0642"}, "19": {"m1": "\u0645\u0631\u06af \u0628\u0631 \u0631\u0648\u06cc \u062e\u0646\u062c\u0631\u0634 \u0645\u0641\u062a\u0648\u0646", "m2": "\u0641\u062a\u062d \u0628\u0631 \u0632\u0644\u0641 \u067e\u0631\u0686\u0645\u0634 \u0645\u0634\u062a\u0627\u0642"}, "20": {"m1": "\u062e\u0646\u06af \u0627\u0648 \u0646\u0646\u06af \u0635\u0631\u0635\u0631 \u0627\u0632 \u062a\u0639\u062c\u06cc\u0644", "m2": "\u062a\u06cc\u063a \u0627\u0648 \u0631\u0634\u06a9 \u062f\u0648\u0632\u062e \u0627\u0632 \u0627\u062d\u0631\u0627\u0642"}, "21": {"m1": "\u0647\u0633\u062a \u0647\u0646\u06af\u0627\u0645 \u06a9\u06cc\u0646 \u0628\u0647 \u067e\u0634\u062a \u0633\u0645\u0646\u062f", "m2": "\u0627\u062d\u0645\u062f\u06cc \u06a9\u06cc\u0646\u0647\u200c\u062c\u0648 \u0628\u0647 \u067e\u0634\u062a \u0628\u0631\u0627\u0642"}, "22": {"m1": "\u062e\u0648\u0646 \u0628\u0628\u0646\u062f\u062f \u0632 \u0628\u0623\u0633 \u0627\u0648 \u0628\u0647 \u0639\u0631\u0648\u0642", "m2": "\u062c\u0627\u0646 \u062f\u0631\u0622\u06cc\u062f \u0632 \u0644\u0637\u0641 \u0627\u0648 \u0628\u0647 \u0639\u0631\u0627\u0642"}, "23": {"m1": "\u062d\u0645\u0631\u062a\u06cc\u200c\u06a9\u0632 \u0627\u0641\u0642 \u067e\u062f\u06cc\u062f \u0622\u06cc\u062f", "m2": "\u0686\u0648\u0646\u200c\u06af\u0634\u0627\u06cc\u06cc \u0646\u0638\u0631 \u0628\u0647 \u0627\u0633\u062a\u062d\u0642\u0627\u0642"}, "24": {"m1": "\u0627\u0632 \u0637\u0644\u0648\u0639 \u0648 \u063a\u0631\u0648\u0628 \u0628\u06cc\u0636\u0627 \u0646\u06cc\u0633\u062a", "m2": "\u06a9\u0634 \u0641\u0644\u0642 \u06cc\u0627 \u0634\u0641\u0642\u200c\u06a9\u0646\u06cc \u0627\u0637\u0644\u0627\u0642"}, "25": {"m1": "\u062e\u0648\u0646 \u200c\u062e\u0635\u0645\u0634\u200c\u200c \u0632 \u0628\u0633\u06a9\u0647 \u200c\u062e\u0648\u0631\u062f\u0647 \u0633\u067e\u0647\u0631", "m2": "\u06a9\u0631\u062f\u0647 \u0627\u0633\u062a \u0627\u0632 \u0645\u0631\u0627\u063a\u0647 \u0633\u0631\u062e \u0622\u0641\u0627\u0642"}, "26": {"m1": "\u0631\u0648\u0632 \u0647\u06cc\u062c\u0627 \u06a9\u0647 \u0646\u0627\u06cc \u0631\u0648\u06cc\u06cc\u0646 \u0631\u0627", "m2": "\u0628\u0648\u062f \u0627\u0632 \u0641\u0631\u0637 \u0646\u0627\u0644\u0647 \u0628\u06cc\u0645 \u062e\u0646\u0627\u0642"}, "27": {"m1": "\u0628\u0647\u0631 \u0646\u0648\u0645\u06cc\u062f\u06cc \u062e\u0635\u0627\u0645\u0634 \u0686\u0631\u062e", "m2": "\u06af\u0648\u06cc\u062f \u0627\u0644\u06cc\u0648\u0645 \u0645\u0627 \u0644\u0647\u0645 \u0645\u0646 \u0648\u0627\u0642\u200c"}, "28": {"m1": "\u0628\u0627\u06a9\u0641\u0634 \u0686\u0648\u0646 \u0639\u0631\u0648\u0633 \u0628\u062e\u0634\u0634 \u0631\u0627", "m2": "\u0639\u0642\u062f \u0628\u0633\u062a \u0622\u0633\u0645\u0627\u0646 \u0628\u0647 \u0635\u062f\u0642 \u0635\u062f\u0627\u0642"}, "29": {"m1": "\u0628\u062d\u0631 \u0648 \u06a9\u0627\u0646 \u0631\u0627 \u0635\u062f\u0627\u0642 \u06a9\u0631\u062f \u0648 \u06a9\u0646\u0648\u0646", "m2": "\u06a9\u0641 \u0627\u0648 \u0645\u06cc\u200c\u06a9\u0646\u062f \u0627\u062f\u0627\u06cc \u0635\u062f\u0627\u0642"}, "30": {"m1": "\u062f\u06cc\u06af\u0631\u0634 \u0627\u06cc\u0646\u0642\u062f\u0631 \u0645\u0639\u0648\u0646\u062a \u0646\u06cc\u0633\u062a", "m2": "\u06a9\u0647\u200c\u06a9\u0646\u062f \u062c\u0641\u062a \u062e\u0648\u06cc\u0634 \u0631\u0627 \u0627\u0646\u0641\u0627\u0642"}, "31": {"m1": "\u0628\u0647\u0631 \u062a\u0642\u062f\u06cc\u0645 \u062e\u062f\u0645\u062a\u0634\u200c \u06a9\u0647 \u0645\u0644\u06a9", "m2": "\u062c\u0633\u062a\u0647 \u067e\u06cc\u0648\u0633\u062a\u0647 \u0627\u0632 \u062d\u0642 \u0627\u0633\u062a\u06cc\u0641\u0627\u0642"}, "32": {"m1": "\u062f\u0627\u062f\u0647 \u067e\u0631\u0648\u0627\u0646\u0647 \u0639\u0642\u0644 \u0631\u0648\u0634\u0646 \u200c\u0631\u0627\u06cc", "m2": "\u0628\u0631\u06a9\u0647 \u0628\u0631 \u0647\u0641\u062a \u0634\u0645\u0639 \u0647\u0641\u062a \u0637\u0628\u0627\u0642"}, "33": {"m1": "\u0639\u062c\u0644\u0648\u0627 \u0628\u0627\u0644\u063a\u062f\u0648\u0651 \u0648 \u0627\u0644\u0622\u0635\u0627\u0644", "m2": "\u0627\u0631\u06a9\u0636\u0648\u0627 \u0628\u0627\u0644\u0639\u0634\u06cc \u0648 \u0627\u0644\u0627\u0634\u0631\u0627\u0642\u200c"}, "34": {"m1": "\u0686\u0631\u062e \u0645\u0627\u0646\u0646\u062f \u0628\u0646\u062f\u06af\u0627\u0646 \u0628\u0633\u062a\u0633\u062a", "m2": "\u06a9\u0645\u0631 \u0627\u0632 \u0628\u0647\u0631 \u062e\u062f\u0645\u062a\u0634 \u0632 \u0646\u0637\u0627\u0642"}, "35": {"m1": "\u0628\u0627\u0632 \u0628\u0627 \u0639\u0642\u0644 \u0646\u06a9\u062a\u0647\u200c\u062f\u0627\u0646\u200c \u06af\u0641\u062a\u0645", "m2": "\u06a9\u0627\u06cc \u0645\u0647\u06cc\u0646 \u062e\u0644\u0642 \u0648\u0627\u0647\u0628 \u062e\u0644\u0627\u0642"}, "36": {"m1": "\u0645\u0644\u06a9 \u0627\u0632 \u0645\u062d\u0631\u0645\u0627\u0646 \u06a9\u0631\u0627 \u06a9\u0631\u062f\u0633\u062a", "m2": "\u062d\u0627\u0631\u0633 \u0627\u06cc\u0646 \u0648\u062b\u0627\u0642 \u0639\u0631\u0634 \u0631\u0648\u0627\u0642"}, "37": {"m1": "\u0645\u0627\u0647 \u062a\u0627\u0628\u0646\u062f\u0647 \u0627\u0633\u062a \u06cc\u0627 \u062e\u0648\u0631\u0634\u06cc\u062f", "m2": "\u0686\u0631\u062e\u200c\u06af\u0631\u062f\u0646\u062f\u0647 \u0627\u0633\u062a \u06cc\u0627 \u0622\u0641\u0627\u0642"}, "38": {"m1": "\u06af\u0641\u062a \u0627\u06cc\u0646\u0627\u0646 \u0646\u06cc\u0646\u062f \u0645\u062d\u0631\u0645 \u0631\u0627\u0632", "m2": "\u0632\u0627\u0646\u06a9\u0647 \u0627\u0632 \u0627\u0647\u0644 \u0631\u06cc\u0645\u0646\u0646\u062f \u0648 \u0646\u0641\u0627\u0642"}, "39": {"m1": "\u06a9\u0633 \u0628\u062f\u06cc\u0646 \u067e\u0627\u06cc\u0647 \u0627\u0632 \u0634\u0631\u0641 \u0646\u0631\u0633\u062f", "m2": "\u062c\u0632 \u0633\u067e\u0647\u0631 \u0648\u0641\u0627 \u0648 \u0642\u0637\u0628 \u0648\u0641\u0627\u0642"}, "40": {"m1": "\u0632\u0627\u062f\u0647\u0654 \u0627\u0644\u0641\u062a \u0622\u0646 \u0633\u062e\u0646\u0648\u0631 \u0639\u0635\u0631", "m2": "\u06a9\u0627\u0633\u0645\u0627\u0646\u0634 \u0633\u062a\u0648\u062f\u0647 \u062f\u0631 \u0627\u062e\u0644\u0627\u0642"}, "41": {"m1": "\u0622\u0646\u06a9\u0647 \u0645\u0627\u0646\u0646\u062f\u0647\u0654 \u0633\u062e\u0646\u0648\u0631 \u0637\u0648\u0633", "m2": "\u062e\u0631\u062f\u0634 \u0628\u0631\u06af\u0632\u06cc\u062f\u0647 \u062f\u0631 \u0627\u0641\u0644\u0627\u0642"}}}