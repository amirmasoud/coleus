{"text": {"0": {"p": ":"}, "1": {"p": ":"}, "2": {"p": ": \u0641\u0631\u062f \u0631\u0627 \u0631\u0628\u0637 \u062c\u0645\u0627\u0639\u062a \u0631\u062d\u0645\u062a \u0627\u0633\u062a"}, "3": {"p": ": \u0627\u0632 \u0686\u0647 \u0631\u0648 \u0628\u0631 \u0628\u0633\u062a\u0647 \u0631\u0628\u0637 \u0645\u0631\u062f\u0645 \u0627\u0633\u062a"}, "4": {"p": ": \u062f\u0631 \u062c\u0647\u0627\u0646 \u06a9\u06cc\u0641 \u0648 \u06a9\u0645 \u06af\u0631\u062f\u06cc\u062f \u0639\u0642\u0644"}, "5": {"p": ": \u0645\u0631\u06af \u0631\u0627 \u0633\u0627\u0645\u0627\u0646 \u0632 \u0642\u0637\u0639 \u0622\u0631\u0632\u0648\u0633\u062a"}, "6": {"p": ": \u0633\u0631 \u062d\u0642 \u062a\u06cc\u0631 \u0627\u0632 \u0644\u0628 \u0633\u0648\u0641\u0627\u0631 \u06af\u0641\u062a"}, "7": {"p": ": \u0634\u0627\u0647 \u0639\u0627\u0644\u0645\u06af\u06cc\u0631 \u06af\u0631\u062f\u0648\u0646 \u0622\u0633\u062a\u0627\u0646"}, "8": {"p": ": \u062a\u0627\u0631\u06a9 \u0622\u0641\u0644 \u0628\u0631\u0627\u0647\u06cc\u0645 \u062e\u0644\u06cc\u0644"}, "9": {"p": ": \u0628\u0648\u062f \u0627\u0646\u0633\u0627\u0646 \u062f\u0631 \u062c\u0647\u0627\u0646 \u0627\u0646\u0633\u0627\u0646 \u067e\u0631\u0633\u062a"}, "10": {"p": ": \u0634\u062f \u0627\u0633\u06cc\u0631 \u0645\u0633\u0644\u0645\u06cc \u0627\u0646\u062f\u0631 \u0646\u0628\u0631\u062f"}, "11": {"p": ": \u0628\u0648\u062f \u0645\u0639\u0645\u0627\u0631\u06cc \u0632 \u0627\u0642\u0644\u06cc\u0645 \u062e\u062c\u0646\u062f"}, "12": {"p": ": \u0647\u0631 \u06a9\u0647 \u067e\u06cc\u0645\u0627\u0646 \u0628\u0627 \u0647\u0648\u0627\u0644\u0645\u0648\u062c\u0648\u062f \u0628\u0633\u062a"}, "13": {"p": ": \u062c\u0648\u0647\u0631 \u0645\u0627 \u0628\u0627 \u0645\u0642\u0627\u0645\u06cc \u0628\u0633\u062a\u0647 \u0646\u06cc\u0633\u062a"}, "14": {"p": ": \u0622\u0646\u0686\u0646\u0627\u0646 \u0642\u0637\u0639 \u0627\u062e\u0648\u062a \u06a9\u0631\u062f\u0647 \u0627\u0646\u062f"}, "15": {"p": ": \u062f\u0631 \u0628\u0647\u0627\u0631\u0627\u0646 \u062c\u0648\u0634 \u0628\u0644\u0628\u0644 \u062f\u06cc\u062f\u0647 \u0626\u06cc"}, "16": {"p": ": \u0645\u0644\u062a\u06cc \u0631\u0627 \u0631\u0641\u062a \u0686\u0648\u0646 \u0622\u0626\u06cc\u0646 \u0632 \u062f\u0633\u062a"}, "17": {"p": ": \u0639\u0647\u062f \u062d\u0627\u0636\u0631 \u0641\u062a\u0646\u0647 \u0647\u0627 \u0632\u06cc\u0631 \u0633\u0631 \u0627\u0633\u062a"}, "18": {"p": ": \u062f\u0631 \u0634\u0631\u06cc\u0639\u062a \u0645\u0639\u0646\u06cc \u062f\u06cc\u06af\u0631 \u0645\u062c\u0648"}, "19": {"p": ": \u0633\u0627\u0626\u0644\u06cc \u0645\u062b\u0644 \u0642\u0636\u0627\u06cc \u0645\u0628\u0631\u0645\u06cc"}, "20": {"p": ": \u0645\u06cc \u06af\u0634\u0627\u06cc\u0645 \u0639\u0642\u062f\u0647 \u0627\u0632 \u06a9\u0627\u0631 \u062d\u06cc\u0627\u062a"}, "21": {"p": ": \u0628\u0627 \u062a\u0648 \u0622\u0645\u0648\u0632\u0645 \u0632\u0628\u0627\u0646 \u06a9\u0627\u0626\u0646\u0627\u062a"}, "22": {"p": ": \u0627\u06cc\u06a9\u0647 \u0628\u0627 \u0646\u0627\u062f\u06cc\u062f\u0647 \u067e\u06cc\u0645\u0627\u0646 \u0628\u0633\u062a\u0647 \u0626\u06cc"}, "23": {"p": ": \u06a9\u0648\u062f\u06a9\u06cc \u0631\u0627 \u062f\u06cc\u062f\u06cc \u0627\u06cc \u0628\u0627\u0644\u063a \u0646\u0638\u0631"}, "24": {"p": ": \u0646\u063a\u0645\u0647 \u062e\u06cc\u0632 \u0627\u0632 \u0632\u062e\u0645\u0647\u0654 \u0632\u0646 \u0633\u0627\u0632 \u0645\u0631\u062f"}, "25": {"p": ": \u0645\u0631\u06cc\u0645 \u0627\u0632 \u06cc\u06a9 \u0646\u0633\u0628\u062a \u0639\u06cc\u0633\u06cc \u0639\u0632\u06cc\u0632"}, "26": {"p": ": \u0627\u06cc \u0631\u062f\u0627\u06cc\u062a \u067e\u0631\u062f\u0647\u0654 \u0646\u0627\u0645\u0648\u0633 \u0645\u0627"}, "27": {"p": ": \u0645\u0646 \u0634\u0628\u06cc \u0635\u062f\u06cc\u0642 \u0631\u0627 \u062f\u06cc\u062f\u0645 \u0628\u062e\u0648\u0627\u0628"}, "28": {"p": ": \u06af\u0631 \u0628\u0647 \u0627\u0644\u0644\u0647 \u0627\u0644\u0635\u0645\u062f \u062f\u0644 \u0628\u0633\u062a\u0647 \u0626\u06cc"}, "29": {"p": ": \u0642\u0648\u0645 \u062a\u0648 \u0627\u0632 \u0631\u0646\u06af \u0648 \u062e\u0648\u0646 \u0628\u0627\u0644\u0627\u062a\u0631 \u0627\u0633\u062a"}, "30": {"p": ": \u0645\u0633\u0644\u0645 \u0686\u0634\u0645 \u0627\u0632 \u062c\u0647\u0627\u0646 \u0628\u0631 \u0628\u0633\u062a\u0647 \u0686\u06cc\u0633\u062a"}, "31": {"p": ": \u0627\u06cc \u0638\u0647\u0648\u0631 \u062a\u0648 \u0634\u0628\u0627\u0628 \u0632\u0646\u062f\u06af\u06cc"}}}