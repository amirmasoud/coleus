{"text": {"0": {"p": ": \u0633\u067e\u0627\u0647 \u062a\u0627\u0632\u0647 \u0628\u0631\u0627\u0646\u06af\u06cc\u0632\u0645 \u0627\u0632 \u0648\u0644\u0627\u06cc\u062a \u0639\u0634\u0642"}, "1": {"p": ": \u067e\u06cc\u0631 \u0631\u0648\u0645\u06cc \u0645\u0631\u0634\u062f \u0631\u0648\u0634\u0646 \u0636\u0645\u06cc\u0631"}, "2": {"p": ": \u0627\u06cc \u0627\u0645\u06cc\u0631 \u062e\u0627\u0648\u0631 \u0627\u06cc \u0645\u0647\u0631 \u0645\u0646\u06cc\u0631"}, "3": {"p": ": \u062a\u0627 \u0646\u0628\u0648\u062a \u062d\u06a9\u0645 \u062d\u0642 \u062c\u0627\u0631\u06cc \u06a9\u0646\u062f"}, "4": {"p": ": \u062d\u06a9\u0645\u062a \u0627\u0631\u0628\u0627\u0628 \u062f\u06cc\u0646 \u06a9\u0631\u062f\u0645 \u0639\u06cc\u0627\u0646"}, "5": {"p": ": \u0646\u06a9\u062a\u0647 \u0626\u06cc \u0645\u06cc\u06af\u0648\u06cc\u0645 \u0627\u0632 \u0645\u0631\u062f\u0627\u0646 \u062d\u0627\u0644"}, "6": {"p": ": \u0686\u06cc\u0633\u062a \u0641\u0642\u0631 \u0627\u06cc \u0628\u0646\u062f\u06af\u0627\u0646 \u0622\u0628 \u0648 \u06af\u0644"}, "7": {"p": ": \u0645\u0631\u062f \u062d\u0631 \u0645\u062d\u06a9\u0645 \u0632 \u0648\u0631\u062f \u00ab\u0644\u0627\u062a\u062e\u0641\u00bb"}, "8": {"p": ": \u0646\u06a9\u062a\u0647 \u0647\u0627 \u0627\u0632 \u067e\u06cc\u0631 \u0631\u0648\u0645 \u0622\u0645\u0648\u062e\u062a\u0645"}, "9": {"p": ": \u0627\u06cc \u0647\u0645\u0627\u0644\u0647 ! \u0627\u06cc \u0627\u0637\u06a9 \u060c \u0627\u06cc \u0631\u0648\u062f \u06af\u0646\u06af"}, "10": {"p": ": \u0645\u06cc \u06a9\u0646\u062f \u0628\u0646\u062f \u063a\u0644\u0627\u0645\u0627\u0646 \u0633\u062e\u062a \u062a\u0631"}, "11": {"p": ": \u0627\u06cc \u062f\u0631 \u0648 \u062f\u0634\u062a \u062a\u0648 \u0628\u0627\u0642\u06cc \u062a\u0627 \u0627\u0628\u062f"}, "12": {"p": ": \u067e\u0633 \u0686\u0647 \u0628\u0627\u06cc\u062f \u06a9\u0631\u062f \u0627\u06cc \u0627\u0642\u0648\u0627\u0645 \u0634\u0631\u0642"}, "13": {"p": ": \u0627\u06cc \u062a\u0648 \u0645\u0627 \u0628\u06cc\u0686\u0627\u0631\u06af\u0627\u0646 \u0631\u0627 \u0633\u0627\u0632 \u0648 \u0628\u0631\u06af"}, "14": {"p": ": \u0627\u06cc \u0632 \u062e\u0648\u062f \u067e\u0648\u0634\u06cc\u062f\u0647 \u062e\u0648\u062f \u0631\u0627 \u0628\u0627\u0632\u06cc\u0627\u0628"}, "15": {"p": ": \u0634\u0647\u0631 \u06a9\u0627\u0628\u0644 \u062e\u0637\u0647\u0654 \u062c\u0646\u062a \u0646\u0638\u06cc\u0631"}, "16": {"p": ": \u0628\u06cc\u0627 \u06a9\u0647 \u0633\u0627\u0632 \u0641\u0631\u0646\u06af \u0627\u0632 \u0646\u0648\u0627 \u0628\u0631 \u0627\u0641\u062a\u0627\u062f \u0627\u0633\u062a"}, "17": {"p": ": \u0627\u0632 \u0646\u0648\u0627\u0632\u0634\u0647\u0627\u06cc \u0633\u0644\u0637\u0627\u0646 \u0634\u0647\u06cc\u062f"}, "18": {"p": ": \u0631\u0627\u0632\u062f\u0627\u0646 \u062e\u06cc\u0631 \u0648 \u0634\u0631 \u06af\u0634\u062a\u0645 \u0632 \u0641\u0642\u0631"}, "19": {"p": ": \u062e\u06cc\u0632\u062f \u0627\u0632 \u062f\u0644 \u0646\u0627\u0644\u0647 \u0647\u0627 \u0628\u06cc \u0627\u062e\u062a\u06cc\u0627\u0631"}, "20": {"p": ": \u0644\u0627\u0644\u0647 \u0628\u0647\u0631 \u06cc\u06a9 \u0634\u0639\u0627\u0639 \u0622\u0641\u062a\u0627\u0628"}, "21": {"p": ": \u0642\u0646\u062f\u0647\u0627\u0631 \u0622\u0646 \u06a9\u0634\u0648\u0631 \u0645\u06cc\u0646\u0648 \u0633\u0648\u0627\u062f"}, "22": {"p": ": \u062a\u0631\u0628\u062a \u0622\u0646 \u062e\u0633\u0631\u0648 \u0631\u0648\u0634\u0646 \u0636\u0645\u06cc\u0631"}, "23": {"p": ": \u0627\u06cc \u0642\u0628\u0627\u06cc \u067e\u0627\u062f\u0634\u0627\u0647\u06cc \u0628\u0631 \u062a\u0648 \u0631\u0627\u0633\u062a"}}}