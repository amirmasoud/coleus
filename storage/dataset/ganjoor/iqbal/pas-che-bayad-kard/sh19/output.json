{"text": {"0": {"m1": "\u0631\u0627\u0632\u062f\u0627\u0646 \u062e\u06cc\u0631 \u0648 \u0634\u0631 \u06af\u0634\u062a\u0645 \u0632 \u0641\u0642\u0631", "m2": "\u0632\u0646\u062f\u0647 \u0648 \u0635\u0627\u062d\u0628 \u0646\u0638\u0631 \u06af\u0634\u062a\u0645 \u0632 \u0641\u0642\u0631"}, "1": {"m1": "\u06cc\u0639\u0646\u06cc \u0622\u0646 \u0641\u0642\u0631\u06cc \u06a9\u0647 \u062f\u0627\u0646\u062f \u0631\u0627\u0647 \u0631\u0627", "m2": "\u0628\u06cc\u0646\u062f \u0627\u0632 \u0646\u0648\u0631 \u062e\u0648\u062f\u06cc \u0627\u0644\u0644\u0647 \u0631\u0627"}, "2": {"m1": "\u0627\u0646\u062f\u0631\u0648\u0646 \u062e\u0648\u06cc\u0634 \u062c\u0648\u06cc\u062f \u0644\u0627\u0627\u0644\u0647", "m2": "\u062f\u0631 \u062a\u0647 \u0634\u0645\u0634\u06cc\u0631 \u06af\u0648\u06cc\u062f \u0644\u0627\u0627\u0644\u0647"}, "3": {"m1": "\u0641\u06a9\u0631 \u062c\u0627\u0646 \u06a9\u0646 \u0686\u0648\u0646 \u0632\u0646\u0627\u0646 \u0628\u0631 \u062a\u0646 \u0645\u062a\u0646", "m2": "\u0647\u0645\u0686\u0648 \u0645\u0631\u062f\u0627\u0646 \u06af\u0648\u06cc \u062f\u0631 \u0645\u06cc\u062f\u0627\u0646 \u0641\u06a9\u0646"}, "4": {"m1": "\u0633\u0644\u0637\u0646\u062a \u0627\u0646\u062f\u0631 \u062c\u0647\u0627\u0646 \u0622\u0628 \u0648 \u06af\u0644", "m2": "\u0642\u06cc\u0645\u062a \u0627\u0648 \u0642\u0637\u0631\u0647 \u0626\u06cc \u0627\u0632 \u062e\u0648\u0646 \u062f\u0644"}, "5": {"m1": "\u0645\u0624\u0645\u0646\u0627\u0646 \u0632\u06cc\u0631 \u0633\u067e\u0647\u0631 \u0644\u0627\u062c\u0648\u0631\u062f", "m2": "\u0632\u0646\u062f\u0647 \u0627\u0632 \u0639\u0634\u0642\u0646\u062f \u0648 \u0646\u06cc \u0627\u0632 \u062e\u0648\u0627\u0628 \u0648 \u062e\u0648\u0631\u062f"}, "6": {"m1": "\u0645\u06cc \u0646\u062f\u0627\u0646\u06cc \u0639\u0634\u0642 \u0648 \u0645\u0633\u062a\u06cc \u0627\u0632 \u06a9\u062c\u0627\u0633\u062a\u061f", "m2": "\u0627\u06cc\u0646 \u0634\u0639\u0627\u0639 \u0622\u0641\u062a\u0627\u0628 \u0645\u0635\u0637\u0641\u06cc \u0627\u0633\u062a"}, "7": {"m1": "\u0632\u0646\u062f\u0647 \u0626\u06cc \u062a\u0627 \u0633\u0648\u0632 \u0627\u0648 \u062f\u0631 \u062c\u0627\u0646 \u062a\u0633\u062a", "m2": "\u0627\u06cc\u0646 \u0646\u06af\u0647\u062f\u0627\u0631\u0646\u062f\u0647\u0654 \u0627\u06cc\u0645\u0627\u0646 \u062a\u0633\u062a"}, "8": {"m1": "\u0628\u0627 \u062e\u0628\u0631 \u0634\u0648 \u0627\u0632 \u0631\u0645\u0648\u0632 \u0622\u0628 \u0648 \u06af\u0644", "m2": "\u067e\u0633 \u0628\u0632\u0646 \u0628\u0631 \u0622\u0628 \u0648 \u06af\u0644 \u0627\u06a9\u0633\u06cc\u0631 \u062f\u0644"}, "9": {"m1": "\u062f\u0644 \u0632 \u062f\u06cc\u0646 \u0633\u0631 \u0686\u0634\u0645\u0647\u0654 \u0647\u0631 \u0642\u0648\u062a \u0627\u0633\u062a", "m2": "\u062f\u06cc\u0646 \u0647\u0645\u0647 \u0627\u0632 \u0645\u0639\u062c\u0632\u0627\u062a \u0635\u062d\u0628\u062a \u0627\u0633\u062a"}, "10": {"m1": "\u062f\u06cc\u0646 \u0645\u062c\u0648 \u0627\u0646\u062f\u0631 \u06a9\u062a\u0628 \u0627\u06cc \u0628\u06cc\u062e\u0628\u0631", "m2": "\u0639\u0644\u0645 \u0648 \u062d\u06a9\u0645\u062a \u0627\u0632 \u06a9\u062a\u0628 \u060c \u062f\u06cc\u0646 \u0627\u0632 \u0646\u0638\u0631"}, "11": {"m1": "\u0628\u0648\u0639\u0644\u06cc \u062f\u0627\u0646\u0646\u062f\u0647\u0654 \u0622\u0628 \u0648 \u06af\u0644 \u0627\u0633\u062a", "m2": "\u0628\u06cc\u062e\u0628\u0631 \u0627\u0632 \u062e\u0633\u062a\u06af\u06cc\u0647\u0627\u06cc \u062f\u0644 \u0627\u0633\u062a"}, "12": {"m1": "\u0646\u06cc\u0634 \u0648 \u0646\u0648\u0634 \u0628\u0648\u0639\u0644\u06cc \u0633\u06cc\u0646\u0627 \u0628\u0647\u0644", "m2": "\u0686\u0627\u0631\u0647 \u0633\u0627\u0632\u06cc\u0647\u0627\u06cc \u062f\u0644 \u0627\u0632 \u0627\u0647\u0644 \u062f\u0644"}, "13": {"m1": "\u0645\u0635\u0637\u0641\u06cc \u0628\u062d\u0631 \u0627\u0633\u062a \u0648 \u0645\u0648\u062c \u0627\u0648 \u0628\u0644\u0646\u062f", "m2": "\u062e\u06cc\u0632 \u0648 \u0627\u06cc\u0646 \u062f\u0631\u06cc\u0627 \u0628\u062c\u0648\u06cc \u062e\u0648\u06cc\u0634 \u0628\u0646\u062f"}, "14": {"m1": "\u0645\u062f\u062a\u06cc \u0628\u0631 \u0633\u0627\u062d\u0644\u0634 \u067e\u06cc\u0686\u06cc\u062f\u0647 \u0626\u06cc", "m2": "\u0644\u0637\u0645\u0647 \u0647\u0627\u06cc \u0645\u0648\u062c \u0627\u0648 \u0646\u0627\u062f\u06cc\u062f\u0647 \u0626\u06cc"}, "15": {"m1": "\u06cc\u06a9 \u0632\u0645\u0627\u0646 \u062e\u0648\u062f \u0631\u0627 \u0628\u0647 \u062f\u0631\u06cc\u0627 \u062f\u0631 \u0641\u06a9\u0646", "m2": "\u062a\u0627 \u0631\u0648\u0627\u0646 \u0631\u0641\u062a\u0647 \u0628\u0627\u0632 \u0622\u06cc\u062f \u0628\u062a\u0646"}, "16": {"m1": "\u0627\u06cc \u0645\u0633\u0644\u0645\u0627\u0646 \u062c\u0632 \u0628\u0631\u0627\u0647 \u062d\u0642 \u0645\u0631\u0648", "m2": "\u0646\u0627\u0627\u0645\u06cc\u062f \u0627\u0632 \u0631\u062d\u0645\u062a \u0639\u0627\u0645\u06cc \u0645\u0634\u0648"}, "17": {"m1": "\u067e\u0631\u062f\u0647 \u0628\u06af\u0630\u0627\u0631 \u0622\u0634\u06a9\u0627\u0631\u0627\u0626\u06cc \u06af\u0632\u06cc\u0646", "m2": "\u062a\u0627 \u0628\u0644\u0631\u0632\u062f \u0627\u0632 \u0633\u062c\u0648\u062f \u062a\u0648 \u0632\u0645\u06cc\u0646"}, "18": {"m1": "\u062f\u0648\u0634 \u062f\u06cc\u062f\u0645 \u0641\u0637\u0631\u062a \u0628\u06cc\u062a\u0627\u0628 \u0631\u0627", "m2": "\u0631\u0648\u062d \u0622\u0646 \u0647\u0646\u06af\u0627\u0645\u0647\u0654 \u0627\u0633\u0628\u0627\u0628 \u0631\u0627"}, "19": {"m1": "\u0686\u0634\u0645 \u0627\u0648 \u0628\u0631 \u0632\u0634\u062a \u0648 \u062e\u0648\u0628 \u06a9\u0627\u0626\u0646\u0627\u062a", "m2": "\u062f\u0631 \u0646\u06af\u0627\u0647 \u0627\u0648 \u063a\u06cc\u0648\u0628 \u06a9\u0627\u0626\u0646\u0627\u062a"}, "20": {"m1": "\u062f\u0633\u062a \u0627\u0648 \u0628\u0627 \u0622\u0628 \u0648 \u062e\u0627\u06a9 \u0627\u0646\u062f\u0631 \u0633\u062a\u06cc\u0632", "m2": "\u0622\u0646 \u0628\u0647\u0645 \u067e\u06cc\u0648\u0633\u062a\u0647 \u0648 \u0627\u06cc\u0646 \u0631\u06cc\u0632 \u0631\u06cc\u0632"}, "21": {"m1": "\u06af\u0641\u062a\u0645\u0634 \u062f\u0631 \u062c\u0633\u062a\u062c\u0648\u06cc \u06a9\u06cc\u0633\u062a\u06cc\u061f", "m2": "\u062f\u0631 \u062a\u0644\u0627\u0634 \u062a\u0627\u0631 \u0648 \u067e\u0648\u06cc \u06a9\u06cc\u0633\u062a\u06cc\u061f"}, "22": {"m1": "\u06af\u0641\u062a \u0627\u0632 \u062d\u06a9\u0645 \u062e\u062f\u0627\u06cc \u0630\u0648\u0627\u0644\u0645\u0646\u0646", "m2": "\u0622\u062f\u0645\u06cc \u0646\u0648 \u0633\u0627\u0632\u0645 \u0627\u0632 \u062e\u0627\u06a9 \u06a9\u0647\u0646"}, "23": {"m1": "\u0645\u0634\u062a \u062e\u0627\u06a9\u06cc \u0631\u0627 \u0628\u0635\u062f \u0631\u0646\u06af \u0622\u0632\u0645\u0648\u062f", "m2": "\u067e\u06cc \u0628\u0647 \u067e\u06cc \u062a\u0627\u0628\u06cc\u062f \u0648 \u0633\u0646\u062c\u06cc\u062f \u0648 \u0641\u0632\u0648\u062f"}, "24": {"m1": "\u0622\u062e\u0631 \u0627\u0648 \u0631\u0627 \u0622\u0628 \u0648 \u0631\u0646\u06af \u0644\u0627\u0644\u0647 \u062f\u0627\u062f", "m2": "\u0644\u0627\u0627\u0644\u0647 \u0627\u0646\u062f\u0631 \u0636\u0645\u06cc\u0631 \u0627\u0648 \u0646\u0647\u0627\u062f"}, "25": {"m1": "\u0628\u0627\u0634 \u062a\u0627 \u0628\u06cc\u0646\u06cc \u0628\u0647\u0627\u0631 \u062f\u06cc\u06af\u0631\u06cc", "m2": "\u0627\u0632 \u0628\u0647\u0627\u0631 \u067e\u0627\u0633\u062a\u0627\u0646 \u0631\u0646\u06af\u06cc\u0646 \u062a\u0631\u06cc"}, "26": {"m1": "\u0647\u0631 \u0632\u0645\u0627\u0646 \u062a\u062f\u0628\u06cc\u0631\u0647\u0627 \u062f\u0627\u0631\u062f \u0631\u0642\u06cc\u0628", "m2": "\u062a\u0627 \u0646\u06af\u06cc\u0631\u06cc \u0627\u0632 \u0628\u0647\u0627\u0631 \u062e\u0648\u062f \u0646\u0635\u06cc\u0628"}, "27": {"m1": "\u0628\u0631 \u062f\u0631\u0648\u0646 \u0634\u0627\u062e \u06af\u0644 \u062f\u0627\u0631\u0645 \u0646\u0638\u0631", "m2": "\u063a\u0646\u0686\u0647 \u0647\u0627 \u0631\u0627 \u062f\u06cc\u062f\u0647 \u0627\u0645 \u0627\u0646\u062f\u0631 \u0633\u0641\u0631"}, "28": {"m1": "\u0644\u0627\u0644\u0647 \u0631\u0627 \u062f\u0631 \u0648\u0627\u062f\u06cc \u0648 \u06a9\u0648\u0647 \u0648 \u062f\u0645\u0646", "m2": "\u0627\u0632 \u062f\u0645\u06cc\u062f\u0646 \u0628\u0627\u0632 \u0646\u062a\u0648\u0627\u0646 \u062f\u0627\u0634\u062a\u0646"}, "29": {"m1": "\u0628\u0634\u0646\u0648\u062f \u0645\u0631\u062f\u06cc \u06a9\u0647 \u0635\u0627\u062d\u0628 \u062c\u0633\u062a\u062c\u0648\u0633\u062a", "m2": "\u0646\u063a\u0645\u0647 \u0626\u06cc \u0631\u0627 \u06a9\u0648 \u0647\u0646\u0648\u0632 \u0627\u0646\u062f\u0631 \u06af\u0644\u0648\u0633\u062a"}}}