{"text": {"0": {"m1": "\u0645\u063a\u0646\u06cc \u062f\u0631\u06cc\u0646 \u067e\u0631\u062f\u0647 \u062f\u06cc\u0631\u0633\u0627\u0644", "m2": "\u0646\u0648\u0627\u0626\u06cc \u0628\u0631\u0627\u0646\u06af\u06cc\u0632 \u0648 \u0628\u0627 \u0627\u0648 \u0628\u0646\u0627\u0644"}, "1": {"m1": "\u0645\u06af\u0631 \u0628\u0631 \u0646\u0648\u0627\u06cc \u0686\u0646\u0627\u0646 \u0646\u0627\u0644\u0647\u200c\u0627\u06cc", "m2": "\u0641\u0631\u0648\u0628\u0627\u0631\u062f \u0627\u0632 \u0627\u0634\u06a9 \u0645\u0646 \u0698\u0627\u0644\u0647\u200c\u0627\u06cc"}, "2": {"m1": "\u0628\u0644\u06cc\u0646\u0627\u0633 \u0631\u0627 \u0686\u0648\u0646 \u0633\u0631 \u0622\u0645\u062f \u062c\u0647\u0627\u0646", "m2": "\u0686\u0646\u06cc\u0646 \u06af\u0641\u062a \u062f\u0631 \u06af\u0648\u0634 \u06a9\u0627\u0631 \u0622\u06af\u0647\u0627\u0646"}, "3": {"m1": "\u06a9\u0647 \u0647\u0646\u06af\u0627\u0645 \u06a9\u0648\u0686 \u0622\u0645\u062f \u0627\u06cc\u0646\u06a9 \u0641\u0631\u0627\u0632", "m2": "\u0628\u0647 \u062c\u0627\u06cc \u062f\u06af\u0631 \u0645\u06cc\u200c\u06a9\u0646\u0645 \u062a\u0631\u06a9\u062a\u0627\u0632"}, "4": {"m1": "\u06af\u0644\u06cc\u0646 \u062e\u0627\u0646\u0647\u0654 \u06a9\u0648 \u0633\u0631\u0627\u06cc \u0645\u0646\u0633\u062a", "m2": "\u0646\u0647 \u0645\u0646 \u0647\u06cc\u06a9\u0644\u06cc \u062f\u0627\u0646 \u06a9\u0647 \u062c\u0627\u06cc \u0645\u0646\u0633\u062a"}, "5": {"m1": "\u0628\u0647 \u0627\u06cc\u0646 \u0647\u0641\u062a \u0647\u06cc\u06a9\u0644 \u06a9\u0647 \u062f\u0627\u0631\u062f \u0633\u067e\u0647\u0631", "m2": "\u0633\u0631\u0645 \u0647\u0645 \u0641\u0631\u0648 \u0646\u0627\u06cc\u062f \u0627\u0632 \u0631\u0627\u0647 \u0645\u0647\u0631"}, "6": {"m1": "\u0645\u0646 \u0622\u0646 \u0627\u0648\u062c \u06af\u0631\u062f\u0648\u0646 \u067e\u0646\u0627 \u062e\u0633\u0631\u0648\u0645", "m2": "\u06a9\u0647 \u062f\u0631 \u062e\u0627\u0646\u0647 \u0645\u06cc\u200c\u0622\u06cc\u0645 \u0648 \u0645\u06cc\u200c\u0631\u0648\u0645"}, "7": {"m1": "\u06af\u0647\u06cc \u062f\u0631 \u062e\u0632\u0645 \u063a\u0646\u0686\u0647\u200c\u0627\u06cc \u0631\u0627 \u0628\u0647 \u06a9\u0627\u062e", "m2": "\u06af\u0647\u06cc \u0628\u0631 \u067e\u0631\u0645 \u0637\u0627\u0648\u0633\u06cc \u0631\u0627 \u0628\u0647 \u0634\u0627\u062e"}, "8": {"m1": "\u067e\u0631\u06cc\u0648\u0627\u0631\u0645 \u0627\u0632 \u0686\u0634\u0645\u0647\u0627 \u0646\u0627\u067e\u062f\u06cc\u062f", "m2": "\u0628\u0647 \u0647\u0631 \u062c\u0627 \u06a9\u0647 \u062e\u0648\u0627\u0647\u0645 \u062a\u0648\u0627\u0646\u0645 \u067e\u0631\u06cc\u062f"}, "9": {"m1": "\u0634\u062f \u0622\u0645\u062f \u0628\u0647 \u0642\u062f\u0631 \u0632\u0645\u0627\u0646 \u06a9\u06cc \u06a9\u0646\u0645", "m2": "\u0632\u0645\u0627\u0646 \u0631\u0627 \u06a9\u062c\u0627 \u067e\u06cc \u0646\u0647\u0645 \u067e\u06cc \u06a9\u0646\u0645"}, "10": {"m1": "\u0686\u0648 \u06a9\u0648\u0634\u0645 \u0646\u0647\u0645 \u0628\u0631 \u0633\u0631 \u0633\u062f\u0631\u0647 \u067e\u0627\u06cc", "m2": "\u0686\u0648 \u062e\u0648\u0627\u0647\u0645 \u06a9\u0646\u0645 \u062f\u0631 \u062f\u0644 \u0635\u062e\u0631\u0647 \u062c\u0627\u06cc"}, "11": {"m1": "\u0628\u0647 \u062f\u0634\u062a \u0648 \u0628\u0647 \u062f\u0631\u06cc\u0627 \u062a\u0648\u0627\u0646\u0645 \u06af\u0630\u0634\u062a", "m2": "\u0647\u0645 \u0627\u0644\u06cc\u0627\u0633 \u062f\u0631\u06cc\u0627 \u0648 \u0647\u0645 \u062e\u0636\u0631 \u062f\u0634\u062a"}, "12": {"m1": "\u062c\u0632 \u0627\u06cc\u0646 \u0647\u0631 \u0686\u0647 \u06cc\u0627\u0628\u06cc \u062f\u0631 \u0627\u06cc\u0648\u0627\u0646 \u0645\u0646", "m2": "\u0646\u0647 \u0645\u0646 \u0647\u0645\u0646\u0634\u06cc\u0646\u06cc\u0633\u062a \u0628\u0631 \u062e\u0648\u0627\u0646 \u0645\u0646"}, "13": {"m1": "\u0645\u0646 \u0622\u0646\u0645 \u06a9\u0647 \u062e\u0648\u0627\u0647\u0645 \u0634\u062f\u0646 \u0628\u0631\u0641\u0631\u0627\u0632", "m2": "\u0628\u0631\u0648\u0646 \u062f\u0627\u0646 \u0632\u0645\u0646 \u0647\u0631 \u0686\u0647 \u06cc\u0627\u0628\u0646\u062f \u0628\u0627\u0632"}, "14": {"m1": "\u0686\u0648 \u06af\u0641\u062a \u0627\u06cc\u0646 \u062a\u0631\u0646\u0645 \u0628\u0647 \u0622\u0648\u0627\u0632 \u0646\u0631\u0645", "m2": "\u0633\u0648\u06cc \u0647\u0645\u0631\u0647\u0627\u0646 \u0628\u0627\u0631\u06af\u06cc \u06a9\u0631\u062f \u06af\u0631\u0645"}, "15": {"m1": "\u0628\u0631\u0622\u0633\u0648\u062f \u0627\u0632 \u0622\u0634\u0648\u0628\u0647\u0627\u06cc \u062c\u0647\u0627\u0646", "m2": "\u06a9\u0647 \u062c\u0634\u0646\u06cc \u0628\u0648\u062f \u0645\u0631\u06af \u0628\u0627 \u0647\u0645\u0631\u0647\u0627\u0646"}}}