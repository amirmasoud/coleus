{"text": {"0": {"m1": "\u0628\u0627 \u062f\u0648 \u062d\u06a9\u06cc\u0645 \u0627\u0632 \u0633\u0631 \u0647\u0645\u062e\u0627\u0646\u06af\u06cc", "m2": "\u0634\u062f \u0633\u062e\u0646\u06cc \u0686\u0646\u062f \u0632 \u0628\u06cc\u06af\u0627\u0646\u06af\u06cc"}, "1": {"m1": "\u0644\u0627\u0641 \u0645\u0646\u06cc \u0628\u0648\u062f \u0648 \u062a\u0648\u06cc \u0628\u0631\u0646\u062a\u0627\u0641\u062a", "m2": "\u0645\u0644\u06a9 \u06cc\u06a9\u06cc \u0628\u0648\u062f \u0648 \u062f\u0648\u06cc \u0628\u0631\u0646\u062a\u0627\u0641\u062a"}, "2": {"m1": "\u062d\u0642 \u062f\u0648 \u0646\u0634\u0627\u06cc\u062f \u06a9\u0647 \u06cc\u06a9\u06cc \u0628\u0634\u0646\u0648\u0646\u062f", "m2": "\u0633\u0631 \u062f\u0648 \u0646\u0628\u0627\u06cc\u062f \u06a9\u0647 \u06cc\u06a9\u06cc \u0628\u062f\u0631\u0648\u0646\u062f"}, "3": {"m1": "\u062c\u0627\u06cc \u062f\u0648 \u0634\u0645\u0634\u06cc\u0631 \u0646\u06cc\u0627\u0645\u06cc \u06a9\u0647 \u062f\u06cc\u062f", "m2": "\u0628\u0632\u0645 \u062f\u0648 \u062c\u0645\u0634\u06cc\u062f \u0645\u0642\u0627\u0645\u06cc \u06a9\u0647 \u062f\u06cc\u062f"}, "4": {"m1": "\u062f\u0631 \u0637\u0645\u0639 \u0622\u0646 \u0628\u0648\u062f \u062f\u0648 \u0641\u0631\u0632\u0627\u0646\u0647 \u0631\u0627", "m2": "\u06a9\u0632 \u062f\u0648 \u06cc\u06a9\u06cc \u062e\u0627\u0635 \u06a9\u0646\u062f \u062e\u0627\u0646\u0647 \u0631\u0627"}, "5": {"m1": "\u0686\u0648\u0646 \u0639\u0635\u0628\u06cc\u062a \u06a9\u0645\u0631 \u06a9\u06cc\u0646 \u06af\u0631\u0641\u062a", "m2": "\u062e\u0627\u0646\u0647 \u0632 \u067e\u0631\u062f\u0627\u062e\u062a\u0646 \u0622\u06cc\u06cc\u0646 \u06af\u0631\u0641\u062a"}, "6": {"m1": "\u0647\u0631 \u062f\u0648 \u0628\u0647 \u0634\u0628\u06af\u06cc\u0631 \u0646\u0648\u0627\u0626\u06cc \u0632\u062f\u0646\u062f", "m2": "\u062e\u0627\u0646\u0647 \u0641\u0631\u0648\u0634\u0627\u0646\u0647 \u0637\u0644\u0627\u0626\u06cc \u0632\u062f\u0646\u062f"}, "7": {"m1": "\u06a9\u0632 \u0633\u0631 \u0646\u0627\u0633\u0627\u062e\u062a\u06af\u06cc \u0628\u06af\u0630\u0631\u0646\u062f", "m2": "\u0633\u0627\u062e\u062a\u0647 \u062e\u0648\u06cc\u0634 \u062f\u0648 \u0634\u0631\u0628\u062a \u062e\u0648\u0631\u0646\u062f"}, "8": {"m1": "\u062a\u0627 \u06a9\u0647 \u062f\u0631\u06cc\u0646 \u067e\u0627\u06cc\u0647 \u0642\u0648\u06cc\u200c\u062f\u0644\u200c\u062a\u0631\u0633\u062a", "m2": "\u0634\u0631\u0628\u062a \u0632\u0647\u0631 \u06a9\u0647 \u0647\u0644\u0627\u0647\u0644\u200c\u062a\u0631\u0633\u062a"}, "9": {"m1": "\u0645\u0644\u06a9 \u062f\u0648 \u062d\u06a9\u0645\u062a \u0628\u0647 \u06cc\u06a9\u06cc \u0641\u0646 \u062f\u0647\u0646\u062f", "m2": "\u062c\u0627\u0646 \u062f\u0648 \u0635\u0648\u0631\u062a \u0628\u0647 \u06cc\u06a9 \u062a\u0646 \u062f\u0647\u0646\u062f"}, "10": {"m1": "\u062e\u0635\u0645 \u0646\u062e\u0633\u062a\u06cc\u0646 \u0642\u062f\u0631\u06cc \u0632\u0647\u0631 \u0633\u0627\u062e\u062a", "m2": "\u06a9\u0632 \u0639\u0641\u062a\u06cc \u0633\u0646\u06af \u0633\u06cc\u0647 \u0631\u0627 \u06af\u062f\u0627\u062e\u062a"}, "11": {"m1": "\u062f\u0627\u062f \u0628\u062f\u0648 \u06a9\u06cc\u0646 \u0645\u06cc \u062c\u0627\u0646\u200c\u067e\u0631\u0648\u0631\u0633\u062a", "m2": "\u0632\u0647\u0631 \u0645\u062f\u0627\u0646\u0634 \u06a9\u0647 \u0628\u0647 \u0627\u0632 \u0634\u06a9\u0631\u0633\u062a"}, "12": {"m1": "\u0634\u0631\u0628\u062a \u0627\u0648 \u0631\u0627 \u0633\u062a\u062f \u0622\u0646 \u0634\u06cc\u0631 \u0645\u0631\u062f", "m2": "\u0632\u0647\u0631 \u0628\u0647 \u06cc\u0627\u062f \u0634\u06a9\u0631 \u0622\u0633\u0627\u0646 \u0628\u062e\u0648\u0631\u062f"}, "13": {"m1": "\u0646\u0648\u0634 \u06af\u06cc\u0627 \u067e\u062e\u062a \u0648 \u0628\u062f\u0648 \u062f\u0631\u0646\u0634\u0633\u062a", "m2": "\u0631\u0647\u06af\u0630\u0631 \u0632\u0647\u0631 \u0628\u0647 \u062a\u0631\u06cc\u0627\u06a9 \u0628\u0633\u062a"}, "14": {"m1": "\u0633\u0648\u062e\u062a \u0686\u0648 \u067e\u0631\u0648\u0627\u0646\u0647 \u0648 \u067e\u0631 \u0628\u0627\u0632 \u06cc\u0627\u0641\u062a", "m2": "\u0634\u0645\u0639 \u0635\u0641\u062a \u0628\u0627\u0632 \u0628\u0647 \u0645\u062c\u0644\u0633 \u0634\u062a\u0627\u0641\u062a"}, "15": {"m1": "\u0627\u0632 \u0686\u0645\u0646 \u0628\u0627\u063a \u06cc\u06a9\u06cc \u06af\u0644 \u0628\u0686\u06cc\u062f", "m2": "\u062e\u0648\u0627\u0646\u062f \u0641\u0633\u0648\u0646\u06cc \u0648 \u0628\u0631 \u0622\u0646 \u06af\u0644 \u062f\u0645\u06cc\u062f"}, "16": {"m1": "\u062f\u0627\u062f \u0628\u0647 \u062f\u0634\u0645\u0646 \u0632 \u067e\u06cc \u0642\u0647\u0631 \u0627\u0648", "m2": "\u0622\u0646 \u06af\u0644 \u067e\u0631 \u06a9\u0627\u0631 \u062a\u0631 \u0627\u0632 \u0632\u0647\u0631 \u0627\u0648"}, "17": {"m1": "\u062f\u0634\u0645\u0646 \u0627\u0632 \u0622\u0646 \u06af\u0644 \u06a9\u0647 \u0641\u0633\u0648\u0646\u062e\u0648\u0627\u0646 \u0628\u062f\u0627\u062f", "m2": "\u062a\u0631\u0633 \u0628\u0631 \u0627\u0648 \u0686\u06cc\u0631\u0647 \u0634\u062f \u0648 \u062c\u0627\u0646 \u0628\u062f\u0627\u062f"}, "18": {"m1": "\u0622\u0646 \u0628\u0639\u0644\u0627\u062c \u0627\u0632 \u062a\u0646 \u062e\u0648\u062f \u0632\u0647\u0631 \u0628\u0631\u062f", "m2": "\u0648\u06cc\u0646 \u0628\u0647 \u06cc\u06a9\u06cc \u06af\u0644 \u0632 \u062a\u0648\u0647\u0645 \u0628\u0645\u0631\u062f"}, "19": {"m1": "\u0647\u0631 \u06af\u0644 \u0631\u0646\u06af\u06cc\u0646 \u06a9\u0647 \u0628\u0647 \u0628\u0627\u063a \u0632\u0645\u06cc\u0633\u062a", "m2": "\u0642\u0637\u0631\u0647\u200c\u0627\u06cc \u0627\u0632 \u062e\u0648\u0646 \u062f\u0644 \u0622\u062f\u0645\u06cc\u0633\u062a"}, "20": {"m1": "\u0628\u0627\u063a \u0632\u0645\u0627\u0646\u0647 \u06a9\u0647 \u0628\u0647\u0627\u0631\u0634 \u062a\u0648\u0626\u06cc", "m2": "\u062e\u0627\u0646\u0647 \u063a\u0645 \u062f\u0627\u0646 \u06a9\u0647 \u0646\u06af\u0627\u0631\u0634 \u062a\u0648\u0626\u06cc"}, "21": {"m1": "\u0633\u0646\u06af \u062f\u0631\u06cc\u0646 \u062e\u0627\u06a9 \u0645\u0637\u0628\u0642 \u0646\u0634\u0627\u0646", "m2": "\u062e\u0627\u06a9 \u0628\u0631\u06cc\u0646 \u0622\u0628 \u0645\u0639\u0644\u0642 \u0646\u0634\u0627\u0646"}, "22": {"m1": "\u0628\u06af\u0630\u0631 \u0627\u0632\u06cc\u0646 \u0622\u0628 \u0648 \u062e\u06cc\u0627\u0644\u0627\u062a \u0627\u0648", "m2": "\u0628\u0631 \u067e\u0631 \u0627\u0632\u06cc\u0646 \u062e\u0627\u06a9 \u0648 \u062e\u0631\u0627\u0628\u0627\u062a \u0627\u0648"}, "23": {"m1": "\u0628\u0631 \u0645\u0647 \u0648 \u062e\u0648\u0631\u0634\u06cc\u062f \u0645\u06cc\u0627\u0648\u0631 \u0648\u0642\u0648\u0641", "m2": "\u0645\u0647 \u062e\u0648\u0631 \u0648 \u062e\u0648\u0631\u0634\u06cc\u062f \u0634\u06a9\u0646 \u0686\u0648\u0646 \u06a9\u0633\u0648\u0641"}, "24": {"m1": "\u06a9\u06cc\u0646 \u0645\u0647 \u0632\u0631\u06cc\u0646 \u06a9\u0647 \u062f\u0631\u06cc\u0646 \u062e\u0631\u06af\u0647\u0633\u062a", "m2": "\u063a\u0648\u0644 \u0631\u0647 \u0639\u0634\u0642 \u062e\u0644\u06cc\u0644 \u0627\u0644\u0644\u0647\u0633\u062a"}, "25": {"m1": "\u0631\u0648\u0632 \u062a\u0631\u0627 \u0635\u0628\u062d \u062c\u06af\u0631\u0633\u0648\u0632 \u06a9\u0631\u062f", "m2": "\u0686\u0631\u062e\u062a \u0627\u0632 \u0622\u0646 \u0631\u0648\u0632 \u0628\u062f\u06cc\u0646 \u0631\u0648\u0632 \u06a9\u0631\u062f"}, "26": {"m1": "\u06af\u0631 \u062f\u0644 \u062e\u0648\u0631\u0634\u06cc\u062f \u0641\u0631\u0648\u0632 \u0622\u0648\u0631\u06cc", "m2": "\u0631\u0648\u0632\u06cc \u0627\u0632 \u0627\u06cc\u0646\u0631\u0648\u0632 \u0628\u0647 \u0631\u0648\u0632 \u0622\u0648\u0631\u06cc"}, "27": {"m1": "\u0627\u0634\u06a9 \u0641\u0634\u0627\u0646 \u0646\u0627 \u0628\u0647 \u06af\u0644\u0627\u0628 \u0627\u0645\u06cc\u062f", "m2": "\u0628\u0633\u062a\u0631\u06cc \u0627\u06cc\u0646 \u0644\u0648\u062d \u0633\u06cc\u0627\u0647 \u0648 \u0633\u0641\u06cc\u062f"}, "28": {"m1": "\u062a\u0627 \u0686\u0648 \u0639\u0645\u0644 \u0633\u0646\u062c \u0633\u0644\u0627\u0645\u062a \u0634\u0648\u06cc", "m2": "\u0686\u0631\u0628 \u062a\u0631\u0627\u0632\u0648\u06cc \u0642\u06cc\u0627\u0645\u062a \u0634\u0648\u06cc"}, "29": {"m1": "\u062f\u06cc\u0646 \u06a9\u0647 \u0642\u0648\u06cc \u062f\u0627\u0631\u062f \u0628\u0627\u0632\u0648\u062a \u0631\u0627", "m2": "\u0631\u0627\u0633\u062a \u06a9\u0646\u062f \u0639\u062f\u0644 \u062a\u0631\u0627\u0632\u0648\u062a \u0631\u0627"}, "30": {"m1": "\u0647\u06cc\u0686 \u0647\u0646\u0631\u067e\u06cc\u0634\u0647 \u0622\u0632\u0627\u062f \u0645\u0631\u062f", "m2": "\u062f\u0631 \u063a\u0645 \u062f\u0646\u06cc\u0627 \u063a\u0645 \u062f\u0646\u06cc\u0627 \u0646\u062e\u0648\u0631\u062f"}, "31": {"m1": "\u0686\u0648\u0646\u06a9\u0647 \u0628\u0647 \u062f\u0646\u06cc\u0627\u0633\u062a \u062a\u0645\u0646\u0627 \u062a\u0631\u0627", "m2": "\u062f\u06cc\u0646 \u0628\u0647 \u0646\u0638\u0627\u0645\u06cc \u062f\u0647 \u0648 \u062f\u0646\u06cc\u0627 \u062a\u0631\u0627"}}}