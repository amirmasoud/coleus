{"text": {"0": {"m1": "\u0686\u0648 \u0634\u0645\u0634\u06cc\u0631 \u067e\u06cc\u06a9\u0627\u0631 \u0628\u0631\u062f\u0627\u0634\u062a\u06cc", "m2": "\u0646\u06af\u0647 \u062f\u0627\u0631 \u067e\u0646\u0647\u0627\u0646 \u0631\u0647 \u0622\u0634\u062a\u06cc"}, "1": {"m1": "\u06a9\u0647 \u0644\u0634\u06a9\u0631 \u0634\u06a9\u0648\u0641\u0627\u0646 \u0645\u063a\u0641\u0631 \u0634\u06a9\u0627\u0641", "m2": "\u0646\u0647\u0627\u0646 \u0635\u0644\u062d \u062c\u0633\u062a\u0646\u062f \u0648 \u067e\u06cc\u062f\u0627 \u0645\u0635\u0627\u0641"}, "2": {"m1": "\u062f\u0644 \u0645\u0631\u062f \u0645\u06cc\u062f\u0627\u0646 \u0646\u0647\u0627\u0646\u06cc \u0628\u062c\u0648\u06cc", "m2": "\u06a9\u0647 \u0628\u0627\u0634\u062f \u06a9\u0647 \u062f\u0631 \u067e\u0627\u06cc\u062a \u0627\u0641\u062a\u062f \u0686\u0648 \u06af\u0648\u06cc"}, "3": {"m1": "\u0686\u0648 \u0633\u0627\u0644\u0627\u0631\u06cc \u0627\u0632 \u062f\u0634\u0645\u0646 \u0627\u0641\u062a\u062f \u0628\u0647 \u0686\u0646\u06af", "m2": "\u0628\u0647 \u06a9\u0634\u062a\u0646 \u062f\u0631\u0634 \u06a9\u0631\u062f \u0628\u0627\u06cc\u062f \u062f\u0631\u0646\u06af"}, "4": {"m1": "\u06a9\u0647 \u0627\u0641\u062a\u062f \u06a9\u0632 \u0627\u06cc\u0646 \u0646\u06cc\u0645\u0647 \u0647\u0645 \u0633\u0631\u0648\u0631\u06cc", "m2": "\u0628\u0645\u0627\u0646\u062f \u06af\u0631\u0641\u062a\u0627\u0631 \u062f\u0631 \u0686\u0646\u0628\u0631\u06cc"}, "5": {"m1": "\u0627\u06af\u0631 \u06a9\u0634\u062a\u06cc \u0627\u06cc\u0646 \u0628\u0646\u062f\u06cc \u0631\u06cc\u0634 \u0631\u0627", "m2": "\u0646\u0628\u06cc\u0646\u06cc \u062f\u06af\u0631 \u0628\u0646\u062f\u06cc \u062e\u0648\u06cc\u0634 \u0631\u0627"}, "6": {"m1": "\u0646\u062a\u0631\u0633\u062f \u06a9\u0647 \u062f\u0648\u0631\u0627\u0646\u0634 \u0628\u0646\u062f\u06cc \u06a9\u0646\u062f", "m2": "\u06a9\u0647 \u0628\u0631 \u0628\u0646\u062f\u06cc\u0627\u0646 \u0632\u0648\u0631\u0645\u0646\u062f\u06cc \u06a9\u0646\u062f\u061f"}, "7": {"m1": "\u06a9\u0633\u06cc \u0628\u0646\u062f\u06cc\u0627\u0646 \u0631\u0627 \u0628\u0648\u062f \u062f\u0633\u062a\u06af\u06cc\u0631", "m2": "\u06a9\u0647 \u062e\u0648\u062f \u0628\u0648\u062f\u0647 \u0628\u0627\u0634\u062f \u0628\u0647 \u0628\u0646\u062f\u06cc \u0627\u0633\u06cc\u0631"}, "8": {"m1": "\u0627\u06af\u0631 \u0633\u0631 \u0646\u0647\u062f \u0628\u0631 \u062e\u0637\u062a \u0633\u0631\u0648\u0631\u06cc", "m2": "\u0686\u0648 \u0646\u06cc\u06a9\u0634 \u0628\u062f\u0627\u0631\u06cc\u060c \u0646\u0647\u062f \u062f\u06cc\u06af\u0631\u06cc"}, "9": {"m1": "\u0627\u06af\u0631 \u062e\u0641\u06cc\u0647 \u062f\u0647 \u062f\u0644 \u0628\u062f\u0633\u062a \u0622\u0648\u0631\u06cc", "m2": "\u0627\u0632 \u0622\u0646 \u0628\u0647 \u06a9\u0647 \u0635\u062f \u0631\u0647 \u0634\u0628\u06cc\u062e\u0648\u0646 \u0628\u0631\u06cc"}}}