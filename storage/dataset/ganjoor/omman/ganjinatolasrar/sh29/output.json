{"text": {"0": {"p": "\u062f\u0631 \u0628\u06cc\u0627\u0646 \u062c\u0648\u0627\u0628 \u062f\u0627\u062f\u0646 \u0622\u0646 \u0648\u0644\u06cc \u0627\u06a9\u0628\u0631 \u0628\u0627 \u062a\u0648\u062c\u0647\u0627\u062a \u0648 \u062a\u0641\u0642\u062f\u0627\u062a \u0645\u0631 \u0646\u0648\u0631\u062f\u06cc\u062f\u0647\u200c\u06cc \u062e\u0648\u062f\u060c \u0639\u0644\u06cc \u0627\u06a9\u0628\u0631 \u0631\u0627 \u0628\u0631\u0645\u0635\u062f\u0627\u0642 \u0627\u06cc\u0646\u06a9\u0647\u060c \u0628\u0647\u0631\u0686\u0647 \u0627\u0632 \u062f\u0648\u0633\u062a \u0648 \u0627\u0645\u0627\u0646\u06cc\u060c \u0686\u0647 \u06a9\u0641\u0631 \u0622\u0646 \u062d\u0631\u0641 \u0648 \u0686\u0647 \u0627\u06cc\u0645\u0627\u0646\u00bb \u0628\u0631 \u0645\u0630\u0627\u0642 \u0627\u0647\u0644 \u0639\u0631\u0641\u0627\u0646 \u06af\u0648\u06cc\u062f:"}, "1": {"m1": "\u062f\u0631 \u062c\u0648\u0627\u0628 \u0627\u0631 \u062a\u0646\u06a9 \u0634\u06a9\u0631 \u0642\u0646\u062f \u0631\u06cc\u062e\u062a", "m2": "\u0634\u06a9\u0631 \u0627\u0632 \u0644\u0628 \u0647\u0627\u06cc \u0634\u06a9\u0631 \u062e\u0646\u062f \u0631\u06cc\u062e\u062a"}, "2": {"m1": "\u06af\u0641\u062a: \u06a9\u0627\u06cc \u0641\u0631\u0632\u0646\u062f \u0645\u0642\u0628\u0644 \u0622\u0645\u062f\u06cc", "m2": "\u0622\u0641\u062a \u062c\u0627\u0646\u060c \u0631\u0647\u0632\u0646 \u062f\u0644 \u0622\u0645\u062f\u06cc"}, "3": {"m1": "\u06a9\u0631\u062f\u0647\u200c\u06cc\u06cc \u0627\u0632 \u062d\u0642\u061b \u062a\u062c\u0644\u06cc \u0627\u06cc \u067e\u0633\u0631", "m2": "\u0632\u06cc\u0646 \u062a\u062c\u0644\u06cc\u060c \u0641\u062a\u0646\u0647\u200c\u0647\u0627 \u062f\u0627\u0631\u06cc \u0628\u0633\u0631"}, "4": {"m1": "\u0631\u0627\u0633\u062a \u0628\u0647\u0631 \u0641\u062a\u0646\u0647\u060c \u0642\u0627\u0645\u062a \u06a9\u0631\u062f\u0647\u200c\u06cc\u06cc", "m2": "\u0648\u0647 \u06a9\u0632\u06cc\u0646 \u0642\u0627\u0645\u062a\u060c \u0642\u06cc\u0627\u0645\u062a \u06a9\u0631\u062f\u0647\u200c\u06cc\u06cc"}, "5": {"m1": "\u0646\u0631\u06af\u0633\u062a \u0628\u0627\u0644\u0627\u0644\u0647 \u062f\u0631 \u0637\u0646\u0627\u0632\u06cc\u0633\u062a", "m2": "\u0633\u0646\u0628\u0644\u062a \u0628\u0627 \u0627\u0631\u063a\u0648\u0627\u0646 \u062f\u0631 \u0628\u0627\u0632\u06cc\u0633\u062a"}, "6": {"m1": "\u0627\u0632 \u0631\u062e\u062a \u0645\u0633\u062a \u063a\u0631\u0648\u0631\u0645 \u0645\u06cc\u200c\u06a9\u0646\u06cc", "m2": "\u0627\u0632 \u0645\u0631\u0627\u062f \u062e\u0648\u06cc\u0634 \u062f\u0648\u0631\u0645 \u0645\u06cc\u200c\u06a9\u0646\u06cc"}, "7": {"m1": "\u06af\u0647 \u062f\u0644\u0645 \u067e\u06cc\u0634 \u062a\u0648 \u06af\u0627\u0647\u06cc \u067e\u06cc\u0634 \u0627\u0648\u0633\u062a", "m2": "\u0631\u0648 \u06a9\u0647 \u062f\u0631 \u06cc\u06a9 \u062f\u0644 \u0646\u0645\u06cc\u200c\u06af\u0646\u062c\u062f \u062f\u0648 \u062f\u0648\u0633\u062a"}, "8": {"m1": "\u0628\u06cc\u0634 \u0627\u0632\u06cc\u0646 \u0628\u0627\u0628\u0627! \u062f\u0644\u0645 \u0631\u0627 \u062e\u0648\u0646 \u0645\u06a9\u0646", "m2": "\u0632\u0627\u062f\u0647\u200c\u06cc \u0644\u06cc\u0644\u06cc\u061b \u0645\u0631\u0627 \u0645\u062c\u0646\u0648\u0646 \u0645\u06a9\u0646"}, "9": {"m1": "\u067e\u0634\u062a \u067e\u0627\u060c \u0628\u0631 \u0633\u0627\u063a\u0631 \u062d\u0627\u0644\u0645 \u0645\u0632\u0646", "m2": "\u0646\u06cc\u0634 \u0628\u0631 \u062f\u0644\u061b \u0633\u0646\u06af \u0628\u0631 \u0628\u0627\u0644\u0645 \u0645\u0632\u0646"}, "10": {"m1": "\u062e\u0627\u06a9 \u063a\u0645 \u0628\u0631 \u0641\u0631\u0642 \u0628\u062e\u062a \u062f\u0644 \u0645\u0631\u06cc\u0632", "m2": "\u0628\u0633 \u0646\u0645\u06a9 \u0628\u0631 \u0644\u062e\u062a \u0644\u062e\u062a \u062f\u0644 \u0645\u0631\u06cc\u0632"}, "11": {"m1": "\u0647\u0645\u0686\u0648 \u0686\u0634\u0645 \u062e\u0648\u062f \u0628\u0647 \u0642\u0644\u0628 \u062f\u0644 \u0645\u062a\u0627\u0632", "m2": "\u0647\u0645\u0686\u0648 \u0632\u0644\u0641 \u062e\u0648\u062f\u060c \u067e\u0631\u06cc\u0634\u0627\u0646\u0645 \u0645\u0633\u0627\u0632"}, "12": {"m1": "\u062d\u0627\u06cc\u0644 \u0631\u0647\u060c \u0645\u0627\u0646\u0639 \u0645\u0642\u0635\u062f \u0645\u0634\u0648", "m2": "\u0628\u0631 \u0633\u0631 \u0631\u0627\u0647 \u0645\u062d\u0628\u062a\u060c \u0633\u062f \u0645\u0634\u0648"}, "13": {"m1": "\u0644\u0646 \u062a\u0646\u0627\u0644\u0648\u0627 \u0627\u0644\u0628\u0631 \u062d\u062a\u06cc \u062a\u0646\u0641\u0642\u0648\u0627", "m2": "\u0628\u0639\u062f \u0627\u0632 \u0622\u0646\u061b \u0645\u0645\u0627 \u062a\u062d\u0628\u0648\u0646 \u06af\u0648\u06cc\u062f \u0627\u0648"}, "14": {"m1": "\u0646\u06cc\u0633\u062a \u0627\u0646\u062f\u0631 \u0628\u0632\u0645 \u0622\u0646 \u0648\u0627\u0644\u0627 \u0646\u06af\u0627\u0631", "m2": "\u0627\u0632 \u062a\u0648 \u0628\u0647\u062a\u0631 \u06af\u0648\u0647\u0631\u06cc\u060c \u0628\u0647\u0631 \u0646\u062b\u0627\u0631"}, "15": {"m1": "\u0647\u0631\u0686\u0647 \u063a\u06cc\u0631 \u0627\u0632 \u0627\u0648\u0633\u062a\u060c \u0633\u062f \u0631\u0627\u0647 \u0645\u0646", "m2": "\u0622\u0646 \u0628\u062a \u0633\u062a \u0648 \u063a\u06cc\u0631\u062a \u0645\u0646\u060c \u0628\u062a \u0634\u06a9\u0646"}, "16": {"m1": "\u062c\u0627\u0646 \u0631\u0647\u06cc\u0646 \u0648 \u062f\u0644 \u0627\u0633\u06cc\u0631 \u0686\u0647\u0631 \u062a\u0633\u062a", "m2": "\u0645\u0627\u0646\u0639 \u0631\u0627\u0647 \u0645\u062d\u0628\u062a\u060c \u0645\u0647\u0631 \u062a\u0633\u062a"}, "17": {"m1": "\u0622\u0646 \u062d\u062c\u0627\u0628 \u0627\u0632 \u067e\u06cc\u0634 \u0686\u0648\u0646 \u062f\u0648\u0631\u0627\u0641\u06a9\u0646\u06cc", "m2": "\u0645\u0646 \u062a\u0648 \u0647\u0633\u062a\u0645 \u062f\u0631 \u062d\u0642\u06cc\u0642\u062a\u060c \u062a\u0648 \u0645\u0646\u06cc"}, "18": {"m1": "\u0686\u0648\u0646 \u062a\u0631\u0627 \u0627\u0648 \u062e\u0648\u0627\u0647\u062f \u0627\u0632 \u0645\u0646 \u0631\u0648 \u0646\u0645\u0627", "m2": "\u0631\u0648 \u0646\u0645\u0627 \u0634\u0648\u060c \u062c\u0627\u0646\u0628 \u0627\u0648\u060c \u0631\u0648\u060c \u0646\u0645\u0627"}}}