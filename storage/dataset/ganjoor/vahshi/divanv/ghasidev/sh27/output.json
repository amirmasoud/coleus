{"text": {"0": {"m1": "\u0628\u0631 \u06a9\u0633\u0627\u0646\u06cc \u06a9\u0647 \u0628\u0628\u06cc\u0646\u0646\u062f \u0628\u0647 \u0631\u0648\u06cc \u062a\u0648 \u0647\u0644\u0627\u0644", "m2": "\u0639\u06cc\u062f \u0628\u0627\u0634\u062f \u0647\u0645\u0647 \u0631\u0648\u0632 \u0648 \u0647\u0645\u0647 \u0645\u0627\u0647 \u0648\u0647\u0645\u0647 \u0633\u0627\u0644"}, "1": {"m1": "\u0645\u06cc\u0631\u0645\u06cc\u0631\u0627\u0646 \u06a9\u0647 \u0628\u0648\u062f \u0637\u0644\u0639\u062a \u0641\u0631\u062e\u0646\u062f\u0647\u0654 \u0627\u0648", "m2": "\u0635\u0628\u062d \u0639\u06cc\u062f\u06cc \u06a9\u0647 \u0634\u062f\u0622\u0641\u0627\u0642 \u0627\u0632 \u0627\u0648 \u0641\u0631\u062e \u0641\u0627\u0644"}, "2": {"m1": "\u06af\u0631 \u0628\u0647 \u0627\u0646\u062f\u0627\u0632\u0647\u0654 \u0642\u062f\u0631 \u062a\u0648 \u0648 \u0635\u062f\u0631 \u062a\u0648 \u0632\u06cc\u0646\u062f", "m2": "\u06a9\u0633 \u062f\u0631 \u0627\u06cc\u0648\u0627\u0646 \u062a\u0648 \u0628\u0631\u0646\u06af\u0630\u0631\u062f \u0627\u0632 \u0635\u0641 \u0646\u0639\u0627\u0644"}, "3": {"m1": "\u0628\u0633\u06a9\u0647 \u0627\u0646\u0635\u0627\u0641 \u062a\u0648 \u0628\u0631\u062a\u0627\u0641\u062a\u0647 \u0633\u0631\u067e\u0646\u062c\u0647\u0654 \u0638\u0644\u0645", "m2": "\u0639\u0628\u062b \u0645\u062d\u0636 \u0646\u0645\u0627\u06cc\u0646\u062f \u067e\u0644\u0646\u06af\u0627\u0646 \u0686\u0646\u06af\u0627\u0644"}, "4": {"m1": "\u0642\u0647\u0631\u062a \u0622\u0646\u062c\u0627 \u06a9\u0647 \u06a9\u0646\u062f \u0632\u0644\u0632\u0644\u0647 \u062a\u0641\u0631\u0642\u0647 \u0639\u0627\u0645", "m2": "\u062d\u0641\u0638 \u062c\u0645\u0639\u06cc\u062a \u0627\u062c\u0632\u0627 \u0646\u06a9\u0646\u062f \u0637\u0628\u0639 \u062c\u0628\u0627\u0644"}, "5": {"m1": "\u0639\u0632\u0645\u062a \u0622\u0646\u062c\u0627 \u06a9\u0647 \u0634\u062f\u0647 \u062f\u0631 \u0645\u062f\u062f \u0646\u0627\u0635\u06cc\u0647 \u0635\u0644\u0628", "m2": "\u0631\u06cc\u0634\u0647 \u062f\u0631 \u0622\u0647\u0646 \u0648 \u0641\u0648\u0644\u0627\u062f \u0641\u0631\u0648 \u0628\u0631\u062f\u0647 \u0646\u0647\u0627\u0644"}, "6": {"m1": "\u0645\u06cc\u200c\u0634\u0648\u062f \u06a9\u0648\u0631 \u062d\u0633\u0648\u062f \u062a\u0648 \u0648 \u062f\u0631\u0645\u0627\u0646\u0634 \u0646\u06cc\u0633\u062a", "m2": "\u06a9\u0647 \u0645\u0635\u0648\u0646 \u0627\u0633\u062a \u06a9\u0645\u0627\u0644 \u062a\u0648 \u0632 \u0622\u0633\u06cc\u0628 \u0632\u0648\u0627\u0644"}, "7": {"m1": "\u062f\u0627\u06cc\u0645 \u0627\u0632 \u0646\u06cc\u0631 \u062a\u0627\u0628\u0646\u062f\u0647 \u0628\u0647 \u0633\u0645\u062a \u0627\u0644\u0631\u0623\u0633 \u0627\u0633\u062a", "m2": "\u06af\u0648 \u0628\u0647 \u0633\u0648\u0631\u0627\u062e \u0646\u0634\u06cc\u0646 \u0634\u0628 \u067e\u0631\u0647 \u060c \u06a9\u0648\u062a\u0647 \u06a9\u0646 \u0628\u0627\u0644"}, "8": {"m1": "\u06af\u0631\u0646\u0647 \u0647\u0645 \u0644\u0637\u0641 \u062a\u0648 \u0628\u0627\u0634\u062f \u0633\u067e\u0631 \u062c\u0627\u0646 \u0639\u062f\u0648", "m2": "\u0633\u0627\u06cc\u0647 \u0628\u0627 \u062a\u06cc\u063a \u0631\u0648\u062f \u062e\u0635\u0645 \u062a\u0631\u0627 \u062f\u0631 \u062f\u0646\u0628\u0627\u0644"}, "9": {"m1": "\u0645\u0648\u0631 \u0627\u0632 \u062a\u0634\u062a \u0628\u0631\u0648\u0646 \u0622\u06cc\u062f \u0648 \u0627\u06cc\u0646 \u0645\u0645\u06a9\u0646 \u0646\u06cc\u0633\u062a", "m2": "\u06a9\u0627\u062e\u062a\u0631 \u062a\u06cc\u0631\u0647\u0654 \u062e\u0635\u0645\u062a \u0628\u062f\u0631 \u0622\u06cc\u062f \u0632\u0648 \u0628\u0627\u0644"}, "10": {"m1": "\u062f\u06cc\u062f\u0647 \u0628\u062e\u062a \u0628\u062f\u0627\u0646\u062f\u06cc\u0634 \u062a\u0648 \u0627\u0632 \u06af\u0631\u062f\u0634 \u0686\u0631\u062e", "m2": "\u0686\u0648\u0646 \u0628\u0628\u06cc\u0646\u062f \u0631\u062e \u0645\u0642\u0635\u0648\u062f \u06a9\u0647 \u0627\u0645\u0631\u06cc\u0633\u062a \u0645\u062d\u0627\u0644"}, "11": {"m1": "\u0686\u0627\u0631\u0647\u0654 \u0628\u0627\u0635\u0631\u0647\u0654 \u0627\u0639\u0645\u06cc \u0641\u0637\u0631\u06cc \u0686\u0647 \u06a9\u0646\u062f", "m2": "\u06af\u0631 \u0686\u0647 \u062f\u0631 \u0635\u0646\u0639\u062a \u062e\u0648\u062f \u0645\u0648\u06cc \u0634\u06a9\u0627\u0641\u062f \u06a9\u062d\u0627\u0644"}, "12": {"m1": "\u06af\u0631 \u0628\u0647 \u062e\u0648\u0646 \u0631\u06cc\u062e\u062a\u0646 \u062e\u0635\u0645 \u062a\u0648 \u0641\u062a\u0648\u0627 \u0637\u0644\u0628\u0646\u062f", "m2": "\u062e\u0648\u0646\u0634 \u0622\u0648\u0627\u0632 \u0628\u0631\u0622\u0631\u062f \u06a9\u0647 \u062d\u0644\u0627\u0644 \u0627\u0633\u062a \u062d\u0644\u0627\u0644"}, "13": {"m1": "\u0641\u0644\u06a9 \u062b\u0627\u0628\u062a \u0627\u0632 \u0622\u0646\u0633\u0648\u06cc \u0632\u0645\u0627\u0646 \u062a\u0627\u0632\u062f \u0631\u062e\u0634", "m2": "\u0627\u0632 \u0633\u0645\u0646\u062f \u062a\u0648 \u0627\u06af\u0631 \u06a9\u0633\u0628 \u06a9\u0646\u062f \u0627\u0633\u062a\u0639\u062c\u0627\u0644"}, "14": {"m1": "\u0631\u0627\u06cc\u062a \u0627\u0631 \u0633\u0631\u0645\u0647 \u06a9\u0634 \u062f\u06cc\u062f\u0647\u0654 \u0627\u0646\u062f\u06cc\u0634\u0647 \u0634\u0648\u062f", "m2": "\u062f\u0631 \u0634\u0628 \u062a\u0627\u0631 \u062a\u0648\u0627\u0646 \u062f\u06cc\u062f \u067e\u06cc \u067e\u0627\u06cc \u062e\u06cc\u0627\u0644"}, "15": {"m1": "\u0635\u06cc\u062a \u0622\u0633\u0627\u06cc\u0634 \u0639\u062f\u0644 \u062a\u0648 \u0628\u0631\u0627\u0646\u06af\u06cc\u0632\u062f\u0634\u0627\u0646", "m2": "\u06a9\u0632 \u0645\u0636\u06cc\u0642 \u0631\u062d\u0645 \u0622\u06cc\u0646\u062f \u0633\u0648\u06cc \u0645\u0647\u062f \u0627\u0637\u0641\u0627\u0644"}, "16": {"m1": "\u062f\u0633\u062a \u0627\u0646\u0635\u0627\u0641 \u062a\u0648 \u0622\u0646 \u06a9\u0631\u062f \u06a9\u0647 \u062f\u0631 \u067e\u0627\u06cc \u062d\u0645\u0627\u0645", "m2": "\u062d\u0644\u0642\u0647\u0654 \u062f\u06cc\u062f\u0647\u0654 \u0628\u0627\u0632 \u0627\u0633\u062a \u0686\u0648 \u0632\u0631\u06cc\u0646 \u062e\u0644\u062e\u0627\u0644"}, "17": {"m1": "\u06af\u0631 \u06a9\u0646\u062f \u062e\u0635\u0645 \u062a\u0648 \u062f\u0631 \u0622\u06cc\u0646\u0647 \u0622\u0646 \u0631\u0648\u06cc \u06a9\u0631\u06cc\u0647", "m2": "\u0627\u0632 \u0631\u062e\u0634 \u062f\u0631 \u067e\u0633 \u0622\u06cc\u06cc\u0646\u0647 \u06af\u0631\u06cc\u0632\u062f \u062a\u0645\u062b\u0627\u0644"}, "18": {"m1": "\u062c\u0648\u062f\u062a \u0627\u0632 \u0628\u0644\u0639\u062c\u0628\u06cc\u0647\u0627 \u0634\u062f\u0647 \u0645\u063a\u0646\u0627\u0637\u06cc\u0633\u06cc", "m2": "\u06a9\u0647 \u06a9\u0634\u062f \u062c\u0630\u0628\u0647\u200c\u0627\u0634 \u0627\u0632 \u06a9\u0627\u0645 \u0648 \u0632\u0628\u0627\u0646 \u062d\u0631\u0641 \u0633\u0627\u0644"}, "19": {"m1": "\u0647\u06cc\u0686 \u062d\u0631\u0641 \u0637\u0645\u0639 \u0627\u0632 \u062f\u0644 \u0628\u0647 \u0633\u0648\u06cc \u0644\u0628 \u0646\u0634\u062a\u0627\u0641", "m2": "\u06a9\u0634 \u0633\u062f \u0622\u0631\u06cc \u0648 \u0628\u0644\u06cc \u0627\u0632 \u062a\u0648 \u0646\u06a9\u0631\u062f \u0627\u0633\u062a\u0642\u0628\u0627\u0644"}, "20": {"m1": "\u062f\u0627\u0648\u0631\u0627 \u0627\u0632 \u0645\u062f\u062f \u0641\u06cc\u0636 \u0648 \u062b\u0646\u0627\u06cc \u062a\u0648 \u0645\u0631\u0627", "m2": "\u062e\u0627\u0637\u0631\u06cc \u0647\u0633\u062a \u0686\u0648 \u0628\u062d\u0631\u06cc \u0632 \u06af\u0647\u0631 \u0645\u0627\u0644\u0627\u0645\u0627\u0644"}, "21": {"m1": "\u0646\u0631\u0633\u062f \u062c\u0632 \u062a\u0648 \u0628\u0647 \u06a9\u0633 \u06af\u0648\u0647\u0631\u06cc \u0627\u0632 \u062e\u0627\u0637\u0631 \u0645\u0646", "m2": "\u06a9\u0631\u062f\u0647\u200c\u0627\u0645 \u0648\u0642\u0641 \u062a\u0648 \u0627\u06cc\u0646 \u0628\u062d\u0631 \u0644\u0628\u0627\u0644\u0628 \u0632 \u0632\u0644\u0627\u0644"}, "22": {"m1": "\u0645\u0639\u062f\u0646 \u0637\u0628\u0639 \u0645\u0631\u0627 \u06a9\u0631\u062f \u067e\u0631 \u0627\u0632 \u062c\u0648\u0647\u0631 \u062e\u0627\u0635", "m2": "\u067e\u0631\u062a\u0648 \u062a\u0631\u0628\u06cc\u062a \u0639\u0627\u0645 \u062a\u0648 \u062e\u0648\u0631\u0634\u06cc\u062f \u0645\u062b\u0627\u0644"}, "23": {"m1": "\u0627\u06cc\u0646 \u062c\u0648\u0627\u0647\u0631 \u0646\u0647 \u0645\u062a\u0627\u0639\u06cc\u0633\u062a \u06a9\u0647 \u0647\u0631 \u062c\u0627 \u06cc\u0627\u0628\u0646\u062f", "m2": "\u0647\u0645\u0647 \u062f\u0627\u0646\u0646\u062f \u06a9\u0647 \u0646\u0627\u062f\u0631 \u0628\u0648\u062f \u0627\u06cc\u0646 \u0637\u0631\u0632 \u0645\u0642\u0627\u0644"}, "24": {"m1": "\u0633\u062e\u0646 \u0645\u0646 \u0646\u0647 \u0632 \u062c\u0646\u0633 \u0633\u062e\u0646 \u0645\u062f\u0639\u06cc \u0627\u0633\u062a", "m2": "\u06a9\u0647 \u0628\u0648\u062f \u0628\u0631 \u0633\u0631 \u06a9\u0648 \u0633\u062f \u0633\u062f \u0627\u0632\u06cc\u0646 \u0633\u0646\u06af \u0648 \u0633\u0641\u0627\u0644"}, "25": {"m1": "\u0648\u062d\u0634\u06cc \u0627\u06cc\u0646\u062c\u0627 \u0686\u0648 \u0631\u0633\u06cc\u062f\u06cc \u0628\u0647 \u0647\u0645\u06cc\u0646 \u0642\u0637\u0639 \u0646\u0645\u0627\u06cc", "m2": "\u06a9\u0647 \u0686\u0648 \u0645\u0645\u062f\u0648\u062d \u062a\u0648 \u062a\u0645\u06cc\u06cc\u0632 \u06a9\u0646\u062f \u0646\u0642\u0635 \u0648 \u06a9\u0645\u0627\u0644"}, "26": {"m1": "\u062a\u0627 \u0645\u0642\u0631\u0631 \u0628\u0648\u062f \u0627\u06cc\u0646 \u0648\u0636\u0639 \u0628\u0647 \u062a\u0627\u0631\u06cc\u062e \u0639\u0631\u0628", "m2": "\u06a9\u0647 \u0628\u0648\u062f \u0639\u06cc\u062f \u0635\u06cc\u0627\u0645 \u0627\u0648\u0644 \u0645\u0627\u0647 \u0634\u0648\u0627\u0644"}, "27": {"m1": "\u0628\u0631 \u062a\u0648 \u0627\u06cc \u0642\u0628\u0644\u0647\u0654 \u0627\u062d\u0631\u0627\u0631 \u0639\u0631\u0628 \u062a\u0627 \u0628\u0647 \u0639\u062c\u0645", "m2": "\u0639\u06cc\u062f \u0628\u0627\u0634\u062f \u0647\u0645\u0647 \u0631\u0648\u0632 \u0648 \u0647\u0645\u0647 \u0645\u0627\u0647 \u0648 \u0647\u0645\u0647 \u0633\u0627\u0644"}}}