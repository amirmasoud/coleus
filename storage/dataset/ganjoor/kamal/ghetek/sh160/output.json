{"text": {"0": {"m1": "\u0645\u0631\u0627 \u0686\u0647 \u062d\u0627\u0635\u0644 \u0627\u0632\u06cc\u0646 \u062e\u0648\u0627\u062c\u06af\u0627\u0646 \u0628\u06cc \u062d\u0627\u0635\u0644", "m2": "\u06a9\u0647 \u0647\u06cc\u0686 \u06a9\u0627\u0631 \u0645\u0631\u0627 \u0627\u0646\u062a\u0638\u0627\u0645  \u0645\u06cc \u0646\u062f\u0647\u0646\u062f"}, "1": {"m1": "\u0646\u0647 \u0627\u0632 \u062f\u06cc\u0627\u0646\u062a \u0648 \u062a\u0642\u0648\u06cc \u0634\u0631\u0627\u0628 \u0645\u06cc \u0646\u062e\u0648\u0631\u0646\u062f", "m2": "\u06a9\u0647 \u06cc\u06a9\u062f\u06af\u0631 \u0631\u0627 \u0627\u0632 \u0628\u062e\u0644 \u062c\u0627\u0645 \u0645\u06cc \u0646\u062f\u0647\u0646\u062f"}, "2": {"m1": "\u0646\u062f\u0627\u0646\u0645 \u0627\u0632 \u06a9\u0631\u0645 \u0622\u062e\u0631 \u0686\u0647 \u062f\u0631 \u0648\u062c\u0648\u062f \u0622\u0645\u062f", "m2": "\u06a9\u0647 \u0647\u06cc\u0686\u06af\u0648\u0646\u0647 \u0628\u0647 \u062f\u0633\u062a\u0634 \u0632\u0645\u0627\u0645 \u0645\u06cc \u0646\u062f\u0647\u0646\u062f"}, "3": {"m1": "\u062c\u0648\u0627\u0628 \u0642\u0635\u0651\u0629 \u0627\u0631\u0628\u0627\u0628 \u062d\u0627\u062c\u062a \u0627\u0632 \u0627\u0645\u0633\u0627\u06a9", "m2": "\u0628\u062c\u0632 \u0628\u0648\u0627\u0633\u0637\u0629 \u062f\u0647 \u067e\u06cc\u0627\u0645 \u0645\u06cc \u0646\u062f\u0647\u0646\u062f"}, "4": {"m1": "\u0634\u06af\u0641\u062a \u0646\u06cc\u0633\u062a \u06a9\u0647 \u0646\u062f\u0647\u0646\u062f \u062a\u06cc\u0632 \u062f\u0631 \u0642\u0648\u0644\u0646\u062c", "m2": "\u06a9\u0647 \u0639\u0637\u0633\u0647 \u0646\u06cc\u0632 \u0628\u0647 \u0648\u0642\u062a \u0632\u06a9\u0627\u0645 \u0645\u06cc \u0646\u062f\u0647\u0646\u062f"}, "5": {"m1": "\u0686\u0648 \u062d\u0646\u0638\u0644\u0633\u062a \u062f\u0631\u0648\u0646\u0634\u0627\u0646 \u0628\u0647 \u0634\u062e\u0645 \u0622\u06a9\u0646\u062f\u0647", "m2": "\u0648 \u0644\u06cc\u06a9 \u0647\u06cc\u0686 \u062f\u0633\u0648\u0645\u062a \u0628\u0647 \u06a9\u0627\u0645 \u0645\u06cc \u0646\u062f\u0647\u0646\u062f"}, "6": {"m1": "\u0628\u0647\u0627\u06cc \u0634\u0639\u0631\u060c \u0627\u06af\u0631 \u0646\u06cc\u0633\u062a \u062c\u0632 \u06a9\u0647 \u0633\u062d\u0631 \u062d\u0644\u0627\u0644", "m2": "\u0632 \u0645\u0627\u0644 \u062e\u0648\u06cc\u0634 \u067e\u0634\u06cc\u0632\u06cc \u062d\u0631\u0627\u0645 \u0645\u06cc \u0646\u062f\u0647\u0646\u062f"}, "7": {"m1": "\u0632 \u0646\u0646\u06af \u0627\u06af\u0631 \u0646\u0628\u0631\u0645 \u0646\u0627\u0645\u0634\u0627\u0646 \u0633\u0632\u062f \u06a9\u06cc\u0646 \u0642\u0648\u0645", "m2": "\u0632 \u0628\u062e\u0644 \u0647\u0631 \u0686\u0647 \u062a\u0648\u0627\u0646 \u0628\u0631\u062f \u0646\u0627\u0645 \u0645\u06cc \u0646\u062f\u0647\u0646\u062f"}, "8": {"m1": "\u062f\u0631\u0648\u063a \u06af\u0641\u062a\u0645 \u0648 \u0627\u0646\u0635\u0627\u06cc \u0631\u0627\u0633\u062a \u0628\u0627\u06cc\u062f \u06af\u0641\u062a", "m2": "\u06a9\u0647 \u0647\u06cc\u0686 \u0645\u06cc \u0646\u062f\u0647\u0646\u062f\u0645\u060c \u0686\u0631\u0627\u0645 \u0645\u06cc \u0646\u062f\u0647\u0646\u062f\u061f"}, "9": {"m1": "\u0686\u0647 \u0686\u0634\u0645 \u062f\u0627\u0631\u0645 \u0627\u0632\u06cc\u0646  \u0645\u0646\u0639\u0645\u0627\u0646 \u06a9\u0647 \u0634\u0627\u0639\u0631 \u0631\u0627", "m2": "\u0628\u0647 \u0635\u062f \u0634\u0641\u06cc\u0639 \u062c\u0648\u0627\u0628 \u0633\u0644\u0627\u0645 \u0645\u06cc \u0646\u062f\u0647\u0646\u062f"}, "10": {"m1": "\u06a9\u062c\u0627 \u0631\u0648\u0645\u061f \u0686\u0647 \u06a9\u0646\u0645 \u0645\u0646\u061f \u0632 \u0628\u0627\u062f \u0634\u0627\u06cc\u062f \u0632\u06cc\u0633\u062a\u061f", "m2": "\u06a9\u0647 \u0642\u0648\u062a \u0631\u0648\u0632 \u0628\u0631\u0648\u0632\u0645 \u062a\u0645\u0627\u0645 \u0645\u06cc \u0646\u062f\u0647\u0646\u062f"}, "11": {"m1": "\u0632 \u06a9\u0648\u0629 \u0645\u06cc \u0646\u062f\u0647\u0646\u062f \u0648 \u06a9\u0631\u0645 \u0646\u0645\u06cc \u0648\u0631\u0632\u0646\u062f", "m2": "\u06a9\u062a\u0627\u0628 \u0645\u06cc \u0646\u062e\u0631\u0646\u062f \u0648 \u0627\u0648\u0627\u0645 \u0645\u06cc \u0646\u062f\u0647\u0646\u062f"}, "12": {"m1": "\u067e\u0646\u0627\u0647 \u0633\u0648\u06cc \u0642\u0646\u0627\u0639\u062a \u0647\u0645\u06cc \u0628\u0631\u0645 \u0632\u06cc\u0646 \u0642\u0648\u0645", "m2": "\u06a9\u0647 \u0627\u0647\u0644 \u062e\u0627\u0646\u0629 \u062e\u0648\u062f \u0631\u0627 \u0627\u0637\u0639\u0627\u0645 \u0645\u06cc \u0646\u062f\u0647\u0646\u062f"}, "13": {"m1": "\u062f\u0644\u0627 \u0628\u062d\u06a9\u0645 \u0636\u0631\u0648\u0631\u062a \u0628\u0633\u0627\u0632 \u0628\u0627 \u0627\u06cc\u0646\u0647\u0627", "m2": "\u0686\u0648 \u0647\u06cc\u0686 \u062c\u0627\u06cc \u0646\u0634\u0627\u0646 \u06a9\u0631\u0627\u0645 \u0645\u06cc \u0646\u062f\u0647\u0646\u062f"}}}