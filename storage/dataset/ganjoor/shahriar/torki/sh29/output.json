{"text": {"0": {"m1": "\u062e\u0627\u0646 \u0646\u0646\u0647 \u060c \u0647\u0627\u06cc\u0627\u0646\u062f\u0627 \u0642\u0627\u0644\u062f\u06cc\u0646", "m2": "\u0628\u0626\u0644\u0647 \u0628\u0627\u0634\u06cc\u0648\u0627 \u062f\u0648\u0644\u0627\u0646\u06cc\u0645"}, "1": {"m1": "\u0646\u0626\u062c\u0647 \u0645\u0646 \u0633\u0646\u06cc \u0627\u06cc\u062a\u06cc\u0631\u062f\u06cc\u0645 !", "m2": "\u062f\u0627 \u0633\u0646\u06cc\u0646 \u062a\u0627\u06cc\u06cc\u0646 \u062a\u0627\u067e\u06cc\u0644\u0645\u0627\u0632"}, "2": {"m1": "\u0633\u0646 \u0627\u0624\u0644\u0646 \u06af\u0648\u0646 \u060c \u0639\u0645\u0647 \u06af\u0644\u062f\u06cc", "m2": "\u0645\u0646\u06cc \u06af\u0647 \u062a\u062f\u06cc \u0622\u06cc\u0631\u06cc \u06a9\u0646\u062f\u0647"}, "3": {"m1": "\u0645\u0646 \u0627\u0648\u0634\u0627\u0642 \u060c \u0646\u0647 \u0622\u0646\u0644\u0627\u06cc\u0627\u06cc\u062f\u06cc\u0645 \u061f", "m2": "\u0628\u0627\u0634\u06cc\u0645\u06cc \u0642\u0627\u062a\u06cc\u0628 \u0627\u0648\u0634\u0627\u0642\u0644\u0627\u0631"}, "4": {"m1": "\u0646\u0626\u0686\u0647 \u06af\u0648\u0646 \u0645\u0646 \u0627\u0648\u0631\u062f\u0627 \u0642\u0627\u0644\u062f\u06cc\u0645", "m2": "\u0642\u0627\u06cc\u06cc\u062f\u06cc\u0628 \u06af\u0644\u0646\u062f\u0647 \u060c \u0628\u0627\u062e\u062f\u06cc\u0645"}, "5": {"m1": "\u06cc\u0626\u0631\u06cc\u0648\u06cc \u06cc\u06cc\u063a\u06cc\u0634\u062f\u06cc\u0631\u06cc\u0628\u0644\u0627\u0631", "m2": "\u0646\u0647 \u0627\u0624\u0632\u0648\u0646 \u060c \u0648 \u0646\u0647 \u06cc\u0626\u0631\u06cc\u0646 \u0648\u0627\u0631"}, "6": {"m1": "\u00ab \u0647\u0627\u0646\u06cc \u062e\u0627\u0646 \u0646\u0646\u0647 \u0645 \u061f \u00bb \u0633\u0648\u0631\u0648\u0634\u062f\u0648\u0645", "m2": "\u062f\u0626\u062f\u06cc\u0644\u0631 \u06a9\u06cc : \u062e\u0627\u0646 \u0646\u0646\u0647 \u0646\u06cc"}, "7": {"m1": "\u0622\u067e\u0627\u0631\u06cc\u0628\u0644\u0627 \u06a9\u0631\u0628\u0644\u0627\u06cc\u0647", "m2": "\u06a9\u06cc \u0634\u0641\u0627\u0633\u06cc\u0646 \u0627\u0648\u0631\u062f\u0627\u0646 \u0622\u0644\u0633\u06cc\u0646"}, "8": {"m1": "\u0633\u0641\u0631\u06cc \u0627\u0648\u0632\u0648\u0646 \u0633\u0641\u0631\u062f\u06cc\u0631", "m2": "\u0628\u06cc\u0631\u0627\u06cc\u06a9\u06cc \u0627\u06cc\u0644 \u0686\u06a9\u0631 \u06af\u0644\u06cc\u0646\u062c\u0647"}, "9": {"m1": "\u0646\u0626\u062c\u0647 \u0622\u063a\u0644\u0627\u0631\u0627\u0645 \u06cc\u0627\u0646\u06cc\u062e\u0644\u06cc", "m2": "\u0646\u0626\u0686\u0647 \u06af\u0648\u0646 \u0627\u0626\u0644\u0647 \u0686\u06cc\u063a\u06cc\u0631\u062f\u06cc\u0645"}, "10": {"m1": "\u06a9\u06cc \u0633\u0647 \u0633\u06cc\u0645 \u060c \u0633\u06cc\u0646\u0645 \u062a\u0648\u062a\u0648\u0644\u062f\u0648", "m2": "\u0627\u0648 \u060c \u0645\u0646 \u0627\u0648\u0644\u0645\u0627\u0633\u0627\u0645 \u06cc\u0627\u0646\u06cc\u0646\u062f\u0627"}, "11": {"m1": "\u0627\u0624\u0632\u06cc \u0647\u0626\u0686 \u06cc\u0626\u0631\u0647 \u06af\u0626\u062f\u0647 \u0646\u0645\u0647 \u0632", "m2": "\u0628\u0648 \u0633\u0641\u0631 \u0646\u0648\u0644\u0648\u0628\u062f\u0648 \u060c \u0645\u0646 \u0633\u06cc\u0632"}, "12": {"m1": "\u0627\u0624\u0632\u0648 \u062a\u06a9 \u0642\u0648\u06cc\u0648\u0628 \u06af\u0626\u062f\u06cc\u0628\u062f\u06cc\u0631 \u061f", "m2": "\u0647\u0627\u0645\u06cc\u062f\u0627\u0646 \u0622\u062c\u06cc\u0642 \u0627\u0626\u062f\u0647 \u0631 \u06a9\u0646"}, "13": {"m1": "\u0647\u0627\u0645\u06cc\u06cc\u0627 \u0622\u062c\u06cc\u0642\u0644\u06cc \u0628\u0627\u062e\u062f\u06cc\u0645", "m2": "\u0633\u0648\u0646\u0631\u0627 \u0628\u0627\u0634\u0644\u0627\u062f\u06cc\u0645 \u06a9\u06cc : \u0645\u0646\u062f\u0647"}, "14": {"m1": "\u06af\u0626\u062f\u06cc\u0631\u0647 \u0645 \u0627\u0648\u0646\u0648\u0646 \u062f\u0627\u0644\u06cc\u0646\u062c\u0627", "m2": "\u062f\u0626\u062f\u06cc\u0644\u0631 : \u0633\u0646\u06cc\u0646 \u06a9\u06cc \u062a\u0626\u0632\u062f\u06cc\u0631"}, "15": {"m1": "\u0627\u0645\u0627\u0645\u06cc\u0646 \u0645\u0632\u0627\u0631\u06cc \u0627\u0648\u0633\u062a\u0647", "m2": "\u0627\u0648\u0634\u0627\u063a\u06cc \u0622\u067e\u0627\u0631\u0645\u0627\u0642 \u0627\u0648\u0644\u0645\u0627\u0632"}, "16": {"m1": "\u0633\u0646 \u0627\u0648\u062e\u06cc \u060c \u0642\u0631\u0622\u0646\u06cc \u062a\u0626\u0632 \u0686\u06cc\u062e", "m2": "\u0633\u0646 \u0627\u0648\u0646\u06cc \u0686\u06cc\u062e\u06cc\u0646\u062c\u0627 \u0628\u0644\u06a9\u06cc"}, "17": {"t2": "\u06af\u0644\u0647 \u062e\u0627\u0646 \u0646\u0646\u0647 \u0633\u0641\u0631\u062f\u0646", "t1": "\u06af\u0644\u0647 \u062e\u0627\u0646 \u0646\u0646\u0647 \u0633\u0641\u0631\u062f\u0646"}, "18": {"m1": "\u062a\u0647 \u0644\u0647 \u0633\u06cc\u06a9 \u0631\u0648\u0627\u0646\u0644\u0627\u0645\u0627\u0642\u062f\u0627", "m2": "\u0627\u0648\u062e\u0648\u06cc\u0648\u0628 \u0642\u0631\u0622\u0646\u06cc \u0686\u06cc\u062e\u062f\u06cc\u0645"}, "19": {"m1": "\u06a9\u06cc \u06cc\u0627\u0632\u06cc\u0645 \u0633\u0646\u0647 : \u06af\u0644 \u0627\u06cc\u0646\u062f\u06cc", "m2": "\u062f\u0627\u0647\u0627 \u0686\u06cc\u062e\u0645\u06cc\u0634\u0627\u0645 \u0642\u0631\u0622\u0646\u06cc"}, "20": {"m1": "\u0645\u0646\u0647 \u0633\u0648\u0642\u062a \u0622\u0644 \u06af\u0644\u0646\u062f\u0647", "m2": "\u0622\u0645\u0627 \u0647\u0631 \u06a9\u0627\u063a\u0627\u0630 \u06cc\u0627\u0632\u0627\u0646\u062f\u0627"}, "21": {"m1": "\u0622\u0642\u0627\u0645\u06cc\u0646 \u06af\u0624\u0632\u0648 \u062f\u0648\u0644\u0627\u0631\u062f\u06cc", "m2": "\u0633\u0646\u062f\u0647 \u06a9\u06cc \u06af\u0644\u06cc\u0628 \u0686\u06cc\u062e\u0645\u0627\u062f\u06cc\u0646"}, "22": {"m1": "\u0646\u0626\u0686\u0647 \u0627\u06cc\u0644 \u0628\u0648 \u0627\u06cc\u0646\u062a\u0638\u0627\u0631\u0644\u0627", "m2": "\u06af\u0648\u0646\u06cc \u060c \u0647\u0641\u062a\u0647 \u0646\u06cc \u0633\u0627\u06cc\u0627\u0631\u062f\u06cc\u0645"}, "23": {"m1": "\u062a\u0627 \u06cc\u0627\u0648\u0627\u0634 \u2013 \u06cc\u0627\u0648\u0627\u0634 \u06af\u0624\u0632 \u0622\u0686\u062f\u06cc\u0645", "m2": "\u0622\u0646\u0644\u0627\u062f\u06cc\u0645 \u06a9\u06cc \u060c \u0633\u0646 \u0627\u0624\u0644\u0648\u0628\u0633\u0646 !"}, "24": {"m1": "\u0628\u06cc\u0644\u0647 \u0628\u06cc\u0644\u0645\u06cc\u06cc\u0647 \u0647\u0646\u0648\u0632\u062f\u0627", "m2": "\u0627\u0648\u0631\u0647 \u06af\u06cc\u0645\u062f\u0647 \u0628\u06cc\u0631 \u0627\u06cc\u062a\u06cc\u06a9 \u0648\u0627\u0631"}, "25": {"m1": "\u06af\u0624\u0632\u0648\u0645 \u0622\u062e\u062a\u0627\u0631\u0627\u0631 \u0647\u0645\u06cc\u0634\u0647", "m2": "\u0646\u0647 \u06cc\u0627\u0645\u0627\u0646\u062f\u06cc \u0628\u0648 \u0627\u06cc\u062a\u06cc\u06a9\u0644\u0631"}, "26": {"m1": "\u062e\u0627\u0646 \u0646\u0646\u0647 \u062c\u0627\u0646\u06cc\u0645 \u060c \u0646\u0648\u0644\u06cc\u062f\u06cc", "m2": "\u0633\u0646\u06cc \u0628\u06cc\u0631\u062f\u0647 \u0645\u0646 \u062a\u0627\u067e\u0627\u06cc\u062f\u06cc\u0645"}, "27": {"m1": "\u0627\u0648 \u0622\u06cc\u0627\u0642\u0644\u0627\u0631 \u0627\u0648\u0633\u062a\u0647 \u060c \u0628\u06cc\u0631\u062f\u0647", "m2": "\u062f\u0624\u0634\u0647 \u0646\u06cc\u0628 \u0628\u06cc\u0631 \u0622\u063a\u0644\u0627\u06cc\u0627\u06cc\u062f\u06cc\u0645"}, "28": {"m1": "\u06a9\u06cc \u062f\u0627\u0647\u0627 \u06af\u0626\u062f\u0647 \u0646\u0645\u06cc\u06cc\u0647 \u06cc\u062f\u06cc\u0646", "m2": "\u06af\u0626\u062c\u0647 \u0644\u0631 \u06cc\u0627\u062a\u0627\u0646\u062f\u0627 \u060c \u0633\u0646 \u062f\u0647"}, "29": {"m1": "\u0645\u0646\u06cc \u0642\u0648\u06cc\u0646\u0648\u0646\u0627 \u0622\u0644\u0627\u0631\u062f\u06cc\u0646", "m2": "\u0646\u0626\u062c\u0647 \u0628\u0627\u063a\u0631\u06cc\u0648\u0627 \u0628\u0627\u0633\u0627\u0631\u062f\u06cc\u0646"}, "30": {"m1": "\u0642\u0648\u0644\u0648\u0646 \u0627\u0648\u0633\u062a\u0647 \u06af\u0627\u0647 \u0633\u0627\u0644\u0627\u0631\u062f\u06cc\u0646", "m2": "\u0622\u062c\u06cc \u062f\u0648\u0646\u06cc\u0627\u0646\u06cc \u0622\u062a\u0627\u0631\u06a9\u0646"}, "31": {"m1": "\u0627\u06cc\u06a9\u06cc\u0645\u06cc\u0632 \u0634\u06cc\u0631\u06cc\u0646 \u06cc\u0627\u062a\u0627\u0631\u062f\u06cc\u0642", "m2": "\u06cc\u0648\u062e\u0648\u062f\u0627 ( \u0644\u0648\u0644\u06cc ) \u0622\u062a\u0627\u0631\u06a9\u0646"}, "32": {"m1": "\u0633\u0646\u06cc \u0645\u0646 \u0628\u0644\u0634\u062f\u06cc\u0631\u0647 \u0631\u062f\u06cc\u0645", "m2": "\u06af\u0626\u062c\u0647 \u0644\u06cc \u060c \u0633\u0648 \u0642\u06cc\u0632\u062f\u06cc\u0631\u0627\u0631\u062f\u06cc\u0646"}, "33": {"m1": "\u0627\u0624\u0632\u0648\u0648\u06cc \u062a\u0645\u06cc\u0632\u0644\u06cc\u0647 \u0631\u062f\u06cc\u0646", "m2": "\u06af\u0626\u0646\u0647 \u062f\u0647 \u0645\u0646\u06cc \u0627\u0624\u067e\u0647 \u0631\u062f\u06cc\u0646"}, "34": {"m1": "\u0647\u0626\u0686 \u0645\u0646\u0647 \u0622\u062c\u06cc\u0642\u0644\u0627\u0645\u0627\u0632\u062f\u06cc\u0646", "m2": "\u0633\u0627\u0648\u0627\u0634\u0627\u0646 \u0645\u0646\u0647 \u06a9\u06cc\u0645 \u0627\u0648\u0644\u0633\u0648\u0646"}, "35": {"m1": "\u0633\u0646 \u0645\u0646\u0647 \u0647\u0627\u0648\u0627\u0631 \u062f\u0648\u0631\u0627\u0631\u062f\u06cc\u0646", "m2": "\u0645\u0646\u06cc \u060c \u0633\u0646 \u0622\u0646\u0627\u0645 \u062f\u0624\u06cc\u0647 \u0646\u062f\u0647"}, "36": {"m1": "\u0642\u0627\u067e\u06cc\u0628 \u0622\u0631\u0627\u062f\u0627\u0646 \u0686\u06cc\u062e\u0627\u0631\u062f\u06cc\u0646", "m2": "\u0627\u0626\u0644\u0647 \u0627\u06cc\u0633\u062a\u06cc\u0644\u06cc\u06a9 \u0627\u0648 \u0627\u06cc\u0633\u062a\u0647 \u06a9"}, "37": {"m1": "\u062f\u0627\u0647\u0627 \u06a9\u06cc\u0645\u0633\u0647 \u062f\u0647 \u0627\u0648\u0644\u0648\u0631\u0645\u0648 \u061f", "m2": "\u0627\u0648\u0631\u0647 \u06af\u06cc\u0645 \u062f\u0626\u06cc\u06cc\u0631 \u06a9\u06cc : \u06cc\u0648\u062e \u2013 \u06cc\u0648\u062e"}, "38": {"m1": "\u0627\u0648 \u062f\u0647 \u0631\u06cc\u0646 \u0635\u0641\u0627\u0644\u06cc \u0627\u06cc\u0633\u062a\u0647 \u06a9", "m2": "\u0645\u0646\u06cc\u0645 \u0627\u0648 \u0639\u0632\u06cc\u0632\u0644\u06cc\u063a\u06cc\u0645 \u062a\u06a9"}, "39": {"m1": "\u0633\u0646\u06cc\u0644\u0647 \u06af\u0626\u062f\u06cc\u0628 \u060c \u062a\u0648\u06a9\u0646\u062f\u06cc", "m2": "\u062e\u0627\u0646 \u0646\u0646\u0647 \u0627\u0624\u0632\u0648\u0646 \u062f\u0626\u06cc\u06cc\u0631\u062f\u06cc\u0646"}, "40": {"m1": "\u06a9\u06cc : \u0633\u0646\u0647 \u0628\u0647\u0634\u062a \u062f\u0647 \u060c \u0627\u0644\u0644\u0647", "m2": "\u0648\u0626\u0631\u0647 \u062c\u0647 \u06a9 \u0646\u0647 \u0627\u06cc\u0633\u062a\u06cc\u0648\u0631 \u0633\u0646"}, "41": {"m1": "\u0628\u0648 \u0633\u0624\u0632\u0648\u0646 \u06cc\u0627\u062f\u06cc\u0646\u062f\u0627 \u0642\u0627\u0644\u0633\u06cc\u0646", "m2": "\u0645\u0646\u0647 \u0642\u0648\u0644\u06cc\u0646\u06cc \u0648\u0626\u0631\u06cc\u0628\u0633\u0647 \u0646"}, "42": {"m1": "\u0627\u0626\u0644\u0647 \u0628\u06cc\u0631 \u06af\u0648\u0646\u0648\u0645 \u0627\u0648\u0644\u0648\u0631\u0633\u0627", "m2": "\u0628\u06cc\u0644\u06cc\u0631\u0633\u0646 \u0646\u0647 \u0627\u06cc\u0633\u062a\u06cc\u0647 \u0631\u0645 \u0645\u0646 \u061f"}, "43": {"m1": "\u0633\u0624\u0632\u06cc\u0645\u0647 \u062f\u0631\u0633\u062a \u0642\u0648\u0644\u0627\u0642 \u0648\u0626\u0631 :", "m2": "\u0633\u0646 \u0627\u06cc\u0644\u0647 \u0646 \u0627\u0648\u0634\u0627\u062e\u0644\u06cc\u0642 \u0639\u0647\u062f\u06cc\u0646"}, "44": {"m1": "\u062e\u0627\u0646 \u0646\u0646\u0647 \u0622\u0645\u0627\u0646 \u060c \u0646\u0648\u0644\u06cc\u062f\u06cc", "m2": "\u0628\u06cc\u0631 \u0627\u0648\u0634\u0627\u062e\u0644\u06cc\u063a\u06cc \u062a\u0627\u067e\u0627\u06cc\u062f\u06cc\u0645"}, "45": {"m1": "\u0628\u06cc\u0631\u062f\u0647 \u0645\u0646 \u0633\u0646\u0647 \u0686\u0627\u062a\u0627\u06cc\u062f\u06cc\u0645", "m2": "\u0633\u0646\u06cc\u0644\u0646 \u0642\u0648\u062c\u0627\u0642\u0644\u0627\u0634\u0627\u06cc\u062f\u06cc\u0645"}, "46": {"m1": "\u0633\u0646\u06cc\u0644\u0646 \u0628\u06cc\u0631 \u0622\u063a\u0644\u0627\u0634\u0627\u06cc\u062f\u06cc\u0645", "m2": "\u06cc\u0626\u0646\u06cc\u062f\u0646 \u0627\u0648\u0634\u0627\u0642 \u0627\u0648\u0644\u0648\u0631\u06a9\u0646"}, "47": {"m1": "\u0642\u0648\u062c\u0627\u063a\u06cc\u0646\u062f\u0627 \u0628\u06cc\u0631 \u06cc\u0627\u062a\u0627\u06cc\u062f\u06cc\u0645", "m2": "\u0627\u0626\u0644\u0647 \u0628\u06cc\u0631 \u0628\u0647\u0634\u062a \u0627\u0648\u0644\u0648\u0631\u0633"}, "48": {"m1": "\u062f\u0627\u0647\u0627 \u0645\u0646 \u0627\u0624\u0632 \u0627\u0644\u0644\u0647 \u0647\u06cc\u0645\u062f\u0627\u0646", "m2": "\u0628\u0627\u0634\u0642\u0627 \u0628\u06cc\u0631 \u0634\u0626\u06cc \u0627\u06cc\u0633\u062a\u0647 \u0645\u0632\u062f\u06cc\u0645"}, "49": {"p": "\u06f1\u06f3\u06f5\u06f5"}}}