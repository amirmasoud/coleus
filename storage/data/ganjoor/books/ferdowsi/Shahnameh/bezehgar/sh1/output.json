{"text": {"0": {"m1": "\u0686\u0648 \u0634\u062f \u067e\u0627\u062f\u0634\u0627 \u0628\u0631 \u062c\u0647\u0627\u0646 \u06cc\u0632\u062f\u06af\u0631\u062f", "m2": "\u0633\u067e\u0647 \u0631\u0627 \u0632 \u062f\u0634\u062a \u0627\u0646\u062f\u0631\u0622\u0648\u0631\u062f \u06af\u0631\u062f"}, "1": {"m1": "\u06a9\u0644\u0627\u0647 \u0628\u0631\u0627\u062f\u0631 \u0628\u0647 \u0633\u0631 \u0628\u0631 \u0646\u0647\u0627\u062f", "m2": "\u0647\u0645\u06cc \u0628\u0648\u062f \u0627\u0632\u0627\u0646 \u0645\u0631\u06af \u0646\u0627\u0634\u0627\u062f \u0634\u0627\u062f"}, "2": {"m1": "\u0686\u0646\u06cc\u0646 \u06af\u0641\u062a \u0628\u0627 \u0646\u0627\u0645\u062f\u0627\u0631\u0627\u0646 \u0634\u0647\u0631", "m2": "\u06a9\u0647 \u0647\u0631\u06a9\u0633 \u06a9\u0647 \u0627\u0632 \u062f\u0627\u062f \u06cc\u0627\u0628\u0646\u062f \u0628\u0647\u0631"}, "3": {"m1": "\u0646\u062e\u0633\u062a \u0627\u0632 \u0646\u06cc\u0627\u06cc\u0634 \u0628\u0647 \u06cc\u0632\u062f\u0627\u0646 \u06a9\u0646\u06cc\u062f", "m2": "\u062f\u0644 \u0627\u0632 \u062f\u0627\u062f \u0645\u0627 \u0634\u0627\u062f \u0648 \u062e\u0646\u062f\u0627\u0646 \u06a9\u0646\u06cc\u062f"}, "4": {"m1": "\u0628\u062f\u0627\u0646 \u0631\u0627 \u0646\u0645\u0627\u0646\u0645 \u06a9\u0647 \u062f\u0627\u0631\u0646\u062f \u0647\u0648\u0634", "m2": "\u0648\u06af\u0631 \u062f\u0633\u062a \u06cc\u0627\u0632\u0646\u062f \u0628\u062f \u0631\u0627 \u0628\u06a9\u0648\u0634"}, "5": {"m1": "\u06a9\u0633\u06cc \u06a9\u0648 \u0628\u062c\u0648\u06cc\u062f \u0632 \u0645\u0627 \u0631\u0627\u0633\u062a\u06cc", "m2": "\u0628\u06cc\u0627\u0631\u0627\u0645\u062f \u0627\u0632 \u06a9\u0698\u06cc \u0648 \u06a9\u0627\u0633\u062a\u06cc"}, "6": {"m1": "\u0628\u0647 \u0647\u0631\u062c\u0627\u06cc \u062c\u0627\u0647 \u0648\u06cc \u0627\u0641\u0632\u0648\u0646 \u06a9\u0646\u06cc\u0645", "m2": "\u0632 \u062f\u0644 \u06a9\u06cc\u0646\u0647 \u0648 \u0622\u0632 \u0628\u06cc\u0631\u0648\u0646 \u06a9\u0646\u06cc\u0645"}, "7": {"m1": "\u0633\u06af\u0627\u0644\u0634 \u0646\u06af\u0648\u06cc\u06cc\u0645 \u062c\u0632 \u0628\u0627 \u0631\u062f\u0627\u0646", "m2": "\u062e\u0631\u062f\u0645\u0646\u062f \u0648 \u0628\u06cc\u062f\u0627\u0631\u062f\u0644 \u0645\u0648\u0628\u062f\u0627\u0646"}, "8": {"m1": "\u06a9\u0633\u06cc \u0631\u0627 \u06a9\u062c\u0627 \u067e\u0631 \u0632 \u0622\u0647\u0648 \u0628\u0648\u062f", "m2": "\u0631\u0648\u0627\u0646\u0634 \u0632 \u0628\u06cc\u0634\u06cc \u0628\u0647 \u0646\u06cc\u0631\u0648 \u0628\u0648\u062f"}, "9": {"m1": "\u0628\u0647 \u0628\u06cc\u0686\u0627\u0631\u06af\u0627\u0646 \u0628\u0631 \u0633\u062a\u0645 \u0633\u0627\u0632\u062f \u0627\u0648\u06cc", "m2": "\u06af\u0631 \u0627\u0632 \u0686\u06cc\u0632 \u062f\u0631\u0648\u06cc\u0634 \u0628\u0641\u0631\u0627\u0632\u062f \u0627\u0648\u06cc"}, "10": {"m1": "\u0628\u06a9\u0648\u0634\u06cc\u0645 \u0648 \u0646\u06cc\u0631\u0648\u0634 \u0628\u06cc\u0631\u0648\u0646 \u06a9\u0646\u06cc\u0645", "m2": "\u0628\u0647 \u062f\u0631\u0648\u06cc\u0634 \u0645\u0627 \u0646\u0627\u0632\u0634 \u0627\u0641\u0632\u0648\u0646 \u06a9\u0646\u06cc\u0645"}, "11": {"m1": "\u06a9\u0633\u06cc \u06a9\u0648 \u0628\u067e\u0631\u0647\u06cc\u0632\u062f \u0627\u0632 \u062e\u0634\u0645 \u0645\u0627", "m2": "\u0647\u0645\u06cc \u0628\u06af\u0630\u0631\u062f \u062a\u06cc\u0632 \u0628\u0631 \u0686\u0634\u0645 \u0645\u0627"}, "12": {"m1": "\u0647\u0645\u06cc \u0628\u0633\u062a\u0631 \u0627\u0632 \u062e\u0627\u06a9 \u062c\u0648\u06cc\u062f \u062a\u0646\u0634", "m2": "\u0647\u0645\u0627\u0646 \u062e\u0646\u062c\u0631 \u0647\u0646\u062f\u0648\u06cc \u06af\u0631\u062f\u0646\u0634"}, "13": {"m1": "\u0628\u0647 \u0641\u0631\u0645\u0627\u0646 \u0645\u0627 \u0686\u0634\u0645 \u0631\u0648\u0634\u0646 \u06a9\u0646\u06cc\u062f", "m2": "\u062e\u0631\u062f \u0631\u0627 \u0628\u0647 \u062a\u0646 \u0628\u0631 \u0686\u0648 \u062c\u0648\u0634\u0646 \u06a9\u0646\u06cc\u062f"}, "14": {"m1": "\u062a\u0646 \u0647\u0631\u06a9\u0633\u06cc \u06af\u0634\u062a \u0644\u0631\u0632\u0627\u0646 \u0686\u0648 \u0628\u06cc\u062f", "m2": "\u06a9\u0647 \u06af\u0648\u067e\u0627\u0644 \u0648 \u0634\u0645\u0634\u06cc\u0631\u0634\u0627\u0646 \u0628\u062f \u0627\u0645\u06cc\u062f"}, "15": {"m1": "\u0686\u0648 \u0634\u062f \u0628\u0631 \u062c\u0647\u0627\u0646 \u067e\u0627\u062f\u0634\u0627\u0647\u06cc\u0634 \u0631\u0627\u0633\u062a", "m2": "\u0628\u0632\u0631\u06af\u06cc \u0641\u0632\u0648\u0646 \u06a9\u0631\u062f \u0648 \u0645\u0647\u0631\u0634 \u0628\u06a9\u0627\u0633\u062a"}, "16": {"m1": "\u062e\u0631\u062f\u0645\u0646\u062f \u0646\u0632\u062f\u06cc\u06a9 \u0627\u0648 \u062e\u0648\u0627\u0631 \u06af\u0634\u062a", "m2": "\u0647\u0645\u0647 \u0631\u0633\u0645 \u0634\u0627\u0647\u06cc\u0634 \u0628\u06cc\u06a9\u0627\u0631 \u06af\u0634\u062a"}, "17": {"m1": "\u06a9\u0646\u0627\u0631\u0646\u06af \u0628\u0627 \u067e\u0647\u0644\u0648\u0627\u0646 \u0648 \u0631\u062f\u0627\u0646", "m2": "\u0647\u0645\u0627\u0646 \u062f\u0627\u0646\u0634\u06cc \u067e\u0631\u062e\u0631\u062f \u0645\u0648\u0628\u062f\u0627\u0646"}, "18": {"m1": "\u06cc\u06a9\u06cc \u06af\u0634\u062a \u0628\u0627 \u0628\u0627\u062f \u0646\u0632\u062f\u06cc\u06a9 \u0627\u0648\u06cc", "m2": "\u062c\u0641\u0627 \u067e\u06cc\u0634\u0647 \u0634\u062f \u062c\u0627\u0646 \u062a\u0627\u0631\u06cc\u06a9 \u0627\u0648\u06cc"}, "19": {"m1": "\u0633\u062a\u0631\u062f\u0647 \u0634\u062f \u0627\u0632 \u062c\u0627\u0646 \u0627\u0648 \u0645\u0647\u0631 \u0648 \u062f\u0627\u062f", "m2": "\u0628\u0647 \u0647\u06cc\u0686 \u0622\u0631\u0632\u0648 \u0646\u06cc\u0632 \u067e\u0627\u0633\u062e \u0646\u062f\u0627\u062f"}, "20": {"m1": "\u06a9\u0633\u06cc \u0631\u0627 \u0646\u0628\u062f \u0646\u0632\u062f \u0627\u0648 \u067e\u0627\u06cc\u06af\u0627\u0647", "m2": "\u0628\u0647 \u0698\u0631\u0641\u06cc \u0645\u06a9\u0627\u0641\u0627\u062a \u06a9\u0631\u062f\u06cc \u06af\u0646\u0627\u0647"}, "21": {"m1": "\u0647\u0631\u0627\u0646\u06a9\u0633 \u06a9\u0647 \u062f\u0633\u062a\u0648\u0631 \u0628\u062f \u0628\u0631 \u062f\u0631\u0634", "m2": "\u0641\u0632\u0627\u06cc\u0646\u062f\u0647\u0654 \u0627\u062e\u062a\u0631 \u0648 \u0627\u0641\u0633\u0631\u0634"}, "22": {"m1": "\u0647\u0645\u0647 \u0639\u0647\u062f \u06a9\u0631\u062f\u0646\u062f \u0628\u0627 \u06cc\u06a9\u062f\u06af\u0631", "m2": "\u06a9\u0647 \u0647\u0631\u06af\u0632 \u0646\u06af\u0648\u06cc\u0646\u062f \u0632\u0627\u0646 \u0628\u0648\u0645 \u0648 \u0628\u0631"}, "23": {"m1": "\u0647\u0645\u0647 \u06cc\u06a9\u0633\u0631 \u0627\u0632 \u0628\u06cc\u0645 \u067e\u06cc\u0686\u0627\u0646 \u0634\u062f\u0646\u062f", "m2": "\u0632 \u0647\u0648\u0644 \u0634\u0647\u0646\u0634\u0627\u0647 \u0628\u06cc\u062c\u0627\u0646 \u0634\u062f\u0646\u062f"}, "24": {"m1": "\u0641\u0631\u0633\u062a\u0627\u062f\u06af\u0627\u0646 \u0622\u0645\u062f\u0646\u062f\u06cc \u0632 \u0631\u0627\u0647", "m2": "\u0647\u0645\u0627\u0646 \u0632\u06cc\u0631\u062f\u0633\u062a\u0627\u0646 \u0641\u0631\u06cc\u0627\u062f\u062e\u0648\u0627\u0647"}, "25": {"m1": "\u0686\u0648 \u062f\u0633\u062a\u0648\u0631 \u0632\u0627\u0646 \u0622\u06af\u0647\u06cc \u06cc\u0627\u0641\u062a\u06cc", "m2": "\u0628\u062f\u0627\u0646 \u06a9\u0627\u0631\u0647\u0627 \u062a\u06cc\u0632 \u0628\u0634\u062a\u0627\u0641\u062a\u06cc"}, "26": {"m1": "\u0628\u0647 \u06af\u0641\u062a\u0627\u0631 \u06af\u0631\u0645 \u0648 \u0628\u0647 \u0622\u0648\u0627\u0632 \u0646\u0631\u0645", "m2": "\u0641\u0631\u0633\u062a\u0627\u062f\u0647 \u0631\u0627 \u0631\u0627\u0647 \u062f\u0627\u062f\u06cc \u0628\u0647 \u0634\u0631\u0645"}, "27": {"m1": "\u0628\u06af\u0641\u062a\u06cc \u06a9\u0647 \u0634\u0627\u0647 \u0627\u0632 \u062f\u0631 \u06a9\u0627\u0631 \u0646\u06cc\u0633\u062a", "m2": "\u0634\u0645\u0627 \u0631\u0627 \u0628\u062f\u0648 \u0631\u0627\u0647 \u062f\u06cc\u062f\u0627\u0631 \u0646\u06cc\u0633\u062a"}, "28": {"m1": "\u0646\u0645\u0648\u062f\u0645 \u0628\u062f\u0648 \u0647\u0631\u0686 \u062f\u0631\u062e\u0648\u0627\u0633\u062a\u06cc", "m2": "\u0628\u0647 \u0641\u0631\u0645\u0627\u0646\u0634 \u067e\u06cc\u062f\u0627 \u0634\u062f \u0622\u0646 \u0631\u0627\u0633\u062a\u06cc"}}}