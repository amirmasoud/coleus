{"text": {"0": {"m1": "\u0646\u0647 \u0628\u0631 \u062d\u06a9\u0645 \u0634\u0631\u0639 \u0622\u0628 \u062e\u0648\u0631\u062f\u0646 \u062e\u0637\u0627\u0633\u062a", "m2": "\u0648\u06af\u0631 \u062e\u0648\u0646 \u0628\u0647 \u0641\u062a\u0648\u06cc \u0628\u0631\u06cc\u0632\u06cc \u0631\u0648\u0627\u0633\u062a"}, "1": {"m1": "\u06a9\u0647 \u0631\u0627 \u0634\u0631\u0639 \u0641\u062a\u0648\u06cc \u062f\u0647\u062f \u0628\u0631 \u0647\u0644\u0627\u06a9", "m2": "\u0627\u0644\u0627 \u062a\u0627 \u0646\u062f\u0627\u0631\u06cc \u0632 \u06a9\u0634\u062a\u0646\u0634 \u0628\u0627\u06a9"}, "2": {"m1": "\u0648\u06af\u0631 \u062f\u0627\u0646\u06cc \u0627\u0646\u062f\u0631 \u062a\u0628\u0627\u0631\u0634 \u06a9\u0633\u0627\u0646", "m2": "\u0628\u0631 \u0627\u06cc\u0634\u0627\u0646 \u0628\u0628\u062e\u0634\u0627\u06cc \u0648 \u0631\u0627\u062d\u062a \u0631\u0633\u0627\u0646"}, "3": {"m1": "\u06af\u0646\u0647 \u0628\u0648\u062f \u0645\u0631\u062f \u0633\u062a\u0645\u06a9\u0627\u0631\u0647 \u0631\u0627", "m2": "\u0686\u0647 \u062a\u0627\u0648\u0627\u0646 \u0632\u0646 \u0648 \u0637\u0641\u0644 \u0628\u06cc\u0686\u0627\u0631\u0647 \u0631\u0627\u061f"}, "4": {"m1": "\u062a\u0646\u062a \u0632\u0648\u0631\u0645\u0646\u062f \u0627\u0633\u062a \u0648 \u0644\u0634\u06a9\u0631 \u06af\u0631\u0627\u0646", "m2": "\u0648\u0644\u06cc\u06a9\u0646 \u062f\u0631 \u0627\u0642\u0644\u06cc\u0645 \u062f\u0634\u0645\u0646 \u0645\u0631\u0627\u0646"}, "5": {"m1": "\u06a9\u0647 \u0648\u06cc \u0628\u0631 \u062d\u0635\u0627\u0631\u06cc \u06af\u0631\u06cc\u0632\u062f \u0628\u0644\u0646\u062f", "m2": "\u0631\u0633\u062f \u06a9\u0634\u0648\u0631\u06cc \u0628\u06cc \u06af\u0646\u0647 \u0631\u0627 \u06af\u0632\u0646\u062f"}, "6": {"m1": "\u0646\u0638\u0631 \u06a9\u0646 \u062f\u0631 \u0627\u062d\u0648\u0627\u0644 \u0632\u0646\u062f\u0627\u0646\u06cc\u0627\u0646", "m2": "\u06a9\u0647 \u0645\u0645\u06a9\u0646 \u0628\u0648\u062f \u0628\u06cc\u200c\u06af\u0646\u0647 \u062f\u0631 \u0645\u06cc\u0627\u0646"}, "7": {"m1": "\u0686\u0648 \u0628\u0627\u0632\u0627\u0631\u06af\u0627\u0646 \u062f\u0631 \u062f\u06cc\u0627\u0631\u062a \u0628\u0645\u0631\u062f", "m2": "\u0628\u0647 \u0645\u0627\u0644\u0634 \u062e\u0633\u0627\u0633\u062a \u0628\u0648\u062f \u062f\u0633\u062a\u0628\u0631\u062f"}, "8": {"m1": "\u06a9\u0632 \u0622\u0646 \u067e\u0633 \u06a9\u0647 \u0628\u0631 \u0648\u06cc \u0628\u06af\u0631\u06cc\u0646\u062f \u0632\u0627\u0631", "m2": "\u0628\u0647 \u0647\u0645 \u0628\u0627\u0632 \u06af\u0648\u06cc\u0646\u062f \u062e\u0648\u06cc\u0634 \u0648 \u062a\u0628\u0627\u0631"}, "9": {"m1": "\u06a9\u0647 \u0645\u0633\u06a9\u06cc\u0646 \u062f\u0631 \u0627\u0642\u0644\u06cc\u0645 \u063a\u0631\u0628\u062a \u0628\u0645\u0631\u062f", "m2": "\u0645\u062a\u0627\u0639\u06cc \u06a9\u0632 \u0627\u0648 \u0645\u0627\u0646\u062f \u0638\u0627\u0644\u0645 \u0628\u0628\u0631\u062f"}, "10": {"m1": "\u0628\u06cc\u0646\u062f\u06cc\u0634 \u0627\u0632 \u0622\u0646 \u0637\u0641\u0644\u06a9 \u0628\u06cc \u067e\u062f\u0631", "m2": "\u0648\u0632 \u0622\u0647 \u062f\u0644 \u062f\u0631\u062f\u0645\u0646\u062f\u0634 \u062d\u0630\u0631"}, "11": {"m1": "\u0628\u0633\u0627 \u0646\u0627\u0645 \u0646\u06cc\u06a9\u0648\u06cc \u067e\u0646\u062c\u0627\u0647 \u0633\u0627\u0644", "m2": "\u06a9\u0647 \u06cc\u06a9 \u0646\u0627\u0645 \u0632\u0634\u062a\u0634 \u06a9\u0646\u062f \u067e\u0627\u06cc\u0645\u0627\u0644"}, "12": {"m1": "\u067e\u0633\u0646\u062f\u06cc\u062f\u0647 \u06a9\u0627\u0631\u0627\u0646 \u062c\u0627\u0648\u06cc\u062f \u0646\u0627\u0645", "m2": "\u062a\u0637\u0627\u0648\u0644 \u0646\u06a9\u0631\u062f\u0646\u062f \u0628\u0631 \u0645\u0627\u0644 \u0639\u0627\u0645"}, "13": {"m1": "\u0628\u0631 \u0622\u0641\u0627\u0642 \u0627\u06af\u0631 \u0633\u0631 \u0628\u0647 \u0633\u0631 \u067e\u0627\u062f\u0634\u0627\u0633\u062a", "m2": "\u0686\u0648 \u0645\u0627\u0644 \u0627\u0632 \u062a\u0648\u0627\u0646\u06af\u0631 \u0633\u062a\u0627\u0646\u062f \u06af\u062f\u0627\u0633\u062a"}, "14": {"m1": "\u0628\u0645\u0631\u062f \u0627\u0632 \u062a\u0647\u06cc\u062f\u0633\u062a\u06cc \u0622\u0632\u0627\u062f \u0645\u0631\u062f", "m2": "\u0632 \u067e\u0647\u0644\u0648\u06cc \u0645\u0633\u06a9\u06cc\u0646 \u0634\u06a9\u0645 \u067e\u0631 \u0646\u06a9\u0631\u062f"}}}