{"text": {"0": {"m1": "\u0628\u0646\u0627\u0632 \u0627\u06cc \u062e\u062f\u0627\u0648\u0646\u062f \u0627\u0642\u0628\u0627\u0644 \u0633\u0631\u0645\u062f", "m2": "\u0628\u0647 \u0628\u062e\u062a \u0647\u0645\u0627\u06cc\u0648\u0646 \u0648 \u062a\u062e\u062a \u0645\u0645\u0647\u062f"}, "1": {"m1": "\u0645\u063a\u06cc\u062b \u0632\u0645\u0627\u0646 \u0646\u0627\u0635\u0631 \u0627\u0647\u0644 \u0627\u06cc\u0645\u0627\u0646", "m2": "\u06af\u0632\u06cc\u0646 \u0627\u062d\u062f \u06cc\u0627\u0648\u0631 \u062f\u06cc\u0646 \u0627\u062d\u0645\u062f"}, "2": {"m1": "\u062e\u062f\u0627\u0648\u0646\u062f \u0641\u0631\u0645\u0627\u0646 \u0645\u0644\u06a9 \u0633\u0644\u06cc\u0645\u0627\u0646", "m2": "\u0634\u0647\u0646\u0634\u0627\u0647 \u0639\u0627\u062f\u0644 \u0627\u062a\u0627\u0628\u06a9 \u0645\u062d\u0645\u062f"}, "3": {"m1": "\u0632 \u0633\u0639\u062f \u0627\u0628\u0648\u0628\u06a9\u0631 \u062a\u0627 \u0633\u0639\u062f \u0632\u0646\u06af\u06cc", "m2": "\u067e\u062f\u0631 \u0628\u0631 \u067e\u062f\u0631 \u0646\u0627\u0645\u0648\u0631 \u062c\u062f \u0628\u0631 \u062c\u062f"}, "4": {"m1": "\u0633\u0631 \u0628\u0646\u062f\u06af\u06cc \u0628\u0631 \u0632\u0645\u06cc\u0646\u0634 \u0646\u0647\u0627\u062f\u0647", "m2": "\u062e\u062f\u0627\u0648\u0646\u062f\u06af\u0627\u0631\u0627\u0646 \u062f\u0631\u06cc\u0627 \u0648 \u0633\u0631\u062d\u062f"}, "5": {"m1": "\u0647\u0645\u0647 \u0646\u0627\u0645\u062f\u0627\u0631\u0627\u0646 \u0648 \u06af\u0631\u062f\u0646 \u0641\u0631\u0627\u0632\u0627\u0646", "m2": "\u0628\u0647 \u0632\u0646\u062c\u06cc\u0631 \u0633\u0628\u0642 \u0627\u0644\u0627\u06cc\u0627\u062f\u06cc \u0645\u0642\u06cc\u062f"}, "6": {"m1": "\u062e\u0631\u062f\u0645\u0646\u062f \u0634\u0627\u0647\u0627 \u0631\u0639\u06cc\u062a \u067e\u0646\u0627\u0647\u0627", "m2": "\u06a9\u0647 \u0645\u062e\u0635\u0648\u0635 \u0628\u0627\u062f\u06cc \u0628\u0647 \u062a\u0623\u06cc\u06cc\u062f \u0633\u0631\u0645\u062f"}, "7": {"m1": "\u06cc\u06a9\u06cc \u067e\u0646\u062f \u067e\u06cc\u0631\u0627\u0646\u0647 \u0628\u0634\u0646\u0648 \u0632 \u0633\u0639\u062f\u06cc", "m2": "\u06a9\u0647 \u0628\u062e\u062a\u062a \u062c\u0648\u0627\u0646 \u0628\u0627\u062f \u0648 \u062c\u0627\u0647\u062a \u0645\u062c\u062f\u062f"}, "8": {"m1": "\u0646\u0628\u0648\u062f\u0633\u062a \u062a\u0627 \u0628\u0648\u062f\u0647 \u062f\u0648\u0631\u0627\u0646 \u06af\u06cc\u062a\u06cc", "m2": "\u0628\u0647 \u0627\u0628\u0642\u0627\u06cc \u0627\u0628\u0646\u0627\u06cc \u06af\u06cc\u062a\u06cc \u0645\u0639\u0648\u062f"}, "9": {"m1": "\u0645\u0628\u062f \u0646\u0645\u06cc\u200c\u0645\u0627\u0646\u062f \u0627\u06cc\u0646 \u0645\u0644\u06a9 \u062f\u0646\u06cc\u0627", "m2": "\u0646\u0634\u0627\u06cc\u062f \u0628\u0631 \u0627\u0648 \u062a\u06a9\u06cc\u0647 \u0628\u0631 \u0647\u06cc\u0686 \u0645\u0633\u0646\u062f"}, "10": {"m1": "\u0686\u0646\u0627\u0646 \u0635\u0631\u0641 \u06a9\u0646 \u062f\u0648\u0644\u062a \u0648 \u0632\u0646\u062f\u06af\u0627\u0646\u06cc", "m2": "\u06a9\u0647 \u0646\u0627\u0645\u062a \u0628\u0647 \u0646\u06cc\u06a9\u06cc \u0628\u0645\u0627\u0646\u062f \u0645\u062e\u0644\u062f"}}}