{"text": {"0": {"m1": "\u06af\u0641\u062a \u06cc\u0632\u062f\u0627\u0646 \u0622\u0646\u06a9 \u0628\u0627\u0634\u062f \u0627\u0635\u0644 \u062f\u0627\u0646", "m2": "\u067e\u0633 \u062a\u0631\u0627 \u06a9\u06cc \u0628\u06cc\u0646\u062f \u0627\u0648 \u0627\u0646\u062f\u0631 \u0645\u06cc\u0627\u0646"}, "1": {"m1": "\u06af\u0631\u0686\u0647 \u062e\u0648\u06cc\u0634 \u0631\u0627 \u0639\u0627\u0645\u0647 \u067e\u0646\u0647\u0627\u0646 \u06a9\u0631\u062f\u0647\u200c\u0627\u06cc", "m2": "\u067e\u06cc\u0634 \u0631\u0648\u0634\u0646\u200c\u062f\u06cc\u062f\u06af\u0627\u0646 \u0647\u0645 \u067e\u0631\u062f\u0647\u200c\u0627\u06cc"}, "2": {"m1": "\u0648\u0627\u0646\u06a9 \u0627\u06cc\u0634\u0627\u0646 \u0631\u0627 \u0634\u06a9\u0631 \u0628\u0627\u0634\u062f \u0627\u062c\u0644", "m2": "\u0686\u0648\u0646 \u0646\u0638\u0631\u0634\u0627\u0646 \u0645\u0633\u062a \u0628\u0627\u0634\u062f \u062f\u0631 \u062f\u0648\u0644"}, "3": {"m1": "\u062a\u0644\u062e \u0646\u0628\u0648\u062f \u067e\u06cc\u0634 \u0627\u06cc\u0634\u0627\u0646 \u0645\u0631\u06af \u062a\u0646", "m2": "\u0686\u0648\u0646 \u0631\u0648\u0646\u062f \u0627\u0632 \u0686\u0627\u0647 \u0648 \u0632\u0646\u062f\u0627\u0646 \u062f\u0631 \u0686\u0645\u0646"}, "4": {"m1": "\u0648\u0627 \u0631\u0647\u06cc\u062f\u0646\u062f \u0627\u0632 \u062c\u0647\u0627\u0646 \u067e\u06cc\u0686\u200c\u067e\u06cc\u0686", "m2": "\u06a9\u0633 \u0646\u06af\u0631\u06cc\u062f \u0628\u0631 \u0641\u0648\u0627\u062a \u0647\u06cc\u0686 \u0647\u06cc\u0686"}, "5": {"m1": "\u0628\u0631\u062c \u0632\u0646\u062f\u0627\u0646 \u0631\u0627 \u0634\u06a9\u0633\u062a \u0627\u0631\u06a9\u0627\u0646\u06cc\u06cc", "m2": "\u0647\u06cc\u0686 \u0627\u0632\u0648 \u0631\u0646\u062c\u062f \u062f\u0644 \u0632\u0646\u062f\u0627\u0646\u06cc\u06cc"}, "6": {"m1": "\u06a9\u0627\u06cc \u062f\u0631\u06cc\u063a \u0627\u06cc\u0646 \u0633\u0646\u06af \u0645\u0631\u0645\u0631 \u0631\u0627 \u0634\u06a9\u0633\u062a", "m2": "\u062a\u0627 \u0631\u0648\u0627\u0646 \u0648 \u062c\u0627\u0646 \u0645\u0627 \u0627\u0632 \u062d\u0628\u0633 \u0631\u0633\u062a"}, "7": {"m1": "\u0622\u0646 \u0631\u062e\u0627\u0645 \u062e\u0648\u0628 \u0648 \u0622\u0646 \u0633\u0646\u06af \u0634\u0631\u06cc\u0641", "m2": "\u0628\u0631\u062c \u0632\u0646\u062f\u0627\u0646 \u0631\u0627 \u0628\u0647\u06cc \u0628\u0648\u062f \u0648 \u0627\u0644\u06cc\u0641"}, "8": {"m1": "\u0686\u0648\u0646 \u0634\u06a9\u0633\u062a\u0634 \u062a\u0627 \u06a9\u0647 \u0632\u0646\u062f\u0627\u0646\u06cc \u0628\u0631\u0633\u062a", "m2": "\u062f\u0633\u062a \u0627\u0648 \u062f\u0631 \u062c\u0631\u0645 \u0627\u06cc\u0646 \u0628\u0627\u06cc\u062f \u0634\u06a9\u0633\u062a"}, "9": {"m1": "\u0647\u06cc\u0686 \u0632\u0646\u062f\u0627\u0646\u06cc \u0646\u06af\u0648\u06cc\u062f \u0627\u06cc\u0646 \u0641\u0634\u0627\u0631", "m2": "\u062c\u0632 \u06a9\u0633\u06cc \u06a9\u0632 \u062d\u0628\u0633 \u0622\u0631\u0646\u062f\u0634 \u0628\u0647 \u062f\u0627\u0631"}, "10": {"m1": "\u062a\u0644\u062e \u06a9\u06cc \u0628\u0627\u0634\u062f \u06a9\u0633\u06cc \u0631\u0627 \u06a9\u0634 \u0628\u0631\u0646\u062f", "m2": "\u0627\u0632 \u0645\u06cc\u0627\u0646 \u0632\u0647\u0631 \u0645\u0627\u0631\u0627\u0646 \u0633\u0648\u06cc \u0642\u0646\u062f"}, "11": {"m1": "\u062c\u0627\u0646 \u0645\u062c\u0631\u062f \u06af\u0634\u062a\u0647 \u0627\u0632 \u063a\u0648\u063a\u0627\u06cc \u062a\u0646", "m2": "\u0645\u06cc\u200c\u067e\u0631\u062f \u0628\u0627 \u067e\u0631 \u062f\u0644 \u0628\u06cc\u200c\u067e\u0627\u06cc \u062a\u0646"}, "12": {"m1": "\u0647\u0645\u200c\u0686\u0648 \u0632\u0646\u062f\u0627\u0646\u06cc \u0686\u0647 \u06a9\u0647 \u0627\u0646\u062f\u0631 \u0634\u0628\u0627\u0646", "m2": "\u062e\u0633\u067e\u062f \u0648 \u0628\u06cc\u0646\u062f \u0628\u0647 \u062e\u0648\u0627\u0628 \u0627\u0648 \u06af\u0644\u0633\u062a\u0627\u0646"}, "13": {"m1": "\u06af\u0648\u06cc\u062f \u0627\u06cc \u06cc\u0632\u062f\u0627\u0646 \u0645\u0631\u0627 \u062f\u0631 \u062a\u0646 \u0645\u0628\u0631", "m2": "\u062a\u0627 \u062f\u0631\u06cc\u0646 \u06af\u0644\u0634\u0646 \u06a9\u0646\u0645 \u0645\u0646 \u06a9\u0631 \u0648 \u0641\u0631"}, "14": {"m1": "\u06af\u0648\u06cc\u062f\u0634 \u06cc\u0632\u062f\u0627\u0646 \u062f\u0639\u0627 \u0634\u062f \u0645\u0633\u062a\u062c\u0627\u0628", "m2": "\u0648\u0627 \u0645\u0631\u0648 \u0648\u0627\u0644\u0644\u0647 \u0627\u0639\u0644\u0645 \u0628\u0627\u0644\u0635\u0648\u0627\u0628"}, "15": {"m1": "\u0627\u06cc\u0646 \u0686\u0646\u06cc\u0646 \u062e\u0648\u0627\u0628\u06cc \u0628\u0628\u06cc\u0646 \u0686\u0648\u0646 \u062e\u0648\u0634 \u0628\u0648\u062f", "m2": "\u0645\u0631\u06af \u0646\u0627\u062f\u06cc\u062f\u0647 \u0628\u0647 \u062c\u0646\u062a \u062f\u0631 \u0631\u0648\u062f"}, "16": {"m1": "\u0647\u06cc\u0686 \u0627\u0648 \u062d\u0633\u0631\u062a \u062e\u0648\u0631\u062f \u0628\u0631 \u0627\u0646\u062a\u0628\u0627\u0647", "m2": "\u0628\u0631 \u062a\u0646 \u0628\u0627 \u0633\u0644\u0633\u0644\u0647 \u062f\u0631 \u0642\u0639\u0631 \u0686\u0627\u0647"}, "17": {"m1": "\u0645\u0624\u0645\u0646\u06cc \u0622\u062e\u0631 \u062f\u0631 \u0622 \u062f\u0631 \u0635\u0641 \u0631\u0632\u0645", "m2": "\u06a9\u0647 \u062a\u0631\u0627 \u0628\u0631 \u0622\u0633\u0645\u0627\u0646 \u0628\u0648\u062f\u0633\u062a \u0628\u0632\u0645"}, "18": {"m1": "\u0628\u0631 \u0627\u0645\u06cc\u062f \u0631\u0627\u0647 \u0628\u0627\u0644\u0627 \u06a9\u0646 \u0642\u06cc\u0627\u0645", "m2": "\u0647\u0645\u200c\u0686\u0648 \u0634\u0645\u0639\u06cc \u067e\u06cc\u0634 \u0645\u062d\u0631\u0627\u0628 \u0627\u06cc \u063a\u0644\u0627\u0645"}, "19": {"m1": "\u0627\u0634\u06a9 \u0645\u06cc\u200c\u0628\u0627\u0631 \u0648 \u0647\u0645\u06cc\u200c\u0633\u0648\u0632 \u0627\u0632 \u0637\u0644\u0628", "m2": "\u0647\u0645\u200c\u0686\u0648 \u0634\u0645\u0639 \u0633\u0631 \u0628\u0631\u06cc\u062f\u0647 \u062c\u0645\u0644\u0647 \u0634\u0628"}, "20": {"m1": "\u0644\u0628 \u0641\u0631\u0648 \u0628\u0646\u062f \u0627\u0632 \u0637\u0639\u0627\u0645 \u0648 \u0627\u0632 \u0634\u0631\u0627\u0628", "m2": "\u0633\u0648\u06cc \u062e\u0648\u0627\u0646 \u0622\u0633\u0645\u0627\u0646\u06cc \u06a9\u0646 \u0634\u062a\u0627\u0628"}, "21": {"m1": "\u062f\u0645 \u0628\u0647 \u062f\u0645 \u0628\u0631 \u0622\u0633\u0645\u0627\u0646 \u0645\u06cc\u200c\u062f\u0627\u0631 \u0627\u0645\u06cc\u062f", "m2": "\u062f\u0631 \u0647\u0648\u0627\u06cc \u0622\u0633\u0645\u0627\u0646 \u0631\u0642\u0635\u0627\u0646 \u0686\u0648 \u0628\u06cc\u062f"}, "22": {"m1": "\u062f\u0645 \u0628\u0647 \u062f\u0645 \u0627\u0632 \u0622\u0633\u0645\u0627\u0646 \u0645\u06cc\u200c\u0622\u06cc\u062f\u062a", "m2": "\u0622\u0628 \u0648 \u0622\u062a\u0634 \u0631\u0632\u0642 \u0645\u06cc\u200c\u0627\u0641\u0632\u0627\u06cc\u062f\u062a"}, "23": {"m1": "\u06af\u0631 \u062a\u0631\u0627 \u0622\u0646\u062c\u0627 \u0628\u0631\u062f \u0646\u0628\u0648\u062f \u0639\u062c\u0628", "m2": "\u0645\u0646\u06af\u0631 \u0627\u0646\u062f\u0631 \u0639\u062c\u0632 \u0648 \u0628\u0646\u06af\u0631 \u062f\u0631 \u0637\u0644\u0628"}, "24": {"m1": "\u06a9\u06cc\u0646 \u0637\u0644\u0628 \u062f\u0631 \u062a\u0648 \u06af\u0631\u0648\u06af\u0627\u0646 \u062e\u062f\u0627\u0633\u062a", "m2": "\u0632\u0627\u0646\u06a9 \u0647\u0631 \u0637\u0627\u0644\u0628 \u0628\u0647 \u0645\u0637\u0644\u0648\u0628\u06cc \u0633\u0632\u0627\u0633\u062a"}, "25": {"m1": "\u062c\u0647\u062f \u06a9\u0646 \u062a\u0627 \u0627\u06cc\u0646 \u0637\u0644\u0628 \u0627\u0641\u0632\u0648\u0646 \u0634\u0648\u062f", "m2": "\u062a\u0627 \u062f\u0644\u062a \u0632\u06cc\u0646 \u0686\u0627\u0647 \u062a\u0646 \u0628\u06cc\u0631\u0648\u0646 \u0634\u0648\u062f"}, "26": {"m1": "\u062e\u0644\u0642 \u06af\u0648\u06cc\u062f \u0645\u0631\u062f \u0645\u0633\u06a9\u06cc\u0646 \u0622\u0646 \u0641\u0644\u0627\u0646", "m2": "\u062a\u0648 \u0628\u06af\u0648\u06cc\u06cc \u0632\u0646\u062f\u0647\u200c\u0627\u0645 \u0627\u06cc \u063a\u0627\u0641\u0644\u0627\u0646"}, "27": {"m1": "\u06af\u0631 \u062a\u0646 \u0645\u0646 \u0647\u0645\u200c\u0686\u0648 \u062a\u0646\u200c\u0647\u0627 \u062e\u0641\u062a\u0647 \u0627\u0633\u062a", "m2": "\u0647\u0634\u062a \u062c\u0646\u062a \u062f\u0631 \u062f\u0644\u0645 \u0628\u0634\u06a9\u0641\u062a\u0647 \u0627\u0633\u062a"}, "28": {"m1": "\u062c\u0627\u0646 \u0686\u0648 \u062e\u0641\u062a\u0647 \u062f\u0631 \u06af\u0644 \u0648 \u0646\u0633\u0631\u06cc\u0646 \u0628\u0648\u062f", "m2": "\u0686\u0647 \u063a\u0645\u0633\u062a \u0627\u0631 \u062a\u0646 \u062f\u0631 \u0622\u0646 \u0633\u0631\u06af\u06cc\u0646 \u0628\u0648\u062f"}, "29": {"m1": "\u062c\u0627\u0646 \u062e\u0641\u062a\u0647 \u0686\u0647 \u062e\u0628\u0631 \u062f\u0627\u0631\u062f \u0632 \u062a\u0646", "m2": "\u06a9\u0648 \u0628\u0647 \u06af\u0644\u0634\u0646 \u062e\u0641\u062a \u06cc\u0627 \u062f\u0631 \u06af\u0648\u0644\u062e\u0646"}, "30": {"m1": "\u0645\u06cc\u200c\u0632\u0646\u062f \u062c\u0627\u0646 \u062f\u0631 \u062c\u0647\u0627\u0646 \u0622\u0628\u06af\u0648\u0646", "m2": "\u0646\u0639\u0631\u0647 \u06cc\u0627 \u0644\u06cc\u062a \u0642\u0648\u0645\u06cc \u06cc\u0639\u0644\u0645\u0648\u0646"}, "31": {"m1": "\u06af\u0631 \u0646\u062e\u0648\u0627\u0647\u062f \u0632\u06cc\u0633\u062a \u062c\u0627\u0646 \u0628\u06cc \u0627\u06cc\u0646 \u0628\u062f\u0646", "m2": "\u067e\u0633 \u0641\u0644\u06a9 \u0627\u06cc\u0648\u0627\u0646 \u06a9\u06cc \u062e\u0648\u0627\u0647\u062f \u0628\u062f\u0646"}, "32": {"m1": "\u06af\u0631 \u0646\u062e\u0648\u0627\u0647\u062f \u0628\u06cc \u0628\u062f\u0646 \u062c\u0627\u0646 \u062a\u0648 \u0632\u06cc\u0633\u062a", "m2": "\u0641\u06cc \u0627\u0644\u0633\u0645\u0627\u0621 \u0631\u0632\u0642\u06a9\u0645 \u0631\u0648\u0632\u06cc \u06a9\u06cc\u0633\u062a"}}}