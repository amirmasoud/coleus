{"text": {"0": {"m1": "\u0627\u0646\u062f\u0631 \u0622\u0646 \u06a9\u0647 \u0628\u0648\u062f \u0627\u0634\u062c\u0627\u0631 \u0648 \u062b\u0645\u0627\u0631", "m2": "\u0628\u0633 \u0645\u0631\u0648\u062f\u06cc \u06a9\u0648\u0647\u06cc \u0622\u0646\u062c\u0627 \u0628\u06cc\u200c\u0634\u0645\u0627\u0631"}, "1": {"m1": "\u06af\u0641\u062a \u0622\u0646 \u062f\u0631\u0648\u06cc\u0634 \u06cc\u0627 \u0631\u0628 \u0628\u0627 \u062a\u0648 \u0645\u0646", "m2": "\u0639\u0647\u062f \u06a9\u0631\u062f\u0645 \u0632\u06cc\u0646 \u0646\u0686\u06cc\u0646\u0645 \u062f\u0631 \u0632\u0645\u0646"}, "2": {"m1": "\u062c\u0632 \u0627\u0632 \u0622\u0646 \u0645\u06cc\u0648\u0647 \u06a9\u0647 \u0628\u0627\u062f \u0627\u0646\u062f\u0627\u062e\u062a\u0634", "m2": "\u0645\u0646 \u0646\u0686\u06cc\u0646\u0645 \u0627\u0632 \u062f\u0631\u062e\u062a \u0645\u0646\u062a\u0639\u0634"}, "3": {"m1": "\u0645\u062f\u062a\u06cc \u0628\u0631 \u0646\u0630\u0631 \u062e\u0648\u062f \u0628\u0648\u062f\u0634 \u0648\u0641\u0627", "m2": "\u062a\u0627 \u062f\u0631 \u0622\u0645\u062f \u0627\u0645\u062a\u062d\u0627\u0646\u0627\u062a \u0642\u0636\u0627"}, "4": {"m1": "\u0632\u06cc\u0646 \u0633\u0628\u0628 \u0641\u0631\u0645\u0648\u062f \u0627\u0633\u062a\u062b\u0646\u0627 \u06a9\u0646\u06cc\u062f", "m2": "\u06af\u0631 \u062e\u062f\u0627 \u062e\u0648\u0627\u0647\u062f \u0628\u0647 \u067e\u06cc\u0645\u0627\u0646 \u0628\u0631 \u0632\u0646\u06cc\u062f"}, "5": {"m1": "\u0647\u0631 \u0632\u0645\u0627\u0646 \u062f\u0644 \u0631\u0627 \u062f\u06af\u0631 \u0645\u06cc\u0644\u06cc \u062f\u0647\u0645", "m2": "\u0647\u0631\u0646\u0641\u0633 \u0628\u0631 \u062f\u0644 \u062f\u06af\u0631 \u062f\u0627\u063a\u06cc \u0646\u0647\u0645"}, "6": {"m1": "\u06a9\u0644 \u0627\u0635\u0628\u0627\u062d \u0644\u0646\u0627 \u0634\u0627\u0646 \u062c\u062f\u06cc\u062f", "m2": "\u06a9\u0644 \u0634\u06cc\u0621 \u0639\u0646 \u0645\u0631\u0627\u062f\u06cc \u0644\u0627 \u06cc\u062d\u06cc\u062f"}, "7": {"m1": "\u062f\u0631 \u062d\u062f\u06cc\u062b \u0622\u0645\u062f \u06a9\u0647 \u062f\u0644 \u0647\u0645\u0686\u0648\u0646 \u067e\u0631\u06cc\u0633\u062a", "m2": "\u062f\u0631 \u0628\u06cc\u0627\u0628\u0627\u0646\u06cc \u0627\u0633\u06cc\u0631 \u0635\u0631\u0635\u0631\u06cc\u0633\u062a"}, "8": {"m1": "\u0628\u0627\u062f \u067e\u0631 \u0631\u0627 \u0647\u0631 \u0637\u0631\u0641 \u0631\u0627\u0646\u062f \u06af\u0632\u0627\u0641", "m2": "\u06af\u0647 \u0686\u067e \u0648 \u06af\u0647 \u0631\u0627\u0633\u062a \u0628\u0627 \u0635\u062f \u0627\u062e\u062a\u0644\u0627\u0641"}, "9": {"m1": "\u062f\u0631 \u062d\u062f\u06cc\u062b \u062f\u06cc\u06af\u0631 \u0627\u06cc\u0646 \u062f\u0644 \u062f\u0627\u0646 \u0686\u0646\u0627\u0646", "m2": "\u06a9\u0628 \u062c\u0648\u0634\u0627\u0646 \u0632 \u0622\u062a\u0634 \u0627\u0646\u062f\u0631 \u0642\u0627\u0632\u063a\u0627\u0646"}, "10": {"m1": "\u0647\u0631 \u0632\u0645\u0627\u0646 \u062f\u0644 \u0631\u0627 \u062f\u06af\u0631 \u0631\u0627\u06cc\u06cc \u0628\u0648\u062f", "m2": "\u0622\u0646 \u0646\u0647 \u0627\u0632 \u0648\u06cc \u0644\u06cc\u06a9 \u0627\u0632 \u062c\u0627\u06cc\u06cc \u0628\u0648\u062f"}, "11": {"m1": "\u067e\u0633 \u0686\u0631\u0627 \u0622\u0645\u0646 \u0634\u0648\u06cc \u0628\u0631 \u0631\u0627\u06cc \u062f\u0644", "m2": "\u0639\u0647\u062f \u0628\u0646\u062f\u06cc \u062a\u0627 \u0634\u0648\u06cc \u0622\u062e\u0631 \u062e\u062c\u0644"}, "12": {"m1": "\u0627\u06cc\u0646 \u0647\u0645 \u0627\u0632 \u062a\u0627\u062b\u06cc\u0631 \u062d\u06a9\u0645\u0633\u062a \u0648 \u0642\u062f\u0631", "m2": "\u0686\u0627\u0647 \u0645\u06cc\u200c\u0628\u06cc\u06cc\u0646\u06cc \u0648 \u0646\u062a\u0648\u0627\u0646\u06cc \u062d\u0630\u0631"}, "13": {"m1": "\u0646\u06cc\u0633\u062a \u062e\u0648\u062f \u0627\u0632\u0645\u0631\u063a \u067e\u0631\u0627\u0646 \u0627\u06cc\u0646 \u0639\u062c\u0628", "m2": "\u06a9\u0647 \u0646\u0628\u06cc\u0646\u062f \u062f\u0627\u0645 \u0648 \u0627\u0641\u062a\u062f \u062f\u0631 \u0639\u0637\u0628"}, "14": {"m1": "\u0627\u06cc\u0646 \u0639\u062c\u0628 \u06a9\u0647 \u062f\u0627\u0645 \u0628\u06cc\u0646\u062f \u0647\u0645 \u0648\u062a\u062f", "m2": "\u06af\u0631 \u0628\u062e\u0648\u0627\u0647\u062f \u0648\u0631 \u0646\u062e\u0648\u0627\u0647\u062f \u0645\u06cc\u200c\u0641\u062a\u062f"}, "15": {"m1": "\u0686\u0634\u0645 \u0628\u0627\u0632 \u0648 \u06af\u0648\u0634 \u0628\u0627\u0632 \u0648 \u062f\u0627\u0645 \u067e\u06cc\u0634", "m2": "\u0633\u0648\u06cc \u062f\u0627\u0645\u06cc \u0645\u06cc\u200c\u067e\u0631\u062f \u0628\u0627 \u067e\u0631 \u062e\u0648\u06cc\u0634"}}}