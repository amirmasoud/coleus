{"text": {"0": {"m1": "\u0628\u0627 \u0648\u06a9\u06cc\u0644 \u0642\u0627\u0636\u06cc \u0627\u062f\u0631\u0627\u06a9\u200c\u0645\u0646\u062f", "m2": "\u0627\u0647\u0644 \u0632\u0646\u062f\u0627\u0646 \u062f\u0631 \u0634\u06a9\u0627\u06cc\u062a \u0622\u0645\u062f\u0646\u062f"}, "1": {"m1": "\u06a9\u0647 \u0633\u0644\u0627\u0645 \u0645\u0627 \u0628\u0647 \u0642\u0627\u0636\u06cc \u0628\u0631 \u06a9\u0646\u0648\u0646", "m2": "\u0628\u0627\u0632\u06af\u0648 \u0622\u0632\u0627\u0631 \u0645\u0627 \u0632\u06cc\u0646 \u0645\u0631\u062f \u062f\u0648\u0646"}, "2": {"m1": "\u06a9\u0646\u062f\u0631\u06cc\u0646 \u0632\u0646\u062f\u0627\u0646 \u0628\u0645\u0627\u0646\u062f \u0627\u0648 \u0645\u0633\u062a\u0645\u0631", "m2": "\u06cc\u0627\u0648\u0647\u200c\u062a\u0627\u0632 \u0648 \u0637\u0628\u0644\u200c\u062e\u0648\u0627\u0631\u0633\u062a \u0648 \u0645\u0636\u0631"}, "3": {"m1": "\u0686\u0648\u0646 \u0645\u06af\u0633 \u062d\u0627\u0636\u0631 \u0634\u0648\u062f \u062f\u0631 \u0647\u0631 \u0637\u0639\u0627\u0645", "m2": "\u0627\u0632 \u0648\u0642\u0627\u062d\u062a \u0628\u06cc \u0635\u0644\u0627 \u0648 \u0628\u06cc \u0633\u0644\u0627\u0645"}, "4": {"m1": "\u067e\u06cc\u0634 \u0627\u0648 \u0647\u06cc\u0686\u0633\u062a \u0644\u0648\u062a \u0634\u0635\u062a \u06a9\u0633", "m2": "\u06a9\u0631 \u06a9\u0646\u062f \u062e\u0648\u062f \u0631\u0627 \u0627\u06af\u0631 \u06af\u0648\u06cc\u06cc\u0634 \u0628\u0633"}, "5": {"m1": "\u0645\u0631\u062f \u0632\u0646\u062f\u0627\u0646 \u0631\u0627 \u0646\u06cc\u0627\u06cc\u062f \u0644\u0642\u0645\u0647\u200c\u0627\u06cc", "m2": "\u0648\u0631 \u0628\u0647 \u0635\u062f \u062d\u06cc\u0644\u062a \u06af\u0634\u0627\u06cc\u062f \u0637\u0639\u0645\u0647\u200c\u0627\u06cc"}, "6": {"m1": "\u062f\u0631 \u0632\u0645\u0627\u0646 \u067e\u06cc\u0634 \u0622\u06cc\u062f \u0622\u0646 \u062f\u0648\u0632\u062e \u06af\u0644\u0648", "m2": "\u062d\u062c\u062a\u0634 \u0627\u06cc\u0646 \u06a9\u0647 \u062e\u062f\u0627 \u06af\u0641\u062a\u0627 \u06a9\u0644\u0648\u0627"}, "7": {"m1": "\u0632\u06cc\u0646 \u0686\u0646\u06cc\u0646 \u0642\u062d\u0637 \u0633\u0647\u200c\u0633\u0627\u0644\u0647 \u062f\u0627\u062f \u062f\u0627\u062f", "m2": "\u0638\u0644 \u0645\u0648\u0644\u0627\u0646\u0627 \u0627\u0628\u062f \u067e\u0627\u06cc\u0646\u062f\u0647 \u0628\u0627\u062f"}, "8": {"m1": "\u06cc\u0627 \u0632 \u0632\u0646\u062f\u0627\u0646 \u062a\u0627 \u0631\u0648\u062f \u0627\u06cc\u0646 \u06af\u0627\u0648\u0645\u06cc\u0634", "m2": "\u06cc\u0627 \u0648\u0638\u06cc\u0641\u0647 \u06a9\u0646 \u0632 \u0648\u0642\u0641\u06cc \u0644\u0642\u0645\u0647\u200c\u0627\u06cc\u0634"}, "9": {"m1": "\u0627\u06cc \u0632 \u062a\u0648 \u062e\u0648\u0634 \u0647\u0645 \u0630\u06a9\u0648\u0631 \u0648 \u0647\u0645 \u0627\u0646\u0627\u062b", "m2": "\u062f\u0627\u062f \u06a9\u0646 \u0627\u0644\u0645\u0633\u062a\u063a\u0627\u062b \u0627\u0644\u0645\u0633\u062a\u063a\u0627\u062b"}, "10": {"m1": "\u0633\u0648\u06cc \u0642\u0627\u0636\u06cc \u0634\u062f \u0648\u06a9\u06cc\u0644 \u0628\u0627 \u0646\u0645\u06a9", "m2": "\u06af\u0641\u062a \u0628\u0627 \u0642\u0627\u0636\u06cc \u0634\u06a9\u0627\u06cc\u062a \u06cc\u06a9 \u0628\u06cc\u06a9"}, "11": {"m1": "\u062e\u0648\u0627\u0646\u062f \u0627\u0648 \u0631\u0627 \u0642\u0627\u0636\u06cc \u0627\u0632 \u0632\u0646\u062f\u0627\u0646 \u0628\u0647 \u067e\u06cc\u0634", "m2": "\u067e\u0633 \u062a\u0641\u062d\u0635 \u06a9\u0631\u062f \u0627\u0632 \u0627\u0639\u06cc\u0627\u0646 \u062e\u0648\u06cc\u0634"}, "12": {"m1": "\u06af\u0634\u062a \u062b\u0627\u0628\u062a \u067e\u06cc\u0634 \u0642\u0627\u0636\u06cc \u0622\u0646 \u0647\u0645\u0647", "m2": "\u06a9\u0647 \u0646\u0645\u0648\u062f\u0646\u062f \u0627\u0632 \u0634\u06a9\u0627\u06cc\u062a \u0622\u0646 \u0631\u0645\u0647"}, "13": {"m1": "\u06af\u0641\u062a \u0642\u0627\u0636\u06cc \u062e\u06cc\u0632 \u0627\u0632\u06cc\u0646 \u0632\u0646\u062f\u0627\u0646 \u0628\u0631\u0648", "m2": "\u0633\u0648\u06cc \u062e\u0627\u0646\u0647\u0654 \u0645\u0631\u062f\u0631\u06cc\u06af \u062e\u0648\u06cc\u0634 \u0634\u0648"}, "14": {"m1": "\u06af\u0641\u062a \u062e\u0627\u0646 \u0648 \u0645\u0627\u0646 \u0645\u0646 \u0627\u062d\u0633\u0627\u0646 \u062a\u0633\u062a", "m2": "\u0647\u0645\u0686\u0648 \u06a9\u0627\u0641\u0631 \u062c\u0646\u062a\u0645 \u0632\u0646\u062f\u0627\u0646 \u062a\u0633\u062a"}, "15": {"m1": "\u06af\u0631 \u0632 \u0632\u0646\u062f\u0627\u0646\u0645 \u0628\u0631\u0627\u0646\u06cc \u062a\u0648 \u0628\u0631\u062f", "m2": "\u062e\u0648\u062f \u0628\u0645\u06cc\u0631\u0645 \u0645\u0646 \u0632 \u062a\u0642\u0635\u06cc\u0631\u06cc \u0648 \u06a9\u062f"}, "16": {"m1": "\u0647\u0645\u0686\u0648 \u0627\u0628\u0644\u06cc\u0633\u06cc \u06a9\u0647 \u0645\u06cc\u200c\u06af\u0641\u062a \u0627\u06cc \u0633\u0644\u0627\u0645", "m2": "\u0631\u0628 \u0627\u0646\u0638\u0631\u0646\u06cc \u0627\u0644\u06cc \u06cc\u0648\u0645 \u0627\u0644\u0642\u06cc\u0627\u0645"}, "17": {"m1": "\u06a9\u0627\u0646\u062f\u0631\u06cc\u0646 \u0632\u0646\u062f\u0627\u0646 \u062f\u0646\u06cc\u0627 \u0645\u0646 \u062e\u0648\u0634\u0645", "m2": "\u062a\u0627 \u06a9\u0647 \u062f\u0634\u0645\u0646\u200c\u0632\u0627\u062f\u06af\u0627\u0646 \u0631\u0627 \u0645\u06cc\u200c\u06a9\u0634\u0645"}, "18": {"m1": "\u0647\u0631 \u06a9\u0647 \u0627\u0648 \u0631\u0627 \u0642\u0648\u062a \u0627\u06cc\u0645\u0627\u0646\u06cc \u0628\u0648\u062f", "m2": "\u0648\u0632 \u0628\u0631\u0627\u06cc \u0632\u0627\u062f \u0631\u0647 \u0646\u0627\u0646\u06cc \u0628\u0648\u062f"}, "19": {"m1": "\u0645\u06cc\u200c\u0633\u062a\u0627\u0646\u0645 \u06af\u0647 \u0628\u0647 \u0645\u06a9\u0631 \u0648 \u06af\u0647 \u0628\u0647 \u0631\u06cc\u0648", "m2": "\u062a\u0627 \u0628\u0631 \u0622\u0631\u0646\u062f \u0627\u0632 \u067e\u0634\u06cc\u0645\u0627\u0646\u06cc \u063a\u0631\u06cc\u0648"}, "20": {"m1": "\u06af\u0647 \u0628\u0647 \u062f\u0631\u0648\u06cc\u0634\u06cc \u06a9\u0646\u0645 \u062a\u0647\u062f\u06cc\u062f\u0634\u0627\u0646", "m2": "\u06af\u0647 \u0628\u0647 \u0632\u0644\u0641 \u0648 \u062e\u0627\u0644 \u0628\u0646\u062f\u0645 \u062f\u06cc\u062f\u0634\u0627\u0646"}, "21": {"m1": "\u0642\u0648\u062a \u0627\u06cc\u0645\u0627\u0646\u06cc \u062f\u0631\u06cc\u0646 \u0632\u0646\u062f\u0627\u0646 \u06a9\u0645\u0633\u062a", "m2": "\u0648\u0627\u0646\u06a9 \u0647\u0633\u062a \u0627\u0632 \u0642\u0635\u062f \u0627\u06cc\u0646 \u0633\u06af \u062f\u0631 \u062e\u0645\u0633\u062a"}, "22": {"m1": "\u0627\u0632 \u0646\u0645\u0627\u0632 \u0648 \u0635\u0648\u0645 \u0648 \u0635\u062f \u0628\u06cc\u0686\u0627\u0631\u06af\u06cc", "m2": "\u0642\u0648\u062a \u0630\u0648\u0642 \u0622\u06cc\u062f \u0628\u0631\u062f \u06cc\u06a9\u0628\u0627\u0631\u06af\u06cc"}, "23": {"m1": "\u0627\u0633\u062a\u0639\u06cc\u0630 \u0627\u0644\u0644\u0647 \u0645\u0646 \u0634\u06cc\u0637\u0627\u0646\u0647", "m2": "\u0642\u062f \u0647\u0644\u06a9\u0646\u0627 \u0622\u0647 \u0645\u0646 \u0637\u063a\u06cc\u0627\u0646\u0647"}, "24": {"m1": "\u06cc\u06a9 \u0633\u06af\u0633\u062a \u0648 \u062f\u0631 \u0647\u0632\u0627\u0631\u0627\u0646 \u0645\u06cc\u200c\u0631\u0648\u062f", "m2": "\u0647\u0631 \u06a9\u0647 \u062f\u0631 \u0648\u06cc \u0631\u0641\u062a \u0627\u0648 \u0627\u0648 \u0645\u06cc\u200c\u0634\u0648\u062f"}, "25": {"m1": "\u0647\u0631 \u06a9\u0647 \u0633\u0631\u062f\u062a \u06a9\u0631\u062f \u0645\u06cc\u200c\u062f\u0627\u0646 \u06a9\u0648 \u062f\u0631\u0648\u0633\u062a", "m2": "\u062f\u06cc\u0648 \u067e\u0646\u0647\u0627\u0646 \u06af\u0634\u062a\u0647 \u0627\u0646\u062f\u0631 \u0632\u06cc\u0631 \u067e\u0648\u0633\u062a"}, "26": {"m1": "\u0686\u0648\u0646 \u0646\u06cc\u0627\u0628\u062f \u0635\u0648\u0631\u062a \u0622\u06cc\u062f \u062f\u0631 \u062e\u06cc\u0627\u0644", "m2": "\u062a\u0627 \u06a9\u0634\u0627\u0646\u062f \u0622\u0646 \u062e\u06cc\u0627\u0644\u062a \u062f\u0631 \u0648\u0628\u0627\u0644"}, "27": {"m1": "\u06af\u0647 \u062e\u06cc\u0627\u0644 \u0641\u0631\u062c\u0647 \u0648 \u06af\u0627\u0647\u06cc \u062f\u06a9\u0627\u0646", "m2": "\u06af\u0647 \u062e\u06cc\u0627\u0644 \u0639\u0644\u0645 \u0648 \u06af\u0627\u0647\u06cc \u062e\u0627\u0646 \u0648 \u0645\u0627\u0646"}, "28": {"m1": "\u0647\u0627\u0646 \u0628\u06af\u0648 \u0644\u0627 \u062d\u0648\u0644\u0647\u0627 \u0627\u0646\u062f\u0631 \u0632\u0645\u0627\u0646", "m2": "\u0627\u0632 \u0632\u0628\u0627\u0646 \u062a\u0646\u0647\u0627 \u0646\u0647 \u0628\u0644\u06a9 \u0627\u0632 \u0639\u06cc\u0646 \u062c\u0627\u0646"}}}