{"text": {"0": {"m1": "\u0627\u0646\u06cc \u0644\u0627\u062e\u062f\u0645 \u0646\u0627\u0635\u062d \u0627\u0644\u062e\u0644\u0641\u0627\u0621", "m2": "\u0645\u0631\u062f \u0627\u0644\u0627\u0626\u0645\u0629 \u062e\u0627\u0636\u0639 \u0627\u0644\u062d\u0646\u0641\u0627\u0621"}, "1": {"m1": "\u0628\u062a\u062d\u06cc\u0629 \u0645\u0634\u0641\u0648\u0639\u0629 \u0628\u0645\u062d\u0627\u0645\u062f", "m2": "\u0648 \u0645\u062d\u0627\u0645\u062f \u0645\u0642\u0631\u0648\u0646\u0629 \u0628\u062f\u0639\u0627\u0621"}, "2": {"m1": "\u0648 \u062a\u0639\u0627\u0631\u0641 \u0627\u06a9\u0628\u0631\u062a\u0647 \u0628\u062a\u0630\u06a9\u0631", "m2": "\u0648 \u062a\u0630\u06a9\u0631 \u0648 \u0634\u062e\u0635\u0629 \u0628\u062b\u0646\u0627\u0621"}, "3": {"m1": "\u0648 \u0644\u062f\u06cc\u0647 \u0644\u06cc \u0645\u0634\u0641\u0639 \u0645\u0646 \u062e\u0644\u0642\u0647", "m2": "\u0642\u062f \u0633\u0631\u0646\u06cc \u0644\u0627\u0632\u0627\u0644 \u0641\u06cc \u0627\u0644\u0633\u0631\u0627\u0621"}, "4": {"m1": "\u0644\u0642\u0628\u0648\u0644 \u0645\u062f\u06cc \u062d\u0628\u0647 \u062d\u0631\u0645\u062a\u0647", "m2": "\u0645\u0633\u0648\u062f\u0629 \u0648 \u062d\u0645\u0627\u0645\u0629 \u0627\u0644\u0628\u06cc\u0636\u0627\u0621"}, "5": {"m1": "\u0648 \u0642\u0628\u0648\u0644\u0647 \u0641\u0631\u0634\u0627\u062a \u0645\u0639\u0627\u0631\u0627 \u0627\u0644\u0627\u0645\u0639\u0627\u0631\u0627", "m2": "\u06cc\u0633\u0648\u062f \u06a9\u062d\u0644\u06cc \u06a9\u0644 \u0627\u0645\u0627\u0621"}, "6": {"m1": "\u0631\u06cc\u062d \u0644\u062c\u0645\u062a \u0633\u0644\u06cc\u0645\u0627\u0646 \u0627\u0644\u062d\u062c\u06cc", "m2": "\u062a\u062e\u062a\u0627\u0631\u0647\u0627 \u0645\u0646 \u0639\u0627\u0635\u0641 \u0648 \u0631\u062e\u0627\u0621"}, "7": {"m1": "\u0637\u0648\u0631 \u06a9\u0633\u0628\u0639\u0629 \u0627\u0628\u062d\u0631 \u0645\u0646 \u0631\u062d\u0644\u0647", "m2": "\u0630\u0648 \u0627\u0631\u0628\u0639 \u0645\u0646 \u0627\u0645\u0647\u0627\u062a \u0647\u0648\u0627\u0621"}, "8": {"m1": "\u0641\u0644\u06a9 \u06cc\u062f\u0648\u0631 \u0645\u0646\u0647 \u0647\u0644\u0627\u0644 \u0633\u0631\u062c\u0647", "m2": "\u06cc\u0639\u0644\u0648\u0647 \u0628\u062f\u0631 \u0635\u0627\u062f\u0642 \u0627\u0644\u0627\u0644\u0627\u0621"}, "9": {"m1": "\u0630\u0648\u0647\u0645\u0629 \u0648 \u0628\u0647\u0630\u0627 \u0639\u0632\u06a9\u0627\u0646\u0647", "m2": "\u0644\u06cc\u0644 \u062a\u0628\u0631\u0642\u0639 \u0645\u0646 \u0628\u0631\u06cc\u0642 \u0636\u062d\u0627\u0621"}, "10": {"m1": "\u0644\u0645\u0627 \u062a\u0645\u0633\u0633\u062a \u0627\u0644\u062e\u0644\u0627\u0644 \u062d\u0633\u0628\u062a\u0647", "m2": "\u0646\u0648\u062d\u0627 \u06a9\u062c\u0648\u062f\u06cc \u0627\u06cc \u0639\u0644\u0627\u0621"}, "11": {"m1": "\u06cc\u0644\u0642\u06cc \u06a9\u0644\u0627\u0645\u200c\u0627\u0644\u0644\u0647 \u0641\u0627\u0631\u0629 \u0637\u0648\u0631\u0647", "m2": "\u0648 \u06cc\u0631\u06cc \u062d\u0628\u06cc\u0628\u200c\u0627\u0644\u0644\u0647 \u0641\u0648\u0642 \u062d\u062f\u0627\u0621"}, "12": {"m1": "\u0648\u0644\u0648 \u0627\u0633\u062a\u0637\u0639\u062a \u0646\u062b\u0631\u0647 \u06a9\u0646\u0648\u0632 \u0644\u0627\u0644\u06cc", "m2": "\u0644\u06cc\u0631\u0627\u0639\u0647 \u0627\u0644\u063a\u0648\u0627\u0635 \u0641\u06cc \u0627\u0644\u062f\u0627\u0645\u0627\u0621"}, "13": {"m1": "\u0647\u0648 \u0642\u0633\u0648\u0631\u0629 \u0648 \u062f\u0648\u0627\u062a\u0647 \u0635\u06cc\u0627\u062f\u0629", "m2": "\u0648 \u0627\u0628\u06cc\u062d \u0639\u06cc\u0646 \u0627\u0644\u0645\u0633\u06a9 \u0644\u0644\u0627\u062f\u0648\u0627\u0621"}, "14": {"m1": "\u0639\u06cc\u0646 \u0628\u0635\u0641\u0631\u062a\u0647\u0627\u06cc\u0631\u06cc \u0648\u062c\u0647\u200c\u0627\u0644\u0645\u0646\u06cc", "m2": "\u0647\u06cc \u062a\u0642\u0645\u0639 \u0627\u0644\u0633\u0648\u062f\u0627\u0621 \u0628\u0627\u0635\u0641\u0631\u0627\u0621"}, "15": {"m1": "\u0645\u0636\u062d\u0627\u06a9 \u0648\u062c\u0647 \u0648\u062c\u0647 \u06a9\u0644 \u0645\u0637\u0627\u0644\u0628", "m2": "\u0645\u0633\u0642\u0627\u0645 \u0639\u06cc\u0646 \u0639\u06cc\u0646 \u06a9\u0644 \u062f\u0648\u0627\u0621"}, "16": {"m1": "\u0639\u06cc\u0646 \u06a9\u0639\u06cc\u0646 \u0627\u0644\u0634\u0645\u0633 \u0628\u0627\u0644\u06cc\u0631\u0642\u0627\u0646 \u0628\u0644", "m2": "\u0648\u062c\u0647 \u06a9\u0648\u062c\u0647 \u0627\u0644\u0645\u0627\u0621 \u0644\u0644\u063a\u0631\u0628\u0627\u0621"}, "17": {"m1": "\u0644\u0637\u0645\u062a \u06cc\u062f \u0627\u0644\u0636\u0631\u0627\u0628 \u0633\u0646\u0629 \u0648\u062c\u0647\u0647\u0627", "m2": "\u0641\u0628\u062f\u0627\u0644\u0647\u0627 \u062d\u0627\u0626\u0644\u0627 \u0639\u062f\u0648\u0627\u0621"}, "18": {"m1": "\u0645\u0631\u0645\u0648\u0642\u0629 \u0627\u0644\u0627\u0641\u0627\u0642 \u0628\u0644 \u0645\u0631\u0641\u0648\u0642\u0629", "m2": "\u0627\u0644\u0627\u062e\u0644\u0627\u0642 \u0628\u0644 \u0645\u062e\u0644\u0648\u0642\u0629 \u0627\u0644\u0627\u0636\u0648\u0627\u0621"}, "19": {"m1": "\u062c\u0648\u0627\u0644\u0629 \u0627\u0644\u0628\u0644\u062f\u0627\u0646 \u0628\u0644 \u0642\u06cc\u0627\u0644\u0629 \u0627\u0644\u062d\u0632\u0627\u0646", "m2": "\u0628\u0644 \u062e\u062a\u0627\u0644\u0629 \u0627\u0644\u0627\u0631\u0627\u0621"}, "20": {"m1": "\u062c\u0631\u062d \u0627\u0644\u0634\u0647\u0648\u062f \u0648 \u0639\u062f\u0644 \u062f\u06cc\u0648\u0627\u0646 \u0627\u0644\u0642\u0636\u0627", "m2": "\u0627\u0642\u0636\u06cc \u0627\u0644\u0642\u0636\u0627\u0629 \u0648 \u0627\u0634\u0641\u0639 \u0627\u0644\u0634\u0641\u0639\u0627\u0621"}, "21": {"m1": "\u0639\u0645\u0631 \u0627\u0644\u06cc\u0647\u0648\u062f \u0644\u0647\u0627 \u0648 \u0644\u0648\u0646 \u063a\u06cc\u0627\u0631\u0647\u0645", "m2": "\u0644\u06a9\u0646 \u0645\u0633\u06cc\u062d \u0627\u0644\u0639\u0647\u062f \u0641\u06cc \u0627\u0644\u0627\u062d\u06cc\u0627\u0621"}, "22": {"m1": "\u0639\u0645\u0631\u062a \u0628\u0647\u062f\u0645 \u0627\u0644\u0641\u0636\u0644 \u0639\u0646\u0648\u0627\u0646 \u0627\u0644\u0647\u0648\u06cc", "m2": "\u0647\u062f\u0645 \u0627\u0644\u0639\u0642\u0648\u0644 \u0639\u0645\u0627\u0631\u0629 \u0627\u0644\u0627\u0647\u0648\u0627\u0621"}, "23": {"m1": "\u062c\u0631\u0645 \u06a9\u062c\u0645\u0631 \u062c\u0627\u0645\u062f \u0645\u0633\u0627\u0626\u0644\u062a\u06cc", "m2": "\u06cc\u0632\u06a9\u06cc \u0628\u0647 \u0642\u0646\u062f\u06cc\u0644 \u06a9\u0644 \u0631\u062e\u0627\u0621"}, "24": {"m1": "\u0641\u0627\u0644\u062c\u0645\u0631 \u06cc\u062e\u0645\u062f\u0644\u0627 \u06cc\u0644\u0648\u062d \u0636\u06cc\u0627\u0624\u0647", "m2": "\u0648 \u0644\u0647\u0627 \u062e\u0645\u0648\u062f \u0641\u06cc \u062c\u0644\u0648\u0629 \u0631\u0636\u06cc\u0627\u0621"}, "25": {"m1": "\u062e\u0645\u0631 \u0627\u0644\u0633\u0639\u062a\u0631 \u06cc\u0631\u06cc \u0631\u062e\u06cc\u0635\u0627 \u0634\u0639\u0631\u0647", "m2": "\u0641\u06cc \u062e\u0645\u0631\u0629 \u06a9\u0627\u0644\u0645\u0633\u06a9 \u0630\u0627\u062a \u0639\u0644\u0627\u0621"}, "26": {"m1": "\u0641\u06a9\u0627\u0646\u0645\u0627 \u06a9\u0645\u0627\u0621 \u0627\u0644\u062d\u0627\u0631 \u0628\u0639\u06cc\u0646\u0647", "m2": "\u0627\u0636\u062d\u06cc \u0628\u0633\u0627\u0637\u0627 \u062e\u0627\u0645\u062f \u0627\u0644\u0627\u062c\u0632\u0627\u0621"}, "27": {"m1": "\u0634\u0631\u0642 \u0645\u0646 \u0639\u0631\u0634 \u0627\u062d\u0628\u0647\u0627 \u0641\u0627\u062d\u0628\u0647\u0627", "m2": "\u0641\u0627\u0646\u06cc \u0628\u0639\u0631\u0634 \u0641\u0627\u0631\u06a9 \u0631\u0639\u0646\u0627\u0621"}, "28": {"m1": "\u0639\u0631\u0634 \u0645\u0637\u0644\u0642\u0629 \u0627\u0644\u0631\u062c\u0627\u0644 \u062a\u0639\u0648\u0630\u062a", "m2": "\u0633\u062c\u0646\u200c\u0627\u0644\u06cc\u062f\u06cc\u0646 \u0648 \u0633\u0627\u0642 \u06a9\u0644 \u0646\u0633\u0627\u0621"}, "29": {"m1": "\u0633\u0631\u0642\u062a \u0639\u0642\u0648\u0644 \u0627\u0644\u0646\u0627\u0633 \u0641\u06cc \u062d\u0631\u0628\u0647", "m2": "\u0628\u0627\u0644\u0636\u0631\u0628 \u062b\u0645 \u0627\u0644\u0642\u0637\u0639 \u0644\u0644\u0627\u0634\u0644\u0627\u0621"}, "30": {"m1": "\u0646\u0628\u0647\u062a\u0647\u0627 \u0628\u0627\u0644\u0634\u0647\u0628 \u0641\u06cc \u0627\u0641\u0644\u0627\u06a9\u0647\u0627", "m2": "\u0647\u0630\u0627 \u0646\u0648\u0627 \u0627\u0644\u06cc\u0644 \u0639\u0644\u06cc \u0627\u0644\u062d\u0631\u0628\u0627\u0621"}, "31": {"m1": "\u0634\u06a9\u0644 \u0627\u0644\u0645\u062c\u0646 \u0642\u0644\u0628 \u0645\u062c\u0646 \u0630\u0648\u0627\u0644\u063a\u0646\u06cc", "m2": "\u06a9\u06cc\u0644\u0627 \u06cc\u0636\u0627\u0641 \u0628\u0633\u0647\u0645 \u06a9\u0644 \u062c\u0641\u0627\u0621"}, "32": {"m1": "\u0644\u06a9\u0646 \u0645\u062c\u0646 \u0627\u0644\u0642\u0644\u0628 \u0644\u0627 \u06cc\u062d\u0645\u06cc \u0627\u0630\u0627", "m2": "\u0642\u0644\u0628 \u0627\u0644\u0645\u062d\u0646 \u0639\u0644\u06cc\u0647 \u0642\u0644\u0628 \u0642\u0636\u0627\u0621"}, "33": {"m1": "\u062c\u0631\u0645 \u0635\u063a\u06cc\u0631 \u0634\u0627\u0646\u0647 \u0645\u062a\u0639\u0638\u0645", "m2": "\u06a9\u0627\u0644\u0642\u0644\u0628 \u0641\u06cc \u0635\u063a\u0631 \u0648 \u0639\u0638\u0645 \u062f\u0647\u0627\u0621"}, "34": {"m1": "\u0646\u0648\u0631 \u062c\u0645\u0627\u062f \u0633\u0627\u06a9\u0646 \u0645\u062a\u0642\u0627\u0628\u0633", "m2": "\u06a9\u0627\u0644\u0638\u0644 \u0648\u0627\u0646 \u0627\u062e\u0630\u0647 \u0628\u0628\u0646\u0627\u0621"}, "35": {"m1": "\u0633\u0647\u0644 \u06cc\u0645\u06cc\u0646\u0647\u0627 \u0648 \u0635\u0639\u0628 \u0628\u0646\u06cc\u0644\u0647\u0627", "m2": "\u0627\u0645\u0646 \u0627\u0644\u06a9\u062a\u0627\u0628 \u0628\u0645\u0639\u0631\u0629 \u0627\u0644\u0639\u0638\u0645\u0627\u0621"}, "36": {"m1": "\u0639\u0642\u062f\u062a \u0639\u0644\u06cc \u0633\u0627\u0642 \u0627\u0644\u062d\u0645\u0627\u0645 \u0635\u063a\u0627\u0631\u0647\u0627", "m2": "\u0644\u06a9\u0646 \u06cc\u0633\u0647\u0644 \u0627\u0635\u0639\u0628 \u0627\u0644\u0627\u0634\u06cc\u0627\u0621"}, "37": {"m1": "\u0645\u0627 \u0647\u0630\u0647 \u0627\u0644\u0639\u06cc\u0646 \u0627\u0644\u062a\u06cc \u0639\u0627\u06cc\u0646\u062a\u0647\u0627", "m2": "\u0627\u062e\u062a \u0627\u0644\u0646\u0647\u06cc \u0627\u0628\u0646\u062a \u0634\u0645\u0633 \u0633\u0645\u0627\u0621"}, "38": {"m1": "\u0644\u0627\u0628\u0644 \u0627\u0628\u0648\u0627\u0644\u0641\u0636\u0644 \u0627\u0644\u0645\u063a\u06cc\u062b \u0628\u0639\u06cc\u0634\u0647", "m2": "\u062d\u062a\u06cc \u06cc\u0639\u062f \u0639\u062f\u0627\u06cc \u0627\u0628\u0648\u0627\u0644\u0648\u0636\u0627\u0621"}, "39": {"m1": "\u062f\u0639 \u06a9\u06cc\u0633\u0647 \u0645\u062c\u0647\u0648\u0644\u0629 \u0647\u0648 \u0639\u0633\u062c\u062f", "m2": "\u0634\u0628\u06cc\u0647 \u0627\u0644\u06a9\u0648\u0627\u06a9\u0628 \u0648 \u0627\u0633\u0645\u0647 \u0627\u0644\u062c\u0648\u0632\u0627\u0621"}, "40": {"m1": "\u0645\u0628\u062f\u0627 \u0639\u0646\u0635\u0631\u0647 \u0627\u0635\u0641\u0647\u0627\u0646 \u0639\u0642\u0648\u062f\u0647", "m2": "\u0627\u062d\u062f\u06cc \u0648 \u0639\u06cc\u0646 \u0639\u0642\u0648\u062f\u0647 \u0627\u0639\u0637\u0627\u0621"}, "41": {"m1": "\u0631\u0648\u062d\u062a \u0645\u0647\u062c\u0629 \u0628\u0627\u0635\u0641\u0647\u0627\u0646 \u0628\u0645\u062f\u062d\u062a\u06cc", "m2": "\u0627\u0636\u0639\u0627\u0641 \u0645\u0627 \u0642\u062f\u0644\u06cc \u0628\u0647\u062c\u0627\u0621"}, "42": {"m1": "\u06a9\u062a\u0628 \u0627\u0644\u062e\u0644\u06cc\u0641\u0629 \u0644\u0644\u06a9\u0644\u0627\u0645 \u0648 \u0633\u06cc\u062f\u06cc", "m2": "\u0633\u0644\u0637\u0627\u0646 \u062a\u0627\u062c \u0627\u0644\u0639\u0644\u0645 \u0641\u06cc \u0627\u0644\u0627\u06a9\u0641\u0627\u0621"}, "43": {"m1": "\u0627\u0647\u062f\u06cc \u0644\u0647 \u0628\u0630\u06cc \u0627\u0644\u062e\u0644\u0627\u0641\u0629 \u0627\u0633\u0648\u062f\u0627", "m2": "\u0648 \u0633\u0648\u0627\u062f \u0628\u0639\u0636 \u0627\u0644\u0630\u06cc \u0644\u0644\u062e\u0644\u0641\u0627\u0621"}, "44": {"m1": "\u0642\u0631\u0636\u062a\u0647 \u0628\u0642\u0635\u06cc\u062f\u0629 \u0627\u0644\u0641\u06cc\u062a\u0647", "m2": "\u0648\u0627\u0644\u062e\u06cc\u0631 \u0641\u0631\u0637\u0646\u06cc \u0628\u062d\u0631\u0641 \u0627\u0644\u06cc\u0627\u0621"}, "45": {"m1": "\u06cc\u0639\u0646\u06cc \u0644\u0647 \u0627\u0644\u062a\u0642\u062f\u06cc\u0645 \u06a9\u0627\u0644\u0627\u0644\u0641 \u0627\u0644\u062a\u06cc", "m2": "\u0642\u0641\u06cc\u062a\u0647\u0627 \u0648 \u0627\u062a\u06cc \u0627\u0628\u0648\u0627\u0644\u0646\u0642\u0637\u0627\u0621"}, "46": {"m1": "\u0647\u0630\u0629 \u0627\u0644\u0642\u0635\u06cc\u062f\u0629 \u0639\u0635\u0628\u062a\u0647 \u0634\u0639\u0631\u0627\u062a\u06cc", "m2": "\u0648 \u0628\u062f\u062a \u0644\u0631\u0627\u0632\u06cc \u062d\u06cc\u0636\u0629 \u0627\u0644\u0634\u0639\u0631\u0627"}, "47": {"m1": "\u0627\u0631\u0627\u06cc\u062a \u062d\u06cc\u0636 \u0644\u0631\u0627\u06cc\u062a \u0634\u0628\u0627\u0628\u0647\u0627", "m2": "\u062c\u0648\u0641 \u0627\u0644\u0627\u0633\u0648\u062f \u0641\u06cc \u0633\u0648\u0627\u062f \u0627\u0644\u0647\u06cc\u062c\u0627\u0621"}, "48": {"m1": "\u0627\u0633\u062f \u0627\u0644\u0633\u0645\u0627\u0621 \u0627\u0630\u0627 \u0637\u0627\u0644 \u0630\u0631\u0627\u0639\u0647", "m2": "\u0641\u0635\u0631\u062a \u0644\u062c\u0628\u0647\u062a\u0647 \u0628\u0631\u0627 \u0627\u0644\u0639\u0648\u062c\u0627\u0621"}, "49": {"m1": "\u0642\u0644\u0645\u06cc \u06a9\u0645\u0646\u0642\u0627\u0631 \u0627\u0644\u062d\u0645\u0627\u0645 \u0628\u0631\u0627\u0633\u0647", "m2": "\u0647\u0644\u06a9 \u0627\u0644\u063a\u0631\u0627\u0628 \u0648 \u0645\u0646\u0637\u0642 \u0627\u0644\u0628\u0628\u063a\u0627\u0621"}, "50": {"m1": "\u0644\u0648\u0645\u0633\u0647 \u0627\u0644\u0637\u0627\u0626\u06cc \u06cc\u0635\u06cc\u0631 \u0639\u0632\u0645\u0647", "m2": "\u0635\u062f\u0642 \u0627\u0644\u063a\u0631\u0627\u0628 \u0645\u062a\u06cc \u0627\u0644\u0627\u0634\u06cc\u0627\u0621"}, "51": {"m1": "\u0636\u0645\u0646\u062a \u0646\u0635\u0641 \u0627\u0644\u0628\u06cc\u062a \u0644\u0644\u0637\u0627\u0626\u06cc \u0648\u0647\u0627", "m2": "\u0648 \u0633\u0645\u062a \u0628\u0627\u0633\u0645 \u0627\u0644\u062d\u062a\u0631\u06cc \u0627\u0644\u0637\u0627\u0626\u06cc"}, "52": {"m1": "\u0627\u0638\u0646\u0646\u062a \u062d\u062a\u06cc \u06a9\u062f\u062a \u0627\u0639\u0631\u0642 \u062e\u062c\u0644\u0629", "m2": "\u0641\u06cc \u0646\u0635\u0641\u0647 \u0627\u0644\u0645\u062d\u0631\u0645 \u0644\u0644\u0631\u062e\u0635\u0627\u0621"}, "53": {"m1": "\u0646\u0641\u0633\u06cc \u06a9\u062a\u0628\u062a \u0648 \u0627\u0646 \u0627\u0641\u0634\u06cc\u062a\u0647\u0627", "m2": "\u0645\u0646 \u062e\u062c\u0644\u062a\u06cc \u062a\u0645\u0634\u06cc \u0639\u0644\u06cc \u0627\u0633\u062a\u062d\u06cc\u0627\u0621"}, "54": {"m1": "\u062f\u0627\u0645\u062a \u062c\u0644\u0627\u0644 \u0627\u0644\u062e\u06cc\u0631 \u0648\u0627\u0641\u06cc\u0647 \u0627\u0644\u0647\u0648\u06cc", "m2": "\u0648 \u0648\u0642\u0627\u0647 \u0627\u0644\u0627\u0644\u0647 \u0627\u062c\u0644 \u0648\u0642\u0627\u0621"}, "55": {"m1": "\u06cc\u0627 \u0641\u0627\u0636\u0644 \u0627\u0644\u062d\u0631\u0645\u0627\u0646 \u0628\u06a9\u0644 \u0645\u0648\u0637\u0646", "m2": "\u0645\u0627 \u0637\u0627\u0648\u0644 \u0627\u0644\u0647\u0631\u0645\u0627\u0646 \u06a9\u0644 \u0628\u0646\u0627\u0621"}}}