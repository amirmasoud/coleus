{"text": {"0": {"m1": "\u0645\u06af\u0631 \u0686\u0648\u0646 \u0631\u0627\u0628\u0639\u0647 \u0635\u0627\u062d\u0628 \u0645\u0642\u0627\u0645\u06cc", "m2": "\u0646\u062e\u0648\u0631\u062f\u0647 \u0628\u0648\u062f \u06cc\u06a9 \u0647\u0641\u062a\u0647 \u0637\u0639\u0627\u0645\u06cc"}, "1": {"m1": "\u062f\u0631\u0627\u0646 \u06cc\u06a9 \u0647\u0641\u062a\u0647 \u0647\u06cc\u0686 \u0627\u0632 \u067e\u0627\u06cc \u0646\u0646\u0634\u0633\u062a", "m2": "\u0635\u0644\u0648\u0629 \u0648\u0635\u0648\u0645 \u0628\u0648\u062f\u0634 \u06a9\u0627\u0631 \u067e\u06cc\u0648\u0633\u062a"}, "2": {"m1": "\u0686\u0648 \u062c\u0648\u0639 \u0627\u0641\u062a\u0627\u062f\u06af\u06cc \u062f\u0631 \u067e\u0627\u06cc\u0634 \u0622\u0648\u0631\u062f", "m2": "\u0634\u06a9\u0633\u062a\u06cc \u0633\u062e\u062a \u062f\u0631 \u0627\u0639\u0636\u0627\u06cc\u0634 \u0622\u0648\u0631\u062f"}, "3": {"m1": "\u06cc\u06a9\u06cc \u0645\u0633\u062a\u0648\u0631\u0647 \u0628\u0648\u062f\u0634 \u062f\u0631 \u062d\u0648\u0627\u0644\u06cc", "m2": "\u0637\u0639\u0627\u0645\u0634 \u06a9\u0627\u0633\u0647\u0654 \u0622\u0648\u0631\u062f \u062d\u0627\u0644\u06cc"}, "4": {"m1": "\u0645\u06af\u0631 \u0634\u062f \u0631\u0627\u0628\u0639\u0647 \u062f\u0631 \u062f\u0631\u062f \u0648\u062f\u0627\u063a\u06cc", "m2": "\u06a9\u0647 \u062a\u0627 \u062f\u0631 \u06af\u06cc\u0631\u062f\u0627\u0632 \u062c\u0627\u0626\u06cc \u0686\u0631\u0627\u063a\u06cc"}, "5": {"m1": "\u0686\u0648 \u0628\u0627\u0632 \u0622\u0645\u062f \u0645\u06af\u0631 \u06cc\u06a9 \u06af\u0631\u0628\u0647 \u0646\u0627\u06af\u0627\u0647", "m2": "\u0641\u06a9\u0646\u062f\u0647 \u0628\u0648\u062f \u067e\u0633\u062a \u0622\u0646 \u06a9\u0627\u0633\u0647 \u062f\u0631 \u0631\u0627\u0647"}, "6": {"m1": "\u062f\u06af\u0631 \u0628\u0627\u0631\u0647 \u0628\u0631\u0641\u062a \u0627\u0632 \u0628\u0647\u0631 \u06a9\u0648\u0632\u0647", "m2": "\u06a9\u0647 \u062a\u0627 \u0628\u06af\u0634\u0627\u06cc\u062f \u0622\u0646 \u062f\u0644 \u062a\u0646\u06af \u0631\u0648\u0632\u0647"}, "7": {"m1": "\u0628\u06cc\u0641\u062a\u0627\u062f \u0622\u0646 \u0632\u0645\u0627\u0646\u0634 \u06a9\u0648\u0632\u0647 \u0627\u0632 \u062f\u0633\u062a", "m2": "\u062c\u06af\u0631 \u062a\u0634\u0646\u0647 \u0628\u0645\u0627\u0646\u062f \u0648 \u06a9\u0648\u0632\u0647 \u0628\u0634\u06a9\u0633\u062a"}, "8": {"m1": "\u0632 \u062f\u0644 \u0622\u0647\u06cc \u0628\u0631\u0622\u0648\u0631\u062f \u0622\u0646 \u062c\u06af\u0631 \u0633\u0648\u0632", "m2": "\u06a9\u0647 \u06af\u0641\u062a\u06cc \u06af\u0634\u062a \u0639\u0627\u0644\u0645 \u0622\u062a\u0634 \u0627\u0641\u0631\u0648\u0632"}, "9": {"m1": "\u0628\u0635\u062f \u0633\u0631\u06af\u0634\u062a\u06af\u06cc \u0645\u06cc\u200c\u06af\u0641\u062a \u0627\u0644\u0647\u06cc", "m2": "\u0627\u0632\u06cc\u0646 \u0628\u06cc\u0686\u0627\u0631\u0647\u0654 \u0645\u0633\u06a9\u06cc\u0646 \u0686\u0647 \u062e\u0648\u0627\u0647\u06cc"}, "10": {"m1": "\u0641\u06a9\u0646\u062f\u06cc \u062f\u0631 \u067e\u0631\u06cc\u0634\u0627\u0646\u06cc \u0645\u0631\u0627 \u062a\u0648", "m2": "\u0628\u062e\u0648\u0646 \u062f\u0631\u0686\u0646\u062f \u06af\u0631\u062f\u0627\u0646\u06cc \u0645\u0631\u0627 \u062a\u0648"}, "11": {"m1": "\u062e\u0637\u0627\u0628 \u0622\u0645\u062f \u06a9\u0647 \u06af\u0631 \u0627\u06cc\u0646 \u0644\u062d\u0638\u0647 \u062e\u0648\u0627\u0647\u06cc", "m2": "\u0628\u062a\u0648 \u0628\u062e\u0634\u0645 \u0645\u0646 \u0627\u0632 \u0645\u0647 \u062a\u0627 \u0628\u0645\u0627\u0647\u06cc"}, "12": {"m1": "\u0648\u0644\u06cc \u0627\u0646\u062f\u0648\u0647 \u0686\u0646\u062f\u06cc\u0646 \u0633\u0627\u0644\u0647\u0654 \u062e\u0648\u06cc\u0634", "m2": "\u0632 \u062f\u0644 \u0628\u06cc\u0631\u0648\u0646 \u0628\u0631\u06cc\u0645\u062a \u0627\u06cc\u0646 \u0628\u06cc\u0646\u062f\u06cc\u0634"}, "13": {"m1": "\u06a9\u0647 \u0627\u0646\u062f\u0648\u0647 \u0645\u0646 \u0648 \u062f\u0646\u06cc\u0627\u06cc \u0645\u062d\u062a\u0627\u0644", "m2": "\u0646\u06cc\u0627\u06cc\u062f \u062c\u0645\u0639 \u062f\u0631 \u06cc\u06a9 \u062f\u0644 \u0628\u0635\u062f \u0633\u0627\u0644"}, "14": {"m1": "\u06af\u0631\u062a \u0627\u0646\u062f\u0648\u0647 \u0645\u0627 \u0628\u0627\u06cc\u062f \u0647\u0645\u06cc\u0634\u0647", "m2": "\u0645\u062f\u0627\u0645\u062a \u062a\u0631\u06a9 \u062f\u0646\u06cc\u0627 \u0628\u0627\u062f \u067e\u06cc\u0634\u0647"}, "15": {"m1": "\u062a\u0631\u0627 \u062a\u0627 \u0647\u0633\u062a \u0627\u06cc\u0646 \u06cc\u06a9 \u0631\u0648\u06cc \u0622\u0646 \u0646\u06cc\u0633\u062a", "m2": "\u06a9\u0647 \u0627\u0646\u062f\u0648\u0647 \u0627\u0644\u0647\u06cc \u0631\u0627\u06cc\u06af\u0627\u0646 \u0646\u06cc\u0633\u062a"}}}