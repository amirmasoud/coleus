{"text": {"0": {"p": "\u062f\u0631 \u0628\u06cc\u0627\u0646 \u0634\u0631\u0630\u0645\u0647\u200c\u06cc\u06cc \u0627\u0632 \u0645\u0642\u0627\u0645\u0627\u062a \u0648 \u0645\u062c\u0645\u0648\u0639\u0647\u200c\u06cc\u06cc \u0627\u0632 \u06a9\u0631\u0627\u0645\u0627\u062a \u0642\u062f\u0648\u0629 \u0627\u0644\u0646\u0642\u0628\u0627\u0621 \u0648 \u0646\u062e\u0628\u0629 \u0627\u0644\u0646\u062c\u0628\u0627\u0621 \u062c\u0646\u0627\u0628 \u0642\u0627\u0633\u0645 \u0633\u0644\u0627\u0645 \u0627\u0644\u0644\u0651\u0647 \u0639\u0644\u06cc\u0647:"}, "1": {"m1": "\u0628\u0627\u0632 \u062f\u0627\u0631\u0645\u061b \u0631\u0627\u062d\u062a \u0648 \u0631\u0646\u062c\u06cc \u0628\u0647\u0645", "m2": "\u0645\u062a\u062d\u062f \u0639\u0646\u0648\u0627\u0646\u06cc \u0627\u0632 \u0634\u0627\u062f\u06cc \u0648 \u063a\u0645"}, "2": {"m1": "\u0646\u0627\u0632 \u067e\u0631\u0648\u0631 \u0646\u0648\u0639\u0631\u0648\u0633\u06cc \u0647\u0633\u062a \u0628\u06a9\u0631", "m2": "\u0645\u0631\u0645\u0631\u0627 \u062f\u0631 \u062d\u062c\u0644\u0647\u200c\u06cc \u0646\u0627\u0645\u0648\u0633 \u0641\u06a9\u0631"}, "3": {"m1": "\u0646\u0648\u0639\u0631\u0648\u0633\u06cc\u060c \u0646\u0642\u062f \u062c\u0627\u0646\u0634\u060c \u0631\u0648\u0646\u0645\u0627", "m2": "\u062a\u0627 \u0646\u06af\u06cc\u0631\u062f\u060c \u06a9\u06cc \u0646\u0645\u0627\u06cc\u062f \u0631\u0648 \u0628\u0645\u0627!!"}, "4": {"m1": "\u062a\u0627 \u06a9\u06cc \u0627\u0646\u062f\u0631 \u062d\u062c\u0644\u0647 \u0645\u0627\u0646\u062f \u0627\u06cc\u0646 \u0639\u0631\u0648\u0633", "m2": "\u062f\u0644 \u0686\u0648 \u062f\u0627\u0645\u0627\u062f \u0627\u0632 \u0641\u0631\u0627\u0642\u0634 \u062f\u0631 \u0641\u0633\u0648\u0633!"}, "5": {"m1": "\u0632\u06cc\u0646 \u0639\u0631\u0648\u0633\u0645\u060c \u0645\u062f\u0639\u0627\u062f\u0627\u0646\u06cc \u06a9\u0647 \u0686\u06cc\u0633\u062a\u061f", "m2": "\u0645\u062f\u0639\u0627 \u0631\u0627 \u0631\u0648\u06cc \u0645\u06cc\u062f\u0627\u0646\u06cc \u0628\u0647 \u06a9\u06cc\u0633\u062a\u061f"}, "6": {"m1": "\u0628\u0627 \u0639\u0631\u0648\u0633 \u0642\u0627\u0633\u0645 \u0627\u06cc\u0646\u062c\u0627 \u0647\u0633\u062a \u0631\u0648", "m2": "\u0645\u062f\u0639\u0627\u06cc\u0645 \u062c\u0645\u0644\u0647 \u0628\u0627\u0634\u062f\u060c \u0630\u06a9\u0631 \u0627\u0648"}, "7": {"m1": "\u0627\u0646\u062f\u0631 \u0622\u0646 \u0631\u0648\u0632\u06cc \u06a9\u0647 \u0628\u0648\u062f \u0627\u0632 \u0645\u0627\u062c\u0631\u0627", "m2": "\u06a9\u0631\u0628\u0644\u0627 \u0628\u0631 \u0639\u0627\u0634\u0642\u0627\u0646\u061b \u0645\u0627\u062a\u0645\u0633\u0631\u0627"}, "8": {"m1": "\u062e\u0648\u0627\u0646\u062f \u0634\u0627\u0647 \u062f\u06cc\u0646\u060c \u0628\u0631\u0627\u062f\u0631\u0632\u0627\u062f\u0647 \u0631\u0627", "m2": "\u0634\u0645\u0639 \u0627\u06cc\u0645\u0627\u0646\u061b \u0642\u0627\u0633\u0645 \u0622\u0632\u0627\u062f\u0647 \u0631\u0627"}, "9": {"m1": "\u0648\u0632\u062f\u06af\u0631 \u0631\u0647\u060c \u062f\u062e\u062a\u0631 \u062e\u0648\u062f \u067e\u06cc\u0634 \u062e\u0648\u0627\u0646\u062f", "m2": "\u062e\u0637\u0628\u06c0 \u0622\u0646 \u0647\u0631 \u062f\u0648 \u0648\u062d\u062f\u062a \u06a9\u06cc\u0634 \u062e\u0648\u0627\u0646\u062f"}, "10": {"m1": "\u0622\u0646\u0686\u0647 \u0642\u0627\u0633\u0645 \u0631\u0627\u0632 \u0647\u0633\u062a\u06cc \u0628\u0648\u062f \u0646\u0642\u062f", "m2": "\u0645\u0631 \u0639\u0631\u0648\u0633\u0634 \u0631\u0627 \u0628\u06a9\u0627\u0628\u06cc\u0646 \u0628\u0633\u062a \u0639\u0642\u062f"}, "11": {"m1": "\u0637\u0627\u0644\u0628 \u0648 \u0645\u0637\u0644\u0648\u0628 \u0631\u0627 \u062f\u0645\u0633\u0627\u0632 \u06a9\u0631\u062f", "m2": "\u0632\u0647\u0631\u0647 \u0631\u0627 \u0628\u0627 \u0645\u0634\u062a\u0631\u06cc \u0627\u0646\u0628\u0627\u0632 \u06a9\u0631\u062f"}, "12": {"m1": "\u0647\u0631 \u062f\u0648 \u0631\u0627 \u0631\u0633\u0645 \u0631\u0636\u0627\u060c \u062a\u0639\u0644\u06cc\u0645 \u062f\u0627\u062f", "m2": "\u062c\u0627\u06cc\u061b \u0627\u0646\u062f\u0631 \u062d\u062c\u0644\u0647\u200c\u06cc \u062a\u0633\u0644\u06cc\u0645 \u062f\u0627\u062f"}, "13": {"m1": "\u0644\u06cc\u06a9 \u062c\u0627 \u0646\u06af\u0631\u0641\u062a\u0647 \u062f\u0627\u0645\u0627\u062f \u0648 \u0639\u0631\u0648\u0633", "m2": "\u06a9\u0632 \u062b\u0631\u06cc \u0634\u062f \u0628\u0631 \u062b\u0631\u06cc\u0627 \u0628\u0627\u0646\u06af \u06a9\u0648\u0633"}, "14": {"m1": "\u06a9\u0627\u06cc \u0642\u062f\u062d \u0646\u0648\u0634\u0627\u0646 \u0635\u0647\u0628\u0627\u06cc \u0627\u0644\u0633\u062a", "m2": "\u0627\u0632 \u0645\u0631\u0627\u062f \u062e\u0648\u06cc\u0634\u062a\u0646 \u0634\u0648\u06cc\u06cc\u062f \u062f\u0633\u062a"}, "15": {"m1": "\u06a9\u0634\u062a\u0647 \u06af\u0634\u062a\u0646 \u0639\u0627\u062f\u062a \u062c\u06cc\u0634 \u0634\u0645\u0627\u0633\u062a", "m2": "\u0646\u0627\u0645\u0631\u0627\u062f\u06cc\u060c \u0628\u0647\u062a\u0631\u06cc\u0646 \u0639\u06cc\u0634 \u0634\u0645\u0627 \u0627\u0633\u062a"}, "16": {"m1": "\u0622\u0631\u0632\u0648 \u0631\u0627 \u062a\u0631\u06a9 \u06af\u0641\u062a\u0646\u060c \u062e\u0648\u0634\u062a\u0631\u0633\u062a", "m2": "\u0628\u0627 \u0639\u0631\u0648\u0633 \u0645\u0631\u06af \u062e\u0641\u062a\u0646\u060c \u062e\u0648\u0634\u062a\u0631\u0633\u062a"}, "17": {"m1": "\u06a9\u06cc \u062e\u0636\u0627\u0628 \u062f\u0633\u062a\u062a\u0627\u0646 \u0628\u0627\u0634\u062f \u0635\u0648\u0627\u0628\u061f", "m2": "\u062f\u0633\u062a \u0639\u0627\u0634\u0642 \u0631\u0627\u0632 \u062e\u0648\u0646 \u0628\u0627\u06cc\u062f \u062e\u0636\u0627\u0628"}, "18": {"m1": "\u0627\u06cc\u0646 \u0635\u062f\u0627 \u0622\u0645\u062f \u0686\u0648 \u0642\u0627\u0633\u0645 \u0631\u0627\u060c \u0628\u06af\u0648\u0634", "m2": "\u0634\u062f \u0632 \u063a\u06cc\u0631\u062a \u0648\u0632 \u062a\u063a\u06cc\u0631 \u062f\u0631 \u062e\u0631\u0648\u0634"}, "19": {"m1": "\u062e\u0627\u0633\u062a \u0627\u0632 \u062c\u0627 \u0648 \u0639\u0631\u0648\u0633 \u0645\u0642\u0628\u0644\u0634", "m2": "\u062f\u0633\u062a \u062d\u0633\u0631\u062a \u0632\u062f \u0628\u062f\u0627\u0645\u0627\u0646 \u062f\u0644\u0634"}, "20": {"m1": "\u0631\u0627\u0647\u0631\u0648 \u0631\u0627 \u067e\u0627\u06cc \u0627\u0632 \u0631\u0641\u062a\u0627\u0631 \u0645\u0627\u0646\u062f", "m2": "\u062f\u0644 \u0632 \u0647\u0645\u0631\u0627\u0647\u06cc \u0648 \u062f\u0633\u062a \u0627\u0632 \u06a9\u0627\u0631\u060c \u0645\u0627\u0646\u062f"}, "21": {"m1": "\u06af\u0641\u062a \u0627\u0632 \u067e\u06cc\u0634 \u0645\u0646 \u0627\u06cc \u0628\u062f\u0631\u062f\u062c\u06cc", "m2": "\u0686\u0648\u0646 \u0628\u0631\u0641\u062a\u06cc\u060c \u0628\u06cc\u0646\u0645\u062a \u062f\u06cc\u06af\u0631 \u06a9\u062c\u0627\u061f"}, "22": {"m1": "\u0646\u0648\u0639\u0631\u0648\u0633 \u062e\u0648\u06cc\u0634 \u0631\u0627\u060c \u0628\u0648\u0633\u06cc\u062f \u0686\u0647\u0631", "m2": "\u062e\u0648\u0634 \u062f\u0631 \u0622\u063a\u0648\u0634\u0634 \u06a9\u0634\u06cc\u062f \u0627\u0632 \u0631\u0648\u06cc \u0645\u0647\u0631"}, "23": {"m1": "\u0632 \u0622\u0633\u062a\u06cc\u0646 \u0627\u0634\u06a9\u0634 \u0632 \u0686\u0634\u0645\u0627\u0646 \u067e\u0627\u06a9 \u06a9\u0631\u062f", "m2": "\u0628\u0639\u062f \u0627\u0632 \u0622\u0646 \u0622\u0646 \u0622\u0633\u062a\u06cc\u0646 \u0631\u0627\u060c \u0686\u0627\u06a9 \u06a9\u0631\u062f"}, "24": {"m1": "\u06af\u0641\u062a: \u062f\u0631 \u0641\u0631\u062f\u0648\u0633 \u0686\u0648\u0646 \u06a9\u0631\u062f\u06cc\u0645 \u0631\u0648", "m2": "\u0645\u0631 \u0645\u0631\u0627 \u0628\u0627 \u0627\u06cc\u0646 \u0646\u0634\u0627\u0646\u060c \u0622\u0646\u062c\u0627 \u0628\u062c\u0648"}}}