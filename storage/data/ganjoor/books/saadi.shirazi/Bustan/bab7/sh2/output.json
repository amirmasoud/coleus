{"text": {"0": {"m1": "\u0627\u06af\u0631 \u067e\u0627\u06cc \u062f\u0631 \u062f\u0627\u0645\u0646 \u0622\u0631\u06cc \u0686\u0648 \u06a9\u0648\u0647", "m2": "\u0633\u0631\u062a \u0632 \u0622\u0633\u0645\u0627\u0646 \u0628\u06af\u0630\u0631\u062f \u062f\u0631 \u0634\u06a9\u0648\u0647"}, "1": {"m1": "\u0632\u0628\u0627\u0646 \u062f\u0631\u06a9\u0634 \u0627\u06cc \u0645\u0631\u062f \u0628\u0633\u06cc\u0627\u0631 \u062f\u0627\u0646", "m2": "\u06a9\u0647 \u0641\u0631\u062f\u0627 \u0642\u0644\u0645 \u0646\u06cc\u0633\u062a \u0628\u0631 \u0628\u06cc \u0632\u0628\u0627\u0646"}, "2": {"m1": "\u0635\u062f\u0641 \u0648\u0627\u0631 \u06af\u0648\u0647\u0631\u0634\u0646\u0627\u0633\u0627\u0646 \u0631\u0627\u0632", "m2": "\u062f\u0647\u0627\u0646 \u062c\u0632 \u0628\u0647 \u0644\u0624\u0644\u0624 \u0646\u06a9\u0631\u062f\u0646\u062f \u0628\u0627\u0632"}, "3": {"m1": "\u0641\u0631\u0648\u0627\u0646 \u0633\u062e\u0646 \u0628\u0627\u0634\u062f \u0622\u06af\u0646\u062f\u0647 \u06af\u0648\u0634", "m2": "\u0646\u0635\u06cc\u062d\u062a \u0646\u06af\u06cc\u0631\u062f \u0645\u06af\u0631 \u062f\u0631 \u062e\u0645\u0648\u0634"}, "4": {"m1": "\u0686\u0648 \u062e\u0648\u0627\u0647\u06cc \u06a9\u0647 \u06af\u0648\u06cc\u06cc \u0646\u0641\u0633 \u0628\u0631 \u0646\u0641\u0633", "m2": "\u0646\u062e\u0648\u0627\u0647\u06cc \u0634\u0646\u06cc\u062f\u0646 \u0645\u06af\u0631 \u06af\u0641\u062a \u06a9\u0633\u061f"}, "5": {"m1": "\u0646\u0628\u0627\u06cc\u062f \u0633\u062e\u0646 \u06af\u0641\u062a \u0646\u0627\u0633\u0627\u062e\u062a\u0647", "m2": "\u0646\u0634\u0627\u06cc\u062f \u0628\u0631\u06cc\u062f\u0646 \u0646\u06cc\u0646\u062f\u0627\u062e\u062a\u0647"}, "6": {"m1": "\u062a\u0623\u0645\u0644 \u06a9\u0646\u0627\u0646 \u062f\u0631 \u062e\u0637\u0627 \u0648 \u0635\u0648\u0627\u0628", "m2": "\u0628\u0647 \u0627\u0632 \u0698\u0627\u0698\u062e\u0627\u06cc\u0627\u0646 \u062d\u0627\u0636\u0631 \u062c\u0648\u0627\u0628"}, "7": {"m1": "\u06a9\u0645\u0627\u0644 \u0627\u0633\u062a \u062f\u0631 \u0646\u0641\u0633 \u0627\u0646\u0633\u0627\u0646 \u0633\u062e\u0646", "m2": "\u062a\u0648 \u062e\u0648\u062f \u0631\u0627 \u0628\u0647 \u06af\u0641\u062a\u0627\u0631 \u0646\u0627\u0642\u0635 \u0645\u06a9\u0646"}, "8": {"m1": "\u06a9\u0645 \u0622\u0648\u0627\u0632 \u0647\u0631\u06af\u0632 \u0646\u0628\u06cc\u0646\u06cc \u062e\u062c\u0644", "m2": "\u062c\u0648\u06cc \u0645\u0634\u06a9 \u0628\u0647\u062a\u0631 \u06a9\u0647 \u06cc\u06a9 \u062a\u0648\u062f\u0647 \u06af\u0644"}, "9": {"m1": "\u062d\u0630\u0631 \u06a9\u0646 \u0632 \u0646\u0627\u062f\u0627\u0646 \u062f\u0647 \u0645\u0631\u062f\u0647 \u06af\u0648\u06cc", "m2": "\u0686\u0648 \u062f\u0627\u0646\u0627 \u06cc\u06a9\u06cc \u06af\u0648\u06cc \u0648 \u067e\u0631\u0648\u0631\u062f\u0647 \u06af\u0648\u06cc"}, "10": {"m1": "\u0635\u062f \u0627\u0646\u062f\u0627\u062e\u062a\u06cc \u062a\u06cc\u0631 \u0648 \u0647\u0631 \u0635\u062f \u062e\u0637\u0627\u0633\u062a", "m2": "\u0627\u06af\u0631 \u0647\u0648\u0634\u0645\u0646\u062f\u06cc \u06cc\u06a9 \u0627\u0646\u062f\u0627\u0632 \u0648 \u0631\u0627\u0633\u062a"}, "11": {"m1": "\u0686\u0631\u0627 \u06af\u0648\u06cc\u062f \u0622\u0646 \u0686\u06cc\u0632 \u062f\u0631 \u062e\u0641\u06cc\u0647 \u0645\u0631\u062f", "m2": "\u06a9\u0647 \u06af\u0631 \u0641\u0627\u0634 \u06af\u0631\u062f\u062f \u0634\u0648\u062f \u0631\u0648\u06cc \u0632\u0631\u062f\u061f"}, "12": {"m1": "\u0645\u06a9\u0646 \u067e\u06cc\u0634 \u062f\u06cc\u0648\u0627\u0631 \u063a\u06cc\u0628\u062a \u0628\u0633\u06cc", "m2": "\u0628\u0648\u062f \u06a9\u0632 \u067e\u0633\u0634 \u06af\u0648\u0634 \u062f\u0627\u0631\u062f \u06a9\u0633\u06cc"}, "13": {"m1": "\u062f\u0631\u0648\u0646 \u062f\u0644\u062a \u0634\u0647\u0631 \u0628\u0646\u062f\u0633\u062a \u0631\u0627\u0632", "m2": "\u0646\u06af\u0631 \u062a\u0627 \u0646\u0628\u06cc\u0646\u062f \u062f\u0631 \u0634\u0647\u0631 \u0628\u0627\u0632"}, "14": {"m1": "\u0627\u0632\u0627\u0646 \u0645\u0631\u062f \u062f\u0627\u0646\u0627 \u062f\u0647\u0627\u0646 \u062f\u0648\u062e\u062a\u0647\u200c\u0633\u062a", "m2": "\u06a9\u0647 \u0628\u06cc\u0646\u062f \u06a9\u0647 \u0634\u0645\u0639 \u0627\u0632 \u0632\u0628\u0627\u0646 \u0633\u0648\u062e\u062a\u0647\u200c\u0633\u062a"}}}