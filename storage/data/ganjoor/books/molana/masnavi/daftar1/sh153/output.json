{"text": {"0": {"m1": "\u0686\u0648\u0646 \u06af\u0646\u0627\u0647 \u0648 \u0641\u0633\u0642 \u062e\u0644\u0642\u0627\u0646 \u062c\u0647\u0627\u0646", "m2": "\u0645\u06cc\u200c\u0634\u062f\u06cc \u0628\u0631 \u0647\u0631 \u062f\u0648 \u0631\u0648\u0634\u0646 \u0622\u0646 \u0632\u0645\u0627\u0646"}, "1": {"m1": "\u062f\u0633\u062a \u062e\u0627\u06cc\u06cc\u062f\u0646 \u06af\u0631\u0641\u062a\u0646\u062f\u06cc \u0632 \u062e\u0634\u0645", "m2": "\u0644\u06cc\u06a9 \u0639\u06cc\u0628 \u062e\u0648\u062f \u0646\u062f\u06cc\u062f\u0646\u062f\u06cc \u0628\u0647 \u0686\u0634\u0645"}, "2": {"m1": "\u062e\u0648\u06cc\u0634 \u062f\u0631 \u0622\u06cc\u06cc\u0646\u0647 \u062f\u06cc\u062f \u0622\u0646 \u0632\u0634\u062a \u0645\u0631\u062f", "m2": "\u0631\u0648 \u0628\u06af\u0631\u062f\u0627\u0646\u06cc\u062f \u0627\u0632 \u0622\u0646 \u0648 \u062e\u0634\u0645 \u06a9\u0631\u062f"}, "3": {"m1": "\u062e\u0648\u06cc\u0634\u200c\u0628\u06cc\u0646 \u0686\u0648\u0646 \u0627\u0632 \u06a9\u0633\u06cc \u062c\u0631\u0645\u06cc \u0628\u062f\u06cc\u062f", "m2": "\u0622\u062a\u0634\u06cc \u062f\u0631 \u0648\u06cc \u0632 \u062f\u0648\u0632\u062e \u0634\u062f \u067e\u062f\u06cc\u062f"}, "4": {"m1": "\u062d\u0645\u06cc\u062a \u062f\u06cc\u0646 \u062e\u0648\u0627\u0646\u062f \u0627\u0648 \u0622\u0646 \u06a9\u0628\u0631 \u0631\u0627", "m2": "\u0646\u0646\u06af\u0631\u062f \u062f\u0631 \u062e\u0648\u06cc\u0634 \u0646\u0641\u0633 \u06af\u0628\u0631 \u0631\u0627"}, "5": {"m1": "\u062d\u0645\u06cc\u062a \u062f\u06cc\u0646 \u0631\u0627 \u0646\u0634\u0627\u0646\u06cc \u062f\u06cc\u06af\u0631\u0633\u062a", "m2": "\u06a9\u0647 \u0627\u0632 \u0622\u0646 \u0622\u062a\u0634 \u062c\u0647\u0627\u0646\u06cc \u0627\u062e\u0636\u0631\u0633\u062a"}, "6": {"m1": "\u06af\u0641\u062a \u062d\u0642\u0634\u0627\u0646 \u06af\u0631 \u0634\u0645\u0627 \u0631\u0648\u0634\u0646 \u06af\u0631\u06cc\u062f", "m2": "\u062f\u0631 \u0633\u06cc\u0647\u200c\u06a9\u0627\u0631\u0627\u0646 \u0645\u063a\u0641\u0644 \u0645\u0646\u06af\u0631\u06cc\u062f"}, "7": {"m1": "\u0634\u06a9\u0631 \u06af\u0648\u06cc\u06cc\u062f \u0627\u06cc \u0633\u067e\u0627\u0647 \u0648 \u0686\u0627\u06a9\u0631\u0627\u0646", "m2": "\u0631\u0633\u062a\u0647\u200c\u0627\u06cc\u062f \u0627\u0632 \u0634\u0647\u0648\u062a \u0648 \u0627\u0632 \u0686\u0627\u06a9\u200c\u0631\u0627\u0646"}, "8": {"m1": "\u06af\u0631 \u0627\u0632 \u0622\u0646 \u0645\u0639\u0646\u06cc \u0646\u0647\u0645 \u0645\u0646 \u0628\u0631 \u0634\u0645\u0627", "m2": "\u0645\u0631 \u0634\u0645\u0627 \u0631\u0627 \u0628\u06cc\u0634 \u0646\u067e\u0630\u06cc\u0631\u062f \u0633\u0645\u0627"}, "9": {"m1": "\u0639\u0635\u0645\u062a\u06cc \u06a9\u0647 \u0645\u0631 \u0634\u0645\u0627 \u0631\u0627 \u062f\u0631 \u062a\u0646\u0633\u062a", "m2": "\u0622\u0646 \u0632 \u0639\u06a9\u0633 \u0639\u0635\u0645\u062a \u0648 \u062d\u0641\u0638 \u0645\u0646\u0633\u062a"}, "10": {"m1": "\u0622\u0646 \u0632 \u0645\u0646 \u0628\u06cc\u0646\u06cc\u062f \u0646\u0647 \u0627\u0632 \u062e\u0648\u062f \u0647\u06cc\u0646 \u0648 \u0647\u06cc\u0646", "m2": "\u062a\u0627 \u0646\u0686\u0631\u0628\u062f \u0628\u0631 \u0634\u0645\u0627 \u062f\u06cc\u0648 \u0644\u0639\u06cc\u0646"}, "11": {"m1": "\u0622\u0646\u0686\u0646\u0627\u0646 \u06a9\u0647 \u06a9\u0627\u062a\u0628 \u0648\u062d\u06cc \u0631\u0633\u0648\u0644", "m2": "\u062f\u06cc\u062f \u062d\u06a9\u0645\u062a \u062f\u0631 \u062e\u0648\u062f \u0648 \u0646\u0648\u0631 \u0627\u0635\u0648\u0644"}, "12": {"m1": "\u062e\u0648\u06cc\u0634 \u0631\u0627 \u0647\u0645 \u0635\u0648\u062a \u0645\u0631\u063a\u0627\u0646 \u062e\u062f\u0627", "m2": "\u0645\u06cc\u200c\u0634\u0645\u0631\u062f \u0622\u0646 \u0628\u062f \u0635\u0641\u06cc\u0631\u06cc \u0686\u0648\u0646 \u0635\u062f\u0627"}, "13": {"m1": "\u0644\u062d\u0646 \u0645\u0631\u063a\u0627\u0646 \u0631\u0627 \u0627\u06af\u0631 \u0648\u0627\u0635\u0641 \u0634\u0648\u06cc", "m2": "\u0628\u0631 \u0645\u0631\u0627\u062f \u0645\u0631\u063a \u06a9\u06cc \u0648\u0627\u0642\u0641 \u0634\u0648\u06cc"}, "14": {"m1": "\u06af\u0631 \u0628\u06cc\u0627\u0645\u0648\u0632\u06cc \u0635\u0641\u06cc\u0631 \u0628\u0644\u0628\u0644\u06cc", "m2": "\u062a\u0648 \u0686\u0647 \u062f\u0627\u0646\u06cc \u06a9\u0648 \u0686\u0647 \u062f\u0627\u0631\u062f \u0628\u0627 \u06af\u0644\u06cc"}, "15": {"m1": "\u0648\u0631 \u0628\u062f\u0627\u0646\u06cc \u0628\u0627\u0634\u062f \u0622\u0646 \u0647\u0645 \u0627\u0632 \u06af\u0645\u0627\u0646", "m2": "\u0686\u0648\u0646 \u0632 \u0644\u0628\u200c\u062c\u0646\u0628\u0627\u0646 \u06af\u0645\u0627\u0646\u0647\u0627\u06cc \u06a9\u0631\u0627\u0646"}}}