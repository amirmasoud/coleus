{"text": {"0": {"m1": "\u06af\u0641\u062a \u067e\u06cc\u0631 \u0631\u0647 \u06a9\u0647 \u0627\u0648 \u0628\u06cc\u062e\u0648\u062f \u0634\u062f\u0647", "m2": "\u062f\u0631 \u0631\u0647 \u0639\u0631\u0641\u0627\u0646 \u062d\u0642 \u0631\u0627\u0634\u062f \u0634\u062f\u0647"}, "1": {"m1": "\u0646\u0642\u0637\u0647\u0654 \u0633\u0631\u0651 \u0642\u0644\u0645 \u0628\u0627 \u0644\u0648\u062d \u06af\u0648\u06cc", "m2": "\u0628\u0639\u062f \u0627\u0632 \u0622\u0646 \u0646\u0642\u0634 \u0635\u0648\u0631 \u0627\u0632 \u0644\u0648\u062d \u0634\u0648\u06cc"}, "2": {"m1": "\u06af\u0641\u062a\u0645\u0634 \u0686\u0648\u0646 \u0639\u0644\u0645 \u062d\u0642 \u0622\u0645\u062f \u062f\u0631\u0648\u0646", "m2": "\u063a\u06cc\u0631 \u062d\u0642 \u0631\u0627 \u0627\u0632\u062f\u0644\u0645 \u06a9\u0631\u062f\u0645 \u0628\u0631\u0648\u0646"}, "3": {"m1": "\u0639\u0634\u0642 \u0628\u0627 \u0647\u0633\u062a\u06cc \u0645\u0646 \u0634\u062f \u0631\u0647\u0646\u0645\u0648\u0646", "m2": "\u062c\u0647\u062f \u06a9\u0646 \u0627\u0632 \u0647\u0633\u062a\u06cc \u062e\u0648\u062f \u0631\u0648 \u0628\u0631\u0648\u0646"}, "4": {"m1": "\u0645\u0646 \u0628\u06a9\u0644\u0651\u06cc \u062e\u0648\u06cc\u0634 \u0631\u0627 \u06a9\u0631\u062f\u0645 \u062a\u0628\u0627\u0647", "m2": "\u0686\u0648\u0646 \u0628\u062f\u06cc\u062f\u0645 \u0645\u0638\u0647\u0631 \u0630\u0627\u062a \u0627\u0644\u0627\u0647"}, "5": {"m1": "\u06cc\u06a9 \u0686\u0644\u0647 \u062f\u0631 \u067e\u06cc\u0634 \u0622\u0646 \u0633\u0644\u0637\u0627\u0646 \u0628\u062f\u0645", "m2": "\u062f\u0631 \u06a9\u0645\u0627\u0644 \u0633\u0631\u0651 \u0627\u0648 \u062d\u06cc\u0631\u0627\u0646 \u0634\u062f\u0645"}, "6": {"m1": "\u0622\u0646\u0686\u0647 \u06af\u0641\u062a \u0627\u0648 \u06af\u0648\u0634 \u06a9\u0631\u062f\u0645 \u0645\u0646 \u062a\u0645\u0627\u0645", "m2": "\u0628\u0631 \u062c\u0645\u0627\u0644 \u0634\u0627\u0647 \u0627\u0648 \u06a9\u0631\u062f\u0645 \u0633\u0644\u0627\u0645"}, "7": {"m1": "\u0622\u0646\u06af\u0647\u06cc \u0627\u0632 \u0648\u06cc \u0627\u062c\u0627\u0632\u062a \u062e\u0648\u0627\u0633\u062a\u0645", "m2": "\u062c\u0627\u0646 \u062e\u0648\u062f \u0627\u0632 \u0641\u06cc\u0636 \u0627\u0648 \u0622\u0631\u0627\u0633\u062a\u0645"}, "8": {"m1": "\u062c\u0645\u0644\u06af\u06cc \u0647\u0633\u062a\u06cc\u0651 \u062e\u0648\u062f \u06a9\u0631\u062f\u0645 \u062a\u0628\u0627\u0647", "m2": "\u062a\u0627 \u0631\u0633\u06cc\u062f\u0645 \u0645\u0646 \u0628\u062f\u0631\u06af\u0627\u0647 \u0627\u0644\u0647"}, "9": {"m1": "\u0647\u0631 \u06a9\u0647 \u0627\u0648 \u0631\u0627\u062f\u06cc\u062f \u062c\u0645\u0644\u0647 \u062d\u0642 \u0628\u062f\u06cc\u062f", "m2": "\u0628\u06cc\u0634\u06a9\u06cc \u0627\u0648 \u062f\u0631\u0645\u0642\u0627\u0645 \u062d\u0642 \u0631\u0633\u06cc\u062f"}, "10": {"m1": "\u0647\u0631 \u06a9\u0647 \u0627\u0648\u0631\u0627 \u062d\u0642 \u0628\u062f\u0627\u0646\u062f \u062d\u0642 \u0634\u0648\u062f", "m2": "\u0628\u06cc\u0634\u06a9\u06cc \u0627\u0648 \u062e\u0648\u062f \u062d\u0642 \u0645\u0637\u0644\u0642 \u0634\u0648\u062f"}, "11": {"m1": "\u0647\u0645\u0686\u0648 \u0645\u0646\u0635\u0648\u0631 \u0627\u0632 \u0627\u0646\u0627\u0627\u0644\u062d\u0642 \u062f\u0645 \u0632\u0646\u062f", "m2": "\u062c\u0645\u0644\u0647 \u0639\u0627\u0644\u0645 \u0631\u0627 \u0647\u0645 \u0627\u0648 \u0628\u0631\u0647\u0645 \u0632\u0646\u062f"}, "12": {"m1": "\u06a9\u0641\u0631 \u0648 \u0627\u06cc\u0645\u0627\u0646 \u0631\u0627 \u06af\u0630\u0627\u0631 \u0648 \u062d\u0642 \u0634\u0646\u0627\u0633", "m2": "\u062a\u0627 \u0646\u06af\u0631\u062f\u06cc \u062f\u0631 \u0631\u0647 \u062f\u06cc\u0646 \u0646\u0627\u0633\u067e\u0627\u0633"}, "13": {"m1": "\u0647\u0631 \u06a9\u0647 \u0627\u0648 \u0627\u0632 \u062f\u06cc\u0646 \u0627\u062d\u0645\u062f \u0631\u0648\u06cc \u062a\u0627\u0641\u062a", "m2": "\u0627\u0648 \u0628\u0686\u0627\u0647 \u0648\u06cc\u0644 \u0634\u06cc\u0637\u0627\u0646 \u0631\u0627\u0647 \u06cc\u0627\u0641\u062a"}, "14": {"m1": "\u0631\u0648 \u0632 \u0627\u062d\u0645\u062f \u067e\u0631\u0633 \u0633\u0631\u0651 \u0645\u0631\u062a\u0636\u06cc", "m2": "\u062d\u0642 \u0628\u0642\u0631\u0622\u0646 \u06af\u0641\u062a\u0647 \u0628\u0627 \u0627\u0648 \u0647\u0644 \u0627\u062a\u06cc"}, "15": {"m1": "\u062a\u0648 \u0686\u0647 \u062f\u0627\u0646\u06cc \u0633\u0631\u0651 \u0627\u06cc\u0646 \u062f\u0631\u06cc\u0627\u06cc \u062f\u06cc\u0646", "m2": "\u0627\u0648 \u06cc\u062f\u0627\u0644\u0644\u0647 \u0627\u0633\u062a \u062f\u0631\u0639\u06cc\u0646 \u0627\u0644\u06cc\u0642\u06cc\u0646"}}}