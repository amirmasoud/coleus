{"text": {"0": {"m1": "\u0632\u062d\u0645\u062a \u0622\u0628 \u0648 \u06af\u0650\u0644 \u062f\u0631 \u0627\u06cc\u0646 \u0639\u0627\u0644\u0645", "m2": "\u0631\u062d\u0645\u062a\u0634 \u0646\u0627\u0645 \u06a9\u0631\u062f\u0647 \u0641\u0636\u0644 \u0642\u0650\u062f\u0645"}, "1": {"m1": "\u0642\u062f\u0631 \u0634\u0628\u0647\u0627\u06cc \u0642\u062f\u0631 \u0627\u0632 \u06af\u0644 \u0627\u0648", "m2": "\u0646\u0648\u0631\u0650 \u0631\u0648\u0632 \u0642\u06cc\u0627\u0645\u062a \u0627\u0632 \u062f\u0644 \u0627\u0648"}, "2": {"m1": "\u062d\u0644\u0642\u0647\u0654 \u062d\u0644\u0642\u0647\u200c\u0647\u0627 \u0628\u0647 \u062d\u0644\u0642\u0647\u0654 \u0645\u0648\u06cc", "m2": "\u0634\u062d\u0646\u0647\u0654 \u0634\u0631\u0639\u0647\u0627 \u0628\u0647 \u0635\u0641\u062d\u0647\u0654 \u0631\u0648\u06cc"}, "3": {"m1": "\u0631\u0627\u0632\u0650 \u062d\u0642 \u067e\u0631\u062f\u0647\u0654 \u0645\u062d\u0627\u0631\u0645 \u0627\u0648", "m2": "\u0646\u0641\u0633 \u06a9\u0644 \u0635\u0648\u0631\u062a \u0645\u06a9\u0627\u0631\u0645 \u0627\u0648"}, "4": {"m1": "\u0639\u0631\u0634 \u0639\u0634\u0642\u0634 \u0628\u0631 \u0622\u0633\u0645\u0627\u0646 \u062c\u0644\u0627\u0644", "m2": "\u0627\u0635\u0644 \u0648 \u0641\u0631\u0639\u0634 \u067e\u064f\u0631 \u0627\u0632 \u0641\u0646\u0648\u0646 \u06a9\u0645\u0627\u0644"}, "5": {"m1": "\u063a\u0631\u0636 \u06a9\u064f\u0646 \u0632 \u062d\u06a9\u0645 \u062f\u0631 \u0627\u0632\u0644 \u0627\u0648", "m2": "\u0627\u0648\u0644 \u0627\u0644\u0641\u06a9\u0631 \u0648 \u0622\u062e\u0631 \u0627\u0644\u0639\u0645\u0644 \u0627\u0648"}, "6": {"m1": "\u0628\u0648\u062f\u0647 \u0627\u0648\u0644 \u0628\u0647 \u062e\u0644\u0642\u062a \u0648 \u0635\u0648\u0631\u062a", "m2": "\u0648 \u0622\u0645\u062f\u0647 \u0622\u062e\u0631 \u0627\u0632 \u067e\u06cc \u062f\u0639\u0648\u062a"}, "7": {"m1": "\u0628\u0648\u062f\u0647 \u062f\u0631 \u0631\u0648\u0636\u0647\u0654 \u062d\u0638\u06cc\u0631\u0647\u0654 \u0627\u0646\u0633", "m2": "\u0645\u0627\u062f\u0631\u0634 \u0627\u0645\u0631 \u0648 \u062f\u0627\u06cc\u0647 \u0631\u0648\u062d\u200c\u0627\u0644\u0642\u062f\u0633"}, "8": {"m1": "\u0642\u062f \u0627\u0648 \u0647\u0631\u06a9\u0647 \u0627\u0632 \u0645\u0647\u06cc \u0648 \u0628\u0647\u06cc", "m2": "\u0633\u062e\u0631\u0647 \u06a9\u0631\u062f\u06cc \u0628\u0647 \u0642\u062f\u0651 \u0633\u0631\u0648 \u0633\u0647\u06cc"}, "9": {"m1": "\u0644\u0648\u0646 \u0627\u0648 \u0645\u0627\u0647 \u0631\u0627 \u0686\u0648 \u06af\u0644 \u06a9\u0631\u062f\u06cc", "m2": "\u0628\u0648\u06cc \u0627\u0648 \u0645\u064f\u0634\u06a9 \u0631\u0627 \u062e\u062c\u0644 \u06a9\u0631\u062f\u06cc"}, "10": {"m1": "\u062d\u0644\u0642 \u062e\u0644\u0642 \u0627\u0632 \u0628\u0631\u0627\u06cc \u0637\u0648\u0642 \u0641\u0631\u0634", "m2": "\u062e\u0644\u0642 \u062e\u0644\u0642 \u0646\u0633\u06cc\u0645 \u062e\u0627\u06a9 \u062f\u0631\u0634"}, "11": {"m1": "\u0641\u0631\u0634 \u0646\u0648 \u0628\u0627\u0631\u0650 \u0641\u0631\u0639 \u0627\u0648 \u06af\u0634\u062a\u0647", "m2": "\u0639\u0631\u0634 \u0645\u063a\u0644\u0648\u0628 \u0634\u0631\u0639 \u0627\u0648 \u06af\u0634\u062a\u0647"}, "12": {"m1": "\u0645\u0646\u062a\u0635\u0628 \u0642\u062f \u0686\u0648 \u0633\u0631\u0648 \u0622\u0632\u0627\u062f\u0647", "m2": "\u0634\u0645\u0633\u0647\u0654 \u0639\u0642\u0644 \u0622\u062f\u0645\u06cc \u0632\u0627\u062f\u0647"}, "13": {"m1": "\u0635\u0628\u062d \u0635\u0627\u062f\u0642 \u0686\u0646\u0648 \u0646\u062f\u06cc\u062f\u0647 \u0628\u0647 \u0631\u0627\u0647", "m2": "\u0622\u0641\u062a\u0627\u0628\u06cc \u0628\u0647 \u0632\u06cc\u0631 \u06af\u0646\u0628\u062f \u0645\u0627\u0647"}, "14": {"m1": "\u0634\u0631\u0639 \u0648 \u062f\u06cc\u0646 \u0686\u0627\u0631 \u0637\u0628\u0639 \u0648 \u0634\u0634 \u0633\u0648\u06cc \u0627\u0648", "m2": "\u0639\u0642\u0644 \u0648 \u062c\u0627\u0646 \u06af\u0648\u0647\u0631 \u062f\u0648 \u06af\u06cc\u0633\u0648\u06cc \u0627\u0648"}, "15": {"m1": "\u0647\u0641\u062f\u0647 \u062a\u0627\u0645\u0648\u06cc \u0686\u0648\u0646 \u0633\u062a\u0627\u0631\u0647 \u0628\u0647 \u0628\u0627\u063a", "m2": "\u0648\u0622\u0646\u0650 \u062f\u06cc\u06af\u0631 \u0633\u06cc\u0627\u0647 \u0686\u0648\u0646 \u067e\u0631\u0650 \u0632\u0627\u063a"}, "16": {"m1": "\u0627\u0646\u062f\u0631\u0627\u0646 \u06af\u06cc\u0633\u0648\u06cc \u0633\u06cc\u0627\u0647 \u0648 \u0633\u067e\u06cc\u062f", "m2": "\u062f\u0648\u062e\u062a\u0647 \u0639\u0642\u0644 \u06a9\u06cc\u0633\u0647\u200c\u0647\u0627\u06cc \u0627\u0645\u06cc\u062f"}, "17": {"m1": "\u06a9\u0631\u062f\u0647 \u0647\u0645\u0632\u0627\u062f \u0628\u0627 \u0627\u0632\u0644 \u0646\u0633\u0628\u0634", "m2": "\u06af\u0634\u062a\u0647 \u0647\u0645\u0631\u0627\u0632 \u0628\u0627 \u0627\u0628\u062f \u0627\u062f\u0628\u0634"}}}