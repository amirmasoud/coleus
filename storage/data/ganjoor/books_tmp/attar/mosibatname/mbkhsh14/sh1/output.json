{"text": {"0": {"m1": "\u0633\u0627\u0644\u06a9 \u0627\u0632 \u062e\u0648\u0631\u0634\u06cc\u062f \u0686\u0648\u0646 \u0622\u06af\u0627\u0647 \u0634\u062f", "m2": "\u0639\u0627\u0642\u0628\u062a \u0628\u0631\u062e\u0627\u0633\u062a \u067e\u06cc\u0634 \u0645\u0627\u0647 \u0634\u062f"}, "1": {"m1": "\u06af\u0641\u062a \u0647\u0627\u0646 \u0627\u06cc \u0686\u0634\u0645\u0647\u0654 \u0627\u0641\u0631\u0648\u062e\u062a\u0647", "m2": "\u0628\u0631 \u0645\u0646\u0627\u0632\u0644 \u0631\u0648\u0632 \u0648 \u0634\u0628 \u0622\u0645\u0648\u062e\u062a\u0647"}, "2": {"m1": "\u0647\u0631 \u0632\u0645\u0627\u0646 \u062f\u0631 \u0645\u0646\u0632\u0644\u06cc \u062f\u06cc\u06af\u0631 \u0631\u0648\u06cc", "m2": "\u06af\u0647 \u0628\u067e\u0627 \u0622\u0626\u06cc \u0648 \u06af\u0647 \u0628\u0627 \u0633\u0631 \u0634\u0648\u06cc"}, "3": {"m1": "\u0647\u0631 \u0633\u0631 \u0645\u0647 \u0645\u06cc\u0634\u0648\u06cc \u0646\u0648 \u0627\u0632 \u06a9\u0645\u0627\u0644", "m2": "\u0644\u0627\u062c\u0631\u0645 \u0631\u0648\u06cc \u062a\u0648 \u0645\u06cc\u06af\u06cc\u0631\u0646\u062f \u0641\u0627\u0644"}, "4": {"m1": "\u062f\u0631 \u0634\u0628 \u062a\u0627\u0631\u06cc\u06a9 \u062a\u0646\u0647\u0627 \u0645\u06cc\u0631\u0648\u06cc", "m2": "\u0645\u0634\u0639\u0644\u0647 \u062f\u0631 \u062f\u0633\u062a \u0632\u06cc\u0628\u0627 \u0645\u06cc\u0631\u0648\u06cc"}, "5": {"m1": "\u0632\u0646\u06af\u06cc \u0634\u0628 \u0631\u0627 \u062a\u0648\u062f\u0627\u062f\u06cc \u06af\u0648\u0634\u0645\u0627\u0644", "m2": "\u06af\u0631\u06af \u0638\u0644\u0645\u062a \u0631\u0627 \u062a\u0648 \u06a9\u0631\u062f\u06cc \u062f\u0631 \u062c\u0648\u0627\u0644"}, "6": {"m1": "\u062e\u06cc\u0645\u0647\u0654 \u062f\u0627\u0631\u06cc \u0632 \u0646\u0648\u0631 \u0622\u0646 \u0631\u0627 \u0637\u0646\u0627\u0628", "m2": "\u0627\u0632 \u0637\u0646\u0627\u0628 \u0627\u0648 \u062c\u0647\u0627\u0646\u06cc \u067e\u0631 \u06a9\u0644\u0627\u0628"}, "7": {"m1": "\u0686\u0648\u0646 \u0633\u0644\u06cc\u0645\u0627\u0646 \u0628\u0627\u062f \u062f\u0631 \u0641\u0631\u0645\u0627\u0646 \u062a\u0631\u0627\u0633\u062a", "m2": "\u0644\u0627\u062c\u0631\u0645 \u0627\u0632 \u0646\u0648\u0631 \u0634\u0627\u062f\u0631\u0648\u0627\u0646 \u062a\u0631\u0627\u0633\u062a"}, "8": {"m1": "\u062a\u0648 \u0633\u0644\u06cc\u0645\u0627\u0646 \u0648\u0634 \u0628\u0634\u0627\u062f\u0631\u0648\u0627\u0646 \u062f\u0631\u06cc", "m2": "\u06a9\u0631\u062f\u0647\u0654 \u0627\u0632\u0645\u0627\u0647 \u0646\u0648 \u0627\u0646\u06af\u0634\u062a\u0631\u06cc"}, "9": {"m1": "\u0627\u06cc\u0646 \u0686\u0646\u06cc\u0646 \u0645\u0644\u06a9\u06cc \u06a9\u0647 \u062d\u0627\u0635\u0644 \u06a9\u0631\u062f\u0647\u0654", "m2": "\u06af\u0648\u0626\u06cc\u0627 \u062a\u0648 \u062d\u0644 \u0645\u0634\u06a9\u0644 \u06a9\u0631\u062f\u0647\u0654"}, "10": {"m1": "\u06a9\u0631\u062f\u0647\u0654 \u0686\u0634\u0645\u06cc \u0633\u067e\u06cc\u062f \u0627\u0632 \u0627\u0646\u062a\u0638\u0627\u0631", "m2": "\u067e\u0633 \u0633\u06cc\u0647 \u06a9\u0627\u0633\u0647 \u0645\u0628\u0627\u0634 \u0648 \u0634\u0631\u0645 \u062f\u0627\u0631"}, "11": {"m1": "\u06af\u0631 \u062e\u0628\u0631 \u062f\u0627\u0631\u06cc \u0632 \u062f\u0631\u062f \u0648 \u0633\u0648\u0632 \u0645\u0646", "m2": "\u0647\u06cc\u0646 \u0646\u0634\u0627\u0646\u06cc \u062f\u0647 \u06a9\u0647 \u0634\u0628 \u0634\u062f \u0631\u0648\u0632 \u0645\u0646"}, "12": {"m1": "\u06af\u0641\u062a \u0627\u06cc \u067e\u0631\u0633\u0646\u062f\u0647 \u0648\u0642\u062a \u06a9\u0627\u0631 \u0631\u0641\u062a", "m2": "\u067e\u06cc\u0634 \u0627\u0632 \u0645\u0627 \u0642\u0627\u0641\u0644\u0647 \u0633\u0627\u0644\u0627\u0631 \u0631\u0641\u062a"}, "13": {"m1": "\u0686\u0648\u0646 \u0646\u062f\u06cc\u062f\u0645 \u0647\u06cc\u0686 \u06af\u0631\u062f \u0627\u0632 \u0642\u0627\u0641\u0644\u0647", "m2": "\u0631\u0648\u06cc \u0645\u0646 \u0627\u0632 \u0627\u0634\u06a9 \u0634\u062f \u067e\u0631 \u0622\u0628\u0644\u0647"}, "14": {"m1": "\u0627\u0648\u0644 \u0645\u0647 \u0639\u0645\u0631 \u06cc\u06a9 \u062f\u0645 \u06cc\u0627\u0641\u062a\u0647", "m2": "\u0636\u062d\u06a9\u0647\u0654 \u0639\u0627\u0644\u0645 \u0634\u062f\u0647 \u063a\u0645 \u06cc\u0627\u0641\u062a\u0647"}, "15": {"m1": "\u0622\u062e\u0631 \u0647\u0631 \u0645\u0627\u0647 \u062f\u0644 \u067e\u0631 \u062a\u0641\u062a \u0648 \u062a\u0627\u0628", "m2": "\u0632\u0627\u0631 \u0628\u0631 \u0632\u0631\u062f\u0645 \u0646\u0634\u06cc\u0646\u062f \u0622\u0641\u062a\u0627\u0628"}, "16": {"m1": "\u0686\u0648\u0646 \u0628\u0631\u0622\u06cc\u062f \u0622\u0641\u062a\u0627\u0628 \u0631\u0648\u0634\u0646\u0645", "m2": "\u0622\u062a\u0634 \u0633\u062e\u062a \u0627\u0641\u06a9\u0646\u062f \u062f\u0631 \u062e\u0631\u0645\u0646\u0645"}, "17": {"m1": "\u06af\u0647 \u062f\u0647\u0627\u0646 \u0634\u06cc\u0631 \u0628\u0627\u0634\u062f \u062c\u0627\u06cc \u0645\u0646", "m2": "\u06af\u0627\u0647 \u06a9\u0698\u062f\u0645 \u0633\u0631 \u0646\u0647\u062f \u062f\u0631 \u067e\u0627\u06cc \u0645\u0646"}, "18": {"m1": "\u06af\u0627\u0647 \u062f\u0631 \u062e\u0648\u0634\u0647 \u06a9\u0634\u0646\u062f\u0645 \u0647\u0645\u0686\u0648 \u062f\u0627\u0633", "m2": "\u06af\u0627\u0647 \u062f\u0631 \u06af\u0627\u0648\u0645 \u0686\u0648 \u0627\u0632 \u0632\u0631 \u0633\u0646\u06af \u0622\u0633"}, "19": {"m1": "\u06af\u0627\u0647 \u0628\u0631 \u0645\u06cc\u0632\u0627\u0646 \u0686\u0646\u0627\u0646\u0645 \u0645\u06cc\u06a9\u0634\u0646\u062f", "m2": "\u06af\u0647 \u0632 \u0647\u06cc \u062f\u0631 \u062e\u0631 \u06a9\u0645\u0627\u0646\u0645 \u0645\u06cc\u06a9\u0634\u0646\u062f"}, "20": {"m1": "\u062f\u0631 \u0645\u06cc\u0627\u0646 \u0627\u06cc\u0646 \u0647\u0645\u0647 \u0633\u062e\u062a\u06cc \u0648 \u062a\u0627\u0628", "m2": "\u0628\u0627\u062f \u067e\u06cc\u0645\u0627\u06cc\u0645 \u0647\u0645\u0647 \u0628\u0627 \u0645\u0627\u0647\u062a\u0627\u0628"}, "21": {"m1": "\u0627\u0632 \u0686\u0646\u06cc\u0646 \u06a9\u0633 \u06a9\u06cc \u06af\u0634\u0627\u06cc\u062f \u0639\u0642\u062f\u0647 \u0628\u0627\u0632", "m2": "\u062e\u0627\u0635\u0647 \u06a9\u0648 \u0631\u0627 \u0639\u0642\u062f\u0647 \u062f\u0627\u0631\u062f \u0632\u06cc\u0631 \u06af\u0627\u0632"}, "22": {"m1": "\u0633\u0627\u0644\u06a9 \u0622\u0645\u062f \u067e\u06cc\u0634 \u067e\u06cc\u0631 \u0633\u0627\u0644\u062e\u0648\u0631\u062f", "m2": "\u06af\u0627\u0647 \u062d\u0627\u0644 \u0648 \u06af\u0647 \u0628\u06cc\u0627\u0646 \u062d\u0627\u0644 \u06a9\u0631\u062f"}, "23": {"m1": "\u067e\u06cc\u0631 \u06af\u0641\u062a\u0634 \u0647\u0633\u062a \u0645\u0627\u0647 \u0627\u0632 \u0636\u0639\u0641 \u062d\u0627\u0644", "m2": "\u0645\u0627\u0646\u062f\u0647 \u0633\u0631\u06af\u0631\u062f\u0627\u0646 \u0632 \u0646\u0642\u0635\u0627\u0646 \u0648 \u06a9\u0645\u0627\u0644"}, "24": {"m1": "\u06af\u0647 \u0634\u0648\u062f \u0628\u0627\u0631\u06cc\u06a9 \u0648 \u0628\u06cc\u0642\u062f\u0631\u06cc \u0634\u0648\u062f", "m2": "\u06af\u0647 \u062c\u0647\u0627\u0646 \u0627\u0641\u0631\u0648\u0632\u062f \u0648 \u0628\u062f\u0631\u06cc \u0634\u0648\u062f"}, "25": {"m1": "\u0686\u0648\u0646 \u0646\u062f\u0627\u0631\u062f \u062a\u0627\u0628 \u062e\u0648\u0631\u0634\u06cc\u062f \u0633\u067e\u0647\u0631", "m2": "\u0645\u06cc\u0646\u0645\u0627\u06cc\u062f \u062f\u0627\u063a \u0627\u06cc\u0646 \u0646\u0642\u0635\u0627\u0646 \u0632 \u0686\u0647\u0631"}, "26": {"m1": "\u0627\u0632 \u067e\u06cc \u0627\u0648 \u0645\u06cc\u0631\u0648\u062f \u0633\u0631\u06af\u0634\u062a\u0647\u0654", "m2": "\u0628\u0627\u0632 \u0645\u06cc\u062c\u0648\u06cc\u062f \u0627\u0632\u0648 \u0633\u0631 \u0631\u0634\u062a\u0647\u0654"}, "27": {"m1": "\u06af\u0631\u0686\u0647 \u062f\u0627\u0631\u062f \u062d\u0633\u0646 \u0645\u0639\u0634\u0648\u0642\u0634 \u06a9\u0645\u0627\u0644", "m2": "\u0627\u0648 \u0646\u062f\u0627\u0631\u062f \u062a\u0627\u0628 \u0627\u0648 \u0627\u0632 \u0647\u06cc\u0686 \u062d\u0627\u0644"}, "28": {"m1": "\u0644\u0627\u062c\u0631\u0645 \u062f\u0631 \u0646\u0648\u0631 \u0642\u0631\u0628 \u0627\u0648 \u0645\u062f\u0627\u0645", "m2": "\u0641\u0627\u0646\u06cc \u0645\u0637\u0644\u0642 \u0634\u0648\u062f \u0627\u0632 \u062e\u0648\u062f \u062a\u0645\u0627\u0645"}, "29": {"m1": "\u0686\u0648\u0646 \u0646\u0628\u0627\u0634\u062f \u0639\u0627\u0634\u0642\u06cc \u0631\u0627 \u062d\u0648\u0635\u0644\u0647", "m2": "\u0630\u0631\u0647\u0654 \u0648\u0635\u0644\u0634 \u062f\u0647\u062f \u0635\u062f \u0648\u0644\u0648\u0644\u0647"}, "30": {"m1": "\u0647\u0631 \u06a9\u0647 \u0627\u0648 \u062f\u0631 \u0639\u0634\u0642 \u0622\u06cc\u062f \u0646\u0627\u062a\u0645\u0627\u0645", "m2": "\u0633\u0639\u06cc \u062e\u0648\u0646 \u062e\u0648\u062f \u06a9\u0646\u062f \u0633\u0639\u06cc \u0645\u062f\u0627\u0645"}}}