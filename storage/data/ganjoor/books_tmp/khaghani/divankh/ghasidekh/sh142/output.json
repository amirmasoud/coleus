{"text": {"0": {"m1": "\u0647\u0631 \u0632\u0645\u0627\u0646 \u0632\u06cc\u0646 \u0633\u0628\u0632 \u06af\u0644\u0634\u0646 \u0631\u062e\u062a \u0628\u06cc\u0631\u0648\u0646 \u0645\u06cc\u200c\u0628\u0631\u0645", "m2": "\u0639\u0627\u0644\u0645\u06cc \u0627\u0632 \u0639\u0627\u0644\u0645 \u0648\u062d\u062f\u062a \u0628\u0647 \u06a9\u0641 \u0645\u06cc\u200c\u0622\u0648\u0631\u0645"}, "1": {"m1": "\u062a\u062e\u062a \u0648 \u062e\u0627\u062a\u0645 \u0646\u06cc \u0648 \u06a9\u0648\u0633 \u0631\u0628 \u0647\u0628\u200c\u0644\u06cc \u0645\u06cc\u200c\u0632\u0646\u0645", "m2": "\u0637\u0648\u0631 \u0622\u062a\u0634 \u0646\u06cc \u0648 \u062f\u0631 \u0627\u0648\u062c \u0627\u0646\u0627 \u0627\u0644\u0644\u0647 \u0645\u06cc\u200c\u067e\u0631\u0645"}, "2": {"m1": "\u0647\u0631\u0686\u0647 \u0646\u0642\u0634 \u0646\u0641\u0633 \u0645\u06cc\u200c\u0628\u06cc\u0646\u0645 \u0628\u0647 \u062f\u0631\u06cc\u0627 \u0645\u06cc\u200c\u062f\u0647\u0645", "m2": "\u0647\u0631 \u0686\u0647 \u0646\u0642\u062f \u0639\u0642\u0644 \u0645\u06cc\u200c\u06cc\u0627\u0628\u0645 \u062f\u0631 \u0622\u062a\u0634 \u0645\u06cc\u200c\u0628\u0631\u0645"}, "3": {"m1": "\u06af\u0647 \u0628\u0647 \u062d\u062f \u0645\u0646\u0632\u0644 \u0627\u0632 \u0633\u062f\u0631\u0647 \u0633\u0631\u06cc\u0631\u06cc \u0645\u06cc\u200c\u06a9\u0646\u0645", "m2": "\u06af\u0647 \u0628\u0647 \u0642\u062f\u0631 \u0647\u0645\u062a \u0627\u0632 \u0634\u0639\u0631\u06cc \u0634\u0639\u0627\u0631\u06cc \u0645\u06cc\u200c\u0628\u0631\u0645"}, "4": {"m1": "\u062f\u0627\u062f\u0647\u0654 \u0646\u0647 \u0686\u0631\u062e \u0631\u0627 \u062f\u0631 \u062e\u0631\u062c \u06cc\u06a9\u062f\u0645 \u0645\u06cc\u200c\u0646\u0647\u0645", "m2": "\u0632\u0627\u062f\u0647\u0654 \u0634\u0634 \u0631\u0648\u0632 \u0631\u0627 \u0628\u0631 \u062e\u0648\u0627\u0646 \u06cc\u06a9 \u0634\u0628 \u0645\u06cc\u200c\u062e\u0648\u0631\u0645"}, "5": {"m1": "\u06af\u0631\u0686\u0647 \u0637\u0628\u0639 \u0627\u0632 \u0622\u0628\u0646\u0648\u0633 \u0631\u0648\u0632 \u0648 \u0634\u0628 \u0632\u062f \u062e\u0631\u06af\u0647\u0645", "m2": "\u0648\u0631\u0686\u0647 \u062f\u0647\u0631 \u0627\u0632 \u0644\u0627\u062c\u0648\u0631\u062f \u0622\u0633\u0645\u0627\u0646 \u06a9\u0631\u062f \u0627\u0641\u0633\u0631\u0645"}, "6": {"m1": "\u0627\u0632 \u0628\u0631\u0648\u0646 \u062a\u0627\u0628\u062e\u0627\u0646\u0647\u0654 \u0637\u0628\u0639 \u06cc\u0627\u0628\u06cc \u0646\u0632\u0647\u062a\u0645", "m2": "\u0648\u0632 \u0648\u0631\u0627\u06cc \u067e\u0627\u0644\u06af\u0627\u0646\u0647\u0654 \u0686\u0631\u062e \u0628\u06cc\u0646\u06cc \u0645\u0646\u0638\u0631\u0645"}, "7": {"m1": "\u06af\u0631 \u0628\u067e\u0631\u0645 \u0628\u0631 \u0641\u0644\u06a9 \u0634\u0627\u06cc\u062f\u060c \u06a9\u0647 \u0645\u06cc\u0645\u0648\u0646 \u0637\u0627\u06cc\u0631\u0645", "m2": "\u0648\u0631 \u0628\u0686\u0631\u0628\u0645 \u0628\u0631 \u062c\u0647\u0627\u0646 \u0632\u06cc\u0628\u062f \u06a9\u0647 \u0648\u0627\u0644\u0627 \u06af\u0648\u0647\u0631\u0645"}, "8": {"m1": "\u0628\u0627\u062e\u062a\u0645 \u0628\u0627 \u067e\u0627\u06a9\u0628\u0627\u0632\u0627\u0646 \u0639\u0627\u0644\u0645 \u062e\u0627\u06a9\u06cc \u0628\u0647 \u062e\u0627\u06a9", "m2": "\u0648\u0632 \u067e\u06cc \u0622\u0646 \u0639\u0627\u0644\u0645 \u0627\u06cc\u0646\u06a9 \u062f\u0631 \u0642\u0645\u0627\u0631\u06cc \u062f\u06cc\u06af\u0631\u0645"}, "9": {"m1": "\u0633\u0627\u062e\u062a\u0645 \u0622\u06cc\u06cc\u0646\u0647\u0654 \u062f\u0644\u060c \u06cc\u0627\u0641\u062a\u0645 \u0622\u0628 \u062d\u06cc\u0627\u062a", "m2": "\u06af\u0631\u0686\u0647 \u0628\u0627\u0648\u0631 \u0646\u0627\u06cc\u062f\u062a \u0647\u0645 \u062e\u0636\u0631 \u0648 \u0647\u0645 \u0627\u0633\u06a9\u0646\u062f\u0631\u0645"}, "10": {"m1": "\u0628\u0631\u062f\u0645 \u0627\u0632 \u0646\u0631\u0627\u062f \u06af\u06cc\u062a\u06cc \u06cc\u06a9 \u062f\u0648 \u062f\u0627\u0648 \u0627\u0646\u062f\u0631 \u0633\u0647 \u0632\u062e\u0645", "m2": "\u06af\u0631\u0686\u0647 \u0627\u0632 \u0686\u0627\u0631 \u0622\u062e\u0634\u06cc\u062c \u0648 \u067e\u0646\u062c \u062d\u0633 \u062f\u0631 \u0634\u0634\u062f\u0631\u0645"}, "11": {"m1": "\u0647\u0627\u062a\u0641 \u0647\u0645\u062a \u0639\u0633\u06cc\u200c\u0627\u0646 \u06cc\u0628\u0639\u062b\u06a9 \u0622\u0648\u0627\u0632 \u062f\u0627\u062f", "m2": "\u0639\u0634\u0642 \u0628\u0627 \u0637\u063a\u0631\u0627\u06cc \u062c\u0627\u0621 \u0627\u0644\u062d\u0642 \u062f\u0631\u0622\u0645\u062f \u0627\u0632 \u062f\u0631\u0645"}, "12": {"m1": "\u0645\u0646 \u0686\u0648 \u0637\u0648\u0637\u06cc \u0648 \u062c\u0647\u0627\u0646 \u062f\u0631 \u067e\u06cc\u0634 \u0645\u0646 \u0686\u0648\u0646 \u0622\u06cc\u0646\u0647 \u0627\u0633\u062a", "m2": "\u0644\u0627\u062c\u0631\u0645 \u0645\u0639\u0630\u0648\u0631\u0645 \u0627\u0631 \u062c\u0632 \u062e\u0648\u06cc\u0634\u062a\u0646 \u0645\u06cc\u200c\u0646\u0646\u06af\u0631\u0645"}, "13": {"m1": "\u0647\u0631\u0686\u0647 \u0639\u0642\u0644\u0645 \u0627\u0632 \u067e\u0633 \u0622\u06cc\u06cc\u0646\u0647 \u062a\u0644\u0642\u06cc\u0646 \u0645\u06cc\u200c\u06a9\u0646\u062f", "m2": "\u0645\u0646 \u0647\u0645\u0627\u0646 \u0645\u0639\u0646\u06cc \u0628\u0647 \u0635\u0648\u0631\u062a \u0628\u0631 \u0632\u0628\u0627\u0646 \u0645\u06cc\u200c\u0622\u0648\u0631\u0645"}, "14": {"m1": "\u067e\u06cc\u0634 \u0645\u0646 \u062c\u0632 \u0627\u062e\u062a\u0631 \u0648 \u0628\u062a \u0646\u06cc\u0633\u062a \u0622\u0632 \u0648 \u0622\u0631\u0632\u0648", "m2": "\u0645\u0646 \u062e\u0644\u06cc\u0644 \u0622\u0633\u0627 \u0646\u0647 \u0645\u0631\u062f \u0628\u062a \u0646\u0647 \u0645\u0631\u062f \u0627\u062e\u062a\u0631\u0645"}, "15": {"m1": "\u0628\u0631 \u0632\u0628\u0627\u0646 \u06af\u0631 \u0646\u0639\u0628\u062f \u0627\u0644\u0627\u0635\u0646\u0627\u0645 \u0631\u0627\u0646\u062f\u0645 \u062a\u0627\u06a9\u0646\u0648\u0646", "m2": "\u062f\u0644 \u0628\u0647 \u0627\u0646\u06cc\u200c\u0644\u0627\u0627\u062d\u0628\u200c\u0627\u0644\u0627\u0641\u0644\u06cc\u0646 \u0634\u062f \u0631\u0647\u0628\u0631\u0645"}, "16": {"m1": "\u062f\u0631 \u0645\u0642\u0627\u0645 \u0639\u0632 \u0639\u0632\u0644\u062a \u062f\u0631 \u0635\u0641 \u062f\u06cc\u0648\u0627\u0646 \u0639\u0647\u062f", "m2": "\u0631\u0627\u0633\u062a \u06af\u0648\u0626\u06cc \u0631\u0648\u0633\u062a\u0645 \u067e\u06cc\u06a9\u0627\u0631 \u0648 \u0639\u0646\u0642\u0627 \u067e\u06cc\u06a9\u0631\u0645"}, "17": {"m1": "\u0642\u0648\u062a \u0639\u0631\u0642 \u0639\u0631\u0627\u0642 \u0627\u0632 \u0645\u0627\u062f\u062a \u0637\u0628\u0639 \u0645\u0646 \u0627\u0633\u062a", "m2": "\u06af\u0631\u0686\u0647 \u0634\u0631\u06cc\u0627\u0646 \u062f\u0644 \u0634\u0631\u0648\u0627\u0646\u06cc\u0627\u0646 \u0631\u0627 \u0646\u0634\u062a\u0631\u0645"}, "18": {"m1": "\u0641\u0642\u0631 \u06a9\u0627\u0646 \u0627\u0641\u06a9\u0646\u062f\u0647\u0654 \u062e\u0644\u0642 \u0627\u0633\u062a \u0645\u0646 \u0628\u0631\u062f\u0627\u0634\u062a\u0645", "m2": "\u0632\u0627\u0644 \u06a9\u0627\u0646 \u0631\u062f \u06a9\u0631\u062f\u0647\u0654 \u0633\u0627\u0645 \u0627\u0633\u062a \u0645\u0646 \u0645\u06cc\u200c\u067e\u0631\u0648\u0631\u0645"}, "19": {"m1": "\u062f\u0631 \u0642\u0644\u0627\u062f\u0647\u0654 \u0633\u06af \u0646\u0698\u0627\u062f\u0627\u0646 \u06af\u0631\u0686\u0647 \u06a9\u0645\u062a\u0631 \u0645\u0647\u0631\u0647\u200c\u0627\u0645", "m2": "\u062f\u0631 \u0637\u0648\u06cc\u0644\u0647\u0654 \u0634\u06cc\u0631 \u0645\u0631\u062f\u0627\u0646 \u0642\u06cc\u0645\u062a\u06cc\u200c\u062a\u0631 \u06af\u0648\u0647\u0631\u0645"}, "20": {"m1": "\u0639\u0627\u0644\u0645 \u0627\u0632 \u0622\u0648\u0627\u0632\u0647\u0654 \u062e\u0627\u0642\u0627\u0646\u06cc \u0627\u0641\u0631\u0648\u0632\u0645 \u0648\u0644\u06cc\u06a9", "m2": "\u0647\u0645\u062a \u0627\u0632 \u0622\u0648\u0627\u0632\u0647\u0654 \u062e\u0627\u0642\u0627\u0646\u06cc \u0622\u0645\u062f \u0628\u0631\u062a\u0631\u0645"}, "21": {"m1": "\u0627\u06cc\u0646 \u062a\u0641\u0627\u062e\u0627\u0631 \u0646\u0642\u0637\u0647\u0654 \u062f\u0644 \u0631\u0627\u0633\u062a \u0648\u06cc\u0646 \u062f\u0645 \u0632\u0627\u0646 \u0627\u0648\u0633\u062a", "m2": "\u0648\u0631\u0646\u0647 \u0645\u0646 \u062e\u0648\u062f \u0631\u0627 \u062f\u0631 \u0627\u06cc\u0646 \u0645\u06cc\u062f\u0627\u0646 \u0632 \u0645\u0631\u062f\u0627\u0646 \u0646\u0634\u0645\u0631\u0645"}, "22": {"m1": "\u062c\u0627\u0647 \u0631\u0627 \u0628\u0631\u062f\u0627\u0631 \u06a9\u0631\u062f\u0645 \u062a\u0627 \u0641\u0644\u06a9 \u06af\u0641\u062a \u0627\u06cc \u062e\u0637\u06cc\u0628", "m2": "\u0646\u0627\u0626\u0628 \u0645\u0646 \u0628\u0627\u0634 \u0627\u06cc\u0646\u06a9 \u062a\u06cc\u063a \u0648 \u0627\u06cc\u0646\u06a9 \u0645\u0646\u0628\u0631\u0645"}}}