{"text": {"0": {"m1": "\u0622\u0645\u062f \u0628\u0631 \u0645\u0646 \u0646\u06af\u0627\u0631 \u0645\u0646 \u062f\u0648\u0634", "m2": "\u0627\u0641\u06a9\u0646\u062f\u0647 \u062f\u0648 \u0632\u0644\u0641 \u0634\u0633\u062a \u0628\u0631 \u062f\u0648\u0634"}, "1": {"m1": "\u0628\u0627 \u0645\u0646 \u0628\u0647 \u0639\u062a\u0627\u0628 \u0648 \u0646\u0627\u0632 \u0645\u06cc \u06af\u0641\u062a", "m2": "\u0627\u06cc \u06a9\u0631\u062f\u0647 \u0648\u0641\u0627 \u0648 \u0639\u0647\u062f \u0641\u0631\u0645\u0648\u0634"}, "2": {"m1": "\u06af\u0648\u06cc\u06cc \u0632 \u06a9\u062c\u0627\u06cc\u06cc \u0648 \u0686\u0631\u0627\u06cc\u06cc", "m2": "\u0628\u0627 \u0645\u062f\u0639\u06cc\u0627\u0646 \u0645\u0627 \u0647\u0645 \u0622\u063a\u0648\u0634"}, "3": {"m1": "\u0645\u0646 \u0645\u06cc \u062e\u0648\u0631\u0645 \u0627\u0632 \u062a\u0648 \u0632\u0647\u0631 \u063a\u0635\u0647", "m2": "\u062a\u0648 \u0645\u06cc \u0628\u0647 \u062e\u0644\u0627\u0641 \u0645\u0646 \u06a9\u0646\u06cc \u0646\u0648\u0634"}, "4": {"m1": "\u062a\u0642\u0635\u06cc\u0631 \u0646\u0645\u06cc \u06a9\u0646\u06cc \u0686\u0648 \u0645\u0631\u062f\u0627\u0646", "m2": "\u062f\u0631 \u0646\u0642\u0636 \u0648\u0641\u0627 \u0648 \u0639\u0647\u062f \u0645\u06cc \u06a9\u0648\u0634"}, "5": {"m1": "\u0634\u0631\u0645\u062a \u0646\u0628\u0648\u062f \u0632 \u0645\u0646 \u0632\u0647\u06cc \u0686\u0634\u0645", "m2": "\u062f\u0631\u062f\u062a \u0646\u06a9\u0646\u062f \u0633\u062e\u0646 \u0632\u0647\u06cc \u06af\u0648\u0634"}, "6": {"m1": "\u06af\u0641\u062a\u0645 \u0633\u062e\u0646\u0627\u0646 \u062a\u0644\u062e \u06af\u0641\u062a\u0646", "m2": "\u062d\u06cc\u0641 \u0627\u0633\u062a \u0627\u0632 \u0622\u0646 \u0644\u0628 \u0634\u06a9\u0631 \u0646\u0648\u0634"}, "7": {"m1": "\u062f\u0633\u062a\u0627\u0646\u0650 \u0686\u0646\u06cc\u0646 \u0645\u0628\u0646\u062f \u0628\u0631 \u0645\u0646", "m2": "\u0628\u0647\u062a\u0627\u0646\u0650 \u0686\u0646\u06cc\u0646 \u0645\u06af\u0648\u06cc \u062e\u0627\u0645\u0648\u0634"}, "8": {"m1": "\u062f\u0634\u0645\u0646 \u0686\u0648 \u062e\u0644\u0627\u0641 \u062f\u0648\u0633\u062a\u0627\u0646 \u0627\u0633\u062a", "m2": "\u0628\u0631 \u0645\u0627 \u0633\u062e\u0646 \u0631\u0642\u06cc\u0628 \u0645\u0646\u06cc\u0648\u0634"}, "9": {"m1": "\u0648\u0631 \u0646\u06cc\u0632 \u0632 \u0645\u0627 \u062e\u06cc\u0627\u0646\u062a\u06cc \u0631\u0641\u062a", "m2": "\u062f\u0627\u0645\u0627\u0646 \u0639\u0646\u0627\u06cc\u062a\u06cc \u0628\u0631  \u0622\u0646 \u067e\u0648\u0634"}, "10": {"m1": "\u0627\u0632 \u062e\u0648\u0627\u0628 \u062f\u0631 \u0622\u0645\u062f\u0645 \u0686\u0648 \u0645\u0633\u062a\u06cc", "m2": "\u0645\u0633\u062a\u06cc \u0648 \u0686\u0647 \u0645\u0633\u062a \u0631\u0641\u062a\u0647 \u0627\u0632 \u0647\u0648\u0634"}, "11": {"m1": "\u062f\u0644 \u06af\u0641\u062a \u0628\u0647 \u0645\u0646 \u06a9\u0647 \u0627\u06cc \u0646\u0632\u0627\u0631\u06cc", "m2": "\u0627\u0633\u0631\u0627\u0631 \u0646\u06af\u0627\u0647 \u062f\u0627\u0631 \u0648 \u0645\u062e\u0631\u0648\u0634"}, "12": {"m1": "\u062f\u0631 \u062e\u0648\u0627\u0628 \u062e\u06cc\u0627\u0644 \u0647\u0627\u06cc \u0645\u0634\u06a9\u0644", "m2": "\u0628\u06cc\u0646\u0646\u062f \u0645\u0648\u0644\u0651\u0650\u0647\u0627\u0646\u0650 \u0645\u062f\u0647\u0648\u0634"}, "13": {"m1": "\u0646\u0642\u062f\u06cc \u0633\u062a \u06af\u0631\u0627\u0646 \u0628\u0647\u0627 \u06a9\u0647 \u062f\u06cc\u062f\u06cc", "m2": "\u0627\u0631\u0632\u0627\u0646 \u0627\u0631\u0632\u0627\u0646 \u0628\u0647 \u0647\u06cc\u0686 \u0645\u0641\u0631\u0648\u0634"}}}