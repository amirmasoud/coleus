{"text": {"0": {"m1": "\u0628\u0627\u062f \u0646\u0648\u0631\u0648\u0632\u06cc \u0647\u0645\u0647 \u06a9\u0644\u0651\u0647 \u0632\u0646\u062f \u062f\u0631 \u0628\u0648\u0633\u062a\u0627\u0646", "m2": "\u0627\u0628\u0631 \u0646\u06cc\u0633\u0627\u0646\u06cc \u0647\u0645\u06cc \u0628\u0631 \u06af\u0644 \u0634\u0648\u062f \u0644\u0624\u0644\u0624\u0641\u0634\u0627\u0646"}, "1": {"m1": "\u0627\u0632 \u062c\u0648\u0627\u0647\u0631 \u06af\u0646\u062c \u06cc\u0627\u0642\u0648\u062a \u0627\u0633\u062a \u06af\u0648\u06cc\u06cc \u0645\u06cc\u0648\u0647\u200c\u062f\u0627\u0631", "m2": "\u0648\u0632 \u0637\u0631\u0627\u06cc\u0641 \u06a9\u064e\u0631\u0652\u062e\u0650 \u0628\u063a\u062f\u0627\u062f\u0633\u062a \u06af\u0648\u06cc\u06cc \u0628\u0648\u0633\u062a\u0627\u0646"}, "2": {"m1": "\u0631\u0627\u063a \u0634\u062f \u0686\u0648\u0646 \u0634\u0634\u062a\u0631\u06cc \u0648 \u0628\u0627\u063a \u0634\u062f \u0686\u0648\u0646 \u0645\u0634\u062a\u0631\u06cc", "m2": "\u0622\u0628 \u0634\u062f \u0686\u0648\u0646 \u0633\u0644\u0633\u0628\u06cc\u0644 \u0648 \u062e\u0627\u06a9 \u0634\u062f \u0686\u0648\u0646 \u067e\u0631\u0646\u06cc\u0627\u0646"}, "3": {"m1": "\u067e\u0631 \u062d\u064f\u0644\u064e\u0644 \u0634\u062f \u06a9\u0648\u0647\u0633\u0627\u0631 \u0648 \u067e\u0631\u062d\u064f\u0644\u06cc \u0634\u062f \u0645\u0631\u063a\u0632\u0627\u0631", "m2": "\u067e\u0631\u062d\u0634\u0631 \u0634\u062f \u062c\u0648\u06cc\u0628\u0627\u0631 \u0648 \u067e\u0631\u06af\u0647\u0631 \u0634\u062f \u06af\u0644\u0633\u062a\u0627\u0646"}, "4": {"m1": "\u0627\u0632 \u0634\u06a9\u0648\u0641\u0647 \u0647\u0631 \u062f\u0631\u062e\u062a\u06cc \u0633\u06cc\u0645\u200c\u067e\u0627\u0634 \u0648 \u062f\u064f\u0631\u0641\u0634\u0627\u0646", "m2": "\u0648\u0632 \u0628\u0646\u0641\u0634\u0647 \u0647\u0631 \u0632\u0645\u06cc\u0646\u06cc \u0645\u0634\u06a9\u0633\u0627\u06cc \u0648 \u0646\u06cc\u0644\u200c\u0633\u0627\u0646"}, "5": {"m1": "\u0647\u0631 \u0646\u0628\u0627\u062a\u06cc \u0631\u0627 \u0632\u062f\u0627\u0646\u06af\u06cc \u062f\u06cc\u06af\u0631 \u0622\u06cc\u062f \u067e\u06cc\u0631\u0647\u0646", "m2": "\u0647\u0631 \u062f\u0631\u062e\u062a\u06cc \u0631\u0627 \u0632\u0631\u0646\u06af\u06cc \u062f\u06cc\u06af\u0631 \u0622\u06cc\u062f \u0637\u06cc\u0644\u0633\u0627\u0646"}, "6": {"m1": "\u0627\u0632\u06a9\u0648\u0627\u06a9\u0628 \u0646\u06cc\u0633\u062a \u067e\u06cc\u062f\u0627 \u0622\u0633\u0645\u0627\u0646 \u0627\u0632 \u06a9\u0648\u0647\u0633\u0627\u0631", "m2": "\u0648\u0632 \u0634\u0642\u0627\u06cc\u0642 \u0646\u06cc\u0633\u062a \u067e\u06cc\u062f\u0627 \u06a9\u0648\u0647\u0633\u0627\u0631 \u0627\u0632 \u0622\u0633\u0645\u0627\u0646"}, "7": {"m1": "\u0647\u0631 \u0633\u062d\u0631\u06af\u0627\u0647 \u0622\u0646 \u0647\u0645\u06cc\u200c \u06a9\u0648\u06a9\u0628 \u0641\u0631\u0633\u062a\u062f \u0633\u0648\u06cc \u0627\u06cc\u0646", "m2": "\u0647\u0631 \u0634\u0628\u0627\u0646\u06af\u0627\u0647 \u0627\u06cc\u0646 \u0647\u0645\u06cc \u0644\u0627\u0644\u0647 \u0641\u0631\u0633\u062a\u062f \u0633\u0648\u06cc \u0622\u0646"}, "8": {"m1": "\u0628\u0646\u06af\u0631 \u0627\u0646\u062f\u0631 \u0633\u0628\u0632\u0647\u200c\u0632\u0627\u0631 \u0648 \u06cc\u0627\u0633\u0645\u06cc\u0646\u0634 \u0628\u0631\u06a9\u0646\u0627\u0631", "m2": "\u0628\u0646\u06af\u0631 \u0627\u0646\u062f\u0631 \u0644\u0627\u0644\u0647\u200c\u0632\u0627\u0631 \u0648 \u0634\u0646\u0628\u0644\u06cc\u062f\u0634 \u062f\u0631 \u0645\u06cc\u0627\u0646"}, "9": {"m1": "\u0622\u0646 \u06cc\u06a9\u06cc \u0686\u0648\u0646 \u062c\u0627\u0645 \u0645\u06cc\u0646\u0627 \u062f\u0631 \u0645\u06cc\u0627\u0646 \u0644\u0627\u062c\u0648\u0631\u062f", "m2": "\u0648\u06cc\u0646 \u062f\u06af\u0631 \u0686\u0648\u0646 \u0637\u0634\u062a \u0632\u0631\u06cc\u0646 \u062f\u0631 \u0645\u06cc\u0627\u0646 \u0632\u0639\u0641\u0631\u0627\u0646"}, "10": {"m1": "\u0631\u0627\u063a \u0631\u0627 \u0628\u0646\u06af\u0631 \u0628\u0647 \u06a9\u0631\u062f\u0627\u0631 \u0628\u0647\u0627\u0631\u06cc \u062f\u0644\u06af\u0634\u0627\u06cc", "m2": "\u0628\u0627\u063a \u0631\u0627 \u0628\u0646\u06af\u0631 \u0628\u0647 \u06a9\u0631\u062f\u0627\u0631 \u0646\u06af\u0627\u0631\u06cc \u062f\u0644\u0633\u062a\u0627\u0646"}, "11": {"m1": "\u0622\u0646 \u0646\u06af\u0627\u0631\u06cc \u06a9\u0632 \u0648\u0635\u0627\u0644 \u0648 \u0647\u062c\u0631 \u0627\u0648 \u067e\u06cc\u062f\u0627 \u0634\u0648\u062f", "m2": "\u0627\u0632 \u062e\u0632\u0627\u0646 \u0645\u0646 \u0628\u0647\u0627\u0631 \u0648 \u0627\u0632 \u0628\u0647\u0627\u0631 \u0645\u0646 \u062e\u0632\u0627\u0646"}, "12": {"m1": "\u0632\u0644\u0641 \u06a9\u0648\u062a\u0627\u0647\u0634 \u0647\u0645\u06cc \u0639\u0634\u0642 \u0645\u0631\u0627 \u062f\u0627\u0631\u062f \u062f\u0631\u0627\u0632", "m2": "\u0642\u062f \u0686\u0648\u0646 \u062a\u06cc\u0631\u0634 \u0647\u0645\u06cc \u067e\u0634\u062a \u0645\u0631\u0627 \u062f\u0627\u0631\u062f \u06a9\u0645\u0627\u0646"}, "13": {"m1": "\u0627\u0631\u063a\u0648\u0627\u0646 \u0627\u0632 \u0631\u0646\u06af \u0631\u0648\u06cc \u0645\u0646 \u0634\u0648\u062f \u0686\u0648\u0646 \u0634\u0646\u0628\u0644\u06cc\u062f", "m2": "\u0634\u0646\u0628\u0644\u06cc\u062f \u0627\u0632 \u0631\u0646\u06af \u0631\u0648\u06cc \u0627\u0648 \u0634\u0648\u062f \u0686\u0648\u0646 \u0627\u0631\u063a\u0648\u0627\u0646"}, "14": {"m1": "\u0686\u0634\u0645 \u0645\u0646 \u062f\u0631 \u0639\u0627\u0634\u0642\u06cc \u06af\u0648\u0647\u0631 \u0641\u0634\u0627\u0646\u062f \u0628\u06cc\u200c\u0642\u06cc\u0627\u0633", "m2": "\u0632\u0644\u0641 \u0627\u0648 \u062f\u0631 \u062f\u0644\u0628\u0631\u06cc \u0639\u0646\u0628\u0631 \u0641\u0634\u0627\u0646\u062f \u0628\u06cc\u200c\u06a9\u0631\u0627\u0646"}, "15": {"m1": "\u0622\u0646 \u0633\u062e\u0627\u0648\u062a\u0647\u0627 \u06a9\u0647 \u0686\u0634\u0645 \u0645\u0646\u200c \u06a9\u0646\u062f \u062f\u0631 \u0639\u0627\u0634\u0642\u06cc", "m2": "\u062f\u0633\u062a \u0645\u0648\u0644\u0627\u0646\u0627 \u06a9\u0646\u062f \u062f\u0631 \u062f\u0648\u0644\u062a \u0635\u0627\u062d\u0628\u0642\u0631\u0627\u0646"}, "16": {"m1": "\u0648\u0627\u0646 \u0635\u0646\u0627\u0639\u062a\u0647\u0627 \u06a9\u0647 \u0632\u0644\u0641 \u0627\u0648 \u06a9\u0646\u062f \u062f\u0631 \u062f\u0644\u0628\u0631\u06cc", "m2": "\u06a9\u0644\u06a9 \u0645\u0648\u0644\u0627\u0646\u0627 \u06a9\u0646\u062f \u062f\u0631 \u062f\u0648\u0644\u062a \u0634\u0627\u0647 \u062c\u0647\u0627\u0646"}, "17": {"m1": "\u0622\u0641\u062a\u0627\u0628 \u0633\u0639\u062f \u0648 \u0646\u0635\u0631\u062a \u0645\u0644\u06a9 \u0633\u0644\u0637\u0627\u0646 \u0631\u0627 \u0634\u0631\u0641", "m2": "\u0646\u0627\u0645\u0648\u0631 \u0628\u0648\u0633\u0639\u062f \u0628\u0646 \u0645\u0646\u0635\u0648\u0631 \u0635\u062f\u0631 \u06a9\u0627\u0645\u0631\u0627\u0646"}, "18": {"m1": "\u0627\u0632 \u06a9\u0645\u0627\u0644 \u062f\u0627\u0646\u0634 \u0648 \u0641\u0631\u0647\u0646\u06af \u0648 \u0645\u0639\u0646\u06cc \u062f\u0631\u06af\u0630\u0634\u062a", "m2": "\u0639\u0642\u0644 \u0627\u0648 \u0627\u0632 \u0627\u0642\u062a\u0631\u0627\u062d \u0648 \u0637\u0628\u0639 \u0627\u0648 \u0627\u0632 \u0627\u0645\u062a\u062d\u0627\u0646"}, "19": {"m1": "\u0646\u06cc\u06a9 \u0628\u0646\u06af\u0631 \u06a9\u0644\u06a9 \u0627\u0648 \u0631\u0627 \u062a\u0627 \u0628\u0628\u06cc\u0646\u06cc \u067e\u06cc\u06a9\u0631\u06cc", "m2": "\u0628\u06cc\u200c\u0628\u0635\u0631 \u0628\u0627\u0631\u06cc\u06a9\u200c\u0628\u06cc\u0646 \u0648 \u0628\u06cc\u200c\u062e\u0631\u062f \u0628\u0633\u06cc\u0627\u0631 \u062f\u0627\u0646"}, "20": {"m1": "\u0646\u0642\u0634 \u0627\u0648 \u0628\u0631 \u0633\u06cc\u0645 \u0631\u0648\u0634\u0646 \u0686\u0648\u0646 \u062f\u062e\u0627\u0646 \u0628\u06cc\u200c\u0686\u0631\u0627\u063a", "m2": "\u0645\u0644\u06a9 \u0627\u0632\u0648 \u0647\u0645\u0648\u0627\u0631\u0647 \u0631\u0648\u0634\u0646 \u0686\u0648\u0646 \u0686\u0631\u0627\u063a \u0628\u06cc\u200c\u062f\u062e\u0627\u0646"}, "21": {"m1": "\u0645\u0631\u063a \u0632\u0631\u06cc\u0646 \u0627\u0633\u062a \u0648 \u0627\u0648 \u0631\u0627 \u062a\u062e\u062a\u0647\u0654 \u0633\u06cc\u0645\u06cc\u0646 \u0686\u0645\u0646", "m2": "\u0645\u0631\u063a \u0631\u0646\u06af\u06cc\u0646 \u0627\u0633\u062a \u0648 \u0627\u0648 \u0631\u0627 \u0628\u0631\u062c \u0645\u0634\u06a9\u06cc\u0646 \u0622\u0634\u06cc\u0627\u0646"}, "22": {"m1": "\u067e\u06cc\u0634\u0647\u200c \u06af\u06cc\u0631\u062f \u062e\u062f\u0645\u062a \u0627\u0648 \u0647\u0631 \u06a9\u0647 \u062e\u0648\u0627\u0647\u062f \u0622\u0628 \u0648 \u062c\u0627\u0647", "m2": "\u062a\u0648\u0634\u0647 \u0633\u0627\u0632\u062f \u0645\u062f\u062d\u062a \u0627\u0648 \u0647\u0631\u06a9\u0647 \u062e\u0648\u0627\u0647\u062f \u0646\u0627\u0645 \u0648 \u0646\u0627\u0646"}, "23": {"m1": "\u0686\u0648\u0646 \u0632 \u06cc\u06a9 \u062e\u062f\u0645\u062a \u062c\u062f\u0627 \u0645\u0627\u0646\u06cc \u0628\u0647 \u062d\u06a9\u0645 \u0627\u0636\u0637\u0631\u0627\u0631", "m2": "\u062c\u0632 \u062f\u0631 \u0627\u06cc\u0646 \u0639\u0627\u0644\u06cc \u0645\u0642\u0631 \u062e\u062f\u0645\u062a \u0646\u0645\u0648\u062f\u0646 \u06a9\u06cc \u062a\u0648\u0627\u0646"}, "24": {"m1": "\u062a\u0627 \u0627\u0645\u06cc\u062f \u0648 \u0628\u06cc\u0645 \u0628\u0627\u0634\u062f \u062f\u0631 \u062f\u0644 \u062e\u0631\u062f \u0648 \u0628\u0632\u0631\u06af", "m2": "\u062a\u0627 \u0632\u06cc\u0627\u0646 \u0648 \u0633\u0648\u062f \u0628\u0627\u0634\u062f \u062f\u0631 \u062a\u0646 \u067e\u06cc\u0631 \u0648 \u062c\u0648\u0627\u0646"}, "25": {"m1": "\u062f\u0631 \u062f\u0644 \u0647\u0631 \u0628\u062f\u0633\u06af\u0627\u0644\u0634 \u0628\u0627\u062f \u0628\u06cc\u0645 \u0628\u06cc\u200c\u0627\u0645\u06cc\u062f", "m2": "\u062f\u0631 \u062a\u0646 \u0647\u0631 \u0646\u06cc\u06a9 \u062e\u0648\u0627\u0647\u0634 \u0628\u0627\u062f \u0633\u0648\u062f \u0628\u06cc\u200c\u0632\u06cc\u0627\u0646"}, "26": {"m1": "\u0628\u0631 \u0647\u0645\u0627\u06cc\u0648\u0646 \u0631\u0648\u0632\u06af\u0627\u0631 \u0648 \u0639\u06cc\u062f \u0627\u0648 \u0641\u0631\u062e\u0646\u062f\u0647 \u0628\u0627\u062f", "m2": "\u0647\u0645\u0686\u0646\u06cc\u0646 \u0646\u0648\u0631\u0648\u0632 \u0648 \u0635\u062f \u0646\u0648\u0631\u0648\u0632 \u0648 \u0639\u06cc\u062f \u0645\u0647\u0631\u06af\u0627\u0646"}}}