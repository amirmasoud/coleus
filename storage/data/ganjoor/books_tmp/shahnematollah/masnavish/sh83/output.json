{"text": {"0": {"m1": "\u0686\u0648\u0646 \u0631\u0633\u0648\u0644 \u062e\u062f\u0627 \u0628\u0647 \u0627\u0645\u0631 \u062e\u062f\u0627", "m2": "\u0639\u0632\u0645 \u0641\u0631\u0645\u0648\u062f \u062a\u0627 \u0628\u0647 \u062f\u0627\u0631 \u0628\u0642\u0627"}, "1": {"m1": "\u062d\u0648\u0631\u06cc\u0627\u0646 \u0635\u0641 \u0632\u062f\u0646\u062f \u0631\u062f \u06af\u0631\u062f\u0634", "m2": "\u06af\u0631\u062f \u0627\u0648 \u0628\u0648\u062f \u06af\u0631\u062f \u0628\u0631 \u06af\u0631\u062f\u0634"}, "2": {"m1": "\u0639\u0644\u0645\u0634 \u0622\u0645\u062f \u06a9\u0647 \u06cc\u0627\u0631 \u063a\u0627\u0631 \u062a\u0648 \u0627\u0645", "m2": "\u0639\u0645\u0644 \u0622\u0645\u062f \u06a9\u0647 \u062f\u0648\u0633\u062a\u062f\u0627\u0631 \u062a\u0648 \u0627\u0645"}, "3": {"m1": "\u0627\u0639\u062a\u0642\u0627\u062f \u0622\u0645\u062f \u0648 \u062c\u0645\u0627\u0644\u06cc \u062e\u0648\u0634", "m2": "\u062d\u0627\u0644 \u0622\u0645\u062f \u0686\u0647 \u062d\u0627\u0644 \u060c \u062d\u0627\u0644\u06cc \u062e\u0648\u0634"}, "4": {"m1": "\u062c\u0644\u0648\u0647 \u06a9\u0631\u062f\u0647 \u0645\u0642\u0627\u0645 \u0627\u0648 \u060c \u0627\u0648 \u0631\u0627", "m2": "\u0631\u0647 \u0646\u0645\u0648\u062f\u0647 \u06a9\u0644\u0627\u0645 \u0627\u0648 \u060c \u0627\u0648 \u0631\u0627"}, "5": {"m1": "\u0631\u0648\u062d \u067e\u06cc\u063a\u0645\u0628\u0631\u0627\u0646 \u0628\u0647 \u0627\u0633\u062a\u0642\u0628\u0627\u0644", "m2": "\u0622\u0645\u062f\u0647 \u0627\u0632 \u0628\u0631\u0627\u06cc \u0639\u0632\u0651 \u0648 \u06a9\u0645\u0627\u0644"}, "6": {"m1": "\u0631\u062d\u0645\u062a \u062d\u0642 \u0646\u0632\u0648\u0644 \u0641\u0631\u0645\u0648\u062f\u0647", "m2": "\u0639\u0630\u0631 \u0627\u0645\u0651\u062a \u0642\u0628\u0648\u0644 \u0641\u0631\u0645\u0648\u062f\u0647"}, "7": {"m1": "\u0627\u0648\u0644 \u0635\u0628\u062d \u0648 \u0639\u0627\u0642\u0628\u062a \u0645\u062d\u0645\u0648\u062f", "m2": "\u0628\u0647 \u0633\u0644\u0627\u0645\u062a \u0639\u0632\u06cc\u0645\u062a\u06cc \u0641\u0631\u0645\u0648\u062f"}, "8": {"m1": "\u062c\u0627\u0646 \u067e\u0627\u06a9\u0634 \u0632 \u062a\u0646 \u0639\u0632\u06cc\u0645\u062a \u06a9\u0631\u062f", "m2": "\u062d\u0633 \u0648 \u0631\u0648\u062d\u0634 \u0631\u0648\u0627\u0646 \u0639\u0632\u06cc\u0645\u062a \u06a9\u0631\u062f"}, "9": {"m1": "\u0631\u0641\u062a \u0631\u0636\u0648\u0627\u0646 \u0632 \u0631\u0648\u0636\u0647 \u06a9\u0641 \u0628\u0631 \u06a9\u0641", "m2": "\u0632\u062f\u0647 \u062e\u0627\u0644 \u0633\u06cc\u0647 \u0628\u062f\u0648 \u0645\u0637\u0631\u0641"}, "10": {"m1": "\u062c\u0646\u0651\u062a \u0622\u0631\u0627\u0633\u062a\u0647 \u06a9\u0647 \u0634\u0627\u0647 \u0622\u0645\u062f", "m2": "\u0645\u0638\u0647\u0631 \u062d\u0636\u0631\u062a \u0627\u0644\u0647 \u0622\u0645\u062f"}, "11": {"m1": "\u062c\u0645\u0639 \u06cc\u0627\u0631\u0627\u0646 \u0686\u0648 \u0622\u0646\u0686\u0646\u0627\u0646 \u062f\u06cc\u062f\u0646\u062f", "m2": "\u063a\u0645 \u062f\u06cc\u0646 \u062f\u0627\u0634\u062a\u0646\u062f \u0648 \u062a\u0631\u0633\u06cc\u062f\u0646\u062f"}, "12": {"m1": "\u06a9\u0647 \u0645\u0628\u0627\u062f\u0627 \u06a9\u0647 \u062f\u06cc\u0646 \u062e\u0644\u0644 \u06cc\u0627\u0628\u062f", "m2": "\u062f\u0634\u0645\u0646\u06cc \u0627\u0632 \u0645\u06cc\u0627\u0646\u0647 \u0628\u0634\u062a\u0627\u0628\u062f"}, "13": {"m1": "\u0631\u0641\u062a\u0646 \u0627\u0648 \u0645\u062b\u0644 \u0628\u0647 \u062e\u0648\u0627\u0628 \u0632\u062f\u0646\u062f", "m2": "\u0686\u0646\u06af \u062f\u0631 \u0633\u0646\u0651\u062a \u0648 \u06a9\u062a\u0627\u0628 \u0632\u062f\u0646\u062f"}, "14": {"m1": "\u0646\u06cc\u06a9 \u062f\u0631\u06cc\u0627\u0628 \u0627\u06cc\u0646 \u0633\u062e\u0646 \u0628\u0647 \u062a\u0645\u0627\u0645", "m2": "\u062a\u0627 \u0628\u06cc\u0627\u0628\u06cc \u0645\u0631\u0627\u062f \u062e\u0648\u062f \u0648\u0627\u0644\u0633\u0644\u0627\u0645"}}}