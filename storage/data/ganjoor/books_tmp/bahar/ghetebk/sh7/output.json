{"text": {"0": {"m1": "\u06af\u0648\u060c \u0632 \u0645\u0646 \u0628\u0627\u062f \u0633\u062d\u0631\u06af\u0647 \u0628\u0647 \u0635\u0641\u0627\u0647\u0627\u0646\u06cc \u0632\u0634\u062a", "m2": "\u06a9\u0647 \u06a9\u06cc\u06cc \u062a\u0648 \u06a9\u0647 \u0628\u0647 \u0631\u062e \u0628\u0633\u062a\u0647\u200c\u0627\u06cc \u0627\u0632 \u062d\u06cc\u0644\u0647 \u0646\u0642\u0627\u0628"}, "1": {"m1": "\u063a\u06cc\u0631\u062a \u0627\u0632 \u062c\u0646\u0633 \u062a\u0648 \u0628\u0631\u062e\u06cc\u0632\u062f \u0627\u06af\u0631 \u0628\u0631\u062e\u06cc\u0632\u062f", "m2": "\u0633\u0646\u0628\u0644 \u0627\u0632 \u0634\u0648\u0631\u0647\u200c\u060c \u0645\u06cc \u0627\u0632 \u0633\u0631\u06a9\u0647 \u0648 \u0645\u0627\u0647\u06cc \u0632 \u0633\u0631\u0627\u0628"}, "2": {"m1": "\u062a\u06cc\u0631 \u062f\u0631 \u0686\u0634\u0645 \u062a\u0648 \u0686\u0648\u0646\u0627\u0646 \u06a9\u0647 \u0628\u0647 \u0686\u0634\u0645 \u062a\u0648 \u0645\u0698\u0647", "m2": "\u062a\u06cc\u0632 \u0628\u0631 \u0631\u06cc\u0634 \u062a\u0648 \u0686\u0648\u0646\u0627\u0646 \u06a9\u0647 \u0628\u0647\u200c \u0631\u06cc\u0634 \u062a\u0648 \u06af\u0644\u0627\u0628"}, "3": {"m1": "\u067e\u0646\u062c\u0647\u200c\u0627\u062a \u0628\u0627\u062f \u0642\u0644\u0645 \u062a\u0627 \u06a9\u0647 \u0628\u0647\u200c \u062f\u0633\u062a \u062a\u0648 \u0642\u0644\u0645", "m2": "\u062f\u0631 \u067e\u06cc\u062a \u0628\u0627\u062f \u0628\u0644\u0627 \u062a\u0627 \u06a9\u0647 \u0628\u0647 \u067e\u06cc\u0634 \u062a\u0648 \u06a9\u062a\u0627\u0628"}, "4": {"m1": "\u06a9\u0634\u0648\u0631 \u0627\u0632 \u062c\u0646\u0633 \u06a9\u062b\u06cc\u0641 \u062a\u0648 \u0686\u0648 \u062c\u0646\u0633 \u062a\u0648 \u06a9\u062b\u06cc\u0641", "m2": "\u0648\u0637\u0646 \u0627\u0632 \u0641\u06a9\u0631 \u062e\u0631\u0627\u0628 \u062a\u0648 \u0686\u0648 \u0641\u06a9\u0631 \u062a\u0648 \u062e\u0631\u0627\u0628"}, "5": {"m1": "\u062e\u0628\u062b\u200c\u060c \u067e\u0646\u0647\u0627\u0646 \u0628\u0647 \u0636\u0645\u06cc\u0631 \u062a\u0648 \u0686\u0648 \u0622\u062a\u0634 \u0628\u0647 \u062d\u062c\u0631", "m2": "\u0641\u062d\u0634\u200c\u060c \u067e\u06cc\u062f\u0627 \u0632 \u06a9\u0644\u0627\u0645 \u062a\u0648 \u0686\u0648 \u0628\u0627\u0631\u0627\u0646 \u0632 \u0633\u062d\u0627\u0628"}, "6": {"m1": "\u062a\u0648 \u0648 \u0646\u0627\u0647\u06cc\u062f \u067e\u062f\u0631 \u0633\u0648\u062e\u062a\u0647 \u0686\u0648\u0646 \u0639\u0648\u062f \u0648 \u0633\u0644\u06cc\u0645", "m2": "\u0646\u0631 \u0648 \u0645\u0627\u062f\u0647 \u0628\u0647\u0645 \u0627\u0641\u062a\u0627\u062f\u0647 \u0686\u0648 \u062f\u0631 \u06a9\u0648\u06cc \u06a9\u0644\u0627\u0628"}, "7": {"m1": "\u0646\u0627\u0645\u0647\u0654 \u062a\u0648 \u0628\u0628\u0631\u062a \u0686\u0648\u0646 \u0628\u0628\u0631 \u0645\u0631\u062f\u0647 \u06a9\u0641\u0646", "m2": "\u062e\u0627\u0645\u0647\u0654 \u0627\u0648 \u0628\u0647 \u06a9\u0641\u0634 \u0686\u0648\u0646 \u0628\u0647 \u06a9\u0641 \u0642\u062d\u0628\u0647 \u062e\u0636\u0627\u0628"}, "8": {"m1": "\u062a\u0648 \u0647\u062c\u0627 \u06af\u0648\u06cc\u06cc \u0648 \u0646\u0627\u0647\u06cc\u062f \u0632 \u062a\u0648 \u0646\u0642\u0644 \u06a9\u0646\u062f", "m2": "\u0646\u06cc\u06a9 \u0628\u0646\u06af\u0631 \u0628\u0647\u200c \u0686\u0647 \u0645\u0627\u0646\u062f \u0639\u0645\u0644 \u0622\u0646 \u062f\u0648 \u062c\u0646\u0627\u0628"}, "9": {"m1": "\u0686\u0648\u0646 \u06cc\u06a9\u06cc \u062e\u0631 \u06a9\u0647 \u0628\u0631\u0647 \u067e\u0634\u06af\u0644\u06a9\u06cc \u0627\u0646\u062f\u0627\u0632\u062f", "m2": "\u067e\u0633 \u062c\u0639\u0644 \u0646\u0642\u0644 \u06a9\u0646\u062f \u067e\u0634\u06af\u0644 \u0627\u0648 \u0631\u0627 \u0628\u0647\u200c \u0634\u062a\u0627\u0628"}, "10": {"m1": "\u062a\u0648 \u0686\u0648 \u0628\u0648\u062c\u0647\u0644\u06cc \u0648\u0646\u0627\u0647\u06cc\u062f \u0686\u0648 \u062d\u0645\u0627\u0644 \u062d\u0637\u0628", "m2": "\u0627\u06cc \u0633\u0631 \u0648 \u06af\u0631\u062f\u0646\u062a\u0627\u0646 \u062f\u0631\u062e\u0648\u0631 \u0634\u0645\u0634\u06cc\u0631 \u0648 \u0637\u0646\u0627\u0628"}, "11": {"m1": "\u0627\u06cc \u0633\u06cc\u0647 \u0646\u0627\u0645\u0647\u0654 \u0646\u0627\u0647\u06cc\u062f \u0648 \u0637\u0631\u0641\u062f\u0627\u0631 ...", "m2": "\u0622\u0644\u062a \u0622\u0644\u062a \u0628\u062f\u062e\u0648\u0627\u0647 \u0648\u0637\u0646 \u062f\u0631 \u0647\u0631 \u0628\u0627\u0628"}, "12": {"m1": "\u0648\u0631\u0642 \u0633\u0631\u062e \u0648 \u0633\u06cc\u0627\u0647 \u062a\u0648 \u0628\u0648\u062f \u06a9\u0647\u0646\u0647\u0654 \u062d\u06cc\u0636", "m2": "\u06cc\u0627 \u0631\u062e \u0641\u0627\u062d\u0634\u0647 \u06cc \u067e\u06cc\u0631\u06a9\u0647 \u0645\u0627\u0644\u062f \u0633\u0631\u062e\u0627\u0628"}, "13": {"m1": "\u0634\u063a\u0644 \u06a9\u0646\u0627\u0633\u06cc \u0648 \u0642\u0635\u0627\u0628\u06cc \u0628\u0627 \u0647\u0645 \u0646\u0633\u0632\u062f", "m2": "\u0627\u06cc \u0627\u062c\u0627\u0646\u0628 \u0631\u0627\u06a9\u0646\u0627\u0633 \u0648 \u0648\u0637\u0646 \u0631\u0627 \u0642\u0635\u0627\u0628"}, "14": {"m1": "\u0632\u0631 \u0632 \u00ab\u200c\u0647\u0627\u0648\u0627\u0631\u062f\u00bb \u0628\u06af\u06cc\u0631\u06cc \u0648 \u062f\u0647\u06cc \u0641\u062d\u0634 \u0628\u0647\u200c\u062e\u0644\u0642", "m2": "\u0632\u0628\u0631 \u0622\u0648\u0627\u0631 \u0628\u0645\u0627\u0646\u06cc \u062a\u0648 \u0648 \u06cc\u0627 \u0644\u06cc\u062a \u062a\u0631\u0627\u0628"}, "15": {"m1": "\u0632 \u0627\u0646\u06af\u0644\u0633\u062a\u0627\u0646 \u0645\u0633\u062a\u0627\u0646 \u067e\u0648\u0644 \u0648 \u0636\u06cc\u0627 \u0631\u0627 \u0645\u0633\u062a\u0627\u06cc", "m2": "\u06af\u0648\u0644 \u00ab\u200c\u0646\u0631\u0645\u0627\u0646\u200c\u00bb \u0645\u062e\u0648\u0631 \u0648 \u0633\u0648\u06cc \u062e\u06cc\u0627\u0646\u062a \u0645\u0634\u062a\u0627\u0628"}, "16": {"m1": "\u0632\u0627\u0646 \u06a9\u0647 \u0627\u0648 \u062e\u0627\u06a9 \u0648\u0637\u0646 \u0631\u0627 \u0628\u0647 \u0627\u062c\u0627\u0646\u0628 \u062f\u0627\u062f\u0633\u062a", "m2": "\u0628\u06af\u0631\u0641\u062a\u0647 \u0632\u0631 \u0648 \u0627\u0645\u0631\u0648\u0632 \u0628\u062a\u0631\u0633\u062f \u0632 \u062d\u0633\u0627\u0628"}, "17": {"m1": "\u0628\u0647\u0631 \u0645\u0634\u0631\u0648\u0637\u0647 \u06cc\u06a9\u06cc ... \u062a\u0631\u0627\u0634\u06cc\u062f\u0647 \u0632 \u0633\u0646\u06af", "m2": "\u0645\u0627\u0644\u0634\u0634 \u062f\u0627\u062f\u0647 \u0648 \u0634\u0642 \u06a9\u0631\u062f\u0647 \u0628\u0647\u200c \u062f\u0633\u062a \u0627\u0635\u062d\u0627\u0628"}, "18": {"m1": "\u0628\u0627\u0634 \u062a\u0627 \u0631\u0648\u0632 \u0628\u0647 \u0634\u0628 \u0646\u0627\u0645\u062f\u0647 \u0622\u0646\u200c...", "m2": "\u062a\u0627 \u062d\u062c\u0627\u0645\u062a\u06af\u0647 \u062a\u0648 \u0628\u0631 \u062f\u0631\u062f \u0627\u0632 \u06cc\u06a9 \u0634\u067e\u0634\u0627\u0628"}, "19": {"m1": "\u0642\u062f\u0631 \u0645\u0634\u0631\u0648\u0637\u0647 \u0646\u062f\u0627\u0646\u0633\u062a\u06cc \u0648 \u063a\u0627\u0641\u0644 \u06a9\u0647 \u062e\u062f\u0627\u06cc", "m2": "\u06a9\u0627\u0641\u0631 \u0646\u0639\u0645\u062a \u0631\u0627 \u0648\u0639\u062f\u0647 \u0646\u0645\u0648\u062f\u0633\u062a \u0639\u0630\u0627\u0628"}, "20": {"m1": "\u0628\u0631\u0634\u06a9\u0646 \u0634\u0627\u062e\u0647\u0654\u200c \u0622\u0632\u0627\u062f\u06cc \u0648 \u0686\u0648\u0646 \u06af\u0627\u0648 \u0628\u062e\u0648\u0631", "m2": "\u0628\u0631\u0641\u06a9\u0646 \u0631\u06cc\u0634\u0647\u0654 \u0645\u0634\u0631\u0648\u0637\u0647 \u0648 \u0686\u0648\u0646\u200c \u062e\u0631\u0633 \u0628\u0647\u200c \u062e\u0648\u0627\u0628"}, "21": {"m1": "\u0645\u06cc\u200c\u0634\u0648\u062f \u0632\u0648\u0631 \u0632 \u0634\u0648\u0645\u06cc \u0636\u06cc\u0627 \u0631\u0648\u0632 \u062a\u0648 \u0634\u0628", "m2": "\u0645\u06cc\u200c\u0634\u0648\u062f \u0632\u0648\u062f \u0632 \u0628\u06cc\u062f\u0627\u062f \u0631\u0636\u0627 \u0642\u0644\u0628 \u062a\u0648 \u0622\u0628"}}}