{"text": {"0": {"m1": "\u062a\u0644\u062e\u06cc \u0646\u06a9\u0646\u062f \u0634\u06cc\u0631\u06cc\u0646 \u0630\u0642\u0646\u0645", "m2": "\u062e\u0627\u0644\u06cc \u0646\u06a9\u0646\u062f \u0627\u0632 \u0645\u06cc \u062f\u0647\u0646\u0645"}, "1": {"m1": "\u0639\u0631\u06cc\u0627\u0646 \u06a9\u0646\u062f\u0645 \u0647\u0631 \u0635\u0628\u062d\u062f\u0645\u06cc", "m2": "\u06af\u0648\u06cc\u062f \u06a9\u0647 \u0628\u06cc\u0627 \u0645\u0646 \u062c\u0627\u0645\u0647 \u06a9\u0646\u0645"}, "2": {"m1": "\u062f\u0631 \u062e\u0627\u0646\u0647 \u062c\u0647\u062f \u0645\u0647\u0644\u062a \u0646\u062f\u0647\u062f", "m2": "\u0627\u0648 \u0628\u0633 \u0646\u06a9\u0646\u062f \u067e\u0633 \u0645\u0646 \u0686\u0647 \u06a9\u0646\u0645"}, "3": {"m1": "\u0627\u0632 \u0633\u0627\u063a\u0631 \u0627\u0648 \u06af\u06cc\u062c \u0627\u0633\u062a \u0633\u0631\u0645", "m2": "\u0627\u0632 \u062f\u06cc\u062f\u0646 \u0627\u0648 \u062c\u0627\u0646 \u0627\u0633\u062a \u062a\u0646\u0645"}, "4": {"m1": "\u062a\u0646\u06af \u0627\u0633\u062a \u0628\u0631 \u0627\u0648 \u0647\u0631 \u0647\u0641\u062a \u0641\u0644\u06a9", "m2": "\u0686\u0648\u0646 \u0645\u06cc \u0631\u0648\u062f \u0627\u0648 \u062f\u0631 \u067e\u06cc\u0631\u0647\u0646\u0645"}, "5": {"m1": "\u0627\u0632 \u0634\u06cc\u0631\u0647 \u0627\u0648 \u0645\u0646 \u0634\u06cc\u0631\u062f\u0644\u0645", "m2": "\u062f\u0631 \u0639\u0631\u0628\u062f\u0647\u200c\u0627\u0634 \u0634\u06cc\u0631\u06cc\u0646 \u0633\u062e\u0646\u0645"}, "6": {"m1": "\u0645\u06cc \u06af\u0641\u062a \u06a9\u0647 \u062a\u0648 \u062f\u0631 \u0686\u0646\u06af \u0645\u0646\u06cc", "m2": "\u0645\u0646 \u0633\u0627\u062e\u062a\u0645\u062a \u0686\u0648\u0646\u062a \u0646\u0632\u0646\u0645"}, "7": {"m1": "\u0645\u0646 \u0686\u0646\u06af \u062a\u0648\u0627\u0645 \u0628\u0631 \u0647\u0631 \u0631\u06af \u0645\u0646", "m2": "\u062a\u0648 \u0632\u062e\u0645\u0647 \u0632\u0646\u06cc \u0645\u0646 \u062a\u0646 \u062a\u0646\u0646\u0645"}, "8": {"m1": "\u062d\u0627\u0635\u0644 \u062a\u0648 \u0632 \u0645\u0646 \u062f\u0644 \u0628\u0631\u0646\u06a9\u0646\u06cc", "m2": "\u062f\u0644 \u0646\u06cc\u0633\u062a \u0645\u0631\u0627 \u0645\u0646 \u062e\u0648\u062f \u0686\u0647 \u06a9\u0646\u0645"}}}