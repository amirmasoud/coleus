{"text": {"0": {"m1": "\u0637\u0648\u0628\u06cc \u0644\u0645\u0646 \u0622\u0648\u0627\u0647 \u0633\u0631 \u0641\u0624\u0627\u062f\u0647", "m2": "\u0633\u06a9\u0646 \u0627\u0644\u0641\u0624\u0627\u062f \u0628\u0639\u0634\u0642\u0647 \u0648 \u0648\u062f\u0627\u062f\u0647"}, "1": {"m1": "\u0646\u0641\u0633 \u0627\u0644\u06a9\u0631\u06cc\u0645 \u06a9\u0645\u0631\u06cc\u0645 \u0648 \u0641\u0624\u0627\u062f\u0647", "m2": "\u0634\u0628\u0647 \u0627\u0644\u0645\u0633\u06cc\u062d \u0648 \u0635\u062f\u0631\u0647 \u06a9\u0645\u0647\u0627\u062f\u0647"}, "2": {"m1": "\u0627\u0630\u0646 \u0627\u0644\u0641\u0624\u0627\u062f \u0644\u06a9\u06cc \u06cc\u0628\u0648\u062d \u0628\u0633\u0631\u0647", "m2": "\u0634\u0631\u062d \u0627\u0644\u0635\u062f\u0648\u0631 \u06a9\u0631\u0627\u0645\u0647 \u0644\u0639\u0628\u0627\u062f\u0647"}, "3": {"m1": "\u0631\u062d\u0645 \u0627\u0644\u0642\u0644\u0648\u0628 \u0628\u0641\u062a\u062d\u200c\u0647\u0627 \u0648 \u0641\u062a\u0648\u062d\u200c\u0647\u0627", "m2": "\u0642\u0647\u0631 \u0627\u0644\u0646\u0641\u0648\u0633 \u0633\u06cc\u0627\u0633\u0647 \u0644\u062c\u0647\u0627\u062f\u0647"}, "4": {"m1": "\u06a9\u0634\u0641 \u0627\u0644\u063a\u0637\u0627\u0621 \u0648 \u0644\u0627 \u0627\u0646\u062a\u0638\u0627\u0631 \u0648 \u0644\u0627 \u0646\u0633\u0627", "m2": "\u0641\u0631\u062d \u0627\u0644\u0633\u0639\u06cc\u062f \u062a\u0627\u0646\u0633\u0627 \u0628\u0639\u062a\u0627\u062f\u0647"}, "5": {"m1": "\u0639\u0634\u0642\u0648\u0627 \u0644\u0631\u0623\u06cc\u0647 \u0631\u0628\u0647\u0645 \u0648 \u062a\u0639\u0644\u0642\u0648\u0627", "m2": "\u0648 \u0627\u0644\u0639\u0631\u0634 \u06cc\u062e\u0636\u0639 \u062d\u0627\u0644\u0647\u0645 \u0628\u0639\u0645\u0627\u062f\u0647"}, "6": {"m1": "\u0648 \u0635\u0644\u0648\u0627 \u0627\u0644\u06cc \u0646\u0638\u0631 \u0627\u0644\u062d\u0628\u06cc\u0628 \u0628\u0641\u0636\u0644\u0647", "m2": "\u0648 \u0627\u0644\u062d\u0642 \u0627\u0631\u0634\u062f\u0647\u0645 \u0628\u062d\u0633\u0646 \u0631\u0634\u0627\u062f\u0647"}, "7": {"m1": "\u0627\u0644\u0642\u0648\u0645 \u0645\u0639\u0634\u0648\u0642\u0648\u0646 \u0641\u06cc \u0627\u0648\u0635\u0627\u0641\u0647\u0645", "m2": "\u0648 \u0627\u0644\u062d\u0642 \u0639\u0627\u0634\u0642\u0647\u0645 \u0639\u0644\u06cc \u0627\u0641\u0631\u0627\u062f\u0647"}, "8": {"m1": "\u062d\u0627\u0631 \u0627\u0644\u0639\u0642\u0648\u0644 \u0628\u0647 \u0639\u0627\u0634\u0642\u06cc\u0647 \u062a\u062d\u06cc\u0631\u0627", "m2": "\u06a9\u06cc\u0641 \u0627\u0644\u0639\u0642\u0648\u0644 \u0628\u0647 \u0645\u0639\u0634\u0642\u06cc\u0647 \u0641\u0646\u0627\u062f\u0647"}, "9": {"m1": "\u0644\u0627 \u062a\u0646\u06a9\u0631\u0646 \u0648 \u0644\u0627 \u062a\u06a9\u0646 \u0645\u062a\u0635\u0631\u0641\u0627", "m2": "\u0628\u0627\u0644\u0639\u0642\u0644 \u0641\u06cc \u0647\u0630\u0627 \u0648 \u062e\u0641 \u0644\u06a9\u06cc\u0627\u062f\u0647"}, "10": {"m1": "\u0641\u0627\u0644\u0627\u0645\u0631 \u0627\u0639\u0638\u0645 \u0645\u0646 \u062a\u0635\u0631\u0641 \u062d\u06a9\u0645\u0646\u0627", "m2": "\u0648 \u0627\u0644\u0648\u062f \u0628\u0627\u0644\u062c\u0628\u0627\u0631 \u0645\u0646 \u0627\u0639\u0642\u0627\u062f\u0647"}, "11": {"m1": "\u0645\u0644\u06a9 \u0627\u0644\u0628\u0635\u06cc\u0631\u0647 \u0645\u0646 \u0645\u0645\u0627\u0644\u06a9 \u0634\u06cc\u062e\u0646\u0627", "m2": "\u06cc\u0639\u0637\u06cc \u0648 \u06cc\u0645\u0646\u0639 \u0645\u0627 \u06cc\u0634\u0627 \u0628\u0645\u0631\u0627\u062f\u0647"}, "12": {"m1": "\u0645\u0627 \u063a\u0627\u0628 \u0645\u0646 \u0642\u0644\u0628\u06cc \u0634\u0639\u0627\u0634\u0639 \u062e\u062f\u0647", "m2": "\u0644\u0627 \u062a\u0634\u0645\u062a\u0648\u0627 \u0628\u0635\u062f\u0648\u062f\u0647 \u0648 \u0628\u0639\u0627\u062f\u0647"}, "13": {"m1": "\u0634\u0645\u0633 \u0627\u0644\u0645\u0635\u06cc\u0641 \u0627\u0630\u0627 \u0646\u06cc \u0628\u063a\u0631\u0648\u0628\u0647", "m2": "\u0645\u0627 \u063a\u0627\u0628 \u062d\u0631 \u0627\u0644\u0634\u0645\u0633 \u0645\u0646 \u0639\u0628\u0627\u062f\u0647"}, "14": {"m1": "\u062a\u0628\u0631\u06cc\u0632 \u062c\u0644 \u0628\u0647 \u0634\u0645\u0633 \u062f\u06cc\u0646 \u0633\u06cc\u062f\u06cc", "m2": "\u0645\u0627 \u0627\u06a9\u0631\u0645 \u0627\u0644\u0645\u0648\u0644\u06cc \u0628\u06a9\u062b\u0631 \u0631\u0645\u0627\u062f\u0647"}}}