{"text": {"0": {"m1": "\u0627\u06cc \u062e\u0648\u0627\u0628 \u0628\u0647 \u0631\u0648\u0632 \u0647\u0645\u062f\u0645\u0627\u0646\u0645", "m2": "\u062a\u0627 \u0628\u06cc\u200c\u06a9\u0633 \u0648 \u0645\u0645\u062a\u062d\u0646 \u0646\u0645\u0627\u0646\u0645"}, "1": {"m1": "\u0686\u0648\u0646\u06a9 \u062f\u06cc\u06a9 \u0628\u0631 \u0622\u062a\u0634\u0645 \u0646\u0634\u0627\u0646\u062f\u06cc", "m2": "\u062f\u0631 \u062f\u06cc\u06a9 \u0686\u0647 \u0645\u06cc\u200c\u067e\u0632\u06cc\u060c \u0686\u0647 \u062f\u0627\u0646\u0645"}, "2": {"m1": "\u06cc\u06a9 \u0644\u062d\u0638\u0647 \u06a9\u0647 \u0645\u0646 \u0633\u0631\u06cc \u0628\u062e\u0627\u0631\u0645", "m2": "\u0627\u06cc \u0639\u0634\u0642 \u0646\u0645\u06cc\u200c\u062f\u0647\u06cc \u0627\u0645\u0627\u0646\u0645"}, "3": {"m1": "\u0627\u0632 \u062e\u0634\u0645 \u062f\u0648 \u06af\u0648\u0634 \u062d\u0644\u0645 \u0628\u0633\u062a\u06cc", "m2": "\u062a\u0627 \u0646\u0634\u0646\u0648\u06cc \u0622\u0647\u0648\u0647 \u0648\u0641\u063a\u0627\u0646\u0645"}, "4": {"m1": "\u0645\u0627 \u0631\u0627 \u0628\u0647 \u062c\u0647\u0627\u0646 \u062d\u0648\u0627\u0644\u0647 \u06a9\u0645 \u06a9\u0646", "m2": "\u0627\u06cc \u062c\u0627\u0646 \u0686\u0648 \u06a9\u0647 \u0645\u0646 \u0646\u0647 \u0632\u06cc\u0646 \u062c\u0647\u0627\u0646\u0645"}, "5": {"m1": "\u0628\u06af\u0634\u0627\u06cc \u0631\u0647\u0645 \u06a9\u0647 \u062a\u0627 \u0633\u0628\u06a9\u062a\u0631", "m2": "\u062c\u0627\u0646 \u0631\u0627 \u0628\u0647 \u062c\u0647\u0627\u0646 \u062c\u0627\u0646 \u0631\u0633\u0627\u0646\u0645"}, "6": {"m1": "\u06cc\u0627\u0631\u06cc \u0641\u0631\u0645\u0627\u060c \u0642\u0644\u0627\u0648\u0632\u06cc \u06a9\u0646", "m2": "\u062a\u0627 \u0631\u062e\u062a \u0628\u06a9\u0648\u06cc \u062a\u0648 \u06a9\u0634\u0627\u0646\u0645"}, "7": {"t2": "\u062a\u0631\u062c\u06cc\u0639 \u06a9\u0646 \u06af\u0631\u06cc\u0646 \u0628\u0646\u0648\u0634\u06cc", "t1": "\u0627\u06cc \u0622\u0646\u06a9 \u062a\u0648 \u062c\u0627\u0646 \u0627\u06cc\u0646 \u0646\u0642\u0648\u0634\u06cc"}, "8": {"m1": "\u062a\u06cc\u0632\u0622\u0628 \u062a\u0648\u06cc\u060c \u0648 \u0686\u0631\u062e \u0645\u0627\u06cc\u06cc\u0645", "m2": "\u0633\u0631\u06af\u0634\u062a\u0647 \u0686\u0648 \u0633\u0646\u06af \u0622\u0633\u06cc\u0627\u06cc\u06cc\u0645"}, "9": {"m1": "\u062a\u0648 \u062e\u0648\u0631\u0634\u06cc\u062f\u06cc \u0648 \u0645\u0627 \u0686\u0648 \u0630\u0631\u0647", "m2": "\u0627\u0632 \u06a9\u0648\u0647 \u0628\u0631\u0622\u06cc \u062a\u0627 \u0628\u0631\u0622\u06cc\u06cc\u0645"}, "10": {"m1": "\u0627\u0632 \u0628\u0647\u0631 \u0633\u06a9\u0646\u062c\u0628\u06cc\u0646 \u0639\u0633\u0644 \u062f\u0647", "m2": "\u0645\u0627 \u062e\u0648\u062f \u0647\u0645\u0647 \u0633\u0631\u06a9\u0647 \u0645\u06cc\u200c\u0641\u0632\u0627\u06cc\u06cc\u0645"}, "11": {"m1": "\u06af\u0647 \u062e\u06cc\u0631\u0647\u0654 \u062a\u0648\u060c \u06a9\u0647 \u062a\u0648 \u06a9\u062c\u0627\u06cc\u06cc", "m2": "\u06af\u0647 \u062e\u06cc\u0631\u0647\u0654 \u062e\u0648\u062f \u06a9\u0647 \u0645\u0627 \u06a9\u062c\u0627\u06cc\u06cc\u0645"}, "12": {"m1": "\u06af\u0647 \u062e\u06cc\u0631\u0647\u0654 \u0628\u0633\u0637 \u062e\u0648\u06cc\u0634 \u0648 \u0627\u06cc\u062b\u0627\u0631", "m2": "\u06cc\u0627 \u0642\u0628\u0636 \u06a9\u0647 \u0645\u0647\u0631\u0647 \u062f\u0631 \u0631\u0628\u0627\u06cc\u06cc\u0645"}, "13": {"m1": "\u06af\u0627\u0647\u06cc \u0645\u0633 \u0648 \u06af\u0627\u0647 \u0632\u0631 \u062e\u0627\u0644\u0635", "m2": "\u06af\u0627\u0647 \u0627\u0632 \u067e\u06cc \u0647\u0631\u062f\u0648 \u06a9\u06cc\u0645\u06cc\u0627\u06cc\u06cc\u0645"}, "14": {"t2": "\u062f\u0631 \u062f\u0627\u062f\u0646 \u0648 \u062f\u0631 \u06af\u0631\u0641\u062a\u0646 \u0627\u0632 \u0686\u06cc", "t1": "\u062a\u0631\u062c\u06cc\u0639 \u062f\u0648\u060c \u0630\u0648\u0642 \u0648 \u0645\u06cc\u0644 \u0627\u06cc\u0686\u06cc"}, "15": {"m1": "\u06af\u0647 \u0634\u0627\u062f \u0628\u062e\u0648\u0631\u062f\u0646\u0633\u062a \u0648 \u062a\u062d\u0635\u06cc\u0644", "m2": "\u06af\u0647 \u0634\u0627\u062f \u0628\u0647 \u062e\u0631\u062c \u0622\u0646 \u0648 \u062a\u062d\u0644\u06cc\u0644"}, "16": {"m1": "\u0686\u0648\u0646 \u0646\u062e\u0644\u060c \u06af\u0647\u06cc \u0628\u0647 \u06a9\u0633\u0628 \u0645\u06cc\u0648\u0647", "m2": "\u06af\u0627\u0647\u06cc \u0628\u0647 \u0646\u062b\u0627\u0631 \u0622\u0646 \u0648 \u062a\u0646\u0632\u06cc\u0644"}, "17": {"m1": "\u06af\u0647 \u062d\u0627\u062a\u0645 \u0648\u0642\u062a \u0627\u0646\u062f\u0631 \u0627\u06cc\u062b\u0627\u0631", "m2": "\u06af\u0647 \u0639\u0628\u0627\u0633\u06cc \u0628\u0647 \u0637\u0648\u0641 \u0648 \u0632\u0646\u0628\u06cc\u0644"}, "18": {"m1": "\u0645\u0627 \u06cc\u0627 \u0622\u0646\u06cc\u0645 \u0648 \u0627\u06cc\u0646 \u062f\u06af\u0631 \u0641\u0631\u0639", "m2": "\u06cc\u0627 \u063a\u06cc\u0631 \u062a\u0648\u06cc\u0645 \u0628\u06cc\u200c\u062f\u0648 \u062a\u0628\u062f\u06cc\u0644"}, "19": {"m1": "\u0648\u0631 \u0632\u0627\u0646\u06a9 \u0645\u0631\u06a9\u0628 \u0627\u0632 \u062f\u0648 \u0636\u062f\u06cc\u0645", "m2": "\u062a\u0630\u0644\u06cc\u0644 \u0646\u0628\u0627\u0634\u062f\u06cc \u0648 \u062a\u0628\u062c\u06cc\u0644"}, "20": {"m1": "\u0647\u0645 \u0627\u0635\u0644\u0627\u062d\u0633\u062a \u0639\u0632 \u0648 \u0630\u0644\u0634", "m2": "\u0645\u0627\u0646\u0646\u062f\u0647\u0654 \u0631\u0641\u0639 \u0648 \u062e\u0641\u0636 \u0642\u0646\u062f\u06cc\u0644"}, "21": {"m1": "\u0628\u0633 \u0627\u0635\u0644\u0627\u062d\u06cc \u0628\u0631\u0627\u06cc \u0627\u0641\u0633\u0627\u062f", "m2": "\u0628\u0633 \u0627\u0641\u0633\u0627\u062f\u06cc \u0628\u0631\u0627\u06cc \u062a\u0646\u062d\u06cc\u0644"}, "22": {"t2": "\u062e\u0631\u0637\u0648\u0645 \u0647\u0632\u0627\u0631 \u067e\u06cc\u0644 \u062e\u0633\u062a\u0647", "t1": "\u0628\u0633 \u0645\u0631\u063a \u0636\u0639\u06cc\u0641 \u067e\u0631\u0634\u06a9\u0633\u062a\u0647"}}}