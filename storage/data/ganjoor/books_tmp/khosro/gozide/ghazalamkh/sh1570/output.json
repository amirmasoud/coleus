{"text": {"0": {"m1": "\u0627\u06cc \u062f\u0644\u060c \u0628\u0647 \u0686\u0634\u0645 \u0639\u0628\u0631\u062a \u0646\u0638\u0627\u0631\u0647 \u062c\u0647\u0627\u0646 \u06a9\u0646", "m2": "\u0638\u0627\u0647\u0631 \u0646\u0647\u0627\u0646 \u0686\u0647 \u0628\u06cc\u0646\u06cc\u060c \u0646\u0638\u0627\u0631\u0647 \u0646\u0647\u0627\u0646 \u06a9\u0646"}, "1": {"m1": "\u067e\u0631\u0648\u0627\u0632 \u06a9\u0646 \u0628\u0647 \u0647\u0645\u062a\u060c \u0628\u0631 \u067e\u0631 \u0628\u0647 \u0627\u0648\u062c \u0639\u0632\u062a", "m2": "\u062c\u0628\u0631\u0626\u06cc\u0644 \u0627\u0648\u062c \u062e\u0648\u062f \u0634\u0648\u060c \u0628\u0631 \u0633\u062f\u0631\u0647 \u0622\u0634\u06cc\u0627\u0646 \u06a9\u0646"}, "2": {"m1": "\u0686\u0634\u0645\u062a \u0686\u0648 \u062a\u0646\u062f\u06af\u06cc\u0631\u06cc \u0686\u0648\u0646 \u067e\u0631\u062f\u0647 \u0647\u0627\u06cc \u062f\u06cc\u062f\u0647", "m2": "\u0628\u06af\u0634\u0627\u06cc \u067e\u0631\u062f\u0647 \u062f\u0644\u060c \u0633\u0631\u067e\u0648\u0634 \u0627\u0632 \u0622\u0646 \u0631\u0648\u0627\u0646 \u06a9\u0646"}, "3": {"m1": "\u0632\u0627\u0646 \u0632\u0631 \u06a9\u0647 \u06a9\u0645 \u062e\u0648\u0631\u06cc \u06af\u0631 \u062f\u0627\u0645\u0646 \u06af\u0631\u0627\u0646\u062a \u0645\u0627\u0646\u062f", "m2": "\u0628\u0631 \u062f\u0627\u0631 \u062e\u0627\u06a9 \u0648 \u062e\u0627\u0631\u0627 \u062f\u0627\u0645\u0627\u0646 \u062e\u0648\u062f \u06af\u0631\u0627\u0646 \u06a9\u0646"}, "4": {"m1": "\u0639\u0645\u0631 \u0631\u0648\u0646\u062f\u0647 \u062e\u0648\u0627\u0647\u06cc \u067e\u0627\u06cc\u0646\u062f\u0647 \u062a\u0627 \u0642\u06cc\u0627\u0645\u062a", "m2": "\u0632\u0646\u0647\u0627\u0631 \u0646\u0627\u0645 \u0646\u06cc\u06a9\u0648 \u0628\u0627 \u0639\u0645\u0631 \u0647\u0645\u0639\u0646\u0627\u0646 \u06a9\u0646"}, "5": {"m1": "\u06af\u0631 \u062a\u062e\u062a \u0639\u0627\u062c \u062e\u0648\u0627\u0647\u06cc\u060c \u062e\u0648\u062f \u0631\u0627 \u0628\u0644\u0646\u062f \u0645\u0646\u06af\u0631", "m2": "\u062f\u0631 \u062e\u0627\u06a9 \u062a\u0633\u062a \u0628\u0627\u062f\u06cc\u060c \u0632\u0627\u0646 \u0645\u0634\u062a\u06cc \u0627\u0633\u062a\u062e\u0648\u0627\u0646 \u06a9\u0646"}, "6": {"m1": "\u0628\u0631 \u062e\u0648\u06cc\u0647\u0627\u06cc \u0646\u0627\u062e\u0648\u0634 \u0646\u0647 \u0628\u0627\u062f \u0628\u0631\u062f\u060c \u0628\u0627\u0631\u06cc", "m2": "\u062f\u0627\u0631\u06cc \u0631\u0645\u06cc \u0632 \u06af\u0631\u06af\u0627\u0646 \u0632\u0631 \u0627\u0628\u0631\u0648\u06cc \u0634\u0628\u0627\u0646 \u06a9\u0646"}, "7": {"m1": "\u0648\u0631 \u062f\u0631 \u0635\u062f\u0641 \u0686\u0646\u0627\u0646\u06cc \u06a9\u0622\u0631\u0646\u062f \u0631\u0648\u06cc \u062f\u0631 \u062a\u0648", "m2": "\u0622\u06cc\u06cc\u0646\u0647 \u0647\u0627\u06cc \u062e\u0648\u062f \u0631\u0627 \u0622\u06cc\u06cc\u0646\u0647 \u062c\u0647\u0627\u0646 \u06a9\u0646"}, "8": {"m1": "\u0631\u062e \u0633\u0648\u06cc \u0634\u0627\u0647 \u062f\u0644 \u0646\u0647\u060c \u06a9\u0634 \u062f\u0631 \u063a\u0632\u0627 \u062e\u0631\u062f \u0631\u0627", "m2": "\u067e\u0633 \u0627\u0633\u067e \u0639\u0634\u0642 \u062f\u0631 \u0631\u0627\u0646\u060c \u0641\u0631\u0632\u06cc\u0646\u0634 \u0631\u0627\u06cc\u06af\u0627\u0646 \u06a9\u0646"}, "9": {"m1": "\u0628\u06cc\u0646 \u0634\u0645\u0639 \u06a9\u0634 \u0632 \u0633\u0648\u0632\u0634 \u06a9\u0634\u062a\u0647 \u0633\u062a \u062c\u0627\u0646 \u0631\u0648\u0634\u0646", "m2": "\u06af\u0631 \u0631\u0648\u0634\u0646\u06cc\u062a \u0628\u0627\u06cc\u062f\u060c \u062a\u0646 \u0631\u0627 \u0628\u0633\u0648\u0632 \u062c\u0627\u0646 \u06a9\u0646"}, "10": {"m1": "\u062e\u0633\u0631\u0648 \u0628\u0647 \u0645\u0644\u06a9 \u0634\u0647\u0631\u062a \u0686\u0646\u062f\u062a \u0632\u0628\u0627\u0646 \u0647\u0631\u0632\u0647", "m2": "\u0639\u0627\u0644\u0645 \u0647\u0645\u0647 \u06af\u0631\u0641\u062a\u06cc\u060c \u0634\u0645\u0634\u06cc\u0631 \u062f\u0631 \u0645\u06cc\u0627\u0646 \u06a9\u0646"}}}