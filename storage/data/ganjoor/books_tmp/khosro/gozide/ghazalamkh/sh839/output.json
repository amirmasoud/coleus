{"text": {"0": {"m1": "\u0627\u0647\u0644 \u062e\u0631\u062f \u06a9\u0647 \u0627\u0632 \u0647\u0645\u0647 \u0639\u0627\u0644\u0645 \u0628\u0631\u06cc\u062f\u0647 \u0627\u0646\u062f", "m2": "\u062f\u0627\u0646\u062f \u062e\u0631\u062f \u06a9\u0647 \u0627\u0632 \u0686\u0647 \u0628\u0647 \u06a9\u0646\u062c \u0622\u0631\u0645\u06cc\u062f\u0647 \u0627\u0646\u062f"}, "1": {"m1": "\u062f\u0627\u0646\u0646\u062f\u06af\u0627\u0646 \u06a9\u0647 \u0648\u0642\u062a \u062c\u0647\u0627\u0646 \u062e\u0648\u0634 \u0628\u062f\u06cc\u062f\u0647 \u0627\u0646\u062f", "m2": "\u062e\u0648\u0634 \u0648\u0642\u062a \u0634\u0627\u0646 \u06a9\u0647 \u06af\u0648\u0634\u0647 \u0639\u0632\u0644\u062a \u06af\u0632\u06cc\u062f\u0647 \u0627\u0646\u062f"}, "2": {"m1": "\u0645\u062d\u0631\u0645 \u062f\u0631\u0648\u0646 \u067e\u0631\u062f\u0647 \u0645\u0642\u0635\u0648\u062f \u0646\u06cc\u0633\u062a\u0646\u062f", "m2": "\u062c\u0632 \u0639\u0627\u0634\u0642\u0627\u0646 \u06a9\u0647 \u067e\u0631\u062f\u0647 \u0639\u0635\u0645\u062a \u062f\u0631\u06cc\u062f\u0647 \u0627\u0646\u062f"}, "3": {"m1": "\u0628\u0631\u062a\u0631 \u062c\u0647\u0627\u0646 \u0628\u0647 \u062c\u0627\u062f\u0647 \u0647\u0645\u062a \u06a9\u0647 \u06a9\u0627\u0647\u0644 \u0627\u0646\u062f", "m2": "\u0622\u0646 \u0628\u062e\u062a\u06cc\u0627\u0646 \u06a9\u0647 \u0633\u062f\u0631\u0647 \u0648 \u0637\u0648\u0628\u06cc \u0686\u0631\u06cc\u062f\u0647 \u0627\u0646\u062f"}, "4": {"m1": "\u062f\u0631 \u0628\u06cc\u0636\u0647 \u067e\u0631 \u0645\u0631\u063a \u0628\u0631\u0648\u06cc\u062f\u060c \u0628\u0631\u0648\u0646 \u062a\u0631 \u0622\u06cc", "m2": "\u06a9\u062a \u067e\u0631 \u062f\u0647\u062f\u060c \u06a9\u0632\u0627\u0646 \u0628\u0647 \u0628\u0644\u0646\u062f\u06cc \u067e\u0631\u06cc\u062f\u0647 \u0627\u0646\u062f"}, "5": {"m1": "\u062c\u0627\u0646 \u0646\u06cc\u0632 \u0647\u0633\u062a \u0628\u0627 \u062f\u06af\u0631\u0627\u0646 \u0627\u06cc\u0646 \u06af\u0631\u0648\u0647 \u0631\u0627", "m2": "\u06a9\u0632 \u0628\u0647\u0631 \u0639\u0632\u0645 \u0639\u0627\u0644\u0645 \u0648\u062d\u062f\u062a \u067e\u0631\u06cc\u062f\u0647 \u0627\u0646\u062f"}, "6": {"m1": "\u0646\u0627\u0631\u0641\u062a\u0647 \u0631\u0647\u060c \u0631\u0648\u0646\u062f\u0647 \u0628\u0647 \u062c\u0627\u06cc\u06cc \u0646\u0645\u06cc \u0631\u0633\u062f", "m2": "\u0646\u0627\u0686\u0627\u0631 \u0631\u0641\u062a\u0647 \u0627\u0646\u062f \u0631\u0647\u060c \u0622\u0646\u06af\u0647 \u0631\u0633\u06cc\u062f\u0647 \u0627\u0646\u062f"}, "7": {"m1": "\u0648\u0627\u0646 \u062c\u0627\u0646 \u06a9\u0646\u0627\u0646 \u06a9\u0647 \u062f\u0631 \u063a\u0645 \u0645\u0627\u0644 \u0627\u0633\u062a \u062c\u0627\u0646 \u0634\u0627\u0646", "m2": "\u062c\u0627\u0646 \u062f\u0627\u062f\u0647 \u0627\u0646\u062f \u0648 \u067e\u0627\u0631\u0647 \u062e\u0627\u06a9\u06cc \u062e\u0631\u06cc\u062f\u0647 \u0627\u0646\u062f"}, "8": {"m1": "\u062e\u0633\u0631\u0648\u060c \u0645\u06af\u0648\u06cc \u0628\u062f \u06a9\u0647 \u062f\u0631\u06cc\u0646 \u06af\u0646\u0628\u062f \u0627\u0632 \u0635\u062f\u0627", "m2": "\u062e\u0644\u0642 \u0622\u0646\u0686\u0647 \u06af\u0641\u062a\u0647 \u0627\u0646\u062f\u060c \u0647\u0645\u0627\u0646 \u0631\u0627 \u0634\u0646\u06cc\u062f\u0647 \u0627\u0646\u062f"}}}