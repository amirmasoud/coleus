{"text": {"0": {"m1": "\u0635\u062f\u0631 \u062f\u06cc\u0646 \u0634\u0645\u0633\u0647\u0654 \u0627\u0626\u0645\u0651\u0647 \u0639\u064f\u0645\u064e\u0631", "m2": "\u06a9\u0647 \u0646\u06cc\u0627\u0631\u062f \u0686\u0646\u0648 \u0632\u0645\u0627\u0646\u0647 \u062f\u06af\u0631"}, "1": {"m1": "\u0634\u0631\u0628\u062a \u0634\u0631\u0639 \u0648 \u062f\u06cc\u0646 \u0632 \u0628\u0627\u063a \u0631\u0633\u0648\u0644", "m2": "\u0627\u0632 \u0646\u0633\u06cc\u0645 \u0641\u062a\u0648\u062d \u06a9\u0631\u062f\u0647 \u0642\u0628\u0648\u0644"}, "2": {"m1": "\u062d\u0627\u0641\u0638 \u0634\u0631\u0639 \u0628\u0647\u0631 \u067e\u06cc\u0648\u0646\u062f\u0634", "m2": "\u062f\u06cc\u062f\u0647\u0654 \u062c\u0627\u0646 \u0646\u062f\u06cc\u062f\u0647 \u0645\u0627\u0646\u0646\u062f\u0634"}, "3": {"m1": "\u0627\u0632 \u0639\u0632\u0627\u0632\u06cc\u0644 \u0646\u0646\u06af\u0631\u06cc  \u06a9\u0647 \u0628\u062a\u0641\u062a", "m2": "\u062f\u06cc\u0631 \u0628\u0634\u0646\u06cc\u062f \u0627\u0645\u0631 \u0648 \u0632\u0648\u062f \u0628\u0631\u0641\u062a"}, "4": {"m1": "\u0627\u0632 \u0646\u0647\u06cc\u0628 \u0628\u0632\u0631\u06af \u0645\u0627\u06cc\u0647\u0654 \u0627\u0648", "m2": "\u0645\u06cc\u200c\u06af\u0631\u06cc\u0632\u062f \u0632 \u0633\u0647\u0645 \u0633\u0627\u06cc\u0647\u0654 \u0627\u0648"}, "5": {"m1": "\u062d\u0641\u0638 \u0627\u0648 \u062a\u0627 \u062c\u0646\u0627\u0628 \u0634\u0631\u0639 \u0633\u067e\u0631\u062f", "m2": "\u062f\u06cc\u0648 \u0646\u0633\u06cc\u0627\u0646 \u0627\u0632\u0648 \u062c\u0646\u0627\u0628\u062a \u0628\u0631\u062f"}, "6": {"m1": "\u062a\u0646\u0634 \u0627\u0632 \u0628\u0633 \u06a9\u0647 \u067e\u0627\u0633 \u062f\u06cc\u0646 \u062f\u0627\u0631\u062f", "m2": "\u0622\u0633\u0645\u0627\u0646 \u0686\u0634\u0645 \u0628\u0631 \u0632\u0645\u06cc\u0646 \u062f\u0627\u0631\u062f"}, "7": {"m1": "\u0635\u0648\u0631\u062a \u0627\u0645\u0646 \u0627\u0648 \u062e\u0641\u06cc\u0641\u200c\u0627\u0644\u062d\u062c\u0645", "m2": "\u0644\u06cc\u06a9 \u0645\u064f\u0631\u0634\u062f \u0628\u0633\u0627\u0646 \u0646\u06a9\u062a\u0647 \u0648 \u0639\u062c\u0645"}, "8": {"m1": "\u0628\u06cc\u0646\u06cc \u0622\u0646 \u0630\u0627\u062a \u067e\u0631 \u0644\u0637\u0627\u0641\u062a \u0627\u0648", "m2": "\u0648\u0627\u0646 \u0635\u0641\u0627\u06cc \u0628\u0631\u06cc \u0632 \u0622\u0641\u062a \u0627\u0648"}, "9": {"m1": "\u0647\u0645 \u0641\u0635\u06cc\u062d \u0633\u0632\u0627\u06cc \u06af\u0641\u062a\u0627\u0631\u0633\u062a", "m2": "\u0647\u0645 \u0635\u0628\u06cc\u062d \u0645\u0644\u06cc\u062d \u062f\u06cc\u062f\u0627\u0631\u0633\u062a"}, "10": {"m1": "\u0644\u0627\u062c\u0631\u0645 \u0646\u0637\u0642\u0634 \u0627\u0646\u062f\u0631\u06cc\u0646 \u0645\u0646\u0632\u0644", "m2": "\u0647\u0645\u0686\u0648 \u0639\u06cc\u0633\u06cc \u0632 \u06af\u0650\u0644 \u0646\u0645\u0627\u06cc\u062f \u062f\u0644"}, "11": {"m1": "\u0647\u0633\u062a \u0631\u0637\u0628\u200c\u0627\u0644\u0644\u0633\u0627\u0646 \u0628\u0647 \u0645\u062f\u062d\u062a \u0627\u0648", "m2": "\u062c\u0628\u0631\u0626\u06cc\u0644 \u0627\u0632 \u06a9\u0645\u0627\u0644 \u0631\u0641\u0639\u062a \u0627\u0648"}, "12": {"m1": "\u0647\u0645 \u0633\u0631\u0627\u06cc \u0633\u0631\u0648\u0631 \u0627\u0632\u0648 \u0622\u0628\u0627\u062f", "m2": "\u0647\u0645 \u0647\u0645\u0647 \u062f\u0648\u0633\u062a\u0627\u0646 \u0627\u0632\u0648 \u062f\u0644\u0634\u0627\u062f"}, "13": {"m1": "\u0686\u0648\u0646 \u062f\u0639\u0627 \u0631\u0627 \u0646\u0647\u0627\u062f \u062e\u0648\u0627\u0647\u062f \u0628\u0631\u062e", "m2": "\u0639\u06cc\u0633\u06cc \u0622\u0645\u06cc\u0646 \u06a9\u0646\u062f \u0632 \u0686\u0627\u0631\u0645 \u0686\u0631\u062e"}, "14": {"m1": "\u0633\u0648\u0632 \u0633\u06cc\u0646\u0647\u200c\u0634 \u0627\u06af\u0631 \u0639\u06cc\u0627\u0646 \u06af\u0631\u062f\u062f", "m2": "\u0686\u0646\u0628\u0631\u0650 \u0686\u0631\u062e \u0631\u0627\u06cc\u06af\u0627\u0646 \u06af\u0631\u062f\u062f"}, "15": {"m1": "\u0634\u0627\u062f\u06cc \u0622\u0645\u062f \u0686\u0648 \u0627\u0648 \u0628\u0647 \u0635\u062f\u0631 \u0646\u0634\u0633\u062a", "m2": "\u0628\u0631 \u0633\u0631\u0650 \u062f\u0633\u062a \u0628\u0631\u0646\u0647\u0627\u062f\u0647 \u0628\u062f\u0633\u062a"}, "16": {"m1": "\u0635\u0641\u062a \u0635\u0641\u0648\u062a \u062f\u0644 \u067e\u0627\u06a9\u0634", "m2": "\u0646\u0639\u062a \u0646\u0637\u0642 \u0634\u06af\u0631\u0641 \u0648 \u0686\u0627\u0644\u0627\u06a9\u0634"}, "17": {"m1": "\u067e\u0631\u062f\u0647\u0654 \u0639\u0631\u0634 \u0648 \u0622\u06cc\u0647\u0654 \u0627\u0644\u06a9\u0631\u0633\u06cc\u0633\u062a", "m2": "\u0634\u0647\u062f \u0641\u0631\u062f\u0648\u0633 \u0648 \u062d\u062c\u0631\u0647\u0654 \u0642\u062f\u0633\u06cc\u0633\u062a"}, "18": {"m1": "\u0627\u0632 \u0645\u0631\u0648\u0651\u062a \u0644\u0637\u06cc\u0641 \u0645\u0646\u0632\u0644\u200c\u062a\u0631", "m2": "\u0648\u0631 \u0642\u0646\u0627\u0639\u062a \u062e\u0641\u06cc\u0641 \u0645\u062d\u0645\u0644\u200c\u062a\u0631"}, "19": {"m1": "\u0647\u0631 \u0639\u0628\u0627\u0631\u062a \u06a9\u0632 \u0622\u0646 \u0641\u0635\u06cc\u062d \u0622\u06cc\u062f", "m2": "\u062f\u0645 \u0628\u064f\u0648\u064e\u062f \u06a9\u0632 \u0644\u0628 \u0645\u0633\u06cc\u062d \u0622\u06cc\u062f"}, "20": {"m1": "\u0647\u0631\u06a9\u0647 \u0628\u0631 \u0622\u0633\u062a\u0627\u0646 \u062f\u06cc\u0646 \u0628\u0627\u0634\u062f", "m2": "\u0639\u06cc\u0633\u06cc \u0645\u0631\u06cc\u0645 \u0622\u0633\u062a\u06cc\u0646 \u0628\u0627\u0634\u062f"}, "21": {"m1": "\u062e\u0635\u0645 \u062f\u0631 \u062f\u0633\u062a \u062e\u0627\u0637\u0631\u0634 \u0686\u06cc\u0631\u0634", "m2": "\u06a9\u064f\u0646\u062f \u0628\u0627\u0634\u062f \u0686\u0648 \u067e\u0634\u062a \u0634\u0645\u0634\u06cc\u0631\u0634"}, "22": {"m1": "\u062a\u0627 \u0628\u062f\u0648 \u062e\u0648\u06cc\u0634\u062a\u0646 \u0628\u06cc\u0627\u0631\u0627\u06cc\u062f", "m2": "\u0645\u0646\u0628\u0631 \u0627\u0632 \u06af\u0631\u06cc\u0647 \u0647\u06cc\u0686 \u0646\u0627\u0633\u0627\u06cc\u062f"}, "23": {"m1": "\u0645\u0639\u0646\u06cc \u0627\u0632 \u0644\u0641\u0638 \u0627\u0648 \u067e\u062f\u06cc\u062f \u0627\u0632 \u062f\u0648\u0631", "m2": "\u0686\u0648\u0646 \u0631\u062e \u062d\u0648\u0631 \u0639\u06cc\u0646 \u0632 \u067e\u0631\u062f\u0647\u0654 \u0646\u0648\u0631"}, "24": {"m1": "\u062f\u0627\u062f\u0647 \u06a9\u0644\u06a9\u0634 \u0686\u0646\u0627\u0646\u06a9\u0647 \u0634\u0627\u0647 \u0639\u0631\u0648\u0633", "m2": "\u0627\u0632 \u0646\u0642\u0627\u0628 \u062a\u064f\u0646\u064f\u06a9 \u062e\u0631\u062f \u0631\u0627 \u0628\u0648\u0633"}, "25": {"m1": "\u0647\u0645 \u062f\u0631\u062e\u062a \u0648\u0641\u0627 \u0627\u0632 \u0627\u0648 \u067e\u0631\u0628\u0627\u0631", "m2": "\u0647\u0645 \u0632\u0628\u0627\u0646 \u062b\u0646\u0627 \u0627\u0632\u0648 \u062f\u0631 \u06a9\u0627\u0631"}, "26": {"m1": "\u062f\u0631 \u062f\u0639\u0627 \u062f\u0633\u062a \u062f\u0644 \u0686\u0648 \u0628\u0631\u06af\u06cc\u0631\u062f", "m2": "\u0686\u0631\u062e \u0686\u062a\u0631 \u0631\u0636\u0627 \u0628\u0647 \u0633\u0631 \u06af\u06cc\u0631\u062f"}, "27": {"m1": "\u062f\u0631 \u062f\u0639\u0627\u0647\u0627 \u0686\u0648 \u062f\u0633\u062a \u0628\u0631\u06a9\u0646\u062f \u0627\u0648", "m2": "\u0686\u0631\u062e \u0631\u0627 \u0635\u062f\u0647\u0632\u0627\u0631 \u062f\u0631 \u06a9\u0646\u062f \u0627\u0648"}, "28": {"m1": "\u0628\u0631\u0633\u062f \u062a\u0627 \u0628\u0647 \u0639\u0631\u0634 \u0648 \u06cc\u0627\u0628\u062f \u0627\u062c\u0627\u0628", "m2": "\u0646\u0634\u0648\u062f \u0646\u064f\u0647 \u0641\u0644\u06a9 \u0632 \u067e\u06cc\u0634 \u062d\u062c\u0627\u0628"}, "29": {"m1": "\u062e\u0644\u0642 \u0627\u0648 \u0647\u0645\u0686\u0648 \u0632\u0647\u0631\u0647 \u0642\u0627\u0626\u062f \u062f\u06cc\u0646", "m2": "\u0630\u0647\u0646 \u0627\u0648 \u062f\u0631 \u0633\u062e\u0646 \u0639\u064f\u0637\u0627\u0631\u062f \u062f\u06cc\u0646"}, "30": {"m1": "\u0686\u0648\u0646 \u062e\u0631\u062f \u06a9\u0627\u0631\u0647\u0627\u0634 \u0631\u0648\u0634\u0646 \u0648 \u0686\u0633\u062a", "m2": "\u0686\u0648\u0646 \u0642\u0636\u0627 \u0633\u0637\u0648\u062a\u0634 \u062f\u0631\u0634\u062a \u0648 \u062f\u0631\u0633\u062a"}, "31": {"m1": "\u0645\u0631\u062f\u0647 \u062f\u0644 \u0645\u0627\u0646\u062f\u0647 \u0628\u0648\u062f \u0627\u0632 \u067e\u06cc \u0622\u0632", "m2": "\u062c\u0627\u0646 \u0686\u0648 \u062f\u0631 \u062f\u0644 \u0646\u0634\u0633\u062a \u06af\u0627\u0647 \u0646\u06cc\u0627\u0632"}, "32": {"m1": "\u0632\u0646\u062f\u0647 \u06a9\u0631\u062f \u0627\u0632 \u0628\u0631\u0627\u06cc \u06cc\u0632\u062f\u0627\u0646 \u0631\u0627", "m2": "\u0645\u0627\u0644 \u0627\u0648 \u062f\u0644 \u062c\u0645\u0627\u0644 \u0627\u0648 \u062c\u0627\u0646 \u0631\u0627"}, "33": {"m1": "\u062a\u0627 \u06a9\u0647 \u0645\u0627\u0644\u0634 \u0631\u0633\u062f \u0628\u0647 \u0647\u0631 \u06cc\u0627\u0631\u06cc", "m2": "\u0627\u0632 \u062c\u0645\u0627\u0644\u0634 \u062a\u0648\u0627\u0646\u06af\u0631\u0645 \u0628\u0627\u0631\u06cc"}, "34": {"m1": "\u062e\u0627\u06a9 \u067e\u0627\u06cc\u0634 \u0627\u06af\u0631 \u0628\u0647 \u062f\u0633\u062a \u06a9\u0646\u062f", "m2": "\u062d\u0648\u0631 \u0627\u0632 \u0622\u0646 \u062e\u0627\u06a9 \u0622\u0628\u062f\u0633\u062a \u06a9\u0646\u062f"}, "35": {"m1": "\u063a\u0645 \u06af\u0631\u06cc\u0632\u062f \u0686\u0648 \u0627\u0648 \u0634\u0648\u062f \u062e\u0646\u062f\u0627\u0646", "m2": "\u0628\u0647 \u062a\u06a9 \u067e\u0627\u06cc \u0648 \u062c\u0627\u0645\u0647 \u062f\u0631 \u062f\u0646\u062f\u0627\u0646"}, "36": {"m1": "\u062d\u0644\u0642\u0647 \u062f\u0631 \u06af\u0648\u0634 \u06a9\u0631\u062f\u0647 \u0645\u0631\u062f\u0645 \u0686\u0634\u0645", "m2": "\u067e\u06cc\u0634 \u0622\u0646 \u0637\u0627\u0642 \u0648 \u0627\u0628\u0631\u0648 \u0648 \u062e\u0645 \u0686\u0634\u0645"}, "37": {"m1": "\u0627\u0646\u062f\u0631\u0627\u0646 \u06a9\u0644\u06a9 \u0648 \u062e\u0637 \u0648 \u0641\u0636\u0644 \u0648 \u062c\u0645\u0627\u0644", "m2": "\u062f\u0633\u062a \u0632\u06cc\u0631 \u0632\u0646\u062e \u0628\u0645\u0627\u0646\u062f\u0647 \u062e\u06cc\u0627\u0644"}, "38": {"m1": "\u062e\u0627\u06a9 \u067e\u0627\u06cc\u0634 \u0627\u06af\u0631\u0686\u0647 \u0632\u0648 \u062f\u0648\u0631\u0633\u062a", "m2": "\u062e\u0648\u0634 \u0686\u0648 \u0622\u0628 \u062f\u0647\u0627\u0646 \u0632\u0646\u0628\u0648\u0631\u0633\u062a"}, "39": {"m1": "\u0627\u0648 \u062e\u0631\u062f \u0628\u0647\u0631 \u0631\u0627\u0647 \u062f\u06cc\u0646 \u062f\u0627\u0631\u062f", "m2": "\u0639\u06cc\u0646 \u062f\u06cc\u0646 \u0627\u0633\u062a \u0632\u0627\u0646 \u0686\u0646\u06cc\u0646 \u062f\u0627\u0631\u062f"}, "40": {"m1": "\u062f\u0631 \u0635\u0644\u0627\u0628\u062a \u0686\u0648 \u0639\u0645\u0651\u0631\u06cc \u062f\u06af\u0631\u0633\u062a", "m2": "\u0645\u0631 \u0633\u0631 \u0639\u0644\u0645 \u0631\u0627 \u0633\u0631\u06cc \u062f\u06af\u0631\u0633\u062a"}, "41": {"m1": "\u0631\u0648\u0632 \u0648 \u0634\u0628 \u0633\u0627\u0632 \u0622\u0646 \u062c\u0647\u0627\u0646 \u0633\u0627\u0632\u062f", "m2": "\u0632\u0627\u0646 \u0628\u0647 \u062f\u06cc\u06af\u0631 \u0639\u0645\u0644 \u0646\u067e\u0631\u062f\u0627\u0632\u062f"}, "42": {"m1": "\u06a9\u0627\u0631 \u0627\u0648 \u0646\u06cc\u0633\u062a \u062c\u0632 \u0635\u0644\u0627\u062d \u062c\u0647\u0627\u0646", "m2": "\u0647\u0633\u062a \u0627\u0632\u0648 \u062a\u0627\u0632\u0647 \u0647\u0631 \u0632\u0645\u0627\u0646 \u0627\u06cc\u0645\u0627\u0646"}, "43": {"m1": "\u0647\u06cc\u0686 \u0646\u0627\u06af\u0634\u062a\u0647 \u06af\u0631\u062f \u0647\u0632\u0644 \u0648 \u0641\u0636\u0648\u0644", "m2": "\u0634\u062f\u0647 \u062e\u0634\u0646\u0648\u062f \u0627\u0632\u0648 \u062e\u062f\u0627 \u0648 \u0631\u0633\u0648\u0644"}, "44": {"m1": "\u0646\u0627\u06cc\u0628 \u0634\u0631\u0639 \u0645\u0635\u0637\u0641\u06cc \u0627\u0648\u06cc\u0633\u062a", "m2": "\u0639\u0627\u0644\u0645 \u0639\u0644\u0645\u0650 \u0645\u0631\u062a\u0636\u06cc \u0627\u0648\u06cc\u0633\u062a"}, "45": {"m1": "\u0639\u0644\u0645 \u062a\u0623\u0648\u06cc\u0644 \u0628\u0631 \u0632\u0628\u0627\u0646 \u062f\u0627\u0631\u062f", "m2": "\u0634\u0631\u062d \u062a\u0646\u0632\u06cc\u0644 \u0631\u0627 \u0628\u06cc\u0627\u0646 \u062f\u0627\u0631\u062f"}, "46": {"m1": "\u0647\u0631\u0686\u0647 \u0628\u0627 \u0645\u0631\u062a\u0636\u06cc \u0628\u06af\u0641\u062a \u0631\u0633\u0648\u0644", "m2": "\u0627\u0648 \u0628\u0647 \u062c\u0627\u0646 \u06a9\u0631\u062f\u0647 \u0627\u0633\u062a \u062c\u0645\u0644\u0647 \u0642\u0628\u0648\u0644"}, "47": {"m1": "\u062a\u0627 \u062f\u0631\u0622\u0645\u062f \u0628\u0647 \u0639\u0627\u0644\u0645 \u0641\u0627\u0646\u06cc", "m2": "\u0628\u0648\u062f \u0634\u0631\u0639 \u0631\u0633\u0648\u0644 \u0631\u0627 \u0628\u0627\u0646\u06cc"}, "48": {"m1": "\u0622\u0646 \u0686\u0646\u0627\u0646 \u0639\u0644\u0645 \u0634\u0631\u0639\u0634 \u0627\u0632 \u0628\u0631 \u0634\u062f", "m2": "\u06a9\u0627\u0646 \u062c\u0647\u0627\u0646\u0634 \u0628\u0647 \u062c\u0627\u0646 \u0645\u0635\u0648\u0651\u0631 \u0634\u062f"}, "49": {"m1": "\u06af\u0634\u062a \u0628\u0627 \u0645\u0631\u062a\u0636\u06cc \u062f\u0631\u06cc\u0646 \u0631\u0647 \u06cc\u0627\u0631", "m2": "\u0644\u0648 \u06a9\u0634\u0641 \u06af\u0634\u062a \u0628\u0631 \u062f\u0644\u0634 \u0686\u0648 \u0646\u06af\u0627\u0631"}, "50": {"m1": "\u0647\u0631\u06a9\u0647 \u062a\u0646 \u062f\u0634\u0645\u0646\u0633\u062a \u0648 \u06cc\u0632\u062f\u0627\u0646 \u062f\u0648\u0633\u062a", "m2": "\u062f\u0627\u0646\u06a9\u0647 \u0648\u0627\u0644\u0631\u0651\u0627\u0633\u062e\u0648\u0646 \u0641\u06cc\u200c\u0627\u0644\u0639\u0644\u0645 \u0627\u0648\u0633\u062a"}, "51": {"m1": "\u062f\u0631 \u062b\u0646\u0627\u06cc\u0634 \u0647\u0631\u0622\u0646\u0686\u0647 \u0627\u0646\u062f\u06cc\u0634\u0645", "m2": "\u0633\u06cc\u0631\u062a\u0634 \u06af\u0648\u06cc\u062f\u0645 \u06a9\u0647 \u0645\u0646 \u0628\u06cc\u0634\u0645"}, "52": {"m1": "\u0639\u062c\u0632 \u067e\u06cc\u0634 \u0622\u0648\u0631\u0645 \u0645\u0646 \u0627\u0632 \u06a9\u0627\u0631\u0634", "m2": "\u0628\u0627\u062f \u06cc\u0632\u062f\u0627\u0646 \u0628\u0647 \u062d\u06a9\u0645 \u062f\u0631 \u06cc\u0627\u0631\u0634"}, "53": {"m1": "\u0639\u0631\u0636 \u0627\u0632 \u0639\u0631\u0636 \u062f\u06cc\u0646 \u0645\u0642\u06cc\u062f \u0628\u0627\u062f", "m2": "\u062a\u0646\u0634 \u0627\u0632 \u0639\u0642\u0644 \u06a9\u0644\u0651 \u0645\u0624\u06cc\u0651\u062f \u0628\u0627\u062f"}, "54": {"m1": "\u0628\u0631 \u0632 \u0639\u0642\u0644 \u0648 \u062e\u0631\u062f \u0645\u06a9\u0627\u0646\u0634 \u0628\u0627\u062f", "m2": "\u0639\u0645\u0631 \u0686\u0648\u0646 \u0639\u0644\u0645 \u062c\u0627\u0648\u062f\u0627\u0646\u0634 \u0628\u0627\u062f"}, "55": {"m1": "\u0628\u0627\u062f \u0627\u06cc\u0646 \u062e\u0627\u06a9 \u062a\u0627 \u0627\u0628\u062f \u062f\u0644\u06a9\u0634", "m2": "\u0647\u0645 \u0686\u0646\u0627\u0646 \u0686\u0648\u0646 \u0633\u0645\u0646\u062f\u0631 \u0627\u0632 \u0622\u062a\u0634"}}}