{"text": {"0": {"m1": "\u0627\u0628\u0644\u0642\u06cc \u0631\u0627 \u06a9\u0647 \u0631\u062e \u0628\u0647 \u062e\u0627\u0646\u0647\u0654 \u0627\u0648\u0633\u062a", "m2": "\u062a\u0627\u0632\u06af\u06cc \u062c\u0627\u0646 \u0632 \u062a\u0627\u0632\u06cc\u0627\u0646\u0647\u0654 \u0627\u0648\u0633\u062a"}, "1": {"m1": "\u0648\u0622\u0646\u06a9\u0647 \u0627\u0632 \u062a\u06cc\u0631 \u0627\u0648 \u0634\u0631\u0641 \u062f\u0627\u0631\u062f", "m2": "\u062f\u06cc\u062f\u06af\u0627\u0646 \u0627\u0632 \u067e\u06cc \u0647\u062f\u0641 \u062f\u0627\u0631\u062f"}, "2": {"m1": "\u0627\u06cc \u0628\u0631 \u0622\u062a\u0634 \u0646\u0647\u0627\u062f\u0647 \u062e\u0631\u0645\u0646 \u062e\u0648\u06cc\u0634", "m2": "\u0628\u0627\u062f \u0628\u0631 \u062f\u0627\u062f\u0647 \u0633\u0642\u0641 \u06af\u0644\u0634\u0646 \u062e\u0648\u06cc\u0634"}, "3": {"m1": "\u0627\u06af\u0631 \u062a\u0631\u0627 \u062a\u06cc\u063a \u062a\u0646 \u0632\u0646\u062f \u0627\u0647 \u06a9\u0646", "m2": "\u0648\u0631 \u062a\u0631\u0627 \u0632\u062e\u0645 \u062d\u0642 \u0632\u0646\u062f \u062e\u0647 \u06a9\u0646"}, "4": {"m1": "\u0628\u06cc\u200c\u0631\u0636\u0627\u06cc \u062d\u0642 \u0622\u0646\u0686\u0647 \u0631\u0627\u062d\u062a \u062a\u0633\u062a", "m2": "\u0622\u0646 \u0646\u0647 \u0631\u0627\u062d\u062a \u06a9\u0647 \u0622\u0646 \u062c\u0631\u0627\u062d\u062a \u062a\u0633\u062a"}, "5": {"m1": "\u062a\u0644\u062e \u0648 \u0634\u06cc\u0631\u06cc\u0646 \u0686\u0648 \u0647\u0631 \u062f\u0648 \u0632\u0648 \u0628\u0627\u0634\u062f", "m2": "\u0632\u0634\u062a \u0646\u0628\u0648\u064e\u062f \u0647\u0645\u0647 \u0646\u06a9\u0648 \u0628\u0627\u0634\u062f"}, "6": {"m1": "\u062f\u0644\u0634\u0627\u0646 \u0628\u0631 \u0641\u0631\u0627\u0642 \u0645\u0627\u0644 \u0648 \u0639\u06cc\u0627\u0644", "m2": "\u062e\u0646\u06a9 \u0648 \u062e\u0648\u0634 \u0686\u0648 \u062f\u0631 \u0628\u0647\u0627\u0631 \u0634\u0645\u0627\u0644"}, "7": {"m1": "\u062a\u0627 \u062f\u0631 \u0627\u06cc\u0646 \u0639\u0627\u0644\u0645 \u0641\u0633\u0631\u062f\u0647 \u062f\u0631\u0646\u062f", "m2": "\u0644\u06af\u062f \u0627\u0634\u062a\u0631\u0627\u0646 \u0686\u0648 \u06af\u0631\u062f\u0647 \u062e\u0648\u0631\u0646\u062f"}, "8": {"m1": "\u062e\u0648\u06cc\u0634\u062a\u0646 \u0686\u0648\u0646 \u0632 \u0639\u0634\u0642 \u06af\u0631\u0645 \u06a9\u0646\u0646\u062f", "m2": "\u06af\u0631\u062f\u0646\u0650 \u0631\u0648\u0632\u06af\u0627\u0631 \u0646\u0631\u0645 \u06a9\u0646\u0646\u062f"}, "9": {"m1": "\u067e\u06cc\u0634 \u0631\u0641\u062a\u0646 \u0628\u0647 \u0631\u063a\u0628\u062a \u0627\u0632 \u062f\u0646\u06cc\u06cc", "m2": "\u0634\u0648\u062f \u0622\u0645\u0627\u062f\u0647 \u0646\u0632\u062f\u0634\u0627\u0646 \u0639\u0642\u0628\u06cc"}, "10": {"m1": "\u0686\u0648\u0646 \u0633\u0631\u0650 \u0639\u0634\u0642 \u0622\u0646 \u062c\u0647\u0627\u0646 \u062f\u0627\u0631\u0646\u062f", "m2": "\u0647\u0645\u0686\u0648 \u0634\u0645\u0639\u200c\u0627\u0646\u062f \u0633\u0648\u0632\u0650 \u062c\u0627\u0646 \u062f\u0627\u0631\u0646\u062f"}, "11": {"m1": "\u067e\u06cc\u0634\u0634\u0627\u0646 \u0631\u0648\u0632\u06af\u0627\u0631 \u0686\u0648\u0646 \u0628\u0646\u062f\u0647", "m2": "\u062f\u0647\u0631 \u0627\u0632 \u0627\u0646\u0641\u0627\u0633\u0634\u0627\u0646 \u0641\u0632\u0627\u06cc\u0646\u062f\u0647"}, "12": {"m1": "\u06a9\u0645\u062a\u0631\u06cc\u0646 \u0628\u0646\u062f\u0647\u200c\u0634\u0627\u0646 \u0632\u0645\u0627\u0646\u0647 \u0628\u064f\u0648\u064e\u062f", "m2": "\u0632 \u0622\u0631\u0632\u0648 \u062f\u0644 \u0686\u0648 \u06af\u0648\u0631\u062e\u0627\u0646\u0647 \u0628\u064f\u0648\u064e\u062f"}, "13": {"m1": "\u0632\u0627\u0646\u06a9\u0634\u0627\u0646 \u062a\u0627 \u0627\u0645\u06cc\u062f \u0646\u0628\u0648\u064e\u062f \u0648 \u0628\u06cc\u0645", "m2": "\u062c\u0627\u0646\u0634\u0627\u0646 \u062a\u0646 \u062e\u0648\u0631\u062f \u0686\u0648 \u0634\u0645\u0639 \u0645\u0642\u06cc\u0645"}, "14": {"m1": "\u062f\u0644 \u0632 \u062a\u0644\u062e\u06cc\u0634 \u0647\u0645\u0686\u0648 \u0645\u06cc \u062e\u0648\u0634\u200c\u062f\u0627\u0631", "m2": "\u0647\u0645\u0686\u0646\u0648 \u062f\u0644 \u067e\u0631 \u0622\u0628 \u0648 \u0622\u062a\u0634 \u062f\u0627\u0631"}, "15": {"m1": "\u062c\u0627\u0646 \u0628\u0647 \u0639\u0647\u062f \u0648 \u0648\u0641\u0627\u0634 \u0628\u0633\u067e\u0631\u062f\u0647", "m2": "\u062f\u0631 \u06a9\u0646\u0641 \u0632\u0646\u062f\u0647 \u062f\u0631 \u06a9\u0641\u0646 \u0645\u0631\u062f\u0647"}, "16": {"m1": "\u067e\u06cc\u0634 \u0627\u0645\u0631\u0634 \u0686\u0648 \u06a9\u0644\u06a9 \u0628\u0631\u062c\u0633\u062a\u0647", "m2": "\u062c\u0627\u0646 \u06a9\u0645\u0631\u0648\u0627\u0631 \u0628\u0631 \u0645\u06cc\u0627\u0646 \u0628\u0633\u062a\u0647"}, "17": {"m1": "\u0627\u0632 \u0628\u0631\u0627\u06cc \u0648\u0641\u0627\u062a \u062a\u062e\u0645 \u0646\u0641\u0627\u0642", "m2": "\u0646\u0642\u062f \u062e\u0648\u0627\u0631\u0632\u0645 \u06a9\u0645 \u0628\u0631\u062f \u0628\u0647 \u0639\u0631\u0627\u0642"}, "18": {"m1": "\u0627\u0632 \u0628\u0631\u0627\u06cc \u062f\u0648 \u062f\u0627\u0646\u06af \u0633\u06cc\u0645 \u062f\u063a\u0644", "m2": "\u0646\u06a9\u0646\u062f \u0628\u0627 \u062e\u062f\u0627\u06cc \u06a9\u06cc\u0633\u0647 \u0628\u062f\u0644"}, "19": {"m1": "\u0647\u0645\u0686\u0646\u0627\u0646 \u0628\u064f\u062e\u062a\u06cc \u06a9\u0645\u0631 \u06a9\u0648\u0647\u0627\u0646", "m2": "\u0633\u0628\u0644\u062a \u062d\u0631\u0635 \u06a9\u0645 \u0632\u0646\u062f \u0633\u0648\u0647\u0627\u0646"}, "20": {"m1": "\u062f\u0631 \u0631\u0636\u0627\u06cc \u062e\u062f\u0627\u06cc \u062e\u0648\u06cc\u0634 \u0628\u06a9\u0648\u0634", "m2": "\u0628\u0647 \u0646\u0647 \u0686\u06cc\u0632\u0634 \u0686\u0648 \u0628\u0646\u062f\u06af\u0627\u0646 \u0645\u0641\u0631\u0648\u0634"}, "21": {"m1": "\u0628\u0627\u0634 \u062f\u0631 \u062d\u06a9\u0645 \u0635\u0648\u0644\u062c\u0627\u0646\u0634 \u06af\u0648\u06cc", "m2": "\u0647\u0645  \u0633\u0645\u0639\u0646\u0627 \u0648 \u0647\u0645 \u0627\u0637\u0639\u0646\u0627 \u06af\u0648\u06cc"}, "22": {"m1": "\u0686\u0648\u0646\u062a \u06af\u0648\u06cc\u062f \u0646\u0645\u0627\u0632 \u06a9\u0646 \u0628\u06af\u0632\u0627\u0631", "m2": "\u0686\u0648\u0646\u062a \u06af\u0648\u06cc\u062f \u0645\u06a9\u0646 \u0628\u0631\u0648 \u0645\u06af\u0630\u0627\u0631"}, "23": {"m1": "\u0686\u0648\u0646\u062a \u06af\u0648\u06cc\u062f \u0628\u0628\u062e\u0634 \u0647\u06cc\u0686 \u0645\u0646\u0647", "m2": "\u0686\u0648\u0646\u062a \u06af\u0648\u06cc\u062f \u0646\u06af\u0627\u0647\u200c\u062f\u0627\u0631 \u0645\u062f\u0647"}, "24": {"m1": "\u0646\u0647 \u0632 \u0631\u0648\u06cc \u0645\u062c\u0627\u0632 \u06a9\u0632 \u062a\u062d\u0642\u06cc\u0642", "m2": "\u0628\u0631 \u0647\u0645\u0647 \u0628\u0631\u0646\u0647\u0627\u062f \u0628\u06cc\u200c\u062a\u0635\u062f\u06cc\u0642"}, "25": {"m1": "\u0627\u0646\u062f\u0631 \u0622\u0645\u062f \u06af\u0644\u06cc\u0645 \u067e\u0648\u0634\u06cc\u062f\u0647", "m2": "\u0635\u0641\u0648 \u0648 \u062f\u0631\u062f\u06cc \u062f\u064f\u0631\u062f \u0646\u0648\u0634\u06cc\u062f\u0647"}, "26": {"m1": "\u067e\u0631\u0650 \u062c\u0628\u0631\u0626\u06cc\u0644 \u0628\u0631 \u0645\u0648\u0627\u0641\u0642\u062a\u0634", "m2": "\u06af\u0634\u062a \u0647\u0645\u0686\u0648\u0646 \u06a9\u0644\u06cc\u0645 \u0645\u0646\u0642\u0628\u062a\u0634"}, "27": {"m1": "\u0631\u062e\u0635\u062a\u0634 \u0647\u062f\u06cc\u0647\u200c\u062f\u0627\u0646 \u06a9\u0632\u0648 \u0628\u0631\u0647\u06cc", "m2": "\u062a\u0648 \u0627\u0632\u0648 \u0631\u062e\u0635\u062a\u0634 \u0686\u0647 \u0628\u0627\u0632 \u062f\u0647\u06cc"}, "28": {"m1": "\u0646\u0647 \u062a\u0648\u06cc\u06cc \u062a\u0648 \u0632 \u062a\u0633\u062a \u0628\u0631 \u06a9\u0627\u0631\u06cc", "m2": "\u062a\u0648 \u06a9\u0626\u06cc \u0627\u0646\u062f\u0631\u06cc\u0646 \u0645\u06cc\u0627\u0646 \u0628\u0627\u0631\u06cc"}, "29": {"m1": "\u0647\u0631\u06a9\u062c\u0627 \u0630\u06a9\u0631 \u0627\u0648 \u0628\u064f\u0648\u064e\u062f \u062a\u0648 \u06a9\u0647\u200c\u0627\u06cc", "m2": "\u062c\u0645\u0644\u0647 \u062a\u0633\u0644\u06cc\u0645 \u06a9\u0646 \u0628\u062f\u0648 \u062a\u0648 \u0686\u0647\u200c\u0627\u06cc"}, "30": {"m1": "\u0622\u0646\u0650 \u0627\u0648\u06cc\u06cc \u062a\u0648 \u06a9\u0645 \u0633\u062a\u06cc\u0632 \u0628\u0631 \u0627\u0648\u06cc", "m2": "\u06af\u0631 \u06af\u0631\u06cc\u0632\u06cc \u0627\u0632\u0648 \u06af\u0631\u06cc\u0632 \u062f\u0631 \u0627\u0648\u06cc"}, "31": {"m1": "\u062c\u0627\u0646 \u0648 \u062a\u0646 \u0631\u0627 \u0628\u0647 \u06a9\u0631\u062f\u06af\u0627\u0631 \u0633\u067e\u0627\u0631", "m2": "\u062a\u0627 \u062f\u0631\u0648\u0646 \u0633\u0631\u0627\u06cc \u06cc\u0627\u0628\u06cc \u0628\u0627\u0631"}, "32": {"m1": "\u06a9\u0627\u0646\u06a9\u0647 \u0633\u062f \u067e\u0627\u0633\u0628\u0627\u0646 \u062e\u0627\u0646\u0647 \u0648 \u0633\u0631", "m2": "\u0686\u0648\u0646 \u06a9\u0644\u06cc\u062f\u0627\u0646 \u0628\u0645\u0627\u0646\u062f \u062f\u0631 \u067e\u0633\u0650 \u062f\u0631"}, "33": {"m1": "\u062c\u0627\u0646 \u0648 \u0627\u0633\u0628\u0627\u0628 \u0627\u0632\u0648 \u0639\u0637\u0627 \u062f\u0627\u0631\u06cc", "m2": "\u067e\u0633 \u062f\u0631\u06cc\u063a\u0634 \u0627\u0632\u0648 \u0686\u0631\u0627 \u062f\u0627\u0631\u06cc"}, "34": {"m1": "\u062c\u0627\u0646 \u0648 \u0627\u0633\u0628\u0627\u0628 \u062f\u0631 \u0631\u0647\u0634 \u062f\u0631 \u0628\u0627\u0632", "m2": "\u0628\u0631 \u0631\u0647 \u0633\u06cc\u0644 \u0648 \u0631\u0648\u062f \u062e\u0627\u0646\u0647 \u0645\u0633\u0627\u0632"}, "35": {"m1": "\u0648\u0642\u0641 \u06a9\u0646 \u062c\u0633\u0645 \u0648 \u0645\u0627\u0644 \u0631\u0627 \u0628\u0631 \u063a\u06cc\u0628", "m2": "\u062a\u0627 \u0628\u0648\u064e\u06cc \u0686\u0648\u0646 \u06a9\u0644\u06cc\u062f\u0634 \u0627\u0646\u062f\u0631 \u062c\u06cc\u0628"}, "36": {"m1": "\u062c\u0628\u0631 \u0631\u0627 \u0645\u0627\u0631\u0645\u06cc\u062a\u064e \u06a9\u0646 \u0627\u0632 \u0628\u0631", "m2": "\u0628\u0627\u0632 \u062f\u0627\u0646 \u0627\u0632 \u0631\u0645\u06cc\u062a \u0633\u0631\u0651\u0650 \u0642\u062f\u0631"}}}