{"text": {"0": {"m1": "\u0622\u0646 \u0646\u0628\u06cc\u0646\u06cc \u06a9\u0647 \u067e\u06cc\u0634\u062a\u0631 \u0632 \u0648\u062c\u0648\u062f", "m2": "\u0686\u0648\u0646 \u062a\u0648 \u0631\u0627 \u06a9\u0631\u062f \u062f\u0631 \u0631\u062d\u0645 \u0645\u0648\u062c\u0648\u062f"}, "1": {"m1": "\u0631\u0648\u0632\u06cc\u062a \u062f\u0627\u062f \u0646\u064f\u0647 \u0645\u0647 \u0627\u0632 \u062e\u0648\u0646\u06cc", "m2": "\u06a9\u0631\u062f\u06af\u0627\u0631 \u062d\u06a9\u06cc\u0645 \u0628\u06cc\u200c\u0686\u0648\u0646\u06cc"}, "2": {"m1": "\u062f\u0631 \u0634\u06a9\u0645 \u0645\u0627\u062f\u0631\u062a \u0647\u0645\u06cc \u067e\u0631\u0648\u0631\u062f", "m2": "\u0628\u0639\u062f \u0646\u064f\u0647 \u0645\u0627\u0647 \u062f\u0631 \u0648\u062c\u0648\u062f \u0622\u0648\u0631\u062f"}, "3": {"m1": "\u0622\u0646 \u062f\u0631\u0650 \u0631\u0632\u0642 \u0628\u0631 \u062a\u0648 \u0686\u064f\u0633\u062a \u0628\u0628\u0633\u062a", "m2": "\u062f\u0648 \u062f\u0631 \u0628\u0647\u062a\u0631\u062a \u0628\u062f\u0627\u062f \u0628\u0647 \u062f\u0633\u062a"}, "4": {"m1": "\u0628\u0639\u062f \u0627\u0632 \u0622\u0646 \u0627\u0650\u0644\u0641 \u062f\u0627\u062f \u0628\u0627 \u067e\u0633\u062a\u0627\u0646", "m2": "\u0631\u0648\u0632 \u0648 \u0634\u0628 \u067e\u06cc\u0634 \u062a\u0648 \u062f\u0631 \u0686\u0634\u0645\u0647 \u0631\u0648\u0627\u0646"}, "5": {"m1": "\u06af\u0641\u062a \u06a9\u06cc\u0646 \u0647\u0631 \u062f\u0648 \u0631\u0627 \u0647\u0645\u06cc \u0622\u0634\u0627\u0645", "m2": "\u06a9\u064f\u0644 \u0647\u064e\u0646\u06cc\u0626\u0627\u064b \u06a9\u0647 \u0646\u06cc\u0633\u062a \u0628\u0631 \u062a\u0648 \u062d\u0631\u0627\u0645"}, "6": {"m1": "\u0686\u0648\u0646 \u0646\u0645\u0648\u062f\u062a \u0641\u0637\u0627\u0645 \u0628\u0639\u062f \u062f\u0648 \u0633\u0627\u0644", "m2": "\u0634\u062f \u062f\u06af\u0631\u06af\u0648\u0646 \u062a\u0631\u0627 \u0647\u0645\u0647 \u0627\u062d\u0648\u0627\u0644"}, "7": {"m1": "\u062f\u0627\u062f \u0631\u0632\u0642 \u062a\u0648 \u0627\u0632 \u062f\u0648 \u062f\u0633\u062a \u0648 \u062f\u0648 \u067e\u0627\u06cc", "m2": "\u0632\u06cc\u0646 \u0628\u06af\u06cc\u0631 \u0648 \u0627\u0632 \u0622\u0646 \u0628\u0631\u0648 \u0647\u0631 \u062c\u0627\u06cc"}, "8": {"m1": "\u06af\u0631 \u062f\u0648 \u0631 \u0628\u0631 \u062a\u0648 \u0628\u0633\u062a\u0647 \u06a9\u0631\u062f \u0631\u0648\u0627\u0633\u062a", "m2": "\u0639\u0648\u0636 \u062f\u0648 \u0686\u0647\u0627\u0631 \u062f\u0631 \u0628\u0631\u062c\u0627\u0633\u062a"}, "9": {"m1": "\u0632\u06cc\u0646 \u0633\u062a\u0627\u0646 \u0632\u0627\u0646 \u0628\u0628\u0631 \u0628\u0647 \u067e\u06cc\u0631\u0648\u0632\u06cc", "m2": "\u06af\u0631\u062f \u0639\u0627\u0644\u0645 \u0647\u0645\u06cc \u0637\u0644\u0628 \u0631\u0648\u0632\u06cc"}, "10": {"m1": "\u0686\u0648\u0646 \u0627\u062c\u0644 \u0646\u0627\u06af\u0647\u0627\u0646 \u0641\u0631\u0627\u0632 \u0622\u06cc\u062f", "m2": "\u06a9\u0627\u0631 \u062f\u0646\u06cc\u0627 \u0647\u0645\u0647 \u0645\u062c\u0627\u0632 \u0622\u06cc\u062f"}, "11": {"m1": "\u0628\u0627\u0632 \u0645\u0627\u0646\u062f \u062f\u0648 \u062f\u0633\u062a \u0648 \u067e\u0627\u06cc \u0627\u0632 \u06a9\u0627\u0631", "m2": "\u0628\u062f\u0644 \u0686\u0627\u0631 \u0628\u062f\u0647\u062a \u062f\u0648 \u0686\u0647\u0627\u0631"}, "12": {"m1": "\u062f\u0631 \u0644\u062d\u062f \u0647\u0631 \u0686\u0647\u0627\u0631 \u0628\u0633\u062a\u0647 \u0634\u0648\u062f", "m2": "\u0647\u0634\u062a \u062c\u0646\u0651\u062a \u062a\u0631\u0627 \u062e\u062c\u0633\u062a\u0647 \u0634\u0648\u062f"}, "13": {"m1": "\u0647\u0634\u062a \u062f\u0631 \u0628\u0631 \u062a\u0648 \u0628\u0627\u0632 \u0628\u06af\u0634\u0627\u06cc\u0646\u062f", "m2": "\u062d\u0648\u0631 \u0648 \u063a\u0644\u0645\u0627\u0646 \u062a\u0631\u0627 \u0628\u0647 \u067e\u06cc\u0634 \u0622\u06cc\u0646\u062f"}, "14": {"m1": "\u062a\u0627 \u0628\u0647 \u0647\u0631 \u062f\u0631 \u0686\u0646\u0627\u0646\u06a9\u0647 \u062e\u0648\u0627\u0647\u06cc \u0634\u0627\u062f", "m2": "\u0645\u06cc\u200c\u0631\u0648\u06cc \u0646\u0627\u0648\u0631\u06cc \u0632 \u062f\u0646\u06cc\u0627 \u06cc\u0627\u062f"}, "15": {"m1": "\u0645\u0647\u0631\u0628\u0627\u0646\u200c\u062a\u0631 \u0632 \u0645\u0627\u062f\u0631 \u0648 \u067e\u062f\u0631 \u0627\u0633\u062a", "m2": "\u0645\u0631 \u062a\u0631\u0627 \u0627\u0648 \u0628\u0647 \u062e\u064f\u0644\u062f \u0631\u0627\u0647\u0628\u0631 \u0627\u0633\u062a"}, "16": {"m1": "\u0627\u06cc \u062c\u0648\u0627\u0646\u0645\u0631\u062f \u0646\u06a9\u062a\u0647\u200c\u0627\u06cc \u0628\u0634\u0646\u0648", "m2": "\u0648\u0632 \u0639\u0637\u0627\u06cc \u062e\u062f\u0627 \u0646\u064f\u0645\u06cc\u062f \u0645\u0634\u0648"}, "17": {"m1": "\u0686\u0648\u0646 \u062a\u0631\u0627 \u062f\u0627\u062f \u0645\u0639\u0631\u0641\u062a \u06cc\u0632\u062f\u0627\u0646", "m2": "\u062f\u0631 \u062f\u0631\u0648\u0646 \u062f\u0644\u062a \u0646\u0647\u0627\u062f \u0627\u06cc\u0645\u0627\u0646"}, "18": {"m1": "\u062e\u0644\u0639\u062a\u06cc \u06a9\u0627\u0646 \u062a\u0631\u0627\u0633\u062a \u0631\u0648\u0632 \u062c\u0647\u06cc\u0632", "m2": "\u0628\u0627\u0632 \u0646\u0633\u062a\u0627\u0646\u062f\u062a \u0628\u0647 \u0631\u0633\u062a\u0627\u062e\u06cc\u0632"}, "19": {"m1": "\u06af\u0631 \u062a\u0631\u0627 \u062f\u0627\u0646\u0634 \u0648 \u062f\u0631\u0645 \u0646\u0628\u0648\u062f", "m2": "\u06a9\u0648 \u062a\u0631\u0627 \u0628\u0648\u062f \u0647\u06cc\u0686 \u06a9\u0645 \u0646\u0628\u0648\u062f"}, "20": {"m1": "\u0627\u0648 \u0628\u0647 \u0641\u062e\u0631 \u0622\u0631\u062f\u062a \u0646\u0628\u06cc\u0646\u06cc \u0639\u0627\u0631", "m2": "\u0627\u0648 \u0639\u0632\u06cc\u0632\u062a \u06a9\u0646\u062f \u0646\u06af\u0631\u062f\u06cc \u062e\u0648\u0627\u0631"}, "21": {"m1": "\u0622\u0646\u0686\u0647 \u062f\u0627\u0631\u06cc \u062a\u0648 \u062f\u0644 \u0628\u062f\u0648 \u0645\u0633\u067e\u0627\u0631", "m2": "\u0622\u0646\u0686\u0647 \u0627\u0648 \u062f\u0627\u062f\u062a \u0627\u0633\u062a\u0648\u0627\u0631 \u0622\u0646 \u062f\u0627\u0631"}, "22": {"m1": "\u062a\u0648 \u062e\u0632\u06cc\u0646\u0647 \u0646\u0647\u06cc \u0646\u06cc\u0627\u0628\u06cc \u0628\u0627\u0632", "m2": "\u0686\u0648\u0646 \u0628\u062f\u0648 \u062f\u0627\u062f\u06cc \u0627\u0648 \u062f\u0647\u062f \u0628\u0647 \u062a\u0648 \u0628\u0627\u0632"}, "23": {"m1": "\u0632\u0631 \u0628\u0647 \u0622\u062a\u0634 \u062f\u0647\u06cc \u062e\u0628\u062b \u0633\u0648\u0632\u062f", "m2": "\u0632\u0631 \u0635\u0627\u0641\u06cc \u062a\u0631\u0627 \u0628\u06cc\u0641\u0631\u0648\u0632\u062f"}, "24": {"m1": "\u0628\u062f \u06a9\u0647 \u0627\u0648 \u0633\u0648\u062e\u062a \u0646\u06cc\u06a9 \u062f\u0627\u062f \u0628\u0647 \u062a\u0648", "m2": "\u062f\u0648\u0644\u062a \u0686\u0631\u062e \u0631\u062e \u0646\u0647\u0627\u062f \u0628\u0647 \u062a\u0648"}, "25": {"m1": "\u0646\u0641\u0639 \u0622\u062a\u0634 \u0627\u06af\u0631 \u0645\u0642\u06cc\u0645\u200c\u062a\u0631\u0633\u062a", "m2": "\u0622\u062a\u0634\u200c\u0622\u0631\u0627\u06cc \u0627\u0632\u0648 \u06a9\u0631\u06cc\u0645\u200c\u062a\u0631\u0633\u062a"}, "26": {"m1": "\u062a\u0648 \u0646\u062f\u0627\u0646\u06cc \u0646\u0647 \u0646\u06cc\u06a9 \u0648 \u0646\u0647 \u0628\u062f \u0631\u0627", "m2": "\u062e\u0627\u0632\u0646 \u0627\u0648 \u0628\u0647 \u062a\u0631\u0627 \u06a9\u0647 \u062a\u0648 \u062e\u0648\u062f \u0631\u0627"}, "27": {"m1": "\u06cc\u0627\u0631 \u0645\u0627\u0631 \u0627\u0633\u062a \u0686\u0648\u0646 \u0632\u0646\u06cc \u062a\u0648 \u062f\u0631\u0634", "m2": "\u0645\u0627\u0631 \u06cc\u0627\u0631 \u0627\u0633\u062a \u0686\u0648\u0646 \u0631\u0645\u06cc \u0632 \u0628\u0631\u0634"}, "28": {"m1": "\u0627\u06cc \u0635\u062f\u0641 \u062c\u0648\u06cc \u062c\u0648\u0647\u0631\u0650 \u0627\u0644\u0651\u0627", "m2": "\u062c\u0627\u0646 \u0648 \u062c\u0627\u0645\u0647 \u0628\u0646\u0647 \u0628\u0647 \u0633\u0627\u062d\u0644 \u0644\u0627"}, "29": {"m1": "\u0647\u0633\u062a \u062d\u0642 \u062c\u0632 \u0628\u0647 \u0646\u06cc\u0633\u062a \u0646\u06af\u0631\u0627\u06cc\u062f", "m2": "\u0632\u0627\u062f \u0627\u06cc\u0646 \u0631\u0627\u0647 \u0646\u06cc\u0633\u062a\u06cc \u0628\u0627\u06cc\u062f"}, "30": {"m1": "\u062a\u0627 \u062a\u0648 \u0627\u0632 \u0646\u06cc\u0633\u062a\u06cc \u06a9\u0644\u0647 \u0646\u0646\u0647\u06cc", "m2": "\u0631\u0648\u06cc \u0631\u0627 \u062f\u0631 \u0628\u0642\u0627 \u0628\u0647 \u0631\u0647 \u0646\u0646\u0647\u06cc"}, "31": {"m1": "\u0686\u0648\u0646 \u0634\u0648\u06cc \u0646\u06cc\u0633\u062a \u0633\u0648\u06cc \u062d\u0642 \u067e\u0648\u06cc\u06cc", "m2": "\u062a\u0627 \u0628\u0648\u06cc \u0647\u0633\u062a \u0631\u0627\u0647 \u062f\u0642 \u062c\u0648\u06cc\u06cc"}, "32": {"m1": "\u0645\u06cc \u0646\u062e\u0648\u0627\u0646\u06cc \u062a\u0648 \u0627\u0632 \u06a9\u062a\u0627\u0628 \u062e\u062f\u0627", "m2": "\u0646\u06cc\u0633\u062a \u0627\u0645\u0648\u0627\u062a \u0645\u0631\u062f \u0628\u0644 \u0627\u062d\u06cc\u0627"}, "33": {"m1": "\u06af\u0631\u062a \u0647\u0633\u062a \u0632\u0645\u0627\u0646\u0647 \u067e\u0633\u062a \u06a9\u0646\u062f", "m2": "\u0627\u062d\u0633\u0646\u200c\u0627\u0644\u062e\u0627\u0644\u0642\u06cc\u0646\u062a \u0647\u0633\u062a \u06a9\u0646\u062f"}}}