{"text": {"0": {"p": "\u0639\u0627\u0631\u0641 \u0642\u0632\u0648\u06cc\u0646\u06cc \u062f\u0631 \u062f\u06cc\u0648\u0627\u0646 \u062e\u0648\u062f \u0648 \u062f\u0631 \u0645\u0642\u062f\u0645\u0647\u200c\u0627\u06cc \u0628\u0631 \u0627\u06cc\u0646 \u062a\u0635\u0646\u06cc\u0641\u060c \u0622\u0648\u0631\u062f\u0647\u200c\u0627\u0633\u062a:"}, "1": {"p": "\u0627\u06cc\u0646 \u062a\u0635\u0646\u06cc\u0641 \u062f\u0631 \u062f\u0648\u0631\u0647 \u062f\u0648\u0645 \u0645\u062c\u0644\u0633 \u0634\u0648\u0631\u0627\u06cc \u0627\u06cc\u0631\u0627\u0646 \u062f\u0631 \u062a\u0647\u0631\u0627\u0646 \u0633\u0627\u062e\u062a\u0647 \u0634\u062f\u0647\u200c\u0627\u0633\u062a. \u0628\u0648\u0627\u0633\u0637\u0647 \u0639\u0634\u0642\u06cc \u06a9\u0647 \u062d\u06cc\u062f\u0631\u062e\u0627\u0646 \u0639\u0645\u0648\u0627\u0648\u063a\u0644\u06cc \u0628\u062f\u0627\u0646 \u062f\u0627\u0634\u062a\u060c \u0645\u06cc\u0644 \u062f\u0627\u0631\u0645 \u0627\u06cc\u0646 \u062a\u0636\u0646\u06cc\u0641 \u0628\u0647 \u06cc\u0627\u062f\u06af\u0627\u0631 \u0622\u0646 \u0645\u0631\u062d\u0648\u0645 \u0637\u0628\u0639 \u06af\u0631\u062f\u062f. \u0627\u06cc\u0646 \u062a\u0635\u0646\u06cc\u0641 \u062f\u0631 \u0622\u063a\u0627\u0632 \u0627\u0646\u0642\u0644\u0627\u0628 \u0645\u0634\u0631\u0648\u0637\u0647 \u0627\u06cc\u0631\u0627\u0646 \u0628\u06cc\u0627\u062f \u0627\u0648\u0644\u06cc\u0646 \u0642\u0631\u0628\u0627\u0646\u06cc\u0627\u0646 \u0622\u0632\u0627\u062f\u06cc \u0633\u0631\u0648\u062f\u0647 \u0634\u062f\u0647\u200c\u0627\u0633\u062a."}, "2": {"m1": "\u0647\u0646\u06af\u0627\u0645 \u0645\u06cc \u0648 \u0641\u0635\u0644 \u06af\u0644 \u0648 \u06af\u0634\u062a \u0648 (\u062c\u0627\u0646\u0645 \u06af\u0634\u062a \u0648 \u062e\u062f\u0627 \u06af\u0634\u062a \u0648) \u0686\u0645\u0646 \u0634\u062f", "m2": "\u062f\u0631\u0628\u0627\u0631 \u0628\u0647\u0627\u0631\u06cc \u062a\u0647\u06cc \u0627\u0632 \u0632\u0627\u063a \u0648 (\u062c\u0627\u0646\u0645 \u0632\u0627\u063a \u0648 \u062e\u062f\u0627 \u0632\u0627\u063a \u0648) \u0632\u063a\u0646 \u0634\u062f"}, "3": {"m1": "\u0627\u0632 \u0627\u0628\u0631 \u06a9\u0631\u0645 \u062e\u0637\u06c0 \u0631\u0634\u06af \u062e\u062a\u0646 \u0634\u062f", "m2": "\u062f\u0644\u062a\u0646\u06af \u0686\u0648 \u0645\u0646 \u0645\u0631\u063a (\u062c\u0627\u0646\u0645 \u0645\u0631\u063a) \u0642\u0641\u0633 \u0628\u0647\u0631 \u0648\u0637\u0646 \u0634\u062f"}, "4": {"t2": "\u0633\u0631 \u06a9\u06cc\u0646 \u062f\u0627\u0631\u06cc \u0627\u06cc \u0686\u0631\u062e! \u0646\u0647 \u062f\u06cc\u0646 \u062f\u0627\u0631\u06cc\u060c \u0646\u0647 \u0622\u06cc\u06cc\u0646 \u062f\u0627\u0631\u06cc (\u0646\u0647 \u0622\u06cc\u06cc\u0646 \u062f\u0627\u0631\u06cc) \u0627\u06cc \u0686\u0631\u062e!", "t1": "\u0686\u0647 \u06a9\u062c \u0631\u0641\u062a\u0627\u0631\u06cc \u0627\u06cc \u0686\u0631\u062e! \u0686\u0647 \u0628\u062f \u06a9\u0631\u062f\u0627\u0631\u06cc \u0627\u06cc \u0686\u0631\u062e!"}, "5": {"m1": "\u0627\u0632 \u062e\u0648\u0646 \u062c\u0648\u0627\u0646\u0627\u0646 \u0648\u0637\u0646 \u0644\u0627\u0644\u0647 \u062f\u0645\u06cc\u062f\u0647", "m2": "\u0627\u0632 \u0645\u0627\u062a\u0645 \u0633\u0631\u0648 \u0642\u062f\u0634\u0627\u0646 \u0633\u0631\u0648 \u062e\u0645\u06cc\u062f\u0647"}, "6": {"m1": "\u062f\u0631 \u0633\u0627\u06cc\u06c0 \u06af\u0644 \u0628\u0644\u0628\u0644 \u0627\u0632\u06cc\u0646 \u063a\u0635\u0647 \u062e\u0632\u06cc\u062f\u0647", "m2": "\u06af\u0644 \u0646\u06cc\u0632 \u0686\u0648 \u0645\u0646 \u062f\u0631 \u063a\u0645\u0634\u0627\u0646 \u062c\u0627\u0645\u0647 \u062f\u0631\u06cc\u062f\u0647"}, "7": {"t2": "\u0686\u0647 \u06a9\u062c\u200c\u0631\u0641\u062a\u0627\u0631\u06cc \u0627\u06cc \u0686\u0631\u062e", "t1": "\u0686\u0647 \u06a9\u062c\u200c\u0631\u0641\u062a\u0627\u0631\u06cc \u0627\u06cc \u0686\u0631\u062e"}, "8": {"m1": "\u062e\u0648\u0627\u0628\u0646\u062f \u0648\u06a9\u06cc\u0644\u0627\u0646 \u0648 \u062e\u0631\u0627\u0628\u0646\u062f \u0648\u0632\u06cc\u0631\u0627\u0646", "m2": "\u0628\u0631\u062f\u0646\u062f \u0628\u0647 \u0633\u0631\u0642\u062a \u0647\u0645\u0647 \u0633\u06cc\u0645 \u0648 \u0632\u0631 \u0627\u06cc\u0631\u0627\u0646"}, "9": {"m1": "\u0645\u0627 \u0631\u0627 \u0646\u06af\u0630\u0627\u0631\u0646\u062f \u0628\u0647 \u06cc\u06a9 \u062e\u0627\u0646\u06c0 \u0648\u06cc\u0631\u0627\u0646", "m2": "\u06cc\u0627 \u0631\u0628 \u0628\u0633\u062a\u0627\u0646 \u062f\u0627\u062f \u0641\u0642\u06cc\u0631\u0627\u0646 \u0632 \u0627\u0645\u06cc\u0631\u0627\u0646"}, "10": {"t2": "\u0633\u0631 \u06a9\u06cc\u0646 \u062f\u0627\u0631\u06cc \u0627\u06cc \u0686\u0631\u062e! \u0646\u0647 \u062f\u06cc\u0646 \u062f\u0627\u0631\u06cc\u060c \u0646\u0647 \u0622\u06cc\u06cc\u0646 \u062f\u0627\u0631\u06cc (\u0646\u0647 \u0622\u06cc\u06cc\u0646 \u062f\u0627\u0631\u06cc) \u0627\u06cc \u0686\u0631\u062e!", "t1": "\u0686\u0647 \u06a9\u062c \u0631\u0641\u062a\u0627\u0631\u06cc \u0627\u06cc \u0686\u0631\u062e! \u0686\u0647 \u0628\u062f \u06a9\u0631\u062f\u0627\u0631\u06cc \u0627\u06cc \u0686\u0631\u062e!"}, "11": {"m1": "\u0627\u0632 \u0627\u0634\u06a9 \u0647\u0645\u0647 \u0631\u0648\u06cc \u0632\u0645\u06cc\u0646 \u0632\u06cc\u0631 \u0648 \u0632\u0628\u0631 \u06a9\u0646", "m2": "\u0645\u0634\u062a\u06cc \u06af\u0631\u062a \u0627\u0632 \u062e\u0627\u06a9 \u0648\u0637\u0646 \u0647\u0633\u062a \u0628\u0647 \u0633\u0631 \u06a9\u0646"}, "12": {"m1": "\u063a\u06cc\u0631\u062a \u06a9\u0646 \u0648 \u0627\u0646\u062f\u06cc\u0634\u06c0 \u0627\u06cc\u0627\u0645 \u0628\u062a\u0631 \u06a9\u0646", "m2": "\u0627\u0646\u062f\u0631 \u062c\u0644\u0648 \u062a\u06cc\u0631 \u0639\u062f\u0648 \u0633\u06cc\u0646\u0647 \u0633\u067e\u0631 \u06a9\u0646"}, "13": {"t2": "\u0633\u0631 \u06a9\u06cc\u0646 \u062f\u0627\u0631\u06cc \u0627\u06cc \u0686\u0631\u062e! \u0646\u0647 \u062f\u06cc\u0646 \u062f\u0627\u0631\u06cc\u060c \u0646\u0647 \u0622\u06cc\u06cc\u0646 \u062f\u0627\u0631\u06cc (\u0646\u0647 \u0622\u06cc\u06cc\u0646 \u062f\u0627\u0631\u06cc) \u0627\u06cc \u0686\u0631\u062e!", "t1": "\u0686\u0647 \u06a9\u062c \u0631\u0641\u062a\u0627\u0631\u06cc \u0627\u06cc \u0686\u0631\u062e! \u0686\u0647 \u0628\u062f \u06a9\u0631\u062f\u0627\u0631\u06cc \u0627\u06cc \u0686\u0631\u062e!"}, "14": {"m1": "\u0627\u0632 \u062f\u0633\u062a \u0639\u062f\u0648 \u0646\u0627\u0644\u06c0 \u0645\u0646 \u0627\u0632 \u0633\u0631 \u062f\u0631\u062f \u0627\u0633\u062a", "m2": "\u0627\u0646\u062f\u06cc\u0634\u0647 \u0647\u0631 \u0622\u0646 \u06a9\u0633 \u06a9\u0646\u062f \u0627\u0632 \u0645\u0631\u06af\u060c \u0646\u0647 \u0645\u0631\u062f \u0627\u0633\u062a"}, "15": {"m1": "\u062c\u0627\u0646\u0628\u0627\u0632\u06cc \u0639\u0634\u0627\u0642 \u0646\u0647 \u0686\u0648\u0646 \u0628\u0627\u0632\u06cc \u0646\u0631\u062f \u0627\u0633\u062a", "m2": "\u0645\u0631\u062f\u06cc \u0627\u06af\u0631\u062a \u0647\u0633\u062a\u060c \u06a9\u0646\u0648\u0646 \u0648\u0642\u062a \u0646\u0628\u0631\u062f \u0627\u0633\u062a"}, "16": {"t2": "\u0633\u0631 \u06a9\u06cc\u0646 \u062f\u0627\u0631\u06cc \u0627\u06cc \u0686\u0631\u062e! \u0646\u0647 \u062f\u06cc\u0646 \u062f\u0627\u0631\u06cc\u060c \u0646\u0647 \u0622\u06cc\u06cc\u0646 \u062f\u0627\u0631\u06cc (\u0646\u0647 \u0622\u06cc\u06cc\u0646 \u062f\u0627\u0631\u06cc) \u0627\u06cc \u0686\u0631\u062e!", "t1": "\u0686\u0647 \u06a9\u062c \u0631\u0641\u062a\u0627\u0631\u06cc \u0627\u06cc \u0686\u0631\u062e! \u0686\u0647 \u0628\u062f \u06a9\u0631\u062f\u0627\u0631\u06cc \u0627\u06cc \u0686\u0631\u062e!"}, "17": {"m1": "\u0639\u0627\u0631\u0641 \u0632 \u0627\u0632\u0644 \u062a\u06a9\u06cc\u0647 \u0628\u0631 \u0627\u06cc\u0627\u0645 \u0646\u062f\u0627\u062f\u0647\u200c\u0633\u062a", "m2": "\u062c\u0632 \u062c\u0627\u0645 \u0628\u0647 \u06a9\u0633 \u062f\u0633\u062a \u0686\u0648 \u062e\u06cc\u0627\u0645 \u0646\u062f\u0627\u062f\u0647\u200c\u0633\u062a"}, "18": {"m1": "\u062f\u0644 \u062c\u0632 \u0628\u0647 \u0633\u0631 \u0632\u0644\u0641 \u062f\u0644\u0627\u0631\u0627\u0645 \u0646\u062f\u0627\u062f\u0647\u200c\u0633\u062a", "m2": "\u0635\u062f \u0632\u0646\u062f\u06af\u06cc \u0646\u0646\u06af \u0628\u0647 \u06cc\u06a9 \u0646\u0627\u0645 \u0646\u062f\u0627\u062f\u0647\u200c\u0633\u062a"}, "19": {"t2": "\u0633\u0631 \u06a9\u06cc\u0646 \u062f\u0627\u0631\u06cc \u0627\u06cc \u0686\u0631\u062e! \u0646\u0647 \u062f\u06cc\u0646 \u062f\u0627\u0631\u06cc\u060c \u0646\u0647 \u0622\u06cc\u06cc\u0646 \u062f\u0627\u0631\u06cc (\u0646\u0647 \u0622\u06cc\u06cc\u0646 \u062f\u0627\u0631\u06cc) \u0627\u06cc \u0686\u0631\u062e!", "t1": "\u0686\u0647 \u06a9\u062c \u0631\u0641\u062a\u0627\u0631\u06cc \u0627\u06cc \u0686\u0631\u062e! \u0686\u0647 \u0628\u062f \u06a9\u0631\u062f\u0627\u0631\u06cc \u0627\u06cc \u0686\u0631\u062e!"}}}