{"text": {"0": {"m1": "\u0627\u0632\u0627\u0646 \u067e\u0633 \u0641\u0632\u0648\u0646 \u0634\u062f \u0628\u0632\u0631\u06af\u06cc \u0634\u0627\u0647", "m2": "\u06a9\u0647 \u062e\u0648\u0631\u0634\u06cc\u062f \u0634\u062f \u0622\u0646 \u06a9\u062c\u0627 \u0628\u0648\u062f \u0645\u0627\u0647"}, "1": {"m1": "\u0647\u0645\u0647 \u0631\u0648\u0632 \u0628\u0627 \u062f\u062e\u062a \u0642\u06cc\u0635\u0631 \u0628\u062f\u06cc", "m2": "\u0647\u0645\u0648 \u0628\u0631 \u0634\u0628\u0633\u062a\u0627\u0646\u0634 \u0645\u0647\u062a\u0631 \u0628\u062f\u06cc"}, "2": {"m1": "\u0632 \u0645\u0631\u06cc\u0645 \u0647\u0645\u06cc\u200c\u0628\u0648\u062f \u0634\u06cc\u0631\u06cc\u0646 \u0628\u062f\u0631\u062f", "m2": "\u0647\u0645\u06cc\u0634\u0647 \u0632 \u0631\u0634\u06a9\u0634 \u062f\u0648 \u0631\u062e\u0633\u0627\u0631\u0647 \u0632\u0631\u062f"}, "3": {"m1": "\u0628\u0647 \u0641\u0631\u062c\u0627\u0645 \u0634\u06cc\u0631\u06cc\u0646 \u0648\u0631\u0627 \u0632\u0647\u0631 \u062f\u0627\u062f", "m2": "\u0634\u062f \u0622\u0646 \u0646\u0627\u0645\u0648\u0631 \u062f\u062e\u062a \u0642\u06cc\u0635\u0631\u0646\u0698\u0627\u062f"}, "4": {"m1": "\u0627\u0632\u0627\u0646 \u0686\u0627\u0631\u0647 \u0622\u06af\u0647 \u0646\u0628\u062f \u0647\u06cc\u0686\u200c\u06a9\u0633", "m2": "\u06a9\u0647 \u0627\u0648 \u062f\u0627\u0634\u062a \u0622\u0646 \u0631\u0627\u0632 \u062a\u0646\u0647\u0627 \u0648 \u0628\u0633"}, "5": {"m1": "\u0686\u0648 \u0633\u0627\u0644\u06cc \u0628\u0631\u0622\u0645\u062f \u06a9\u0647 \u0645\u0631\u06cc\u0645 \u0628\u0645\u0631\u062f", "m2": "\u0634\u0628\u0633\u062a\u0627\u0646 \u0632\u0631\u06cc\u0646 \u0628\u0647 \u0634\u06cc\u0631\u06cc\u0646 \u0633\u067e\u0631\u062f"}, "6": {"m1": "\u0686\u0648 \u0634\u06cc\u0631\u0648\u06cc\u0647 \u0631\u0627 \u0633\u0627\u0644 \u0634\u062f \u0628\u0631 \u062f\u0648 \u0647\u0634\u062a", "m2": "\u0628\u0647 \u0628\u0627\u0644\u0627 \u0632\u0633\u06cc \u0633\u0627\u0644\u06af\u0627\u0646 \u0628\u0631\u06af\u0630\u0634\u062a"}, "7": {"m1": "\u0628\u06cc\u0627\u0648\u0631\u062f \u0641\u0631\u0632\u0627\u0646\u06af\u0627\u0646 \u0631\u0627 \u067e\u062f\u0631", "m2": "\u0628\u062f\u0627\u0646 \u062a\u0627 \u0634\u0648\u062f \u0646\u0627\u0645\u0648\u0631 \u067e\u0631 \u0647\u0646\u0631"}, "8": {"m1": "\u0647\u0645\u06cc\u200c\u062f\u0627\u0634\u062a \u0645\u0648\u0628\u062f \u0645\u0631 \u0627\u0648 \u0631\u0627 \u0646\u06af\u0627\u0647", "m2": "\u0634\u0628 \u0648 \u0631\u0648\u0632 \u0634\u0627\u062f\u0627\u0646 \u0628\u0647 \u0641\u0631\u0645\u0627\u0646 \u0634\u0627\u0647"}, "9": {"m1": "\u0686\u0646\u0627\u0646 \u0628\u062f \u06a9\u0647 \u06cc\u06a9 \u0631\u0648\u0632 \u0645\u0648\u0628\u062f \u0632 \u062a\u062e\u062a", "m2": "\u0628\u06cc\u0627\u0645\u062f \u0628\u0647 \u0646\u0632\u062f\u06cc\u06a9 \u0622\u0646 \u0646\u06cc\u06a9 \u0628\u062e\u062a"}, "10": {"m1": "\u0686\u0648 \u0622\u0645\u062f \u0628\u0647 \u0646\u0632\u062f\u06cc\u06a9 \u0634\u06cc\u0631\u0648\u06cc\u0647 \u0628\u0627\u0632", "m2": "\u0647\u0645\u06cc\u0634\u0647 \u0628\u0647 \u0628\u0627\u0632\u06cc\u0634 \u0628\u0648\u062f\u06cc \u0646\u06cc\u0627\u0632"}, "11": {"m1": "\u06cc\u06a9\u06cc \u062f\u0641\u062a\u0631\u06cc \u062f\u06cc\u062f \u067e\u06cc\u0634 \u0627\u0646\u062f\u0631\u0634", "m2": "\u0646\u0648\u0634\u062a\u0647 \u06a9\u0644\u06cc\u0644\u0647 \u0628\u0631\u0627\u0646 \u062f\u0641\u062a\u0631\u0634"}, "12": {"m1": "\u0628\u062f\u0633\u062a \u0686\u067e \u0622\u0646 \u062c\u0648\u0627\u0646 \u0633\u062a\u0631\u06af", "m2": "\u0628\u0631\u06cc\u062f\u0647 \u06cc\u06a9\u06cc \u062e\u0634\u06a9 \u0686\u0646\u06af\u0627\u0644 \u06af\u0631\u06af"}, "13": {"m1": "\u0633\u0631\u0648\u06cc \u0633\u0631 \u06af\u0627\u0648\u0645\u06cc\u0634\u06cc \u0628\u0631\u0627\u0633\u062a", "m2": "\u0647\u0645\u06cc \u0627\u06cc\u0646 \u0628\u0631\u0627\u0646 \u0628\u0631 \u0632\u062f\u06cc \u0686\u0648\u0646\u06a9 \u062e\u0648\u0627\u0633\u062a"}, "14": {"m1": "\u063a\u0645\u06cc \u0634\u062f \u062f\u0644 \u0645\u0648\u0628\u062f \u0627\u0632 \u06a9\u0627\u0631\u0627\u0648\u06cc", "m2": "\u0632 \u0628\u0627\u0632\u06cc \u0648 \u0628\u06cc\u0647\u0648\u062f\u0647 \u06a9\u0631\u062f\u0627\u0631 \u0627\u0648\u06cc"}, "15": {"m1": "\u0628\u0647 \u0641\u0627\u0644\u0634 \u0628\u062f \u0622\u0645\u062f \u0647\u0645 \u0622\u0646 \u0686\u0646\u06af \u06af\u0631\u06af", "m2": "\u0634\u062e \u06af\u0627\u0648 \u0648 \u0631\u0627\u06cc \u062c\u0648\u0627\u0646 \u0633\u062a\u0631\u06af"}, "16": {"m1": "\u0632 \u06a9\u0627\u0631 \u0632\u0645\u0627\u0646\u0647 \u063a\u0645\u06cc \u06af\u0634\u062a \u0633\u062e\u062a", "m2": "\u0627\u0632\u0627\u0646 \u0628\u0631\u0645\u0646\u0634 \u06a9\u0648\u062f\u06a9 \u0634\u0648\u0631 \u0628\u062e\u062a"}, "17": {"m1": "\u06a9\u062c\u0627 \u0637\u0627\u0644\u0639 \u0632\u0627\u062f\u0646\u0634 \u062f\u06cc\u062f\u0647 \u0628\u0648\u062f", "m2": "\u0632 \u062f\u0633\u062a\u0648\u0631 \u0648\u06af\u0646\u062c\u0648\u0631 \u0628\u0634\u0646\u06cc\u062f\u0647 \u0628\u0648\u062f"}, "18": {"m1": "\u0633\u0648\u06cc \u0645\u0648\u0628\u062f \u0645\u0648\u0628\u062f \u0622\u0645\u062f \u0628\u06af\u0641\u062a", "m2": "\u06a9\u0647 \u0628\u0627\u0632\u06cc\u0633\u062a \u0628\u0627\u0622\u0646 \u06af\u0631\u0627\u0646\u0645\u0627\u06cc\u0647 \u062c\u0641\u062a"}, "19": {"m1": "\u0628\u0634\u062f \u0632\u0648\u062f \u0645\u0648\u0628\u062f \u0628\u06af\u0641\u062a \u0622\u0646 \u0628\u0647 \u0634\u0627\u0647", "m2": "\u0647\u0645\u06cc\u200c\u062f\u0627\u0634\u062a \u062e\u0633\u0631\u0648 \u0645\u0631 \u0627\u0648 \u0631\u0627 \u0646\u06af\u0627\u0647"}, "20": {"m1": "\u0632 \u0641\u0631\u0632\u0646\u062f \u0631\u0646\u06af \u0631\u062e\u0634 \u0632\u0631\u062f \u0634\u062f", "m2": "\u0632 \u06a9\u0627\u0631 \u0632\u0645\u0627\u0646\u0647 \u067e\u0631\u0627\u0632 \u062f\u0631\u062f \u0634\u062f"}, "21": {"m1": "\u0632 \u06af\u0641\u062a\u0627\u0631 \u0645\u0631\u062f \u0633\u062a\u0627\u0631\u0647 \u0634\u0645\u0631", "m2": "\u062f\u0644\u0634 \u0628\u0648\u062f \u067e\u0631 \u062f\u0631\u062f \u0648 \u067e\u06cc\u0686\u0627\u0646 \u062c\u06af\u0631"}, "22": {"m1": "\u0647\u0645\u06cc\u200c\u06af\u0641\u062a \u062a\u0627 \u06a9\u0631\u062f\u06af\u0627\u0631 \u0633\u067e\u0647\u0631", "m2": "\u0686\u06af\u0648\u0646\u0647 \u0646\u0645\u0627\u06cc\u062f \u0628\u062f\u06cc\u0646 \u06a9\u0631\u062f\u0647 \u0686\u0647\u0631"}, "23": {"m1": "\u0686\u0648 \u0628\u0631 \u067e\u0627\u062f\u0634\u0627\u0647\u06cc\u0634 \u0628\u06cc\u0633\u062a \u0648\u0633\u0647 \u0633\u0627\u0644", "m2": "\u06af\u0630\u0631 \u06a9\u0631\u062f \u0634\u06cc\u0631\u0648\u06cc\u0647 \u0628\u0647 \u0641\u0631\u0627\u062e\u062a \u06cc\u0627\u0644"}, "24": {"m1": "\u0628\u06cc\u0627\u0632\u0631\u062f \u0632\u0648 \u0634\u0647\u0631\u06cc\u0627\u0631 \u0628\u0632\u0631\u06af", "m2": "\u06a9\u0647 \u06a9\u0648\u062f\u06a9 \u062c\u0648\u0627\u0646 \u0628\u0648\u062f \u0648 \u06af\u0634\u062a\u0647 \u0633\u062a\u0631\u06af"}, "25": {"m1": "\u067e\u0631 \u0627\u0632 \u062f\u0631\u062f \u0634\u062f \u062c\u0627\u0646 \u062e\u0646\u062f\u0627\u0646 \u0627\u0648\u06cc", "m2": "\u0648\u0632 \u0627\u06cc\u0648\u0627\u0646 \u0627\u0648 \u06a9\u0631\u062f \u0632\u0646\u062f\u0627\u0646 \u0627\u0648\u06cc"}, "26": {"m1": "\u0647\u0645 \u0622\u0646 \u0631\u0627 \u06a9\u0647 \u067e\u06cc\u0648\u0633\u062a\u0647\u0654 \u0627\u0648\u0628\u062f\u0646\u062f", "m2": "\u06af\u0647 \u0631\u0627\u06cc \u062c\u0633\u062a\u0646 \u0628\u0631\u0627\u0648 \u0634\u062f\u0646\u062f"}, "27": {"m1": "\u0628\u0633\u06cc \u062f\u06cc\u06af\u0631 \u0627\u0632 \u0645\u0647\u062a\u0631 \u0648 \u06a9\u0647\u062a\u0631\u0627\u0646", "m2": "\u06a9\u0647 \u0628\u0648\u062f\u0646\u062f \u0628\u0627 \u0627\u0648 \u0628\u0628\u0646\u062f\u06af\u0631\u0627\u0646"}, "28": {"m1": "\u0647\u0645\u06cc\u200c\u0628\u0631\u06af\u0631\u0641\u062a\u0646\u062f \u0632\u06cc\u0634\u0627\u0646 \u0634\u0645\u0627\u0631", "m2": "\u06a9\u0647 \u067e\u0631\u0633\u0647 \u0641\u0632\u0648\u0646 \u0622\u0645\u062f \u0627\u0632 \u0633\u0647 \u0647\u0632\u0627\u0631"}, "29": {"m1": "\u0647\u0645\u0647 \u06a9\u0627\u062e\u0647\u0627 \u0631\u0627\u06cc\u06a9 \u0627\u0646\u062f\u0631 \u062f\u06af\u0631", "m2": "\u0628\u0631\u06cc\u062f \u0622\u0646\u06a9 \u0628\u062f \u0634\u0627\u0647 \u0631\u0627 \u06a9\u0627\u0631\u06af\u0631"}, "30": {"m1": "\u0632 \u067e\u0648\u0634\u06cc\u062f\u0646\u06cc\u0647\u0627 \u0648 \u0627\u0632 \u062e\u0648\u0631\u062f\u0646\u06cc", "m2": "\u0632 \u0628\u062e\u0634\u06cc\u062f\u0646\u06cc \u0647\u0645 \u0632 \u06af\u0633\u062a\u0631\u062f\u0646\u06cc"}, "31": {"m1": "\u0628\u0647 \u0627\u06cc\u0648\u0627\u0646\u0647\u0627\u0634\u0627\u0646 \u0628\u06cc\u0627\u0631\u0627\u0633\u062a\u0646\u062f", "m2": "\u067e\u0631\u0633\u062a\u0646\u062f\u0647 \u0648 \u0628\u0646\u062f\u06af\u0627\u0646 \u062e\u0648\u0627\u0633\u062a\u0646\u062f"}, "32": {"m1": "\u0647\u0645\u0627\u0646 \u0645\u06cc\u200c\u0641\u0631\u0633\u062a\u0627\u062f \u0648 \u0631\u0627\u0645\u0634\u06af\u0631\u0627\u0646", "m2": "\u0647\u0645\u0647 \u06a9\u0627\u062e \u062f\u06cc\u0646\u0627\u0631 \u0628\u062f \u0628\u06cc\u200c\u06a9\u0631\u0627\u0646"}, "33": {"m1": "\u0628\u0647 \u0647\u0646\u06af\u0627\u0645\u0634\u0627\u0646 \u0631\u0627\u0645\u0634 \u0648 \u062e\u0648\u0631\u062f \u0628\u0648\u062f", "m2": "\u0646\u06af\u0647\u0628\u0627\u0646 \u0627\u06cc\u0634\u0627\u0646 \u0686\u0647\u0644 \u0645\u0631\u062f \u0628\u0648\u062f"}}}