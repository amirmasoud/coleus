{"text": {"0": {"m1": "\u0632 \u0634\u0627\u0647\u06cc\u0634 \u0628\u06af\u0630\u0634\u062a \u067e\u0646\u062c\u0627\u0647 \u0633\u0627\u0644", "m2": "\u06a9\u0647 \u0627\u0646\u062f\u0631 \u0632\u0645\u0627\u0646\u0647 \u0646\u0628\u0648\u062f\u0634 \u0647\u0645\u0627\u0644"}, "1": {"m1": "\u0628\u06cc\u0627\u0645\u062f \u06cc\u06a9\u06cc \u0645\u0631\u062f \u06af\u0648\u06cc\u0627 \u0632 \u0686\u06cc\u0646", "m2": "\u06a9\u0647 \u0686\u0648\u0646 \u0627\u0648 \u0645\u0635\u0648\u0631 \u0646\u0628\u06cc\u0646\u062f \u0632\u0645\u06cc\u0646"}, "2": {"m1": "\u0628\u062f\u0627\u0646 \u0686\u0631\u0628\u0647 \u062f\u0633\u062a\u06cc \u0631\u0633\u06cc\u062f\u0647 \u0628\u0647 \u06a9\u0627\u0645", "m2": "\u06cc\u06a9\u06cc \u0628\u0631\u0645\u0646\u0634 \u0645\u0631\u062f \u0645\u0627\u0646\u06cc \u0628\u0647 \u0646\u0627\u0645"}, "3": {"m1": "\u0628\u0647 \u0635\u0648\u0631\u062a\u06af\u0631\u06cc \u06af\u0641\u062a \u067e\u06cc\u063a\u0645\u0628\u0631\u0645", "m2": "\u0632 \u062f\u06cc\u0646\u200c\u0622\u0648\u0631\u0627\u0646 \u062c\u0647\u0627\u0646 \u0628\u0631\u062a\u0631\u0645"}, "4": {"m1": "\u0632 \u0686\u06cc\u0646 \u0646\u0632\u062f \u0634\u0627\u067e\u0648\u0631 \u0634\u062f \u0628\u0627\u0631 \u062e\u0648\u0627\u0633\u062a", "m2": "\u0628\u0647 \u067e\u06cc\u063a\u0645\u0628\u0631\u06cc \u0634\u0627\u0647 \u0631\u0627 \u06cc\u0627\u0631 \u062e\u0648\u0627\u0633\u062a"}, "5": {"m1": "\u0633\u062e\u0646 \u06af\u0641\u062a \u0645\u0631\u062f \u06af\u0634\u0627\u062f\u0647\u200c\u0632\u0628\u0627\u0646", "m2": "\u062c\u0647\u0627\u0646\u062f\u0627\u0631 \u0634\u062f \u0632\u0627\u0646 \u0633\u062e\u0646 \u0628\u062f\u06af\u0645\u0627\u0646"}, "6": {"m1": "\u0633\u0631\u0634 \u062a\u06cc\u0632 \u0634\u062f \u0645\u0648\u0628\u062f\u0627\u0646 \u0631\u0627 \u0628\u062e\u0648\u0627\u0646\u062f", "m2": "\u0632\u0645\u0627\u0646\u06cc \u0641\u0631\u0627\u0648\u0627\u0646 \u0633\u062e\u0646\u0647\u0627 \u0628\u0631\u0627\u0646\u062f"}, "7": {"m1": "\u06a9\u0632\u06cc\u0646 \u0645\u0631\u062f \u0686\u06cc\u0646\u06cc \u0648 \u0686\u06cc\u0631\u0647\u200c\u0632\u0628\u0627\u0646", "m2": "\u0641\u062a\u0627\u062f\u0633\u062a\u0645 \u0627\u0632 \u062f\u06cc\u0646 \u0627\u0648 \u062f\u0631 \u06af\u0645\u0627\u0646"}, "8": {"m1": "\u0628\u06af\u0648\u06cc\u06cc\u062f \u0648 \u0647\u0645 \u0632\u0648 \u0633\u062e\u0646 \u0628\u0634\u0646\u0648\u06cc\u062f", "m2": "\u0645\u06af\u0631 \u062e\u0648\u062f \u0628\u0647 \u06af\u0641\u062a\u0627\u0631 \u0627\u0648 \u0628\u06af\u0631\u0648\u06cc\u062f"}, "9": {"m1": "\u0628\u06af\u0641\u062a\u0646\u062f \u06a9\u06cc\u0646 \u0645\u0631\u062f \u0635\u0648\u0631\u062a \u067e\u0631\u0633\u062a", "m2": "\u0646\u0647 \u0628\u0631 \u0645\u0627\u06cc\u0647\u0654 \u0645\u0648\u0628\u062f\u0627\u0646 \u0645\u0648\u0628\u0647 \u062f\u0633\u062a"}, "10": {"m1": "\u0632\u0645\u0627\u0646\u06cc \u0633\u062e\u0646 \u0628\u0634\u0646\u0648 \u0627\u0648 \u0631\u0627 \u0628\u062e\u0648\u0627\u0646", "m2": "\u0686\u0648 \u0628\u06cc\u0646\u062f \u0648\u0631\u0627 \u06a9\u06cc \u06af\u0634\u0627\u06cc\u062f \u0632\u0628\u0627\u0646"}, "11": {"m1": "\u0628\u0641\u0631\u0645\u0648\u062f \u062a\u0627 \u0645\u0648\u0628\u062f \u0622\u0645\u062f\u0634 \u067e\u06cc\u0634", "m2": "\u0633\u062e\u0646 \u06af\u0641\u062a \u0628\u0627 \u0627\u0648 \u0632 \u0627\u0646\u062f\u0627\u0632\u0647 \u0628\u06cc\u0634"}, "12": {"m1": "\u0641\u0631\u0648 \u0645\u0627\u0646\u062f \u0645\u0627\u0646\u06cc \u0645\u06cc\u0627\u0646 \u0633\u062e\u0646", "m2": "\u0628\u0647 \u06af\u0641\u062a\u0627\u0631 \u0645\u0648\u0628\u062f \u0632 \u062f\u06cc\u0646 \u06a9\u0647\u0646"}, "13": {"m1": "\u0628\u062f\u0648 \u06af\u0641\u062a \u06a9\u0627\u06cc \u0645\u0631\u062f \u0635\u0648\u0631\u062a \u067e\u0631\u0633\u062a", "m2": "\u0628\u0647 \u06cc\u0632\u062f\u0627\u0646 \u0686\u0631\u0627 \u0622\u062e\u062a\u06cc \u062e\u06cc\u0631\u0647\u200c\u062f\u0633\u062a"}, "14": {"m1": "\u06a9\u0633\u06cc \u06a9\u0648 \u0628\u0644\u0646\u062f \u0622\u0633\u0645\u0627\u0646 \u0622\u0641\u0631\u06cc\u062f", "m2": "\u0628\u062f\u0648 \u062f\u0631 \u0645\u06a9\u0627\u0646 \u0648 \u0632\u0645\u0627\u0646 \u0622\u0641\u0631\u06cc\u062f"}, "15": {"m1": "\u06a9\u062c\u0627 \u0646\u0648\u0631 \u0648 \u0638\u0644\u0645\u062a \u0628\u062f\u0648 \u0627\u0646\u062f\u0631\u0633\u062a", "m2": "\u0632 \u0647\u0631 \u06af\u0648\u0647\u0631\u06cc \u06af\u0648\u0647\u0631\u0634 \u0628\u0631\u062a\u0631\u0633\u062a"}, "16": {"m1": "\u0634\u0628 \u0648 \u0631\u0648\u0632 \u0648 \u06af\u0631\u062f\u0627\u0646 \u0633\u067e\u0647\u0631 \u0628\u0644\u0646\u062f", "m2": "\u06a9\u0632\u0648\u06cc\u062a \u067e\u0646\u0627\u0647\u0633\u062a \u0648 \u0632\u0648\u06cc\u062a \u06af\u0632\u0646\u062f"}, "17": {"m1": "\u0647\u0645\u0647 \u06a9\u0631\u062f\u0647\u0654 \u06a9\u0631\u062f\u06af\u0627\u0631\u0633\u062a \u0648 \u0628\u0633", "m2": "\u062c\u0632\u0648 \u06a9\u0631\u062f \u0646\u062a\u0648\u0627\u0646\u062f \u0627\u06cc\u0646 \u06a9\u0631\u062f\u0647 \u06a9\u0633"}, "18": {"m1": "\u0628\u0647 \u0628\u0631\u0647\u0627\u0646 \u0635\u0648\u0631\u062a \u0686\u0631\u0627 \u0628\u06af\u0631\u0648\u06cc", "m2": "\u0647\u0645\u06cc \u067e\u0646\u062f \u062f\u06cc\u0646\u200c\u0622\u0648\u0631\u0627\u0646 \u0646\u0634\u0646\u0648\u06cc"}, "19": {"m1": "\u0647\u0645\u0647 \u062c\u0641\u062a \u0648 \u0647\u0645\u062a\u0627 \u0648 \u06cc\u0632\u062f\u0627\u0646 \u06cc\u06a9\u06cc\u0633\u062a", "m2": "\u062c\u0632 \u0627\u0632 \u0628\u0646\u062f\u06af\u06cc \u06a9\u0631\u062f\u0646\u062a \u0631\u0627\u06cc \u0646\u06cc\u0633\u062a"}, "20": {"m1": "\u06af\u0631\u06cc\u0646 \u0635\u0648\u0631\u062a \u06a9\u0631\u062f\u0647 \u062c\u0646\u0628\u0627\u0646 \u06a9\u0646\u06cc", "m2": "\u0633\u0632\u062f \u06af\u0631 \u0632 \u062c\u0646\u0628\u062f\u0647 \u0628\u0631\u0647\u0627\u0646 \u06a9\u0646\u06cc"}, "21": {"m1": "\u0646\u062f\u0627\u0646\u06cc \u06a9\u0647 \u0628\u0631\u0647\u0627\u0646 \u0646\u06cc\u0627\u06cc\u062f \u0628\u0647 \u06a9\u0627\u0631", "m2": "\u0646\u062f\u0627\u0631\u062f \u06a9\u0633\u06cc \u0627\u06cc\u0646 \u0633\u062e\u0646 \u0627\u0633\u062a\u0648\u0627\u0631"}, "22": {"m1": "\u0627\u06af\u0631 \u0627\u0647\u0631\u0645\u0646 \u062c\u0641\u062a \u06cc\u0632\u062f\u0627\u0646 \u0628\u062f\u06cc", "m2": "\u0634\u0628 \u062a\u06cc\u0631\u0647 \u0686\u0648\u0646 \u0631\u0648\u0632 \u062e\u0646\u062f\u0627\u0646 \u0628\u062f\u06cc"}, "23": {"m1": "\u0647\u0645\u0647 \u0633\u0627\u0644\u0647 \u0628\u0648\u062f\u06cc \u0634\u0628 \u0648 \u0631\u0648\u0632 \u0631\u0627\u0633\u062a", "m2": "\u0628\u0647 \u06af\u0631\u062f\u0634 \u0641\u0632\u0648\u0646\u06cc \u0646\u0628\u0648\u062f\u06cc \u0646\u0647 \u06a9\u0627\u0633\u062a"}, "24": {"m1": "\u0646\u06af\u0646\u062c\u062f \u062c\u0647\u0627\u0646\u200c\u0622\u0641\u0631\u06cc\u0646 \u062f\u0631 \u06af\u0645\u0627\u0646", "m2": "\u06a9\u0647 \u0627\u0648 \u0628\u0631\u062a\u0631\u0633\u062a \u0627\u0632 \u0632\u0645\u0627\u0646 \u0648 \u0645\u06a9\u0627\u0646"}, "25": {"m1": "\u0633\u062e\u0646\u0647\u0627\u06cc \u062f\u06cc\u0648\u0627\u0646\u06af\u0627\u0646\u0633\u062a \u0648 \u0628\u0633", "m2": "\u0628\u062f\u06cc\u0646\u200c\u0628\u0631 \u0646\u0628\u0627\u0634\u062f \u062a\u0631\u0627 \u06cc\u0627\u0631 \u06a9\u0633"}, "26": {"m1": "\u0633\u062e\u0646\u0647\u0627 \u062c\u0632\u06cc\u0646 \u0646\u06cc\u0632 \u0628\u0633\u06cc\u0627\u0631 \u06af\u0641\u062a", "m2": "\u06a9\u0647 \u0628\u0627 \u062f\u0627\u0646\u0634 \u0648 \u0645\u0631\u062f\u0645\u06cc \u0628\u0648\u062f \u062c\u0641\u062a"}, "27": {"m1": "\u0641\u0631\u0648 \u0645\u0627\u0646\u062f \u0645\u0627\u0646\u06cc \u0632 \u06af\u0641\u062a\u0627\u0631 \u0627\u0648\u06cc", "m2": "\u0628\u067e\u0698\u0645\u0631\u062f \u0634\u0627\u062f\u0627\u0628 \u0628\u0627\u0632\u0627\u0631 \u0627\u0648\u06cc"}, "28": {"m1": "\u0632 \u0645\u0627\u0646\u06cc \u0628\u0631\u0622\u0634\u0641\u062a \u067e\u0633 \u0634\u0647\u0631\u06cc\u0627\u0631", "m2": "\u0628\u0631\u0648 \u062a\u0646\u06af \u0634\u062f \u06af\u0631\u062f\u0634 \u0631\u0648\u0632\u06af\u0627\u0631"}, "29": {"m1": "\u0628\u0641\u0631\u0645\u0648\u062f \u067e\u0633 \u062a\u0627\u0634 \u0628\u0631\u062f\u0627\u0634\u062a\u0646\u062f", "m2": "\u0628\u0647 \u062e\u0648\u0627\u0631\u06cc \u0632 \u062f\u0631\u06af\u0627\u0647 \u0628\u06af\u0630\u0627\u0634\u062a\u0646\u062f"}, "30": {"m1": "\u0686\u0646\u06cc\u0646 \u06af\u0641\u062a \u06a9\u0627\u06cc\u0646 \u0645\u0631\u062f \u0635\u0648\u0631\u062a\u200c\u067e\u0631\u0633\u062a", "m2": "\u0646\u06af\u0646\u062c\u062f \u0647\u0645\u06cc \u062f\u0631 \u0633\u0631\u0627\u06cc \u0646\u0634\u0633\u062a"}, "31": {"m1": "\u0686\u0648 \u0622\u0634\u0648\u0628 \u0648 \u0622\u0631\u0627\u0645 \u06af\u06cc\u062a\u06cc \u0628\u0647 \u062f\u0648\u0633\u062a", "m2": "\u0628\u0628\u0627\u06cc\u062f \u06a9\u0634\u06cc\u062f\u0646 \u0633\u0631\u0627\u067e\u0627\u0634 \u067e\u0648\u0633\u062a"}, "32": {"m1": "\u0647\u0645\u0627\u0646 \u062e\u0627\u0645\u0634 \u0622\u06af\u0646\u062f\u0647 \u0628\u0627\u06cc\u062f \u0628\u0647 \u06a9\u0627\u0647", "m2": "\u0628\u062f\u0627\u0646 \u062a\u0627 \u0646\u062c\u0648\u06cc\u062f \u06a9\u0633 \u0627\u06cc\u0646 \u067e\u0627\u06cc\u06af\u0627\u0647"}, "33": {"m1": "\u0628\u06cc\u0627\u0648\u06cc\u062e\u062a\u0646\u062f \u0627\u0632 \u062f\u0631 \u0634\u0627\u0631\u0633\u062a\u0627\u0646", "m2": "\u062f\u06af\u0631 \u067e\u06cc\u0634 \u062f\u06cc\u0648\u0627\u0631 \u0628\u06cc\u0645\u0627\u0631\u0633\u062a\u0627\u0646"}, "34": {"m1": "\u062c\u0647\u0627\u0646\u06cc \u0628\u0631\u0648 \u0622\u0641\u0631\u06cc\u0646 \u062e\u0648\u0627\u0646\u062f\u0646\u062f", "m2": "\u0647\u0645\u06cc \u062e\u0627\u06a9 \u0628\u0631 \u06a9\u0634\u062a\u0647 \u0627\u0641\u0634\u0627\u0646\u062f\u0646\u062f"}}}