{"text": {"0": {"m1": "\u0686\u0646\u06cc\u0646 \u0628\u0648\u062f \u06cc\u06a9 \u0647\u0641\u062a\u0647 \u067e\u06cc\u0648\u0633\u062a\u0647 \u062c\u0646\u06af", "m2": "\u062c\u0647\u0627\u0646 \u06af\u0634\u062a \u0628\u0631 \u0686\u06cc\u0646\u06cc\u0627\u0646 \u062a\u0627\u0631 \u0648 \u062a\u0646\u06af"}, "1": {"m1": "\u0628\u062f \u0627\u0632 \u062e\u06cc\u0644\u0634\u0627\u0646 \u062c\u0627\u0648\u062f\u0627\u0646 \u0628\u06cc \u0634\u0645\u0627\u0631", "m2": "\u06af\u0631\u0641\u062a\u0647 \u0628\u06cc \u0627\u0646\u062f\u0627\u0632\u0647 \u067e\u0631\u0651\u0646\u062f\u0647 \u0645\u0627\u0631"}, "2": {"m1": "\u0628\u0647 \u0627\u0641\u0633\u0648\u0646\u06af\u0631\u06cc \u0628\u0631 \u0633\u0631 \u062a\u06cc\u063a \u06a9\u0648\u0647", "m2": "\u0634\u062f\u0646\u062f \u0627\u0632 \u067e\u0633 \u067e\u0634\u062a \u0627\u06cc\u0631\u0627\u0646 \u06af\u0631\u0648\u0647"}, "3": {"m1": "\u0647\u0645\u06cc \u0645\u0627\u0631 \u06a9\u0631\u062f\u0646\u062f \u067e\u0631\u0651\u0627\u0646 \u0631\u0647\u0627", "m2": "\u0646\u0645\u0648\u062f\u0646\u062f \u0627\u0631 \u0627\u0628\u0631 \u0627\u0646\u062f\u0631\u0648\u0646 \u0627\u0698\u062f\u0647\u0627"}, "4": {"m1": "\u062a\u06af\u0631\u06af \u0622\u0648\u0631\u06cc\u062f\u0646\u062f \u0628\u0627 \u0628\u0627\u062f \u0633\u062e\u062a", "m2": "\u067e\u0633 \u0627\u0632 \u0628\u0627\u062f \u0633\u0631\u0645\u0627 \u06a9\u0647 \u062f\u0631\u0651\u062f \u062f\u0631\u062e\u062a"}, "5": {"m1": "\u0628\u062f \u0627\u0632 \u0633\u0648\u06cc \u062a\u0648\u0631\u0627\u0646 \u0632\u0645\u06cc\u0646 \u0627\u0641\u062a\u0627\u0628", "m2": "\u0648\u0632 \u0627\u06cc\u0646 \u0633\u0648 \u0632 \u0633\u0631\u0645\u0627 \u0647\u0645\u06cc \u06cc\u062e \u0634\u062f \u0622\u0628"}, "6": {"m1": "\u0686\u0646\u0627\u0646 \u06af\u0634\u062a \u06a9\u0632 \u0628\u0627\u062f \u0628\u0641\u0633\u0631\u062f \u0634\u062e", "m2": "\u0647\u0645\u0647 \u062f\u0634\u062a  \u0648 \u06a9\u064f\u0647 \u0628\u0631\u0641 \u06af\u0633\u062a\u0631\u062f \u06cc\u062e"}, "7": {"m1": "\u062f\u0631\u062e\u0634 \u062c\u0647\u0646\u062f\u0647 \u062c\u0647\u0627\u0646 \u0628\u0631\u0641\u0631\u0648\u062e\u062a", "m2": "\u0633\u06cc\u0627\u0647 \u0627\u0628\u0631 \u0628\u0627 \u0686\u0631\u062e \u062f\u0627\u0645\u0646 \u0628\u062f\u0648\u062e\u062a"}, "8": {"m1": "\u0628\u0631 \u0627\u06cc\u0631\u0627\u0646\u06cc\u0627\u0646 \u062e\u0648\u0627\u0633\u062a \u0622\u0645\u062f \u0634\u06a9\u0633\u062a", "m2": "\u06a9\u0647 \u0628\u06cc\u06a9\u0627\u0631 \u0634\u062f\u0634\u0627\u0646 \u0632 \u067e\u06cc\u06a9\u0627\u0631 \u062f\u0633\u062a"}, "9": {"m1": "\u062e\u0628\u0631 \u06cc\u0627\u0641\u062a \u0627\u0632 \u062c\u0627\u0648\u062f\u0627\u0646 \u067e\u0647\u0644\u0648\u0627\u0646", "m2": "\u0641\u0631\u0633\u062a\u0627\u062f \u0686\u0646\u062f\u06cc \u062f\u0644\u0627\u0648\u0631 \u06af\u0648\u0627\u0646"}, "10": {"m1": "\u0628\u0631\u0627\u06cc\u0634\u0627\u0646 \u0632 \u0646\u0627\u06af\u0647 \u06a9\u0645\u06cc\u0646 \u0633\u0627\u062e\u062a\u0646\u062f", "m2": "\u0633\u0631\u0627\u0646\u0634\u0627\u0646 \u0628\u0647 \u062e\u0646\u062c\u0631 \u0628\u06cc\u0646\u062f\u0627\u062e\u062a\u0646\u062f"}, "11": {"m1": "\u0647\u0645\u0627\u0646 \u06af\u0647 \u0632 \u0633\u0631\u0645\u0627 \u062c\u0647\u0627\u0646 \u067e\u0627\u06a9 \u0634\u062f", "m2": "\u0647\u0645\u0647 \u062a\u0646\u0628\u0644 \u062c\u0627\u0648\u062f\u0627\u0646 \u067e\u0627\u06a9 \u0634\u062f"}, "12": {"m1": "\u0628\u0631 \u06a9\u0627\u0631 \u06cc\u0632\u062f\u0627\u0646 \u06a9\u06cc\u0647\u0627\u0646 \u062e\u062f\u06cc\u0648", "m2": "\u0686\u0647 \u062f\u0627\u0631\u062f \u0628\u0647\u0627 \u06a9\u0627\u0631 \u062c\u0627\u062f\u0648 \u0648 \u062f\u06cc\u0648"}, "13": {"m1": "\u0647\u0645\u0647 \u06af\u06cc\u062a\u06cc \u0627\u0631 \u062f\u0634\u0645\u0646 \u062a\u0633\u062a \u067e\u0627\u06a9", "m2": "\u0686\u0648 \u0627\u06cc\u0632\u062f \u0646\u06af\u0647\u062f\u0627\u0631 \u0628\u0627\u0634\u062f \u0686\u0647 \u0628\u0627\u06a9"}, "14": {"m1": "\u0633\u067e\u0647\u062f\u0627\u0631 \u0628\u0631 \u067e\u06cc\u0644 \u0647\u0645 \u062f\u0631 \u0632\u0645\u0627\u0646", "m2": "\u062e\u0631\u0648\u0634\u06cc\u062f \u0648 \u067e\u06cc\u0634 \u0635\u0641 \u0622\u0645\u062f \u062f\u0645\u0627\u0646"}, "15": {"m1": "\u06a9\u0647 \u06af\u0631\u062a\u0627\u0646 \u062f\u0644\u06cc\u0631\u06cc\u0633\u062a \u062c\u0646\u06af \u0622\u0648\u0631\u06cc\u062f", "m2": "\u0646\u0647 \u062f\u0631 \u062c\u0646\u06af \u0646\u06cc\u0631\u0646\u06af \u0648 \u0631\u0646\u06af \u0622\u0648\u0631\u06cc\u062f"}, "16": {"m1": "\u0647\u0645\u06cc   \u0627\u0698\u062f\u0647\u0627   \u0632    \u0627\u0628\u0631   \u0633\u0627\u0632\u06cc\u062f   \u0648  \u0633\u0646\u06af", "m2": "\u0686\u0646\u0627\u0646     \u06a9\u0648\u062f\u06a9\u0627\u0646     \u0631\u0627     \u0646\u0645\u0627\u06cc\u06cc\u062f     \u0631\u0646\u06af"}, "17": {"m1": "\u0628\u064e\u0631       \u0645\u0627       \u062f\u0645\u0627\u0646      \u0627\u0698\u062f\u0647\u0627\u06cc     \u0646\u0628\u0631\u062f", "m2": "\u06a9\u0645\u0646\u062f     \u06cc\u0644\u0627\u0646    \u0627\u0633\u062a   \u062f\u0631    \u062a\u06cc\u0631\u0647     \u06af\u0631\u062f"}, "18": {"m1": "\u0647\u0645\u0627\u0646     \u062e\u0634\u062a   \u0648   \u062a\u06cc\u0631\u0633\u062a    \u0645\u0627\u0631    \u0628\u067e\u0631", "m2": "\u0641\u0633\u0648\u0646\u06af\u0631          \u0633\u0648\u0627\u0631\u0627\u0646          \u067e\u0631\u062e\u0627\u0634\u062e\u0631"}, "19": {"m1": "\u062a\u06af\u0631\u06af       \u0641\u0634\u0627\u0646\u0646\u062f\u0647         \u0628\u0627\u0631\u0627\u0646        \u062a\u06cc\u0631", "m2": "\u062f\u0645    \u0628\u062f    \u062f\u0644\u0627\u0646    \u0632\u0627\u0646    \u0634\u062f\u0647   \u0632  \u0645\u0647\u0631\u06cc\u0631"}, "20": {"m1": "\u0628\u0647     \u0646\u0627\u0645     \u062e\u062f\u0627\u06cc     \u0633\u0631\u0648\u0634\u06cc     \u0633\u0631\u0634\u062a", "m2": "\u0628\u0647    \u0634\u0647\u0631\u06cc\u0648\u0631   \u0648   \u0645\u0647\u0631    \u0648    \u0627\u0631\u062f\u06cc\u0628\u0647\u0634\u062a"}, "21": {"m1": "\u0628\u0647     \u0641\u0631\u0651    \u0641\u0631\u06cc\u062f\u0648\u0646   \u0648    \u0627\u0631\u062c\u0634   \u0628\u0647  \u0647\u0645", "m2": "\u0628\u0647   \u06af\u0627\u0647   \u0648   \u06af\u0647   \u0634\u0627\u0647   \u0647\u0648\u0634\u0646\u06af   \u0648   \u062c\u0645"}, "22": {"m1": "\u06a9\u0647   \u0627\u0632  \u0645\u0646   \u0631\u0647\u0627\u06cc\u06cc     \u062f\u0631 \u06cc\u0646   \u06a9\u0627\u0631 \u0632\u0627\u0631", "m2": "\u0646\u06cc\u0627\u0628\u06cc\u062f       \u06a9\u0633      \u0646\u0627\u0634\u062f\u0647      \u06a9\u0627\u0631     \u0632\u0627\u0631"}, "23": {"m1": "\u0628\u0632\u062f    \u062e\u0634\u062a     \u0633\u0627\u0644\u0627\u0631\u0634\u0627\u0646    \u0631\u0627   \u0632  \u0632\u06cc\u0646", "m2": "\u0641\u06a9\u0646\u062f   \u0648   \u060c   \u0628\u0647    \u0627\u06cc\u0631\u0627\u0646\u06cc\u0627\u0646   \u06af\u0641\u062a    \u0647\u06cc\u0646"}, "24": {"m1": "\u06a9\u0631\u0627     \u0628\u0631     \u0633\u0631    \u0622\u06cc\u062f     \u062f\u0645     \u0631\u0633\u062a\u062e\u06cc\u0632", "m2": "\u0628\u0647     \u0627\u06cc\u0631\u0627\u0646     \u0646\u062e\u0648\u0627\u0647\u06cc\u062f     \u0628\u0631\u062f\u0646     \u06af\u0631\u06cc\u0632"}, "25": {"m1": "\u0633\u0631    \u0627\u0632    \u06a9\u06cc\u0646   \u0627\u0628\u0631   \u06a9\u0648\u0647\u0647   \u0632\u06cc\u0646   \u0646\u0647\u06cc\u062f", "m2": "\u0628\u0647   \u062a\u06cc\u063a   \u0648  \u0628\u0647   \u06af\u0631\u0632   \u0648 \u0648 \u062a\u0628\u0631\u0632\u06cc\u0646  \u062f\u0647\u06cc\u062f"}, "26": {"m1": "\u0645\u0631\u0627    \u06af\u0648\u0646\u0647   \u067e\u06cc\u0631\u06cc    \u0628\u0628\u0633\u062a\u06cc    \u0628\u0647    \u062c\u0627\u06cc", "m2": "\u0628\u0647      \u062a\u0646\u0647\u0627\u06cc\u06cc      \u0622\u0648\u0631\u062f\u0645\u06cc\u0634\u0627\u0646     \u0632    \u067e\u0627\u06cc"}, "27": {"m1": "\u062f\u0648     \u0644\u0634\u06a9\u0631    \u0646\u0647\u0627\u062f\u0646\u062f   \u062f\u0644 \u0647\u0627   \u0628\u0647   \u0645\u0631\u06af", "m2": "\u0628\u06cc\u0627\u0631\u06cc\u062f    \u062a\u06cc\u0631   \u0627\u0632    \u062f\u0648 \u0633\u0648   \u0686\u0648\u0646  \u062a\u06af\u0631\u06af"}, "28": {"m1": "\u0686\u0648    \u0628\u064f\u062f   \u062c\u0646\u06af   \u0686\u0646\u062f\u06cc   \u0628\u0647  \u062a\u06cc\u0631  \u062e\u062f\u0646\u06af", "m2": "\u067e\u0633    \u0627\u0632    \u062a\u06cc\u0631   \u0628\u0627   \u0646\u06cc\u0632\u0647   \u06a9\u0631\u062f\u0646\u062f   \u062c\u0646\u06af"}, "29": {"m1": "\u067e\u0633   \u0627\u0632   \u0646\u06cc\u0632\u0647   \u0632\u06cc    \u062a\u06cc\u063a    \u06a9\u06cc\u0646   \u0622\u062e\u062a\u0646\u062f", "m2": "\u067e\u0633    \u0627\u0632    \u062a\u06cc\u063a   \u06a9\u0634\u062a\u06cc     \u0641\u0631\u0648    \u0633\u0627\u062e\u062a\u0646\u062f"}, "30": {"m1": "\u0632\u062f\u0647     \u062f\u0633\u062a    \u0627\u0632    \u06a9\u06cc\u0646\u0647    \u0628\u0631     \u06cc\u06a9\u062f\u06af\u0631", "m2": "\u06cc\u06a9\u06cc     \u062f\u0631    \u06af\u0631\u06cc\u0628\u0627\u0646   \u06cc\u06a9\u06cc    \u062f\u0631    \u06a9\u0645\u0631"}, "31": {"m1": "\u0628\u0647    \u062f\u0634\u0646\u0647   \u06cc\u06a9\u06cc    \u06af\u0634\u062a\u0647    \u0633\u06cc\u0646\u0647   \u0634\u06a9\u0627\u0641", "m2": "\u0628\u0647    \u062e\u0634\u062a    \u0622\u0646   \u062f\u06af\u0631  \u0628\u0627\u0632   \u062f\u0631\u0651\u06cc\u062f\u0647   \u0646\u0627\u0641"}, "32": {"m1": "\u0633\u0631\u0627\u0646\u062c\u0627\u0645    \u0634\u062f    \u0631\u0648\u0632    \u062a\u0631\u06a9\u0627\u0646     \u062f\u0631\u0634\u062a", "m2": "\u0628\u0647      \u0646\u0627\u06a9\u0627\u0645       \u06cc\u06a9\u0633\u0631     \u0628\u062f\u0627\u062f\u0646\u062f     \u067e\u0634\u062a"}, "33": {"m1": "\u06cc\u06a9\u06cc    \u062a\u0631\u06a9\u0634     \u0627\u0646\u062f\u0627\u062e\u062a     \u062f\u06cc\u06af\u0631    \u06a9\u0644\u0627\u0647", "m2": "\u06af\u0631\u06cc\u0632\u0627\u0646      \u0628\u0631\u0641\u062a\u0646\u062f      \u0628\u06cc \u0631\u0627\u0647     \u0648    \u0631\u0627\u0647"}, "34": {"m1": "\u067e\u0633         \u0627\u0646\u062f\u0631        \u0646\u0634\u0633\u062a\u0646\u062f       \u0627\u06cc\u0631\u0627\u0646\u06cc\u0627\u0646", "m2": "\u06af\u0634\u0627\u062f\u0647    \u0628\u0647    \u06a9\u06cc\u0646   \u062f\u0633\u062a   \u0648  \u0628\u0633\u062a\u0647  \u0645\u06cc\u0627\u0646"}, "35": {"m1": "\u0647\u0645\u0647     \u0631\u0647     \u0628\u062f     \u0627\u0641\u06a9\u0646\u062f\u0647     \u067e\u0646\u062c\u0627\u0647   \u0645\u06cc\u0644", "m2": "\u06af\u0631\u0641\u062a\u0646\u062f      \u062a\u06cc\u0631\u0633\u062a      \u0648      \u067e\u0646\u062c\u0627\u0647     \u067e\u06cc\u0644"}, "36": {"m1": "\u0632    \u062e\u0631\u06af\u0627\u0647   \u0648   \u0627\u0632   \u062e\u06cc\u0645\u0647    \u0631\u0646\u06af \u0631\u0646\u06af", "m2": "\u0632    \u0634\u0645\u0634\u06cc\u0631   \u0648   \u0627\u0632   \u062a\u0631\u06a9\u0634   \u067e\u064f\u0631  \u062e\u062f\u0646\u06af"}, "37": {"m1": "\u0632    \u062f\u06cc\u0628\u0627    \u0648     \u0627\u0632      \u0622\u0644\u062a     \u06af\u0648\u0646\u0647 \u06af\u0648\u0646", "m2": "\u0647\u0645\u0647    \u06af\u0631\u062f    \u06a9\u0631\u062f\u0646\u062f   \u06cc\u06a9    \u0645\u0647     \u0641\u0632\u0648\u0646"}, "38": {"m1": "\u0686\u0646\u0627\u0646   \u062a\u0648\u062f\u0647 \u0627\u06cc   \u06af\u0634\u062a   \u0628\u0631  \u0686\u0631\u062e  \u0648  \u0645\u0627\u0647", "m2": "\u06a9\u0647    \u062f\u06cc\u062f\u06cc    \u0627\u0632\u0648    \u062f\u06cc\u062f\u0647     \u06cc\u06a9\u0645\u0627\u0647\u0647   \u0631\u0627\u0647"}, "39": {"m1": "\u0632    \u067e\u06cc\u0631\u0627\u0645\u0646\u0634   \u0632\u0631\u062f   \u0648   \u0633\u0631\u062e  \u0648   \u0628\u0646\u0641\u0634", "m2": "\u0632\u062f\u0647      \u06af\u0648\u0646\u0647  \u06af\u0648\u0646     \u067e\u0631\u0646\u06cc\u0627\u0646\u06cc      \u062f\u0631\u0641\u0634"}, "40": {"m1": "\u062a\u0648   \u06af\u0641\u062a\u06cc    \u06a9\u0647   \u06a9\u0648\u0647\u06cc\u0633\u062a    \u067e\u064f\u0631  \u0644\u0627\u0644\u0647 \u0632\u0627\u0631", "m2": "\u0634\u06a9\u0641\u062a\u0647     \u062f\u0631\u062e\u062a    \u0627\u0646\u062f\u0631\u0648      \u0635\u062f   \u0647\u0632\u0627\u0631"}, "41": {"m1": "\u0633\u067e\u0647\u062f\u0627\u0631    \u0627\u0632   \u0627\u0648    \u0628\u0647\u0631     \u0634\u0647    \u0628\u0631\u06af\u0632\u06cc\u062f", "m2": "\u062f\u06af\u0631    \u0628\u0631    \u06af\u0631\u0641\u062a   \u0622\u0646\u0686\u0647   \u0627\u0648   \u0631\u0627  \u0633\u0632\u06cc\u062f"}, "42": {"m1": "\u0628\u0628\u062e\u0634\u06cc\u062f      \u0628\u0647\u0631     \u062f \u06af\u0631       \u0628\u0631      \u0633\u067e\u0627\u0647", "m2": "\u0633\u0648\u06cc     \u062c\u0646\u06af    \u0641\u063a\u0641\u0648\u0631    \u0628\u0631\u062f\u0627\u0634\u062a    \u0631\u0627\u0647"}}}