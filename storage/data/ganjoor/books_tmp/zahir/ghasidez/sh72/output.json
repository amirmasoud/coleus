{"text": {"0": {"m1": "\u0632\u0627\u0646 \u0632\u0644\u0641 \u0639\u0646\u0628\u0631\u06cc\u0646 \u06a9\u0647 \u0628\u0647 \u06af\u0644 \u0628\u0631 \u0646\u0647\u0627\u062f\u0647 \u0627\u06cc", "m2": "\u0635\u062f \u06af\u0648\u0646\u0647 \u062f\u0627\u063a \u0628\u0631 \u062f\u0644 \u0639\u0646\u0628\u0631 \u0646\u0647\u0627\u062f\u0647 \u0627\u06cc"}, "1": {"m1": "\u0645\u062e\u0645\u0648\u0631 \u0639\u0634\u0642 \u0631\u0627 \u0646\u0628\u0648\u062f\u0647 \u0686\u0627\u0631\u0647 \u0627\u06cc \u06a9\u0647 \u062a\u0648", "m2": "\u0645\u0647\u0631 \u0639\u0642\u06cc\u0642 \u0628\u0631 \u0645\u06cc \u0648 \u0634\u06a9\u0631 \u0646\u0647\u0627\u062f\u0647 \u0627\u06cc"}, "2": {"m1": "\u0627\u0632 \u0627\u0634\u06a9 \u0644\u0639\u0644 \u0633\u0627\u063a\u0631 \u0686\u0634\u0645\u0645 \u0644\u0628\u0627\u0644\u0628 \u0627\u0633\u062a", "m2": "\u062a\u0627 \u0644\u0628 \u0686\u0631\u0627 \u0628\u0631 \u0622\u0646 \u0644\u0628 \u0633\u0627\u063a\u0631 \u0646\u0647\u0627\u062f\u0647 \u0627\u06cc"}, "3": {"m1": "\u062e\u0648\u062f \u0627\u0632 \u0628\u0631\u0627\u06cc \u0633\u0631 \u0632\u0631\u0647 \u0627\u0632 \u0628\u0647\u0631 \u062a\u0646 \u0628\u0648\u062f", "m2": "\u062a\u0648 \u0645\u0627\u0647 \u0631\u0648\u06cc \u0639\u0627\u062f\u062a \u062f\u06cc\u06af\u0631 \u0646\u0647\u0627\u062f\u0647 \u0627\u06cc"}, "4": {"m1": "\u062f\u0631 \u0628\u0631 \u06af\u0631\u0641\u062a\u0647 \u0627\u06cc \u062f\u0644 \u0686\u0648\u0646 \u062e\u0648\u062f \u0622\u0647\u0646\u06cc\u0646", "m2": "\u0648\u0627\u0646 \u0632\u0644\u0641 \u0686\u0648\u0646 \u0632\u0631\u0647  \u0631\u0627 \u0628\u0631 \u0633\u0631 \u0646\u0647\u0627\u062f\u0647 \u0627\u06cc"}, "5": {"m1": "\u0633\u0631 \u0628\u0631 \u0646\u0645\u06cc \u06a9\u0646\u06cc \u0632 \u062a\u06a9\u0628\u0631 \u0645\u06af\u0631 \u06a9\u0647 \u067e\u0627\u06cc", "m2": "\u0628\u0631 \u0622\u0633\u062a\u0627\u0646 \u0634\u0627\u0647 \u0645\u0638\u0641\u0631 \u0646\u0647\u0627\u062f\u0647 \u0627\u06cc"}, "6": {"m1": "\u0622\u0646 \u0634\u0627\u0647 \u0634\u0627\u0647\u0632\u0627\u062f\u0647 \u06a9\u0647 \u0627\u0642\u0628\u0627\u0644 \u06af\u0648\u06cc\u062f\u0634", "m2": "\u06a9\u0632 \u0641\u062e\u0631 \u067e\u0627\u06cc \u0628\u0631 \u0633\u0631 \u0627\u062e\u062a\u0631 \u0646\u0647\u0627\u062f\u0647 \u0627\u06cc"}, "7": {"m1": "\u0628\u0648\u0628\u06a9\u0631\u0628\u0646 \u0645\u062d\u0645\u062f \u06a9\u0627\u0646\u062f\u0631 \u062f\u06cc\u0627\u0631 \u06a9\u0641\u0631", "m2": "\u0622\u062a\u0634 \u0647\u0632\u0627\u0631 \u0628\u0627\u0631 \u0686\u0648 \u062d\u06cc\u062f\u0631 \u0646\u0647\u0627\u062f\u0647 \u0627\u06cc"}, "8": {"m1": "\u062f\u0648\u0644\u062a \u0628\u0647 \u062a\u0648\u0633\u062a \u0632\u0646\u062f\u0647 \u0648 \u0645\u0644\u062a \u0628\u0647 \u062a\u0648\u0633\u062a \u0634\u0627\u062f", "m2": "\u06a9\u0627\u06cc\u06cc\u0646 \u0647\u0631 \u062f\u0648 \u0644\u0627\u06cc\u0642 \u0648 \u062f\u0631 \u062e\u0648\u0631 \u0646\u0647\u0627\u062f\u0647 \u0627\u06cc"}, "9": {"m1": "\u0628\u0627 \u0622\u0646\u06a9 \u062f\u0631 \u0628\u062f\u0627\u06cc\u062a \u0639\u0645\u0631\u06cc \u0647\u0632\u0627\u0631 \u0628\u0627\u0631", "m2": "\u067e\u0627 \u0628\u0631 \u0633\u0631 \u0633\u067e\u0647\u0631 \u0645\u064f\u0639\u064e\u0645\u0651\u064e\u0631 \u0646\u0647\u0627\u062f\u0647 \u0627\u06cc"}, "10": {"m1": "\u06a9\u0633 \u0631\u0627 \u0641\u0631\u0627\u0632 \u062e\u0648\u06cc\u0634 \u0646\u0628\u06cc\u0646\u06cc \u0686\u0648 \u0627\u0632 \u0639\u064f\u0644\u064f\u0648\u0651", "m2": "\u0645\u0646\u0628\u0631 \u0641\u0631\u0627\u0632 \u06af\u0646\u0628\u062f \u0627\u062e\u0636\u0631 \u0646\u0647\u0627\u062f\u0647 \u0627\u06cc"}, "11": {"m1": "\u0632\u0627\u0646 \u062f\u0645 \u06a9\u0647 \u0627\u0632 \u0644\u0628 \u062a\u0648 \u0628\u0634\u0633\u062a\u0647 \u0633\u062a \u062f\u0627\u06cc\u0647 \u0634\u06cc\u0631", "m2": "\u0644\u0628 \u0631\u0627 \u0628\u0647 \u0645\u0647\u0631 \u0628\u0631 \u0644\u0628 \u062e\u0646\u062c\u0631 \u0646\u0647\u0627\u062f\u0647 \u0627\u06cc"}, "12": {"m1": "\u0647\u0631 \u06a9\u0633 \u06a9\u0647 \u0628\u0627 \u0645\u0646\u0627\u0642\u0628 \u062d\u06cc\u062f\u0631 \u0628\u0628\u06cc\u0646\u062f\u062a", "m2": "\u062f\u0627\u0646\u062f \u06a9\u0647 \u062c\u0633\u0631 \u0628\u0631 \u062f\u0631 \u062e\u06cc\u0628\u0631 \u0646\u0647\u0627\u062f\u0647 \u0627\u06cc"}, "13": {"m1": "\u062a\u0627 \u06a9\u0631\u062f\u0647 \u0627\u06cc \u0632\u0628\u0627\u0646\u0647 \u0633\u0646\u062c\u0642 \u0633\u0648\u06cc \u0647\u0648\u0627", "m2": "\u062a\u06a9\u0628\u06cc\u0631 \u062f\u0631 \u0632\u0628\u0627\u0646 \u062f\u0648 \u067e\u06cc\u06a9\u0631 \u0646\u0647\u0627\u062f\u0647 \u0627\u06cc"}, "14": {"m1": "\u062f\u06cc\u0631\u0633\u062a \u062a\u0627\u0647\u0645 \u0627\u0632 \u062a\u06a9 \u0627\u0633\u0628 \u0648 \u0632 \u06af\u0631\u062f \u0631\u0627\u0647", "m2": "\u0631\u062e\u062a \u0645\u0633\u06cc\u062d\u06cc\u0627\u0646 \u0647\u0645\u0647 \u0628\u0631 \u062e\u0631 \u0646\u0647\u0627\u062f\u0647 \u0627\u06cc"}, "15": {"m1": "\u062f\u06cc\u0631\u0633\u062a \u062a\u0627 \u0628\u0647 \u062c\u0627\u06cc \u0635\u0644\u06cc\u0628 \u0648 \u06a9\u0644\u06cc\u0633\u06cc\u0627", "m2": "\u0645\u062d\u0631\u0627\u0628 \u0631\u0627\u0633\u062a \u06a9\u0631\u062f\u0647 \u0648 \u0645\u0646\u0628\u0631 \u0646\u0647\u0627\u062f\u0647 \u0627\u06cc"}, "16": {"m1": "\u0632\u064f\u0646\u0651\u0627\u0631 \u0628\u0633\u062a \u062e\u0635\u0645 \u062a\u0648 \u0686\u0648\u0646 \u062f\u06cc\u062f \u06a9\u0632 \u0638\u0641\u0631", "m2": "\u062a\u0648 \u062f\u0627\u063a \u0628\u0631 \u062c\u0628\u06cc\u0646 \u0645\u0647 \u0648 \u062e\u0648\u0631 \u0646\u0647\u0627\u062f\u0647 \u0627\u06cc"}, "17": {"m1": "\u0627\u0642\u0628\u0627\u0644 \u0628\u0627 \u062a\u0648 \u0632\u0627\u062f \u0628\u0631\u0627\u0628\u0631 \u0628\u0647 \u06cc\u06a9 \u0634\u06a9\u0645", "m2": "\u062e\u0648\u062f \u0631\u0627 \u0628\u0647 \u062f\u06cc\u06af\u0631\u0627\u0646 \u0686\u0647 \u0628\u0631\u0627\u0628\u0631 \u0646\u0647\u0627\u062f\u0647 \u0627\u06cc\u061f"}, "18": {"m1": "\u062f\u0627\u0646\u0646\u062f \u0647\u0645\u06a9\u0646\u0627\u0646 \u06a9\u0647 \u062a\u0648 \u062a\u0646\u0647\u0627 \u0628\u0647 \u0630\u0627\u062a \u062e\u0648\u06cc\u0634", "m2": "\u0635\u062f \u0644\u0634\u06a9\u0631\u06cc\u060c\u0686\u0648 \u0631\u0648\u06cc \u0628\u0647 \u06a9\u0627\u0641\u0631 \u0646\u0647\u0627\u062f\u0647 \u0627\u06cc"}, "19": {"m1": "\u0641\u0631 \u062e\u062f\u0627\u06cc \u0628\u0627 \u062a\u0648 \u0648 \u0627\u0639\u062c\u0627\u0632 \u0645\u0635\u0637\u0641\u06cc", "m2": "\u0628\u0631 \u062e\u0648\u062f \u0686\u0631\u0627 \u0645\u0624\u0648\u0646\u062a \u0644\u0634\u06a9\u0631 \u0646\u0647\u0627\u062f\u0647 \u0627\u06cc\u061f"}, "20": {"m1": "\u067e\u0634\u062a \u0648 \u062f\u0644\u062a \u0647\u0645\u06cc\u0634\u0647 \u0642\u0648\u06cc \u0628\u0627\u062f \u0628\u0647\u0631 \u0622\u0646\u06a9", "m2": "\u0628\u0646\u06cc\u0627\u062f \u0645\u0644\u06a9 \u0647\u0631 \u0686\u0647 \u0642\u0648\u06cc \u062a\u0631 \u0646\u0647\u0627\u062f\u0647 \u0627\u06cc"}}}