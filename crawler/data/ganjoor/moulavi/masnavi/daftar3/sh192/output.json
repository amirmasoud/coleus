{"text": {"0": {"m1": "\u06af\u0641\u062a \u0627\u0648 \u0627\u06cc \u0646\u0627\u0635\u062d\u0627\u0646 \u0645\u0646 \u0628\u06cc \u0646\u062f\u0645", "m2": "\u0627\u0632 \u062c\u0647\u0627\u0646 \u0632\u0646\u062f\u06af\u06cc \u0633\u06cc\u0631 \u0622\u0645\u062f\u0645"}, "1": {"m1": "\u0645\u0646\u0628\u0644\u06cc\u200c\u0627\u0645 \u0632\u062e\u0645 \u062c\u0648 \u0648 \u0632\u062e\u0645\u200c\u062e\u0648\u0627\u0647", "m2": "\u0639\u0627\u0641\u06cc\u062a \u06a9\u0645 \u062c\u0648\u06cc \u0627\u0632 \u0645\u0646\u0628\u0644 \u0628\u0631\u0627\u0647"}, "2": {"m1": "\u0645\u0646\u0628\u0644\u06cc \u0646\u06cc \u06a9\u0648 \u0628\u0648\u062f \u062e\u0648\u062f \u0628\u0631\u06af\u200c\u062c\u0648", "m2": "\u0645\u0646\u0628\u0644\u06cc\u200c\u0627\u0645 \u0644\u0627\u0627\u0628\u0627\u0644\u06cc \u0645\u0631\u06af\u200c\u062c\u0648"}, "3": {"m1": "\u0645\u0646\u0628\u0644\u06cc \u0646\u06cc \u06a9\u0648 \u0628\u0647 \u06a9\u0641 \u067e\u0648\u0644 \u0622\u0648\u0631\u062f", "m2": "\u0645\u0646\u0628\u0644\u06cc \u0686\u0633\u062a\u06cc \u06a9\u0632\u06cc\u0646 \u067e\u0644 \u0628\u06af\u0630\u0631\u062f"}, "4": {"m1": "\u0622\u0646 \u0646\u0647 \u06a9\u0648 \u0628\u0631 \u0647\u0631 \u062f\u06a9\u0627\u0646\u06cc \u0628\u0631 \u0632\u0646\u062f", "m2": "\u0628\u0644 \u062c\u0647\u062f \u0627\u0632 \u06a9\u0648\u0646 \u0648 \u06a9\u0627\u0646\u06cc \u0628\u0631 \u0632\u0646\u062f"}, "5": {"m1": "\u0645\u0631\u06af \u0634\u06cc\u0631\u06cc\u0646 \u06af\u0634\u062a \u0648 \u0646\u0642\u0644\u0645 \u0632\u06cc\u0646 \u0633\u0631\u0627", "m2": "\u0686\u0648\u0646 \u0642\u0641\u0633 \u0647\u0634\u062a\u0646 \u067e\u0631\u06cc\u062f\u0646 \u0645\u0631\u063a \u0631\u0627"}, "6": {"m1": "\u0622\u0646 \u0642\u0641\u0633 \u06a9\u0647 \u0647\u0633\u062a \u0639\u06cc\u0646 \u0628\u0627\u063a \u062f\u0631", "m2": "\u0645\u0631\u063a \u0645\u06cc\u200c\u0628\u06cc\u0646\u062f \u06af\u0644\u0633\u062a\u0627\u0646 \u0648 \u0634\u062c\u0631"}, "7": {"m1": "\u062c\u0648\u0642 \u0645\u0631\u063a\u0627\u0646 \u0627\u0632 \u0628\u0631\u0648\u0646 \u06af\u0631\u062f \u0642\u0641\u0633", "m2": "\u062e\u0648\u0634 \u0647\u0645\u06cc\u200c\u062e\u0648\u0627\u0646\u0646\u062f \u0632 \u0622\u0632\u0627\u062f\u06cc \u0642\u0635\u0635"}, "8": {"m1": "\u0645\u0631\u063a \u0631\u0627 \u0627\u0646\u062f\u0631 \u0642\u0641\u0633 \u0632\u0627\u0646 \u0633\u0628\u0632\u0647\u200c\u0632\u0627\u0631", "m2": "\u0646\u0647 \u062e\u0648\u0631\u0634 \u0645\u0627\u0646\u062f\u0633\u062a \u0648 \u0646\u0647 \u0635\u0628\u0631 \u0648 \u0642\u0631\u0627\u0631"}, "9": {"m1": "\u0633\u0631 \u0632 \u0647\u0631 \u0633\u0648\u0631\u0627\u062e \u0628\u06cc\u0631\u0648\u0646 \u0645\u06cc\u200c\u06a9\u0646\u062f", "m2": "\u062a\u0627 \u0628\u0648\u062f \u06a9\u06cc\u0646 \u0628\u0646\u062f \u0627\u0632 \u067e\u0627 \u0628\u0631\u06a9\u0646\u062f"}, "10": {"m1": "\u0686\u0648\u0646 \u062f\u0644 \u0648 \u062c\u0627\u0646\u0634 \u0686\u0646\u06cc\u0646 \u0628\u06cc\u0631\u0648\u0646 \u0628\u0648\u062f", "m2": "\u0622\u0646 \u0642\u0641\u0633 \u0631\u0627 \u062f\u0631 \u06af\u0634\u0627\u06cc\u06cc \u0686\u0648\u0646 \u0628\u0648\u062f"}, "11": {"m1": "\u0646\u0647 \u0686\u0646\u0627\u0646 \u0645\u0631\u063a \u0642\u0641\u0633 \u062f\u0631 \u0627\u0646\u062f\u0647\u0627\u0646", "m2": "\u06af\u0631\u062f \u0628\u0631 \u06af\u0631\u062f\u0634 \u0628\u0647 \u062d\u0644\u0642\u0647 \u06af\u0631\u0628\u06af\u0627\u0646"}, "12": {"m1": "\u06a9\u06cc \u0628\u0648\u062f \u0627\u0648 \u0631\u0627 \u062f\u0631\u06cc\u0646 \u062e\u0648\u0641 \u0648 \u062d\u0632\u0646", "m2": "\u0622\u0631\u0632\u0648\u06cc \u0627\u0632 \u0642\u0641\u0633 \u0628\u06cc\u0631\u0648\u0646 \u0634\u062f\u0646"}, "13": {"m1": "\u0627\u0648 \u0647\u0645\u06cc\u200c\u062e\u0648\u0627\u0647\u062f \u06a9\u0632\u06cc\u0646 \u0646\u0627\u062e\u0648\u0634 \u062d\u0635\u0635", "m2": "\u0635\u062f \u0642\u0641\u0633 \u0628\u0627\u0634\u062f \u0628\u06af\u0631\u062f \u0627\u06cc\u0646 \u0642\u0641\u0633"}}}