{"text": {"0": {"m1": "\u0627\u06cc \u0622\u0646\u06a9 \u0627\u0632 \u0645\u06cc\u0627\u0646\u0647 \u06a9\u0631\u0627\u0646 \u0645\u06cc\u200c\u06a9\u0646\u06cc \u0645\u06a9\u0646", "m2": "\u0628\u0627 \u0645\u0627 \u0632 \u062e\u0634\u0645 \u0631\u0648\u06cc \u06af\u0631\u0627\u0646 \u0645\u06cc\u200c\u06a9\u0646\u06cc \u0645\u06a9\u0646"}, "1": {"m1": "\u062f\u0631\u0628\u0646\u062f \u0633\u0648\u062f \u062e\u0648\u06cc\u0634\u06cc \u0648 \u0627\u0646\u062f\u0631 \u0632\u06cc\u0627\u0646 \u0645\u0627", "m2": "\u06a9\u0633 \u0632\u06cc\u0646 \u0646\u06a9\u0631\u062f \u0633\u0648\u062f \u0632\u06cc\u0627\u0646 \u0645\u06cc\u200c\u06a9\u0646\u06cc \u0645\u06a9\u0646"}, "2": {"m1": "\u0631\u0627\u0636\u06cc \u0634\u062f\u06cc \u06a9\u0647 \u0628\u06cc\u0634 \u0646\u062c\u0648\u06cc\u06cc \u0632\u06cc\u0627\u0646 \u0645\u0627", "m2": "\u0627\u06cc\u0646 \u0627\u0632 \u067e\u06cc \u0631\u0636\u0627\u06cc \u06a9\u06cc\u0627\u0646 \u0645\u06cc\u200c\u06a9\u0646\u06cc \u0645\u06a9\u0646"}, "3": {"m1": "\u0628\u0631 \u062c\u0627\u06cc \u0628\u0627\u062f\u0647 \u0633\u0631\u06a9\u0647 \u063a\u0645 \u0645\u06cc\u200c\u062f\u0647\u06cc \u0645\u062f\u0647", "m2": "\u062f\u0631 \u062c\u0648\u06cc \u0622\u0628 \u062e\u0648\u0646 \u0686\u0647 \u0631\u0648\u0627\u0646 \u0645\u06cc\u200c\u06a9\u0646\u06cc \u0645\u06a9\u0646"}, "4": {"m1": "\u0627\u0632 \u0686\u0647\u0631\u0647\u200c\u0627\u0645 \u0646\u0634\u0627\u0637 \u0637\u0631\u0628 \u0645\u06cc\u200c\u0628\u0631\u06cc \u0645\u0628\u0631", "m2": "\u0628\u0631 \u0686\u0647\u0631\u0647\u200c\u0627\u0645 \u0632 \u062f\u06cc\u062f\u0647 \u0646\u0634\u0627\u0646 \u0645\u06cc\u200c\u06a9\u0646\u06cc \u0645\u06a9\u0646"}, "5": {"m1": "\u0645\u0638\u0644\u0648\u0645 \u0645\u06cc\u200c\u06a9\u0634\u06cc \u0648 \u062a\u0638\u0644\u0645 \u0647\u0645\u06cc\u200c\u06a9\u0646\u06cc", "m2": "\u062e\u0648\u062f \u0631\u0627\u0647 \u0645\u06cc\u200c\u0632\u0646\u06cc \u0648 \u0641\u063a\u0627\u0646 \u0645\u06cc\u200c\u06a9\u0646\u06cc \u0645\u06a9\u0646"}, "6": {"m1": "\u067e\u0627\u06cc\u0645 \u0628\u0647 \u06a9\u0627\u0631 \u0646\u06cc\u0633\u062a \u06a9\u0647 \u0633\u0631\u0645\u0633\u062a \u062f\u0644\u0628\u0631\u0645", "m2": "\u0645\u0631 \u0645\u0633\u062a \u0631\u0627 \u0628\u0647\u0644 \u0686\u0647 \u06a9\u0634\u0627\u0646 \u0645\u06cc\u200c\u06a9\u0646\u06cc \u0645\u06a9\u0646"}, "7": {"m1": "\u06af\u0648\u06cc\u06cc \u0628\u06cc\u0627 \u06a9\u0647 \u0628\u0631 \u062a\u0648 \u06a9\u0646\u0645 \u0635\u0628\u0631 \u0631\u0627 \u0634\u0628\u0627\u0646", "m2": "\u0628\u0631 \u0628\u0631\u0647 \u06af\u0631\u06af \u0631\u0627 \u0686\u0647 \u0634\u0628\u0627\u0646 \u0645\u06cc\u200c\u06a9\u0646\u06cc \u0645\u06a9\u0646"}, "8": {"m1": "\u062f\u0631 \u0631\u0648\u0632 \u0632\u0627\u0647\u062f\u06cc \u0648 \u0628\u0647 \u0634\u0628 \u0632\u0627\u0647\u062f\u0627\u0646 \u06a9\u0634\u06cc", "m2": "\u0627\u0645\u0634\u0628 \u06a9\u0647 \u0622\u0634\u062a\u06cc \u0627\u0633\u062a \u0647\u0645\u0627\u0646 \u0645\u06cc\u200c\u06a9\u0646\u06cc \u0645\u06a9\u0646"}, "9": {"m1": "\u0627\u06cc \u062f\u0648\u0633\u062a\u0627\u0646 \u0632 \u0631\u0634\u06a9 \u062a\u0648 \u062e\u0635\u0645\u0627\u0646 \u0647\u0645\u062f\u06af\u0631", "m2": "\u0627\u06cc\u0646 \u062f\u0648\u0633\u062a \u0631\u0627 \u0686\u0647 \u062f\u0634\u0645\u0646 \u0622\u0646 \u0645\u06cc\u200c\u06a9\u0646\u06cc \u0645\u06a9\u0646"}, "10": {"m1": "\u06af\u0648\u06cc\u06cc \u06a9\u0647 \u0645\u06cc \u0645\u062e\u0648\u0631 \u067e\u0633 \u0627\u06af\u0631 \u0645\u06cc \u0647\u0645\u06cc\u200c\u062f\u0647\u06cc", "m2": "\u0645\u062e\u0645\u0648\u0631 \u0631\u0627 \u0686\u0647 \u062e\u0634\u06a9 \u062f\u0647\u0627\u0646 \u0645\u06cc\u200c\u06a9\u0646\u06cc \u0645\u06a9\u0646"}, "11": {"m1": "\u06af\u0648\u06cc\u06cc \u0686\u0648 \u062a\u06cc\u0631 \u0631\u0627\u0633\u062a \u0631\u0648 \u0627\u0646\u062f\u0631 \u0647\u0648\u0627\u06cc \u0645\u0627", "m2": "\u067e\u0633 \u062a\u06cc\u0631 \u0631\u0627\u0633\u062a \u0631\u0627 \u0686\u0647 \u06a9\u0645\u0627\u0646 \u0645\u06cc\u200c\u06a9\u0646\u06cc \u0645\u06a9\u0646"}, "12": {"m1": "\u06af\u0648\u06cc\u06cc \u062e\u0645\u0648\u0634 \u06a9\u0646 \u062a\u0648 \u062e\u0645\u0648\u0634\u0645 \u0646\u0645\u06cc\u200c\u0647\u0644\u06cc", "m2": "\u0647\u0631 \u0645\u0648\u06cc \u0631\u0627 \u0632 \u0639\u0634\u0642 \u0632\u0628\u0627\u0646 \u0645\u06cc\u200c\u06a9\u0646\u06cc \u0645\u06a9\u0646"}}}