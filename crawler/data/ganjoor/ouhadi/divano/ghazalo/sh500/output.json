{"text": {"0": {"m1": "\u0627\u0632 \u0622\u0646 \u0644\u0628 \u0686\u0648\u0646 \u0628\u0647 \u06cc\u06a9 \u0628\u0648\u0633\u0647 \u0645\u0646 \u0628\u06cc\u0645\u0627\u0631 \u062e\u0631\u0633\u0646\u062f\u0645", "m2": "\u0646\u062e\u0648\u0627\u0647\u0645 \u0634\u06cc\u0634\u0647\u0654 \u0646\u0648\u0634 \u0648 \u0646\u0628\u0627\u06cc\u062f \u0634\u0631\u0628\u062a \u0642\u0646\u062f\u0645"}, "1": {"m1": "\u0645\u06af\u0631 \u06cc\u0632\u062f\u0627\u0646 \u0628\u0647 \u0631\u0648\u06cc \u0645\u0646 \u062f\u0631 \u0648\u0635\u0644 \u062a\u0648 \u0628\u06af\u0634\u0627\u06cc\u062f", "m2": "\u0648 \u06af\u0631\u0646\u0647 \u0645\u0646 \u062f\u0631 \u06af\u06cc\u062a\u06cc \u0628\u0647 \u0631\u0648\u06cc \u062e\u0648\u062f \u0641\u0631\u0648 \u0628\u0646\u062f\u0645"}, "2": {"m1": "\u0646\u0634\u0627\u0646 \u0645\u0647\u0631 \u0648\u0631\u0632\u06cc\u062f\u0646 \u0647\u0645\u0627\u0646 \u0628\u0627\u0634\u062f \u06a9\u0647: \u0647\u0631 \u0633\u0627\u0639\u062a", "m2": "\u0645\u0631\u0627 \u0686\u0648\u0646 \u0634\u0645\u0639 \u0645\u06cc\u200c\u0633\u0648\u0632\u06cc \u0648 \u0645\u0646 \u0686\u0648\u0646 \u06af\u0644 \u0647\u0645\u06cc \u062e\u0646\u062f\u0645"}, "3": {"m1": "\u062d\u062f\u06cc\u062b \u0645\u062d\u0646\u062a \u0641\u0631\u0647\u0627\u062f \u0648 \u06a9\u0648\u0647 \u0628\u06cc\u0633\u062a\u0648\u0646 \u06a9\u0646\u062f\u0646", "m2": "\u0628\u0647 \u06a9\u0627\u0631 \u0645\u0646 \u0686\u0647 \u0645\u06cc\u200c\u0645\u0627\u0646\u062f\u061f \u06a9\u0647 \u062f\u0631 \u0639\u0634\u0642 \u062a\u0648 \u062c\u0627\u0646 \u06a9\u0646\u062f\u0645"}, "4": {"m1": "\u0628\u0647 \u062f\u0633\u062a \u062f\u06cc\u06af\u0631\u0627\u0646 \u0645\u0627\u0644\u0633\u062a \u0648 \u0627\u0633\u0628\u0627\u0628\u0633\u062a \u0648 \u0633\u06cc\u0645 \u0648 \u0632\u0631", "m2": "\u0645\u0646 \u0645\u0633\u06a9\u06cc\u0646 \u0633\u0631\u06cc \u062f\u0627\u0631\u0645 \u06a9\u0647 \u062f\u0631 \u067e\u0627\u06cc \u062a\u0648 \u0627\u0641\u06af\u0646\u062f\u0645"}, "5": {"m1": "\u067e\u0633\u0646\u062f \u0645\u0646 \u0646\u062e\u0648\u0627\u0647\u062f \u0628\u0648\u062f \u062f\u0631 \u0639\u0642\u0628\u06cc \u0628\u063a\u06cc\u0631 \u0627\u0632 \u062a\u0648", "m2": "\u0627\u0632\u06cc\u0646 \u062f\u0646\u06cc\u0627 \u0648 \u0645\u0627\u0641\u06cc\u0647\u0627 \u0628\u0647 \u062c\u0632 \u0631\u0648\u06cc \u062a\u0648 \u0646\u067e\u0633\u0646\u062f\u0645"}, "6": {"m1": "\u0633\u06af\u0645 \u06af\u0641\u062a\u06cc \u0648 \u062f\u0644\u0634\u0627\u062f\u0645 \u0628\u062f\u06cc\u0646 \u062a\u0634\u0631\u06cc\u0641\u0647\u0627\u060c \u0644\u06cc\u06a9\u0646", "m2": "\u0628\u0647 \u0634\u0631\u0637 \u0622\u0646\u06a9\u0647 \u0627\u0632 \u06a9\u0648\u06cc\u062a \u0628\u06af\u0648\u06cc\u06cc \u062a\u0627: \u0646\u0631\u0627\u0646\u0646\u062f\u0645"}, "7": {"m1": "\u0632 \u0631\u0648\u06cc \u0647\u0645\u0686\u0648 \u0645\u0627\u0647 \u062e\u0648\u062f \u0645\u062f\u0647 \u06a9\u0627\u0645 \u062f\u0644\u0645 \u0647\u0631\u06af\u0632", "m2": "\u0627\u06af\u0631 \u0628\u0627 \u062f\u06cc\u06af\u0631\u06cc \u0628\u06cc\u0646\u06cc \u0632 \u0631\u0648\u06cc \u0645\u0647\u0631 \u067e\u06cc\u0648\u0646\u062f\u0645"}, "8": {"m1": "\u0646\u0647 \u0686\u0634\u0645 \u0648 \u0633\u0631 \u0628\u067e\u06cc\u0686\u06cc\u062f\u06cc\u060c \u0632 \u0645\u0646 \u062d\u0627\u0644\u0645 \u0628\u067e\u0631\u0633\u06cc\u062f\u06cc", "m2": "\u0627\u06af\u0631 \u06af\u0648\u0634 \u062a\u0648 \u0628\u0634\u0646\u06cc\u062f\u06cc \u06a9\u0647: \u0686\u0648\u0646\u062a \u0622\u0631\u0632\u0648\u0645\u0646\u062f\u0645\u061f"}, "9": {"m1": "\u0646\u0628\u06cc\u0646\u06cc \u0628\u0639\u062f \u0627\u0632\u06cc\u0646 \u0631\u0648\u0632\u06cc\u060c \u0645\u0631\u0627 \u0628\u06cc\u200c\u0639\u0634\u0642 \u062f\u0644\u0633\u0648\u0632\u06cc", "m2": "\u06af\u0630\u0634\u062a \u0622\u0646 \u06a9\u0632 \u067e\u0631\u06cc \u0631\u0648\u06cc\u0627\u0646 \u0641\u0631\u0627\u063a\u062a \u0628\u0648\u062f \u06cc\u06a9 \u0686\u0646\u062f\u0645"}, "10": {"m1": "\u0628\u06cc\u0627\u0648\u0631 \u0646\u0627\u06cc \u0648 \u0686\u0646\u06af \u0648 \u062f\u0641\u060c \u0645\u06cc\u200c\u0635\u0627\u0641\u0645 \u0628\u0646\u0647 \u0628\u0631 \u06a9\u0641", "m2": "\u0646\u0634\u0627\u06cc\u062f \u0634\u062f \u0628\u0631\u0648\u0646 \u0632\u06cc\u0646 \u0635\u0641\u060c \u06a9\u0647 \u0635\u0648\u0641\u06cc \u0645\u06cc\u200c\u062f\u0647\u062f \u067e\u0646\u062f\u0645"}, "11": {"m1": "\u0628\u0647 \u0647\u0645\u0631\u0627\u0647 \u0633\u0641\u0631 \u06af\u0648\u06cc\u0646\u062f \u062a\u0627: \u0645\u0648\u0642\u0648\u0641 \u0646\u0646\u0634\u06cc\u0646\u062f", "m2": "\u06a9\u0647 \u0627\u06cc\u0634\u0627\u0646 \u0628\u0627\u0631 \u0645\u06cc\u200c\u0628\u0646\u062f\u062f \u0648 \u0645\u0646 \u062f\u0631 \u0628\u0627\u0631 \u0648 \u062f\u0631\u0628\u0646\u062f\u0645"}, "12": {"m1": "\u0645\u0631\u0627 \u06af\u0631 \u0627\u0648\u062d\u062f\u06cc \u0632\u06cc\u0646 \u067e\u0633 \u0645\u0644\u0627\u0645\u062a \u06a9\u0645 \u06a9\u0646\u062f \u0634\u0627\u06cc\u062f", "m2": "\u06a9\u0647 \u0645\u0646 \u062a\u0627 \u0639\u0627\u0634\u0642\u0645 \u06af\u0648\u0634 \u0627\u0632 \u0646\u0635\u06cc\u062d\u062a\u200c\u0647\u0627 \u0628\u06cc\u0627 \u06af\u0646\u062f\u0645"}}}