{"text": {"0": {"m1": "\u062f\u0648\u0631 \u0645\u0627\u0647\u0633\u062a \u0648 \u062e\u0644\u0642 \u0631\u0627 \u0627\u0632 \u0645\u0627\u0647", "m2": "\u0639\u0645\u0631 \u0645\u0627\u0647\u0633\u062a \u0686\u0648\u0646 \u0631\u0647\u0634 \u06a9\u0648\u062a\u0627\u0647"}, "1": {"m1": "\u0647\u0631\u06a9\u0631\u0627 \u0645\u0627\u0647 \u067e\u0631\u0648\u0631\u062f \u0628\u0647 \u06a9\u0646\u0627\u0631", "m2": "\u0634\u06cc\u0631 \u062e\u0648\u0627\u0631\u0647\u200c\u0627\u0634 \u062f\u0648\u062a\u0627 \u06a9\u0646\u062f \u0686\u0648 \u062e\u06cc\u0627\u0631"}, "2": {"m1": "\u0628\u0627 \u0631\u0648\u0646\u062f\u0647 \u0631\u0648\u0646\u062f\u06af\u0627\u0646 \u067e\u0627\u06cc\u0646\u062f", "m2": "\u0628\u0627\u0645 \u062e\u0631\u06af\u0647 \u0628\u0647 \u06af\u0650\u0644 \u0646\u06cc\u0646\u062f\u0627\u06cc\u0646\u062f"}, "3": {"m1": "\u062e\u0627\u0646\u0647\u0654 \u062c\u0627\u0646\u062a \u0631\u0627 \u0628\u0647 \u0633\u0627\u0644 \u0648 \u0628\u0647 \u0645\u0627\u0647", "m2": "\u067e\u0627\u0631\u0647 \u067e\u0627\u0631\u0647 \u06a9\u0646\u0646\u062f \u0686\u0648\u0646 \u062e\u0631\u06af\u0627\u0647"}, "4": {"m1": "\u0686\u0646\u0628\u0631 \u0686\u0631\u062e \u0648 \u0627\u062e\u062a\u0631 \u0634\u0631 \u0648 \u0634\u0648\u0631", "m2": "\u0627\u06cc\u0646 \u0686\u0648 \u062d\u0631\u0651\u0627\u0642\u0647 \u062f\u0627\u0646 \u0648 \u0622\u0646 \u0686\u0648 \u0628\u0644\u0648\u0631"}, "5": {"m1": "\u0645\u06cc\u200c\u06a9\u0634\u0646\u062f\u062a \u0628\u0647 \u062e\u0648\u062f \u0628\u0647 \u062f\u0627\u0645 \u0648 \u0628\u0647 \u062f\u0645", "m2": "\u067e\u0627\u0633\u0628\u0627\u0646\u0627\u0646 \u06af\u0646\u0628\u062f \u0627\u0639\u0638\u0645"}, "6": {"m1": "\u0644\u06cc\u06a9 \u0627\u06af\u0631 \u06cc\u0648\u0631\u062a\u06af\u0647 \u0632 \u0639\u0632\u0651 \u0633\u0627\u0632\u0646\u062f", "m2": "\u0647\u0645 \u0627\u0632\u0648 \u062e\u0631\u06af\u0647\u06cc\u062a \u067e\u0631\u062f\u0627\u0632\u0646\u062f"}, "7": {"m1": "\u0628\u0631 \u062a\u0648 \u0639\u0645\u0631 \u062a\u0648 \u0627\u0644\u0642\u06cc\u0627\u0645\u0629 \u062e\u0648\u0627\u0646\u062f", "m2": "\u0632\u0627\u0646\u06a9\u0647 \u0648\u0627\u0644\u0644\u06cc\u0644 \u0648\u0627\u0644\u0636\u062d\u0627\u0634 \u0646\u0645\u0627\u0646\u062f"}, "8": {"m1": "\u0686\u0648\u0646 \u0628\u0631\u0622\u06cc\u062f \u0632 \u0686\u0631\u062e \u0639\u0645\u0631 \u062a\u0648 \u0634\u06cc\u062f", "m2": "\u0634\u06cc\u062f \u0645\u0631\u06af \u0622\u0646\u06af\u0647 \u0639\u0648\u062f \u0627\u0648 \u0634\u062f \u0628\u06cc\u062f"}, "9": {"m1": "\u0686\u0648\u0646 \u0628\u0628\u06cc\u0646\u062f\u062a \u0622\u0646 \u0632\u0645\u0627\u0646 \u0628\u0627 \u0630\u0644", "m2": "\u0631\u0627\u0633\u062a \u0686\u0648\u0646 \u062f\u0631 \u0628\u0647\u0627\u0631 \u0646\u0631\u06af\u0633 \u0648 \u06af\u0644"}, "10": {"m1": "\u0644\u06cc\u06a9 \u06af\u0647 \u0639\u0632\u0651 \u0648 \u06af\u0627\u0647 \u0630\u064f\u0644 \u0633\u0627\u0632\u0646\u062f", "m2": "\u06a9\u0627\u0631 \u0648 \u0628\u0627\u0631\u062a \u0647\u0645\u0647 \u0628\u0631\u0627\u0646\u062f\u0627\u0632\u0646\u062f"}, "11": {"m1": "\u0639\u0642\u0644 \u062f\u0627\u0646\u062f \u0628\u0647 \u0639\u0642\u0644 \u0628\u0627\u0632 \u0634\u0646\u0627\u062e\u062a", "m2": "\u062f\u06cc\u062f\u0647 \u0631\u0627 \u062c\u0632 \u0628\u0647 \u062f\u06cc\u062f\u0647 \u0646\u062a\u0648\u0627\u0646 \u06cc\u0627\u0641\u062a"}, "12": {"m1": "\u06a9\u0647 \u06cc\u06a9\u06cc \u0634\u0645\u0639 \u0632\u0646\u062f\u0647 \u06a9\u0631\u062f \u0628\u0647 \u0628\u0627\u063a", "m2": "\u0628\u0647 \u06cc\u06a9\u06cc \u0628\u0648\u0633\u0647 \u0635\u062f\u0647\u0632\u0627\u0631 \u0686\u0631\u0627\u063a"}, "13": {"m1": "\u06af\u0631 \u06a9\u0633\u06cc \u0627\u0632 \u0627\u062b\u06cc\u0631 \u0628\u0631\u06af\u0630\u0631\u062f", "m2": "\u062f\u0648\u0631\u0628\u06cc\u0646 \u0632\u0627\u0646 \u0628\u064f\u0648\u064e\u062f \u06a9\u0647 \u062f\u06cc\u062f\u0647 \u062e\u0648\u0631\u062f"}, "14": {"m1": "\u062c\u0646\u0633 \u0627\u0632 \u062c\u0646\u0633 \u0628\u0627\u0632 \u062f\u0627\u0631\u062f \u0631\u0646\u062c", "m2": "\u06a9\u0647 \u062a\u0631\u0627\u0632\u0648 \u0628\u064f\u0648\u064e\u062f \u062a\u0631\u0627\u0632\u0648 \u0633\u0646\u062c"}, "15": {"m1": "\u0645\u0628\u0631\u062f \u0627\u0631 \u0686\u0646\u062f \u0686\u06cc\u0632\u0647\u0627 \u0633\u0627\u06cc\u062f", "m2": "\u0645\u0628\u0631\u062f \u062f\u06cc\u06af\u0631\u0634 \u0628\u0641\u0631\u0633\u0627\u06cc\u062f"}, "16": {"m1": "\u0628\u0627 \u06af\u0631\u0627\u0646\u062c\u0627\u0646 \u0645\u06af\u0648\u06cc \u0647\u0631\u06af\u0632 \u0631\u0627\u0632", "m2": "\u06a9\u0627\u0633\u06cc\u0627 \u0686\u0648\u0646 \u062f\u0648 \u0634\u062f \u0634\u0648\u062f \u063a\u0645\u0651\u0627\u0632"}, "17": {"m1": "\u0627\u0646\u062f\u0631\u06cc\u0646 \u062e\u0631 \u0633\u0631\u0627\u06cc \u0646\u0648\u06cc\u06cc \u062a\u0648", "m2": "\u0628\u0647 \u0686\u0647 \u0645\u0627\u0646\u06cc \u0645\u0631\u0627 \u0646\u06af\u0648\u06cc\u06cc \u062a\u0648"}, "18": {"m1": "\u062e\u0631\u0650 \u0639\u06cc\u0633\u06cc \u06af\u0631\u0633\u0646\u0647 \u0628\u0631 \u0622\u062e\u064f\u0631", "m2": "\u062f\u0627\u0645\u0646 \u0631\u0627\u0647 \u06a9\u0647\u06a9\u0634\u0627\u0646 \u067e\u0631 \u062f\u064f\u0631"}, "19": {"m1": "\u0627\u0631 \u0628\u0633\u0627\u0646 \u0630\u0628\u0627\u0628 \u0645\u0627\u0646\u062f\u06cc \u0628\u0627\u0632", "m2": "\u0686\u06a9\u0646\u06cc \u062a\u062e\u0645 \u062e\u0634\u0645 \u0648 \u0634\u0647\u0648\u062a \u0648 \u0622\u0632"}, "20": {"m1": "\u062f\u0633\u062a \u062f\u06cc\u0648\u0627\u0646 \u06af\u0634\u0627\u062f\u0647 \u062e\u0627\u062a\u0645 \u062c\u0645", "m2": "\u062e\u0648\u0627\u0628 \u0634\u0647 \u0628\u0633\u062a\u0647 \u0634\u0628 \u0628\u0647 \u0633\u062d\u0631 \u0648 \u0628\u0647 \u062f\u0645"}, "21": {"m1": "\u06cc\u0627\u0631 \u062f\u0631 \u0631\u0627\u0647 \u0686\u0648\u0646 \u0631\u0648\u0627\u0646 \u0628\u0627\u0634\u062f", "m2": "\u0628\u06cc\u200c\u0631\u0648\u0627\u0646 \u0645\u0631\u062f \u0686\u0648\u0646 \u0631\u0648\u0627\u0646 \u0628\u0627\u0634\u062f"}, "22": {"m1": "\u062f\u0648\u0633\u062a\u0627\u0646 \u062f\u0631 \u0631\u0647 \u0635\u0644\u0627\u062d \u0648 \u0635\u0648\u0627\u0628", "m2": "\u06cc\u06a9\u062f\u06af\u0631 \u0631\u0627 \u0645\u062f\u062f \u0628\u0648\u0646\u062f \u0686\u0648 \u0622\u0628"}, "23": {"m1": "\u0645\u0631\u062f \u0628\u0627\u06cc\u062f \u06a9\u0647 \u0627\u0647\u0644 \u062f\u06cc\u062f\u0647 \u0628\u064f\u0648\u064e\u062f", "m2": "\u062a\u0627 \u062f\u0631 \u0627\u06cc\u0646 \u0631\u0627\u0647 \u062d\u0642 \u06af\u0632\u06cc\u062f\u0647 \u0628\u064f\u0648\u064e\u062f"}, "24": {"m1": "\u0686\u0648\u0646 \u0646\u062f\u0627\u0631\u062f \u0628\u0635\u0627\u0631\u062a \u0627\u0646\u062f\u0631 \u06a9\u0627\u0631", "m2": "\u0646\u0634\u0646\u0648\u062f\u0647 \u0627\u0633\u062a \u06cc\u0627 \u0627\u0648\u0644\u06cc\u200c\u0627\u0644\u0627\u0628\u0635\u0627\u0631"}, "25": {"m1": "\u062f\u06cc\u062f\u0647\u0654 \u062f\u0644 \u062a\u0631\u0627 \u0686\u0648 \u0646\u06cc\u0633\u062a \u0642\u0631\u06cc\u0631", "m2": "\u0646\u06cc\u0633\u062a\u06cc \u062f\u0631 \u0646\u0647\u0627\u062f \u06a9\u0627\u0631 \u0628\u0635\u06cc\u0631"}, "26": {"m1": "\u0627\u0647\u0644 \u062f\u06cc\u0646 \u0631\u0627 \u062c\u0632 \u0627\u0647\u0644 \u062f\u06cc\u0646 \u0646\u06af\u0632\u06cc\u062f", "m2": "\u062f\u06cc\u062f \u0631\u0627 \u062c\u0632 \u0628\u0647 \u062f\u06cc\u062f\u0647 \u0646\u062a\u0648\u0627\u0646 \u062f\u06cc\u062f"}, "27": {"m1": "\u06cc\u0627\u0631 \u0646\u0627\u062c\u0646\u0633 \u062a\u062e\u0645 \u062e\u0648\u0627\u0628 \u0622\u0645\u062f", "m2": "\u06cc\u0627\u0631 \u0647\u0645\u062c\u0646\u0633 \u067e\u0627\u06cc \u0622\u0628 \u0622\u0645\u062f"}, "28": {"m1": "\u062f\u0648\u0633\u062a\u0627\u0646 \u0647\u0645\u0686\u0648 \u0622\u0628 \u0631\u0647 \u0633\u067e\u0631\u0646\u062f", "m2": "\u06a9\u0622\u0628\u0647\u0627 \u067e\u0627\u06cc\u0647\u0627\u06cc \u06cc\u06a9\u062f\u06af\u0631\u0646\u062f"}, "29": {"m1": "\u0631\u0627\u0647 \u0628\u06cc\u200c\u06cc\u0627\u0631 \u0632\u0641\u062a \u0628\u0627\u0634\u062f \u0632\u0641\u062a", "m2": "\u062c\u0632 \u0628\u0647 \u0622\u0628 \u0622\u0628 \u06a9\u06cc \u062a\u0648\u0627\u0646\u062f \u0631\u0641\u062a"}, "30": {"m1": "\u0628\u0627 \u0631\u0641\u06cc\u0642\u0627\u0646 \u0633\u0641\u0631 \u0645\u0642\u0631 \u0628\u0627\u0634\u062f", "m2": "\u0628\u06cc\u200c\u0631\u0641\u06cc\u0642\u0627\u0646 \u0645\u0642\u0631 \u0633\u0642\u0631 \u0628\u0627\u0634\u062f"}, "31": {"m1": "\u0628\u0633 \u0646\u06a9\u0648 \u06af\u0641\u062a\u0647\u200c\u0627\u0646\u062f \u0647\u0634\u06cc\u0627\u0631\u0627\u0646", "m2": "\u062e\u0627\u0646\u0647 \u0631\u0627 \u0631\u0627\u0647 \u0648 \u0631\u0627\u0647 \u0631\u0627 \u06cc\u0627\u0631\u0627\u0646"}, "32": {"m1": "\u06a9\u0627\u0631 \u0628\u062f \u0647\u0631 \u06a9\u0647 \u0631\u0627 \u0631\u0641\u06cc\u0642 \u0628\u062f\u0633\u062a", "m2": "\u0632\u0627\u0646\u06a9\u0647 \u0628\u062f \u0631\u0646\u06af \u0639\u0627\u062c\u0632 \u0627\u0632 \u062e\u0631\u062f\u0633\u062a"}, "33": {"m1": "\u0632\u06cc\u0646 \u062c\u0647\u0627\u0646 \u0647\u0645\u0647 \u0633\u0631\u0627\u0633\u0631 \u063a\u0645", "m2": "\u062f\u0644\u0645 \u0627\u0632 \u062f\u0644 \u06af\u0631\u0641\u062a \u0648 \u0627\u0632 \u062c\u0627\u0646 \u0647\u0645"}, "34": {"m1": "\u0622\u0646\u06a9\u0647 \u0632\u0648 \u0686\u0627\u0631\u0647 \u0646\u06cc\u0633\u062a \u06cc\u0627\u0631\u0634 \u062f\u0627\u0646", "m2": "\u0648\u0622\u0646\u06a9\u0647 \u0646\u0647 \u06cc\u0627\u0631\u0650 \u062a\u0633\u062a \u0628\u0627\u0631\u0634 \u062f\u0627\u0646"}, "35": {"m1": "\u062a\u0627\u0632\u06af\u06cc \u0633\u0631\u0648 \u0648 \u06af\u0644 \u0632 \u0628\u0627\u0631\u0627\u0646\u0633\u062a", "m2": "\u0632\u0646\u062f\u06af\u06cc \u062c\u0627\u0646 \u0648 \u062f\u0644 \u0632 \u06cc\u0627\u0631\u0627\u0646\u0633\u062a"}, "36": {"m1": "\u062f\u0648\u0633\u062a \u0631\u0627 \u06a9\u0633 \u0628\u0647 \u06cc\u06a9 \u0628\u0644\u0627 \u0646\u0641\u0631\u0648\u062e\u062a", "m2": "\u0628\u0647\u0631 \u06a9\u06cc\u06a9\u06cc \u06af\u0644\u06cc\u0645 \u0646\u062a\u0648\u0627\u0646 \u0633\u0648\u062e\u062a"}, "37": {"m1": "\u0622\u0628 \u0631\u0627 \u0686\u0648\u0646 \u0645\u062f\u062f \u0628\u0648\u062f \u0647\u0645 \u0627\u0632 \u0622\u0628", "m2": "\u06af\u0644\u0633\u062a\u0627\u0646 \u06af\u0631\u062f\u062f \u0622\u0646\u0686\u0647 \u0628\u0648\u062f \u062e\u0631\u0627\u0628"}, "38": {"m1": "\u067e\u0633 \u0627\u06af\u0631 \u0627\u06cc\u0646 \u0645\u062f\u062f \u0628\u0631\u06cc\u062f\u0647 \u0634\u0648\u062f", "m2": "\u0645\u06cc\u0648\u0647 \u0628\u0631 \u0634\u0627\u062e \u067e\u0698\u0645\u0631\u06cc\u062f\u0647 \u0634\u0648\u062f"}, "39": {"m1": "\u0631\u0627\u0647 \u0628\u06cc\u200c\u06cc\u0627\u0631 \u0646\u06cc\u06a9 \u0646\u062a\u0648\u0627\u0646 \u0631\u0641\u062a", "m2": "\u0648\u0631\u0646\u0647 \u067e\u06cc\u0634 \u0622\u06cc\u062f\u062a \u0647\u0632\u0627\u0631 \u0622\u06a9\u0641\u062a"}, "40": {"m1": "\u06cc\u0627\u0631 \u0646\u06cc\u06a9 \u0627\u0646\u062f\u0631\u06cc\u0646 \u0632\u0645\u0627\u0646\u0647 \u06a9\u0645\u0633\u062a", "m2": "\u0632\u0627\u0646\u06a9\u0647 \u063a\u062b\u0651 \u0648 \u0633\u0645\u06cc\u0646 \u06a9\u0646\u0648\u0646 \u0628\u0647\u0645\u0633\u062a"}}}