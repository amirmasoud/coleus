{"text": {"0": {"m1": "\u0645\u0644\u06a9 \u0634\u0627\u0639\u0631\u0627\u0646 \u0628\u0646\u0638\u0645 \u0648 \u0628\u0646\u062b\u0631", "m2": "\u062e\u0627\u062f\u0645 \u0635\u062f\u0631 \u062e\u0648\u062f \u060c \u0645\u0631\u0627 \u0628\u0633\u062a\u0648\u062f"}, "1": {"m1": "\u062a\u0627 \u0628\u062f\u0627\u0646 \u0646\u0638\u0645 \u0648 \u0646\u062b\u0631 \u062d\u0631\u0645\u062a \u0627\u0648", "m2": "\u0646\u0632\u062f \u0627\u0634\u0631\u0627\u0641 \u0645\u0645\u0644\u06a9\u062a \u0628\u0641\u0632\u0648\u062f"}, "2": {"m1": "\u0646\u0638\u0645 \u0648 \u0646\u062b\u0631\u06cc\u060c \u06a9\u0647 \u062f\u0631 \u0637\u0631\u0627\u0648\u062a \u0648 \u0644\u0637\u0641", "m2": "\u0647\u06cc\u0686 \u06af\u0648\u0634\u06cc \u0646\u0638\u06cc\u0631 \u0622\u0646 \u0646\u0634\u0646\u0648\u062f"}, "3": {"m1": "\u062f\u0631\u062c \u0633\u062d\u0631\u06cc \u060c \u06a9\u0632\u0648 \u0628\u0628\u0646\u062f\u0647 \u0631\u0633\u06cc\u062f", "m2": "\u062f\u0631\u062c \u06a9\u06cc \u0628\u0648\u062f\u061f \u062f\u0631\u062c \u06af\u0648\u0647\u0631 \u0628\u0648\u062f"}, "4": {"m1": "\u062c\u0627\u0646 \u0628\u06cc\u0627\u0633\u0648\u062f \u0627\u0632 \u0622\u0646 \u062e\u0637\u0627\u0628 \u0648 \u0644\u06cc\u06a9", "m2": "\u0644\u062d\u0638\u0647 \u0627\u06cc \u0627\u0632 \u062b\u0646\u0627 \u0632\u0628\u0627\u0646 \u0646\u0627\u0633\u0648\u062f"}, "5": {"m1": "\u0628\u0631 \u0628\u06cc\u0627\u0636 \u0648 \u0633\u0648\u0627\u062f \u0627\u0648 \u06a9\u0631\u062f\u0646\u062f", "m2": "\u0622\u0641\u0631\u06cc\u0646 \u0627\u062e\u062a\u0631\u0627\u0646 \u0686\u0631\u062e \u06a9\u0628\u0648\u062f"}, "6": {"m1": "\u0634\u062f \u062a\u0631\u0627\u0632 \u0645\u0641\u0627\u062e\u0631 \u0628\u0646\u062f\u0647", "m2": "\u0627\u06cc\u0646 \u062e\u062f\u0627\u0648\u0646\u062f\u06cc\u06cc\u060c \u06a9\u0647 \u0627\u0648 \u0641\u0631\u0645\u0648\u062f"}, "7": {"m1": "\u0628\u0627\u0631 \u0645\u062d\u0646\u062a \u0632 \u0634\u062e\u0635 \u0645\u0646 \u0628\u0631\u062f\u0627\u0634\u062a", "m2": "\u0632\u0646\u06af \u0627\u0646\u062f\u0647 \u0632 \u062c\u0627\u0646 \u0645\u0646 \u0628\u0632\u062f\u0648\u062f"}, "8": {"m1": "\u0627\u06cc \u0628\u0632\u0631\u06af\u06cc \u06a9\u0647 \u062f\u0631 \u06a9\u0645\u0627\u0644 \u0648 \u0647\u0646\u0631", "m2": "\u0686\u0631\u062e \u06af\u0631\u062f\u0627\u0646 \u0646\u0638\u06cc\u0631 \u062a\u0648 \u0646\u0646\u0645\u0648\u062f"}, "9": {"m1": "\u0686\u0634\u0645 \u0639\u0642\u0644 \u062a\u0648 \u0631\u0627\u0632 \u062f\u0647\u0631 \u0628\u062f\u06cc\u062f", "m2": "\u067e\u0627\u06cc \u0642\u062f\u0631 \u062a\u0648 \u0627\u0648\u062c \u0686\u0631\u062e \u0628\u0633\u0648\u062f"}, "10": {"m1": "\u062f\u0631 \u0639\u0644\u0648 \u062f\u0633\u062a \u0647\u0645\u062a \u0639\u0627\u0644\u06cc\u062a", "m2": "\u0642\u0635\u0628 \u0633\u0628\u0642 \u0627\u0632 \u0622\u0633\u0645\u0627\u0646 \u0628\u0631\u0628\u0648\u062f"}, "11": {"m1": "\u062f\u0627\u0646\u0634\u062a \u0631\u0627 \u0639\u062f\u0648 \u0646\u06a9\u0631\u062f \u0627\u0646\u06a9\u0627\u0631", "m2": "\u06a9\u0633 \u0628\u06af\u0644 \u0622\u0641\u062a\u0627\u0628 \u0631\u0627 \u0646\u0646\u062f\u0648\u062f"}, "12": {"m1": "\u062f\u0627\u0646\u062f \u0627\u06cc\u0632\u062f \u06a9\u0647: \u0634\u062e\u0635 \u0645\u0646 \u0628\u0646\u062f\u0647", "m2": "\u0632\u06cc\u0631 \u067e\u0627\u06cc \u0641\u0631\u0627\u0642 \u062a\u0648 \u0641\u0631\u0633\u0648\u062f"}, "13": {"m1": "\u0639\u06cc\u0634\u0645 \u0627\u0632 \u0628\u0627\u062f \u0627\u0646\u062f\u0647\u0627\u0646 \u067e\u0698\u0645\u0631\u062f", "m2": "\u0631\u0648\u06cc\u0645 \u0627\u0632 \u062e\u0648\u0646 \u062f\u06cc\u062f\u06af\u0627\u0646 \u0622\u0644\u0648\u062f"}, "14": {"m1": "\u062f\u0631 \u062f\u0644\u0645 \u0622\u062a\u0634 \u063a\u0645\u06cc\u0633\u062a\u060c \u06a9\u0632\u0648", "m2": "\u0646\u0641\u0633\u0645  \u067e\u0631 \u0634\u0631\u0627\u0631 \u06af\u0634\u062a \u0686\u0648 \u062f\u0648\u062f"}, "15": {"m1": "\u062f\u06cc\u062f\u0647\u0654 \u0645\u0646 \u0627\u06af\u0631 \u063a\u0646\u062f\u0648\u0633\u062a\u06cc", "m2": "\u062c\u0632 \u0628\u0631\u0627\u06cc \u062e\u06cc\u0627\u0644 \u062a\u0648 \u0646\u063a\u0646\u0648\u062f"}, "16": {"m1": "\u0645\u0646 \u0632 \u062a\u0648 \u062f\u0648\u0631\u0645 \u0648 \u0628\u062f\u0648 \u0645\u0639\u0646\u06cc", "m2": "\u0647\u0633\u062a\u0645 \u0627\u0632 \u0628\u062e\u062a \u062e\u0648\u06cc\u0634 \u0646\u0627\u062e\u0634\u0646\u0648\u062f"}, "17": {"m1": "\u0627\u06cc \u062f\u0631\u06cc\u063a\u0627! \u0647\u0645\u06cc \u0632\u0646\u0645\u060c \u0644\u06cc\u06a9\u0646", "m2": "\u0627\u06cc \u062f\u0631\u06cc\u063a\u0627! \u06a9\u0647 \u0645\u06cc \u0646\u062f\u0627\u0631\u062f \u0633\u0648\u062f"}, "18": {"m1": "\u0628\u0627\u0632 \u062e\u0631 \u0627\u0632 \u0641\u0631\u0627\u0642 \u062e\u0648\u06cc\u0634 \u0645\u0631\u0627", "m2": "\u06a9\u0647 \u062f\u0644\u0645 \u062f\u0631 \u0641\u0631\u0627\u0642 \u062a\u0648 \u067e\u0627\u0644\u0648\u062f"}, "19": {"m1": "\u0645\u0644\u06a9 \u0634\u0627\u0639\u0631\u0627\u0646 \u062a\u0648\u06cc\u06cc\u060c \u0644\u0627\u0628\u062f", "m2": "\u0628\u0631 \u0631\u0639\u0627\u06cc\u0627 \u0628\u0628\u0627\u06cc\u062f\u062a \u0628\u062e\u0634\u0648\u062f"}, "20": {"m1": "\u062f\u0627\u0631\u0645 \u0627\u0632 \u0641\u0636\u0644 \u062d\u0642 \u0637\u0645\u0639 \u06a9\u0647 : \u0645\u0631\u0627", "m2": "\u0628\u0631\u0633\u0627\u0646\u062f \u0628\u0628\u0627\u0631\u06af\u0627\u0647 \u062a\u0648 \u0632\u0648\u062f"}}}