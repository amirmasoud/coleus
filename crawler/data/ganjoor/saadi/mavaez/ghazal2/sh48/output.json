{"text": {"0": {"m1": "\u062e\u062f\u0627\u0648\u0646\u062f\u06cc \u0686\u0646\u06cc\u0646 \u0628\u062e\u0634\u0646\u062f\u0647 \u062f\u0627\u0631\u06cc\u0645", "m2": "\u06a9\u0647 \u0628\u0627 \u0686\u0646\u062f\u06cc\u0646 \u06af\u0646\u0647 \u0627\u0645\u06cc\u062f\u0648\u0627\u0631\u06cc\u0645"}, "1": {"m1": "\u06a9\u0647 \u0628\u06af\u0634\u0627\u06cc\u062f \u062f\u0631\u06cc \u06a9\u0627\u06cc\u0632\u062f \u0628\u0628\u0646\u062f\u062f", "m2": "\u0628\u06cc\u0627 \u062a\u0627 \u0647\u0645 \u0628\u062f\u06cc\u0646 \u062f\u0631\u06af\u0647 \u0628\u0632\u0627\u0631\u06cc\u0645"}, "2": {"m1": "\u062e\u062f\u0627\u06cc\u0627 \u06af\u0631 \u0628\u062e\u0648\u0627\u0646\u06cc \u0648\u0631 \u0628\u0631\u0627\u0646\u06cc", "m2": "\u062c\u0632 \u0627\u0646\u0639\u0627\u0645\u062a \u062f\u0631\u06cc \u062f\u06cc\u06af\u0631 \u0646\u062f\u0627\u0631\u06cc\u0645"}, "3": {"m1": "\u0633\u0631\u0627\u0641\u0631\u0627\u0632\u06cc\u0645 \u0627\u06af\u0631 \u0628\u0631 \u0628\u0646\u062f\u0647 \u0628\u062e\u0634\u06cc", "m2": "\u0648\u06af\u0631\u0646\u0647 \u0627\u0632 \u06af\u0646\u0647 \u0633\u0631 \u0628\u0631\u0646\u06cc\u0627\u0631\u06cc\u0645"}, "4": {"m1": "\u0632 \u0645\u0634\u062a\u06cc \u062e\u0627\u06a9 \u0645\u0627 \u0631\u0627 \u0622\u0641\u0631\u06cc\u062f\u06cc", "m2": "\u0686\u06af\u0648\u0646\u0647 \u0634\u06a9\u0631 \u0627\u06cc\u0646 \u0646\u0639\u0645\u062a \u06af\u0632\u0627\u0631\u06cc\u0645"}, "5": {"m1": "\u062a\u0648 \u0628\u062e\u0634\u06cc\u062f\u06cc \u0631\u0648\u0627\u0646 \u0648 \u0639\u0642\u0644 \u0648 \u0627\u06cc\u0645\u0627\u0646", "m2": "\u0648\u06af\u0631\u0646\u0647 \u0645\u0627 \u0647\u0645\u0627\u0646 \u0645\u0634\u062a\u06cc \u063a\u0628\u0627\u0631\u06cc\u0645"}, "6": {"m1": "\u062a\u0648 \u0628\u0627 \u0645\u0627 \u0631\u0648\u0632 \u0648 \u0634\u0628 \u062f\u0631 \u062e\u0644\u0648\u062a \u0648 \u0645\u0627", "m2": "\u0634\u0628 \u0648 \u0631\u0648\u0632\u06cc \u0628\u0647 \u063a\u0641\u0644\u062a \u0645\u06cc\u200c\u06af\u0630\u0627\u0631\u06cc\u0645"}, "7": {"m1": "\u0646\u06af\u0648\u06cc\u0645 \u062e\u062f\u0645\u062a \u0622\u0648\u0631\u062f\u06cc\u0645 \u0648 \u0637\u0627\u0639\u062a", "m2": "\u06a9\u0647 \u0627\u0632 \u062a\u0642\u0635\u06cc\u0631 \u062e\u062f\u0645\u062a \u0634\u0631\u0645\u0633\u0627\u0631\u06cc\u0645"}, "8": {"m1": "\u0645\u0628\u0627\u062f \u0622\u0646 \u0631\u0648\u0632 \u06a9\u0632 \u062f\u0631\u06af\u0627\u0647 \u0644\u0637\u0641\u062a", "m2": "\u0628\u0647 \u062f\u0633\u062a \u0646\u0627\u0627\u0645\u06cc\u062f\u06cc \u0633\u0631 \u0628\u062e\u0627\u0631\u06cc\u0645"}, "9": {"m1": "\u062e\u062f\u0627\u0648\u0646\u062f\u0627 \u0628\u0647 \u0644\u0637\u0641\u062a \u0628\u0627\u0635\u0644\u0627\u062d \u0622\u0631", "m2": "\u06a9\u0647 \u0645\u0633\u06a9\u06cc\u0646 \u0648 \u067e\u0631\u06cc\u0634\u0627\u0646 \u0631\u0648\u0632\u06af\u0627\u0631\u06cc\u0645"}, "10": {"m1": "\u0632 \u062f\u0631\u0648\u06cc\u0634\u0627\u0646 \u06a9\u0648\u06cc \u0627\u0646\u06af\u0627\u0631 \u0645\u0627 \u0631\u0627", "m2": "\u06af\u0631 \u0627\u0632 \u062e\u0627\u0635\u0627\u0646 \u062d\u0636\u0631\u062a \u0628\u0631\u06a9\u0646\u0627\u0631\u06cc\u0645"}, "11": {"m1": "\u0646\u062f\u0627\u0646\u0645 \u062f\u06cc\u062f\u0646\u0634 \u0631\u0627 \u062e\u0648\u062f \u0635\u0641\u062a \u0686\u06cc\u0633\u062a", "m2": "\u062c\u0632 \u0627\u06cc\u0646 \u0631\u0627 \u06a9\u0632 \u0633\u0645\u0627\u0639\u0634 \u0628\u06cc\u0642\u0631\u0627\u0631\u06cc\u0645"}, "12": {"m1": "\u0634\u0631\u0627\u0628\u06cc \u062f\u0631 \u0627\u0632\u0644 \u062f\u0631\u062f\u0627\u062f \u0645\u0627 \u0631\u0627", "m2": "\u0647\u0646\u0648\u0632 \u0627\u0632 \u062a\u0627\u0628 \u0622\u0646 \u0645\u06cc \u062f\u0631 \u062e\u0645\u0627\u0631\u06cc\u0645"}, "13": {"m1": "\u0686\u0648 \u0639\u0642\u0644 \u0627\u0646\u062f\u0631 \u0646\u0645\u06cc\u200c\u06af\u0646\u062c\u06cc\u062f \u0633\u0639\u062f\u06cc", "m2": "\u0628\u06cc\u0627 \u062a\u0627 \u0633\u0631 \u0628\u0647 \u0634\u06cc\u062f\u0627\u06cc\u06cc \u0628\u0631\u0622\u0631\u06cc\u0645"}}}