{"text": {"0": {"m1": "\u062f\u0644 \u0645\u0627\u060c \u0686\u0648\u0646 \u0686\u0631\u0627\u063a \u0639\u0634\u0642 \u0627\u0641\u0631\u0648\u062e\u062a", "m2": "\u062e\u0631\u0645\u0646 \u062e\u0648\u06cc\u0634\u062a\u0646 \u0628\u0647 \u0639\u0634\u0642 \u0628\u0633\u0648\u062e\u062a"}, "1": {"m1": "\u0627\u0646\u062c\u0645 \u0627\u0641\u0631\u0648\u0632 \u0627\u0646\u062f\u0631\u0648\u0646 \u0639\u0634\u0642 \u0627\u0633\u062a", "m2": "\u0639\u0644\u062a \u062d\u06a9\u0645 \u06a9\u0627\u0641 \u0648 \u0646\u0648\u0646 \u0639\u0634\u0642 \u0627\u0633\u062a"}, "2": {"m1": "\u0686\u0648\u0646 \u0632 \u0642\u0648\u062a \u0633\u0648\u06cc \u06a9\u0645\u0627\u0644 \u0622\u0645\u062f", "m2": "\u06a9\u0631\u0633\u06cc \u062a\u062e\u062a \u0644\u0627\u06cc\u0632\u0627\u0644 \u0622\u0645\u062f"}, "3": {"m1": "\u0639\u0634\u0642 \u0645\u0639\u0646\u06cc \u0635\u0631\u0627\u0637 \u0639\u0634\u0627\u0642 \u0627\u0633\u062a", "m2": "\u0639\u0634\u0642 \u0635\u0648\u0631\u062a \u0631\u0628\u0627\u0637 \u0639\u0634\u0627\u0642 \u0627\u0633\u062a"}, "4": {"m1": "\u062a\u0627 \u0627\u0632\u06cc\u0646 \u0631\u0627\u0647 \u0628\u0631 \u06a9\u0631\u0627\u0646 \u0646\u0634\u0648\u06cc", "m2": "\u062f\u0631 \u062e\u0648\u0631 \u062e\u06cc\u0644 \u0635\u0627\u062f\u0642\u0627\u0646 \u0646\u0634\u0648\u06cc"}, "5": {"m1": "\u0686\u0648\u0646 \u062a\u0648\u06cc\u06cc \u0635\u0648\u0631\u062a \u0648 \u062a\u0648\u06cc\u06cc \u0645\u0639\u0646\u06cc", "m2": "\u0645\u06a9\u0646 \u0627\u0632 \u0639\u0634\u0642 \u062e\u0648\u06cc\u0634\u062a\u0646 \u062f\u0639\u0648\u06cc"}, "6": {"m1": "\u062e\u0648\u06cc\u0634\u062a\u0646 \u0631\u0627 \u0645\u0628\u06cc\u0646\u060c \u0686\u0648 \u0639\u0634\u0642 \u0622\u0645\u062f", "m2": "\u0634\u0631\u0628\u062a \u0639\u0634\u0642 \u0628\u06cc\u200c\u062e\u0648\u062f \u0622\u0634\u0627\u0645\u062f"}, "7": {"m1": "\u0647\u0631 \u06a9\u0647 \u0632\u06cc\u0646 \u0628\u0627\u062f\u0647 \u062c\u0631\u0639\u0647\u200c\u0627\u06cc \u0628\u062e\u0648\u0631\u062f", "m2": "\u0628\u0647 \u062a\u0646 \u0648 \u062c\u0627\u0646 \u062e\u0648\u06cc\u0634 \u06a9\u06cc \u0646\u06af\u0631\u062f\u061f"}, "8": {"m1": "\u0627\u0646\u062f\u0631\u0648\u0646\u06cc \u06a9\u0647 \u062f\u0631\u062f \u0627\u0648 \u062f\u0627\u0631\u062f", "m2": "\u0647\u0631\u06af\u0632 \u0627\u0648 \u0631\u0627 \u0632\u06cc\u0627\u062f \u0646\u06af\u0630\u0627\u0631\u062f"}, "9": {"m1": "\u0647\u0631 \u0645\u062d\u0628\u062a\u060c \u06a9\u0647 \u062f\u0631 \u062f\u0644\u06cc \u067e\u06cc\u062f\u0627\u0633\u062a", "m2": "\u0628\u06cc \u0634\u06a9 \u0622\u0646 \u0627\u0646\u0642\u0637\u0627\u0639 \u063a\u06cc\u0631 \u062e\u062f\u0627\u0633\u062a"}, "10": {"m1": "\u0627\u0628\u062c\u062f \u0639\u0634\u0642\u060c \u0647\u0631 \u06a9\u0647 \u062e\u0648\u0627\u0647\u0646\u062f \u0646\u062e\u0633\u062a", "m2": "\u0632 \u0622\u0646\u0686\u0647 \u0622\u0645\u0648\u062e\u062a \u0644\u0648\u062d \u0630\u0647\u0646 \u0628\u0634\u0633\u062a"}, "11": {"m1": "\u0686\u0648\u0646 \u062f\u0644\u062a \u062a\u062e\u062a\u0647 \u0631\u0627 \u0641\u0631\u0648 \u0634\u0648\u06cc\u062f", "m2": "\u0628\u0627 \u062a\u0648 \u0627\u06cc\u0646 \u0631\u0627\u0632 \u062e\u0648\u062f \u062f\u0644\u062a \u06af\u0648\u06cc\u062f"}, "12": {"m1": "\u0627\u06cc \u062f\u0644\u060c \u0627\u06cc \u062f\u0644\u060c \u062e\u0645\u06cc\u0631 \u0645\u0627\u06cc\u0647 \u062a\u0648\u06cc\u06cc", "m2": "\u0637\u0641\u0644 \u0631\u0627 \u0647\u0633\u062a \u0634\u06cc\u0631 \u0648 \u062f\u0627\u06cc\u0647 \u062a\u0648\u06cc\u06cc"}, "13": {"m1": "\u062c\u0627\u06cc \u0639\u0634\u0642\u06cc \u0648 \u062c\u0627\u06cc \u0645\u0639\u0634\u0648\u0642\u06cc", "m2": "\u0647\u0645\u06af\u06cc \u0627\u0632 \u0628\u0631\u0627\u06cc \u0645\u0639\u0634\u0648\u0642\u06cc"}, "14": {"m1": "\u0645\u06cc\u200c\u0631\u0648\u06cc \u062f\u0631 \u0633\u0631\u0627\u06cc \u062e\u0633\u062a\u0647\u200c\u062f\u0644\u0627\u0646", "m2": "\u0627\u06cc\u0646 \u06a9\u0631\u0645 \u0628\u06cc\u0646 \u062a\u0648 \u0628\u0627 \u0634\u06a9\u0633\u062a\u0647\u200c\u062f\u0644\u0627\u0646"}, "15": {"m1": "\u0645\u0646\u0632\u0644\u0634 \u062f\u0644 \u0634\u062f \u0648 \u0647\u0648\u0627\u06cc\u0634 \u0639\u0634\u0642", "m2": "\u062f\u0648\u0633\u062a\u0634 \u062f\u0644 \u0634\u062f\u060c \u0622\u0634\u0646\u0627\u06cc\u0634 \u0639\u0634\u0642"}}}